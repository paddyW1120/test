tem>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item :label=\"$t('userView.table_password')+':'\" prop=\"pwd\">\r\n              <el-input v-model=\"form.pwd\" :placeholder=\"$t('userView.input')+$t('userView.table_password')\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item :label=\"$t('userView.table_gender')+':'\" prop=\"gender\">\r\n              <el-select v-model=\"form.gender\" :placeholder=\"$t('i18nView.selectPlaceholder')+$t('userView.table_gender')\"\r\n                         class=\"filter-item\">\r\n                <el-option v-for=\"item in genderList\" :key=\"item.key\" :label=\"item.name\"\r\n                           :value=\"item.key\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item :label=\"$t('userView.table_phone')+':'\" prop=\"phone\">\r\n              <el-input v-model=\"form.phone\" :placeholder=\"$t('userView.input')+$t('userView.table_phone')\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item :label=\"$t('userView.table_mail')+':'\" prop=\"email\">\r\n              <el-input v-model=\"form.email\" :placeholder=\"$t('userView.input')+$t('userView.table_mail')\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item :label=\"$t('userView.weComId')+':'\" prop=\"weComId\">\r\n              <el-input v-model=\"form.weComId\" :placeholder=\"$t('userView.input')+$t('userView.weComId')\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"20\"><el-col :span=\"12\">\r\n          <el-form-item :label=\"$t('userView.table_state')+':'\" prop=\"userState\">\r\n            <el-select v-model=\"form.userState\" :placeholder=\"$t('i18nView.selectPlaceholder')+$t('userView.table_state')\"\r\n                       class=\"filter-item\">\r\n              <el-option v-for=\"item in stateList\" :key=\"item.key\" :label=\"item.name\"\r\n                         :value=\"item.key\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col></el-row>\r\n        <el-row :gutter=\"20\"><el-col :span=\"12\">\r\n          <el-form-item :label=\"$t('userView.table_avatar')+':'\" prop=\"avatar\">\r\n            <el-upload :auto-upload=\"false\" :before-upload=\"beforeAvatarUpload\" :on-change='changeUpload'\r\n                       :show-file-list=\"false\"\r\n                       action=\"#\" class=\"avatar-uploader\">\r\n              <img v-if=\"form.avatar\" :src=\"form.avatar\" class=\"avatar\">\r\n              <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n            </el-upload>\r\n          </el-form-item>\r\n        </el-col></el-row>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button size=\"small\" icon='el-icon-circle-close' type=\"primary\" @click=\"dialogVisible = false\"> {{ $t('table.cancel') }} </el-button>\r\n          <el-button v-auth=\"'sys-user:add'\" class=\"el-icon-success\" size=\"small\" type=\"primary\" @click=\"submitForm\"> {{\r\n              $t('table.confirm')\r\n            }} </el-button>\r\n        </span>\r\n    </el-dialog>\r\n\r\n    <el-dialog\r\n      v-dialog-drag\r\n      :visible.sync=\"showEditRole\"\r\n      @close=\"showEditRole=false\">\r\n      <div>\r\n        <cus-page-title-back :isDialog=\"true\" @backTo=\"showEditRole=false\" :label=\"titleMap[dialogStatus]\" />\r\n        <el-table ref=\"multipleTable\" :data=\"roleConfigs\" :row-key=\"getRowKey\" align=\"center\"\r\n                  border class=\"wraper-table\"\r\n                  element-loading-text=\"Loading\"\r\n                  fit highlight-current-row size=\"mini\" @selection-change=\"handleSelectionChange\">\r\n          <el-table-column :reserve-selection=\"true\" type=\"selection\" width=\"50\"></el-table-column>\r\n          <el-table-column :label=\"$t('roleView.table_roleName')\" align=\"center\" prop=\"roleName\"></el-table-column>\r\n          <el-table-column :label=\"$t('roleView.table_roleCode')\" align=\"center\" prop=\"roleCode\"></el-table-column>\r\n        </el-table>\r\n        <!-- 分页 -->\r\n        <cus-pagination ref=\"rolePage\" v-model=\"roleConfigs\" :pageSize=\"10\" :params=\"Roleparams\" url=\"/sys-role/list\"/>\r\n      </div>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button size=\"small\" icon='el-icon-circle-close' type=\"primary\" @click=\"showEditRole = false\">取 消</el-button>\r\n        <el-button size=\"small\" type=\"primary\" class=\"el-icon-success\" @click=\"commitUserRole\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\n\r\n\r\nimport {addUser, selectRoleByUserId, updateUser,deleteUser,updateUserRole} from '@/api/system/user'\r\n\r\nexport default {\r\n  name: 'User',\r\n  data() {\r\n    return {\r\n      //静态数据\r\n      genderList: [{key: '1', name: this.$t('userView.table_man')}, {\r\n        key: '0',\r\n        name: this.$t('userView.table_woman')\r\n      }],\r\n      stateList: [{key: '1', name: this.$t('userView.table_enable')}, {\r\n        key: '0',\r\n        name: this.$t('userView.table_disable')\r\n      }],\r\n      moreCondition:false,\r\n      //表格数据\r\n      configs: [],\r\n      roleConfigs: [],// 角色表格数据\r\n      tenantConfigs: [],// 租户表格数据\r\n      multipleSelection: [],// 角色表格选中项\r\n      multipleTenantSelection: [],// 租户表格选中项\r\n      //查询参数可以是模糊或者等值 like_  eq_\r\n      userRoleList: [],// 用户对应的角色\r\n      userId: null, // 角色表格所对应的用户Id\r\n      isSuperAdmin: false, // 用户是否为超级管理员\r\n      tableKey: 0,\r\n      params: {\r\n        userState: '',\r\n        userName: '',\r\n        userCode: '',\r\n        phone: '',\r\n      },\r\n      optionsConfig: [],\r\n      Roleparams: {\r\n        like_roleName: '',\r\n        like_roleCode: ''\r\n      },\r\n      Tenantparams: {\r\n        like_comCode: '',\r\n        like_name: '',\r\n        like_shortName: ''\r\n      },\r\n      selectRoleParams: [],\r\n      roleParams: [], // 角色表提交参数\r\n      tenantparams: [], // 租户表提交参数\r\n      form: {  //表单数据与后端实体类对应\r\n        id: '', //主键ID\r\n        userCode: '', //账号\r\n        userName: '', //账号\r\n        pwd: '', //登录密码\r\n        gender: '', //性别 0:女 1:男\r\n        phone: '', //手机号码\r\n        email: '', //邮箱\r\n        avatar: '', //头像\r\n        userState: '', //状态\r\n        qqOpenId: '', //qq登录id\r\n        wxOpenId: '', //wx登录id\r\n        weComId: '' //wx登录id\r\n      },\r\n      rules: {\r\n        userCode: [\r\n          {required: true, pattern: /^[0-9a-zA-Z]{1,8}$/, message: this.$t('userView.ruleuserCode')},\r\n          {required: true, message: this.$t('userView.input') + this.$t('userView.table_userCode'), trigger: 'blur'},\r\n        ],\r\n        pwd: [\r\n          {required: true, pattern: /^(\\w){6,50}$/, message: this.$t('userView.rulepwd')}\r\n        ],\r\n        userName: [\r\n          {required: true, message: this.$t('userView.input') + this.$t('userView.table_userName'), trigger: 'blur'},\r\n        ],\r\n        userState: [\r\n          {\r\n            required: true,\r\n            message: this.$t('i18nView.selectPlaceholder') + this.$t('userView.table_state'),\r\n            trigger: 'blur'\r\n          },\r\n        ]\r\n      },\r\n      showEditRole: false,\r\n      showEditTenant: false,\r\n      dialogVisible: false, //dialog显示还是隐藏\r\n      dialogStatus: '', //表单状态是新增还是修改\r\n      titleMap: {\r\n        update: this.$t('userView.update'),\r\n        create: this.$t('userView.add'),\r\n        editRole: this.$t('userView.editRole'),\r\n        editTenant: this.$t('userView.editTenant')\r\n      },\r\n    }\r\n  },\r\n  computed: {},\r\n  created() {\r\n    let rolecodes = this.$store.getters.rolecodes\r\n    if (rolecodes.indexOf('superAdmin', 0) != -1) {\r\n      this.isSuperAdmin = true;\r\n    }\r\n  },\r\n  methods: {\r\n    handleSearch() {\r\n      this.$refs.page.reload()\r\n    },\r\n    handleCreate() {\r\n      this.resetForm()\r\n      this.dialogStatus = 'create'\r\n      this.dialogVisible = true\r\n    },\r\n    updateState(val, row) {\r\n      updateUser(row).then(res => {\r\n        this.notifySuccess('状态更新成功')\r\n      })\r\n    },\r\n    handleUpdate(row) {\r\n      this.form = Object.assign({}, row)\r\n      this.dialogStatus = 'update'\r\n      this.dialogVisible = true\r\n    },\r\n    handleUpdateRole(id) {\r\n      this.showEditRole = true;\r\n      this.dialogStatus = 'editRole'\r\n      this.userId = id;\r\n      this.multipleSelection = [];\r\n      selectRoleByUserId(id).then(res => {\r\n        this.multipleSelection = res.data\r\n        this.$refs.multipleTable.clearSelection()\r\n        res.data.forEach(row => {\r\n          this.$refs.multipleTable.toggleRowSelection(row, true);\r\n        })\r\n        this.$refs.rolePage.reload()\r\n      })\r\n    },\r\n    beforeAvatarUpload(file) {\r\n      const isJPG = file.type === 'image/jpeg'\r\n      const isLt2M = file.size / 1024 < 100\r\n\r\n      if (!isJPG) {\r\n        this.$message.error('上传头像图片只能是 JPG 格式!')\r\n      }\r\n      if (!isLt2M) {\r\n        this.$message.error('上传头像图片大小不能超过 100KB!')\r\n      }\r\n      return isJPG && isLt2M\r\n    },\r\n    changeUpload: function (file, fileList) {\r\n      let reader = new FileReader()\r\n      reader.onload = async (e) => {\r\n        this.form.avatar = e.target.result\r\n      }\r\n      reader.readAsDataURL(file.raw)\r\n    },\r\n    getRowKey(row) {\r\n      return row.id\r\n    },\r\n    handleSelectionChange(val) {\r\n      this.multipleSelection = val;\r\n    },\r\n    commitUserRole() {\r\n      let vm = this;\r\n      vm.roleParams = []\r\n      let tmp = []\r\n      for (let i = 0; i < vm.multipleSelection.length; i++) {\r\n        tmp.push(vm.multipleSelection[i].id)\r\n      }\r\n      let myset = new Set(tmp)\r\n      for (let val of myset) {\r\n        let roleParam = {\r\n          \"userId\": vm.userId,\r\n          \"roleId\": val\r\n        }\r\n        vm.roleParams.push(roleParam)\r\n      }\r\n      updateUserRole(vm.roleParams).then(response => {\r\n        this.notifySuccess(response.message)\r\n        vm.showEditRole = false;\r\n      })\r\n    },\r\n    handleDelete(id) {\r\n      deleteUser(id).then(response => {\r\n        this.handleSearch()\r\n        this.notifySuccess(response.message)\r\n        this.dialogVisible = false\r\n      })\r\n    },\r\n    submitForm() {\r\n      this.$refs.dataForm.validate(valid => {\r\n        if (valid) {\r\n          let back;\r\n          if (this.form.id) {\r\n            back = updateUser(this.form)\r\n          } else {\r\n            back = addUser(this.form)\r\n          }\r\n          back.then(response => {\r\n            this.handleSearch()\r\n            this.notifySuccess(response.message)\r\n            this.dialogVisible = false\r\n          })\r\n        }\r\n      })\r\n    },\r\n    resetForm() {\r\n      this.form = {  //表单数据与后端实体类对应\r\n        id: '', //主键ID\r\n        userCode: '', //账号\r\n        userName: '', //账号\r\n        pwd: '', //登录密码\r\n        gender: '', //性别 0:女 1:男\r\n        phone: '', //手机号码\r\n        email: '', //邮箱\r\n        avatar: '', //头像\r\n        userState: '', //状态\r\n        qqOpenId: '', //qq登录id\r\n        wxOpenId: '', //wx登录id\r\n        weComId: '', //wx登录id\r\n      }\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.spacing {\r\n  margin-bottom: 22px !important;\r\n}\r\n\r\n.avatar-uploader\r\n.el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n.avatar-uploader\r\n.el-upload:hover {\r\n  border-color: #409eff;\r\n}\r\n\r\n.avatar-uploader-icon {\r\n  border: 1px dashed #d9d9d9;\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 90px;\r\n  height: 90px;\r\n  line-height: 90px;\r\n  text-align: center;\r\n}\r\n\r\n.avatar {\r\n  width: 90px;\r\n  height: 90px;\r\n  display: block;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/system/user"}]}