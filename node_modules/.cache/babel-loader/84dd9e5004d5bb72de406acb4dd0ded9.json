bel=\"$t('customComponents.pdEdition')\">\r\n              <el-select clearable filterable v-model=\"params.pdEdition\">\r\n                <el-option :key=\"edition\" :label=\"edition\" :value=\"edition\"\r\n                           v-for=\"edition in pdEditionList\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n          <cus-btn-wrapper>\r\n              <el-button  size=\"small\" @click=\"handleSearch\" icon=\"el-icon-search\" type=\"primary\">\r\n                {{ $t('pdClaimVerification.search') }}\r\n              </el-button>\r\n          </cus-btn-wrapper>\r\n        </el-row>\r\n      </el-form>\r\n      <cus-table-title :label=\"$t('pdClaimVerification.claimVerificationProduct')\"> </cus-table-title>\r\n      <el-table :data=\"verificationTableData\" border element-loading-text=\"Loading\" fit highlight-current-row size=\"mini\"\r\n                style=\"margin: 20px 0 20px 0\">\r\n        <el-table-column :label=\"$t('table.select')\" width=\"50\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <el-radio-group @change=\"selectedVerificationChanged\" v-model=\"selectedVerification\">\r\n              <span>\r\n                <el-radio :label=\"scope.row\">{{''}}</el-radio>\r\n              </span>\r\n            </el-radio-group>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.pdCode')\" align=\"center\" prop=\"pdCode\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.pdName')\" align=\"center\" prop=\"pdName\"></el-table-column>\r\n        <el-table-column :label=\"$t('customComponents.pdEdition')\" align=\"center\" prop=\"pdEdition\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.defType')\" align=\"center\" prop=\"defType\">\r\n          <template slot-scope=\"scope\">\r\n            {{filterDefType(scope.row.defType)}}\r\n          </template>\r\n        </el-table-column>\r\n\r\n        <el-table-column :label=\"$t('i18nView.limitNumber')\" align=\"center\"\r\n                         prop=\"limitCount\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.deductiblesNumber')\" align=\"center\"\r\n                         prop=\"deductionCount\"></el-table-column>\r\n        <el-table-column :label=\"$t('pdClaimVerification.observationPeriodsNumber')\" align=\"center\"\r\n                         prop=\"observationCount\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.pdDefApplyDate')\" align=\"center\"\r\n                         prop=\"pdApplDate\"></el-table-column>\r\n      </el-table>\r\n      <!-- 分页 -->\r\n     <cus-pagination :pageSize=\"10\" :params=\"params\" ref=\"page\" url=\"/pdtemp-pd-claim-verification/base/page\"\r\n                      v-model=\"verificationTableData\"/>\r\n\r\n  </cus-wraper>\r\n</template>\r\n\r\n<script>\r\n\r\n  import {claimVerificationList,findAllProductByPdDetailType} from \"@/api/basicinfo/pdclaimverification\"\r\n  import {closeVisitTag} from \"@/utils/tagsView\"\r\n\r\n  export default {\r\n    name: \"pdClaimVerification\",\r\n    filters: {},\r\n    data() {\r\n      return {\r\n        // 产品信息\r\n        product: {\r\n          id: undefined,\r\n          pdCode: '',\r\n          pdEdition: '',\r\n          pdVerId: undefined,\r\n          pdSalechnl: '',\r\n          pdName: '',\r\n          pdShortname: '',\r\n          pdEnName: '',\r\n          pdShortEnName: '',\r\n          operator: '',\r\n          pdApplDate: undefined,\r\n          pdProcessId: undefined,\r\n          createTime: undefined,\r\n          updateTime: undefined\r\n        },\r\n        //待核赔产品表格\r\n        verificationTableData: [],\r\n        params: {\r\n          pdRegionType:'',\r\n          pdCode: '',\r\n          pdEdition: ''\r\n        },\r\n        selectedVerification:{},\r\n        productList: [],\r\n        productOriginalList: [],\r\n        pdEditionList: [],\r\n        selectedPd:{},\r\n      };\r\n    },\r\n\r\n    created() {\r\n      this.initProductQueryData()\r\n    },\r\n    methods: {\r\n      handleSearch() {\r\n        this.$refs.page.reload()\r\n      },\r\n      filterDefType(code) {\r\n        return this.getCodeName('pdAuditType', code)\r\n      },\r\n      initProductQueryData(){\r\n        this.filterProductByNode(this,'2',true)\r\n      },\r\n      selectedDetailTypeChanged(val) {\r\n        val === ''?  this.initProductQueryData() : this.filterProductByCategory(this,val)\r\n      },\r\n      selectedPdCodeChanged(product) {\r\n        this.filterEditionByProduct(this,product)\r\n      },\r\n      selectedVerificationChanged(row){\r\n        Object.assign(this.product, row)\r\n        closeVisitTag()\r\n        this.$router.push({name: '产品核赔定义主页', params: this.product})\r\n        // this.$router.push({path: '/product-definition/pd-operation-def/pd-compensation-def/main', params: {}})\r\n      }\r\n\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n  .showDiv{\r\n    width:100%;\r\n    height:100%;\r\n    margin-top: -40px;\r\n    margin-left: -40px;\r\n    position: absolute;\r\n    background: #ffffff00;\r\n    z-index: 999;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/views/product-definition/pd-operation-def/pd-compensation-def/apply"}]}