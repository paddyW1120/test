AAA,GAAA,KAAA,QAAA;AAEA,WAAA,KAAA,CAAA,EAAA,CAAA,OAAA,EAAA,UAAA,MAAA,EAAA;AACA,YAAA,MAAA,CAAA,aAAA,IAAA,OAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,UAAA,QAAA,IAAA,QAAA,CAAA,MAAA,CAAA;AACA,SAHA,MAGA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,IAAA;AACA,UAAA,QAAA,IAAA,QAAA,CAAA,MAAA,CAAA,IAAA,CAAA;AAEA;AAEA,OAVA;AAWA;AAtFA;AA9DA,C","sourcesContent":["<template>\r\n  <div :id=\"id\" :style=\"{height:height,width:width}\"/>\r\n</template>\r\n\r\n<script>\r\n  import echarts from 'echarts'\r\n\r\n  require('echarts/theme/macarons') // echarts theme\r\n  import resize from './mixins/resize'\r\n\r\n  const animationDuration = 2000\r\n\r\n  export default {\r\n    mixins: [resize],\r\n    props: {\r\n      callBack: {\r\n        type: Function,\r\n        required: false,\r\n        default: null\r\n      },\r\n      xData: {\r\n        type: Array,\r\n        default: [],\r\n        required: true\r\n      },\r\n      yData: {\r\n        type: Array,\r\n        default: [],\r\n        required: true\r\n      },\r\n      theme: {\r\n        type: String,\r\n        default: 'tech-blue'\r\n      },\r\n      title: {\r\n        type: String,\r\n        default: ''\r\n      },\r\n      id: {\r\n        type: String,\r\n        default: 'chart',\r\n        required: true\r\n      },\r\n      width: {\r\n        type: String,\r\n        default: '100%'\r\n      },\r\n      height: {\r\n        type: String,\r\n        default: '300px'\r\n      }\r\n    },\r\n    watch: {\r\n      xData(val, oldVal) {\r\n        this.initChart()\r\n      }\r\n    },\r\n    data() {\r\n      return {\r\n        chart: null\r\n      }\r\n    },\r\n    mounted() {\r\n      this.$nextTick(() => {\r\n        this.initChart()\r\n      })\r\n    },\r\n    beforeDestroy() {\r\n      if (!this.chart) {\r\n        return\r\n      }\r\n      this.chart.dispose()\r\n      this.chart = null\r\n    },\r\n    methods: {\r\n      initChart() {\r\n        let chartObj = document.getElementById(this.id)\r\n\r\n        this.chart = echarts.init(chartObj, 'macarons')\r\n\r\n        this.chart.setOption({\r\n          title: {\r\n            text: this.title,\r\n            show: this.title !== '',\r\n            x: 'center',\r\n            textStyle:{\r\n               fontSize: 16\r\n            }\r\n          },\r\n          tooltip: {\r\n            trigger: 'axis',\r\n            axisPointer: { // 坐标轴指示器，坐标轴触发有效\r\n              type: 'shadow' // 默认为直线，可选为：'line' | 'shadow'\r\n            }\r\n          },\r\n          grid: {\r\n            left: '2%',\r\n            right: '2%',\r\n            bottom: '3%',\r\n            containLabel: true\r\n          },\r\n          xAxis: [{\r\n            type: 'category',\r\n            data: this.xData,\r\n            axisTick: {\r\n              alignWithLabel: true\r\n            },\r\n            triggerEvent:true,\r\n            axisLabel: {\r\n              showMaxLabel: true,\r\n              interval: 0,\r\n              formatter: function(value, index) {\r\n                if (index % 2 != 0) {\r\n                  return '\\n\\n' + value\r\n                } else {\r\n                  return value\r\n                }\r\n              }\r\n            }\r\n          }],\r\n          yAxis: [{\r\n            type: 'value',\r\n            axisTick: {\r\n              show: false\r\n            }\r\n          }],\r\n          series: [\r\n            {\r\n              type: 'bar',\r\n              stack: 'vistors',\r\n              barWidth: '20%',\r\n              data: this.yData,\r\n              animationDuration,\r\n              itemStyle: {\r\n                normal: {\r\n                  //这里是重点\r\n                  color: function(params) {\r\n                    //注意，如果颜色太少的话，后面颜色不会自动循环，最好多定义几个颜色\r\n                    let colorList = ['#c23531', '#2f4554', '#61a0a8', '#d48265', '#91c7ae', '#749f83', '#ca8622']\r\n                    return colorList[params.dataIndex]\r\n                  }\r\n                }\r\n              }\r\n            }\r\n          ]\r\n        })\r\n\r\n        let callBack = this.callBack\r\n\r\n        this.chart.on('click', function(params) {\r\n          if(params.componentType ==\"xAxis\" ){\r\n            console.dir(params)\r\n            callBack && callBack(params)\r\n          }else{\r\n            console.dir(params.name)\r\n            callBack && callBack(params.name)\r\n\r\n          }\r\n\r\n        })\r\n      }\r\n    }\r\n  }\r\n</script>\r\n"],"sourceRoot":"src/views/dashboard/admin/components"}]}