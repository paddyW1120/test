vel) {\r\n          this.options = this.ldcodes[this.codetype]\r\n          if (this.filter) {\r\n            this.options = this.filterOptions(this.filter, this.options)\r\n          }\r\n        } else {\r\n          if (this.filter) {\r\n            let secondKey = this.codetype + \".\" + this.filter\r\n            this.options = this.ldcodes[secondKey]\r\n          }\r\n        }\r\n      },\r\n      doChange(val) {\r\n        this.$emit('change', val)\r\n      }\r\n\r\n    },\r\n    data() {\r\n      return {\r\n        options: [],\r\n        checkBoxValue: [],\r\n        isDisabled: false\r\n      }\r\n    },\r\n    watch: {\r\n      value(val, oldVal) {\r\n        this.checkBoxValue = val\r\n      },\r\n      checkBoxValue(val, oldVal) {\r\n        if (val != oldVal) {\r\n          this.$emit('input', this.checkBoxValue)\r\n        }\r\n      },\r\n      disabled(val, oldVal) {\r\n        if (val != oldVal) {\r\n          this.isDisabled = val\r\n        }\r\n      }, filter(val, oldVal) {\r\n        if (val != oldVal) {\r\n          this.checkBoxValue = []\r\n          this.buildSelectOptions()\r\n        }\r\n      }\r\n\r\n    },\r\n    computed: {\r\n      ...mapGetters([\r\n        'ldcodes'\r\n      ])\r\n    }\r\n  }\r\n</script>\r\n\r\n\r\n<style>\r\n\r\n</style>\r\n"],"sourceRoot":"src/customComponents/CheckBoxSelect"}]}