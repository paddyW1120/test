\n    ...elementJaLocale,\r\n  },\r\n  zh_tr: {\r\n    ...zhTrLocale,\r\n    ...elementTrLocale\r\n  }\r\n}\r\n\r\nfunction registerAllI18nJs(requireContext) {\r\n  return requireContext.keys().forEach(comp => {\r\n    const jsComp = requireContext(comp)\r\n    Object.assign(messages.zh, jsComp.default.zh)\r\n    Object.assign(messages.en, jsComp.default.en)\r\n    Object.assign(messages.ja, jsComp.default.ja)\r\n    Object.assign(messages.zh_tr, jsComp.default.zh_tr)\r\n  })\r\n}\r\n\r\nregisterAllI18nJs(require.context('../views', true, /-local.js$/))\r\n\r\n\r\nexport function getLanguage() {\r\n  const chooseLanguage = Cookies.get('language')\r\n  if (chooseLanguage) return chooseLanguage\r\n\r\n  // if has not choose language\r\n  const language = (navigator.language || navigator.browserLanguage).toLowerCase()\r\n  const locales = Object.keys(messages)\r\n  for (const locale of locales) {\r\n    if (language.indexOf(locale) > -1) {\r\n      return locale\r\n    }\r\n  }\r\n  return 'en'\r\n}\r\n\r\nconst i18n = new VueI18n({\r\n  // set locale\r\n  // options: en | zh | es\r\n  locale: getLanguage(),\r\n  // set locale messages\r\n  messages,\r\n  silentTranslationWarn: false\r\n})\r\n\r\nexport default i18n\r\n"]}]}