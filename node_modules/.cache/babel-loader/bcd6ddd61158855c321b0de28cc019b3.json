CAAA,mBAAA,CAFA;AAGA,UAAA,OAAA,EAAA;AAHA,SADA,CADA;AAQA,QAAA,IAAA,EAAA,CACA;AACA,UAAA,QAAA,EAAA,IADA;AAEA,UAAA,OAAA,EAAA,KAAA,EAAA,CAAA,iBAAA,IAAA,KAAA,EAAA,CAAA,gBAAA,CAFA;AAGA,UAAA,OAAA,EAAA;AAHA,SADA;AARA,OAtBA;AAsCA,MAAA,aAAA,EAAA,KAtCA;AAsCA;AACA,MAAA,YAAA,EAAA,EAvCA;AAuCA;AACA,MAAA,QAAA,EAAA;AACA,QAAA,MAAA,EAAA,KAAA,EAAA,CAAA,kBAAA,CADA;AAEA,QAAA,MAAA,EAAA,KAAA,EAAA,CAAA,eAAA;AAFA;AAxCA,KAAA;AA6CA,GAhDA;AAiDA,EAAA,QAAA,EAAA,EAjDA;AAkDA,EAAA,OAlDA,qBAkDA,CACA,CAnDA;AAoDA,EAAA,OAAA,EAAA;AACA,IAAA,YADA,0BACA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,MAAA;AACA,KAHA;AAIA,IAAA,YAJA,0BAIA;AACA,WAAA,SAAA;AACA,WAAA,YAAA,GAAA,QAAA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,KARA;AASA,IAAA,YATA,wBASA,GATA,EASA;AACA,WAAA,IAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,GAAA,CAAA;AACA,WAAA,YAAA,GAAA,QAAA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,KAbA;AAcA,IAAA,YAdA,wBAcA,EAdA,EAcA;AAAA;;AACA,mCAAA,EAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,YAAA;;AACA,QAAA,KAAA,CAAA,aAAA,CAAA,QAAA,CAAA,OAAA;;AACA,QAAA,KAAA,CAAA,aAAA,GAAA,KAAA;AACA,OAJA;AAKA,KApBA;AAqBA,IAAA,UArBA,wBAqBA;AAAA;;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,cAAA,IAAA;;AACA,cAAA,MAAA,CAAA,IAAA,CAAA,EAAA,EAAA;AACA,YAAA,IAAA,GAAA,6BAAA,MAAA,CAAA,IAAA,CAAA;AACA,WAFA,MAEA;AACA,YAAA,IAAA,GAAA,0BAAA,MAAA,CAAA,IAAA,CAAA;AACA;;AACA,UAAA,IAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,MAAA,CAAA,YAAA;;AACA,YAAA,MAAA,CAAA,aAAA,CAAA,QAAA,CAAA,OAAA;;AACA,YAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,WAJA;AAKA;AACA,OAdA;AAeA,KArCA;AAsCA,IAAA,SAtCA,uBAsCA;AACA,WAAA,IAAA,GAAA;AACA;AACA,QAAA,EAAA,EAAA,EAFA;AAEA;AACA,QAAA,OAAA,EAAA,EAHA;AAGA;AACA,QAAA,IAAA,EAAA,EAJA;AAIA;AACA,QAAA,SAAA,EAAA,EALA;AAKA;AACA,QAAA,UAAA,EAAA,EANA;AAMA;AACA,QAAA,OAAA,EAAA,EAPA;AAOA;AACA,QAAA,KAAA,EAAA,EARA;AAQA;AACA,QAAA,KAAA,EAAA,EATA;AASA;AACA,QAAA,OAAA,EAAA,EAVA;AAUA;AACA,QAAA,GAAA,EAAA,EAXA,CAWA;;AAXA,OAAA;AAaA;AApDA;AApDA,C","sourcesContent":["<template>\r\n  <cus-wraper>\r\n\r\n\r\n    <el-form label-width=\"auto\">\r\n      <el-row :gutter=\"10\">\r\n        <el-col :span=\"7\">\r\n          <el-form-item :label=\"$t('sysTenant.name')\">\r\n            <el-input\r\n              v-model=\"params.like_name\"\r\n              :placeholder=\"$t('form.please_enter')\"\r\n              class=\"filter-item\"\r\n              clearable\r\n            ></el-input>\r\n          </el-form-item>\r\n\r\n        </el-col>\r\n        <el-col :span=\"7\">\r\n          <el-form-item :label=\"$t('sysTenant.comCode')\">\r\n            <el-input\r\n              v-model=\"params.like_comCode\"\r\n              :placeholder=\"$t('form.please_enter')\"\r\n              class=\"filter-item\"\r\n              clearable\r\n            ></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n        <cus-btn-wrapper>\r\n          <el-button v-waves\r\n                     class=\"filter-button\"\r\n                     icon=\"el-icon-search\"\r\n                     size=\"small\"\r\n                     type=\"primary\"\r\n                     @click=\"handleSearch\"\r\n          >{{ $t(\"table.search\") }}\r\n          </el-button>\r\n          <el-button v-auth=\"'sys-tenant:add'\"\r\n                     v-waves\r\n                     class=\"filter-button\"\r\n                     icon=\"el-icon-plus\"\r\n                     size=\"small\"\r\n                     type=\"primary\"\r\n                     @click=\"handleCreate\"\r\n          >{{ $t(\"table.add\") }}\r\n          </el-button>\r\n        </cus-btn-wrapper>\r\n      </el-row>\r\n    </el-form>\r\n\r\n    <cus-table-title :label=\"$t('sysTenant.tenantList')\"></cus-table-title>\r\n    <el-table\r\n      :data=\"configs\"\r\n      align=\"center\"\r\n      border\r\n      class=\"wraper-table\"\r\n      element-loading-text=\"Loading\"\r\n      fit\r\n      highlight-current-row\r\n      size=\"mini\"\r\n    >\r\n      >\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" sortable type=\"index\"></el-table-column>\r\n      <el-table-column :label=\"$t('sysTenant.comCode')\" align=\"center\" prop=\"comCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('sysTenant.name')\" align=\"center\" prop=\"name\"></el-table-column>\r\n      <el-table-column :label=\"$t('sysTenant.shortName')\" align=\"center\" prop=\"shortName\"></el-table-column>\r\n      <el-table-column :label=\"$t('sysTenant.webAddress')\" align=\"center\" prop=\"webAddress\"></el-table-column>\r\n      <el-table-column :label=\"$t('sysTenant.address')\" align=\"center\" prop=\"address\"></el-table-column>\r\n      <el-table-column :label=\"$t('sysTenant.phone')\" align=\"center\" prop=\"phone\"></el-table-column>\r\n      <el-table-column :label=\"$t('sysTenant.email')\" align=\"center\" prop=\"email\"></el-table-column>\r\n      <el-table-column :label=\"$t('sysTenant.zipCode')\" align=\"center\" prop=\"zipCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('sysTenant.fax')\" align=\"center\" prop=\"fax\"></el-table-column>\r\n      <el-table-column :label=\"$t('table.actions')\" align=\"center\" width=\"200px\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            v-auth=\"'sys-tenant:update'\"\r\n            class=\"filter-button\"\r\n            icon=\"el-icon-edit\"\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"handleUpdate(scope.row)\"\r\n          >{{ $t(\"table.edit\") }}\r\n          </el-button>\r\n          <cus-del-btn style=\"margin-left: 15px\" v-auth=\"'sys-tenant:delete'\" @ok=\"handleDelete(scope.row.id)\"/>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <cus-pagination\r\n      ref=\"page\"\r\n      v-model=\"configs\"\r\n      :pageSize=\"10\"\r\n      :params=\"params\"\r\n      url=\"/sys-tenant/list\"\r\n    />\r\n\r\n    <!---新增修改dialog---->\r\n\r\n    <el-dialog\r\n      v-dialog-drag\r\n      :visible.sync=\"dialogVisible\"\r\n      @close=\"dialogVisible = false\"\r\n    >\r\n      <cus-page-title-back @backTo=\"dialogVisible = false\" :label=\"titleMap[dialogStatus]\" :isDialog=\"true\" />\r\n      <el-form\r\n        ref=\"dataForm\"\r\n        :model=\"form\"\r\n        :rules=\"rules\"\r\n        label-position=\"right\"\r\n        label-width=\"40%\"\r\n        style=\"width: 60%;margin-left: 10%\"\r\n      >\r\n        <el-form-item :label=\"$t('sysTenant.comCode') + ':'\" prop=\"comCode\">\r\n          <el-input\r\n            v-model=\"form.comCode\"\r\n            :placeholder=\"$t('sysTenant.input') + $t('sysTenant.comCode')\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('sysTenant.name') + ':'\" prop=\"name\">\r\n          <el-input v-model=\"form.name\" :placeholder=\"$t('sysTenant.input') + $t('sysTenant.name')\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('sysTenant.shortName') + ':'\" prop=\"shortName\">\r\n          <el-input\r\n            v-model=\"form.shortName\"\r\n            :placeholder=\"$t('sysTenant.input') + $t('sysTenant.shortName')\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('sysTenant.webAddress') + ':'\" prop=\"webAddress\">\r\n          <el-input\r\n            v-model=\"form.webAddress\"\r\n            :placeholder=\"$t('sysTenant.input') + $t('sysTenant.webAddress')\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('sysTenant.address') + ':'\" prop=\"address\">\r\n          <el-input\r\n            v-model=\"form.address\"\r\n            :placeholder=\"$t('sysTenant.input') + $t('sysTenant.address')\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('sysTenant.phone') + ':'\" prop=\"phone\">\r\n          <el-input\r\n            v-model=\"form.phone\"\r\n            :placeholder=\"$t('sysTenant.input') + $t('sysTenant.phone')\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('sysTenant.email') + ':'\" prop=\"email\">\r\n          <el-input\r\n            v-model=\"form.email\"\r\n            :placeholder=\"$t('sysTenant.input') + $t('sysTenant.email')\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('sysTenant.zipCode') + ':'\" prop=\"zipCode\">\r\n          <el-input\r\n            v-model=\"form.zipCode\"\r\n            :placeholder=\"$t('sysTenant.input') + $t('sysTenant.zipCode')\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('sysTenant.fax') + ':'\" prop=\"fax\">\r\n          <el-input v-model=\"form.fax\" :placeholder=\"$t('sysTenant.input') + $t('sysTenant.fax')\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button size=\"small\" type=\"primary\" icon='el-icon-circle-close' @click=\"dialogVisible = false\">{{ $t(\"table.cancel\") }}</el-button>\r\n        <el-button v-auth=\"'sys-user:add'\"\r\n                   size=\"small\"\r\n                   type=\"primary\"\r\n                   class=\"el-icon-success\"\r\n                   @click=\"submitForm\"\r\n        >{{ $t(\"table.confirm\") }}</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\nimport {addSysTenant, deleteSysTenant, updateSysTenant} from \"@/api/system/tenant\";\r\n\r\nexport default {\r\n  name: \"sysTenant\",\r\n  data() {\r\n    return {\r\n      //表格数据\r\n      configs: [],\r\n      //查询参数\r\n      params: {\r\n        like_comCode: \"\",\r\n        like_name: \"\"\r\n      },\r\n      form: {\r\n        //表单数据与后端实体类对应\r\n        id: \"\", //主键ID\r\n        comCode: \"\", //租户编码\r\n        name: \"\", //租户名\r\n        shortName: \"\", //租户简称\r\n        webAddress: \"\", //网址\r\n        address: \"\", //地址\r\n        phone: \"\", //手机\r\n        email: \"\", //邮箱\r\n        zipCode: \"\", //邮编\r\n        fax: \"\" //传真\r\n      },\r\n      //检验规则\r\n      rules: {\r\n        comCode: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"sysTenant.input\") + this.$t(\"sysTenant.comCode\"),\r\n            trigger: \"blur\"\r\n          }\r\n        ],\r\n        name: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"sysTenant.input\") + this.$t(\"sysTenant.name\"),\r\n            trigger: \"blur\"\r\n          }\r\n        ]\r\n      },\r\n      dialogVisible: false, //dialog显示还是隐藏\r\n      dialogStatus: \"\", //表单状态是新增还是修改\r\n      titleMap: {\r\n        update: this.$t(\"sysTenant.update\"),\r\n        create: this.$t(\"sysTenant.add\")\r\n      }\r\n    };\r\n  },\r\n  computed: {},\r\n  created() {\r\n  },\r\n  methods: {\r\n    handleSearch() {\r\n      this.$refs.page.reload();\r\n    },\r\n    handleCreate() {\r\n      this.resetForm();\r\n      this.dialogStatus = \"create\";\r\n      this.dialogVisible = true;\r\n    },\r\n    handleUpdate(row) {\r\n      this.form = Object.assign({}, row);\r\n      this.dialogStatus = \"update\";\r\n      this.dialogVisible = true;\r\n    },\r\n    handleDelete(id) {\r\n      deleteSysTenant(id).then(response => {\r\n        this.handleSearch();\r\n        this.notifySuccess(response.message);\r\n        this.dialogVisible = false;\r\n      });\r\n    },\r\n    submitForm() {\r\n      this.$refs.dataForm.validate(valid => {\r\n        if (valid) {\r\n          let back;\r\n          if (this.form.id) {\r\n            back = updateSysTenant(this.form);\r\n          } else {\r\n            back = addSysTenant(this.form);\r\n          }\r\n          back.then(response => {\r\n            this.handleSearch();\r\n            this.notifySuccess(response.message);\r\n            this.dialogVisible = false;\r\n          });\r\n        }\r\n      });\r\n    },\r\n    resetForm() {\r\n      this.form = {\r\n        //表单数据与后端实体类对应\r\n        id: \"\", //主键ID\r\n        comCode: \"\", //租户编码\r\n        name: \"\", //租户名\r\n        shortName: \"\", //租户简称\r\n        webAddress: \"\", //网址\r\n        address: \"\", //地址\r\n        phone: \"\", //手机\r\n        email: \"\", //邮箱\r\n        zipCode: \"\", //邮编\r\n        fax: \"\" //传真\r\n      };\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped></style>\r\n"],"sourceRoot":"src/views/system/tenant"}]}