t :codetype=\"'ibrmsresulttype'\" placeholder v-model=\"form.resultType\"></cus-select>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleCommand.commType')+':'\" prop=\"commType\">\r\n                  <cus-select\r\n                    :codetype=\"'ibrmscommtype'\"\r\n                    placeholder\r\n                    v-model=\"form.commType\"\r\n                    :disabled=\"'disabled'\"\r\n                  ></cus-select>\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n            <el-row :gutter=\"10\">\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleCommand.commDetail')+':'\" prop=\"commDetail\">\r\n                  <el-input\r\n                    v-model=\"form.commDetail\"\r\n                    :placeholder=\"$t('ruleCommand.commDetail')\"\r\n                    maxlength=\"300\"\r\n                    show-word-limit\r\n                  ></el-input>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleCommand.valid')+':'\" prop=\"valid\">\r\n                  <el-switch active-value=\"1\" inactive-value=\"0\" v-model=\"form.valid\"></el-switch>\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n            <el-row :gutter=\"10\">\r\n              <cus-col :span=\"20\">\r\n                <el-form-item :label=\"$t('ruleCommand.description')+':'\" prop=\"description\">\r\n                  <el-input\r\n                    :placeholder=\"$t('ruleCommand.description') \"\r\n                    :rows=\"5\"\r\n                    type=\"textarea\"\r\n                    v-model=\"form.discription\"\r\n                    maxlength=\"200\"\r\n                    show-word-limit\r\n                  />\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n          </el-form>\r\n          <span slot=\"footer\" class=\"dialog-footer\">\r\n            <el-button  size=\"mini\" type=\"primary\" v-auth=\"'rule-command:add'\" @click=\"submitForm\">\r\n              <i class=\"el-icon-success el-icon--left\"></i>\r\n              {{ $t('table.save') }}\r\n            </el-button>\r\n            <el-button  size=\"mini\" @click=\"resetForm('dataForm')\" type=\"warning\">\r\n              <i class=\"el-icon-refresh el-icon--left\"></i>\r\n              {{$t('table.refresh')}}\r\n            </el-button>\r\n          </span>\r\n    </el-dialog>\r\n    <el-dialog :visible.sync=\"dialogVisible_msg\" @close=\"dialogVisible_msg=false\"  v-dialog-drag  width=\"80%\">\r\n      <cus-page-title-back @backTo=\"goBackInfo_msg\" :label=\"$t('ruleCommand.ruleMessage')\" :isDialog=\"true\"></cus-page-title-back>\r\n      <ruleMessageInfoBom :keyId.sync=\"keyId\" :msgType.sync=\"msgType\"></ruleMessageInfoBom>\r\n    </el-dialog>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\nimport {\r\n  addRuleCommand,\r\n  deleteRuleCommand,\r\n  updateRuleCommand,\r\n  findRuleCommandByName\r\n} from \"@/api/ibrms/ruleCommand\";\r\nimport { mapGetters } from \"vuex\";\r\nimport ruleMessageInfoBom from \"@/views/ibrms/objectManagement/ruleMessageInfoBom\";\r\nexport default {\r\n  name: \"ruleCommand\",\r\n  data() {\r\n    const isNum = (rule, value, callback) => {\r\n      const bomLevel = /^[0-9]*$/;\r\n      if (!bomLevel.test(value)) {\r\n        callback(new Error(\"请输入数字\"));\r\n      } else {\r\n        callback();\r\n      }\r\n    };\r\n    return {\r\n      //表格数据\r\n      configs: [],\r\n      computed: {\r\n        ...mapGetters([\"ldcodes\"])\r\n      },\r\n      //查询参数\r\n      params: {\r\n        like_name: \"\",\r\n        eq_commandType: \"\",\r\n        eq_commType: \"0\",\r\n        eq_valid: \"1\"\r\n      },\r\n      form: {\r\n        //表单数据与后端实体类对应\r\n        id: \"\", //主键ID\r\n        name: \"\",\r\n        commandType: \"\",\r\n        commType: \"1\",\r\n        valid: \"1\",\r\n        functionType: \"0\",\r\n        paraType:\"\",\r\n        resultType:\"\"\r\n      },\r\n      radio: false,\r\n      commandId: \"\",\r\n      commandName: \"\",\r\n      keyId: \"\",\r\n      msgType: \"\",\r\n      dialogVisible_msg: false,\r\n      //检验规则\r\n      rules: {\r\n        paraNum: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleCommand.paraNumMessage\"),\r\n            trigger: \"blur\"\r\n          },\r\n          {\r\n            validator: isNum,\r\n            message: this.$t(\"ruleCommand.paraNumMessage1\"),\r\n            trigger: \"blur\"\r\n          }\r\n        ],\r\n        name: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleCommand.nameMessage\"),\r\n            trigger: \"blur\"\r\n          }\r\n        ],\r\n        implenmation: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleCommand.implenmationMessage\"),\r\n            trigger: \"blur\"\r\n          }\r\n        ],\r\n        display: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleCommand.displayMessage\"),\r\n            trigger: \"blur\"\r\n          }\r\n        ],\r\n        commandType: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleCommand.commandTypeMessage\"),\r\n            trigger: \"change\"\r\n          }\r\n        ],\r\n        paraType: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleCommand.paraTypeMessage\"),\r\n            trigger: \"change\"\r\n          }\r\n        ],\r\n        resultType: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleCommand.resultTypeMessage\"),\r\n            trigger: \"change\"\r\n          }\r\n        ]\r\n      },\r\n      dialogVisible: false, //dialog显示还是隐藏\r\n      dialogStatus: \"\", //表单状态是新增还是修改\r\n      titleMap: {\r\n        update: this.$t(\"ruleCommand.update\"),\r\n        create: this.$t(\"ruleCommand.add\")\r\n      },\r\n      commandTypeArray: [],\r\n      paraTypeArray: [],\r\n      resultTypeArray: [],\r\n      commTypeArray: [],\r\n      validTypeArray: []\r\n    };\r\n  },\r\n  computed: {},\r\n  components: {\r\n    ruleMessageInfoBom\r\n  },\r\n  created() {\r\n    let ldcodes = this.$store.state.ldcode.ldcodes;\r\n    this.commandTypeArray = ldcodes[\"ibrmscommandtype\"];\r\n    this.paraTypeArray = ldcodes[\"ibrmsparatype\"];\r\n    this.resultTypeArray = ldcodes[\"ibrmsresulttype\"];\r\n    this.commTypeArray = ldcodes[\"ibrmscommtype\"];\r\n    this.validArray = ldcodes[\"ibrmsvalid\"];\r\n  },\r\n  methods: {\r\n    handleCurrentChange(val) {\r\n      this.commandId = val.id;\r\n      this.commandName = val.name;\r\n      this.radio = val.id;\r\n    },\r\n    handleSearch() {\r\n      this.$refs.page.reload();\r\n    },\r\n    handleCreate() {\r\n      this.resetForm();\r\n      this.form.name = this.params.like_name;\r\n      this.form.commandType = this.params.eq_commandType;\r\n      this.form.commType = this.params.eq_commType;\r\n      this.form.valid = this.params.eq_valid;\r\n\r\n      this.dialogStatus = \"create\";\r\n      this.dialogVisible = true;\r\n    },\r\n    handleUpdate(row) {\r\n      this.form = Object.assign({}, row);\r\n      this.dialogStatus = \"update\";\r\n      this.dialogVisible = true;\r\n    },\r\n    handleDelete(row) {\r\n      deleteRuleCommand(row.id).then(response => {\r\n        var result = response.data;\r\n        if(result.type = \"success\"){\r\n            var keyId = 'CMD_'+row.name;\r\n            var param = {   \r\n                        obj:row,\r\n                         type:'CMD',\r\n                         item:'delete'\r\n                         };\r\n            var rule_param = {\r\n                              key:keyId,\r\n                              type:\"CMD\"\r\n                              };\r\n\r\n            this.$store.dispatch(\"ruleCache/updateRuleCache\",param);\r\n            this.$store.dispatch(\"ruleMessage/deleteRuleMessage\",rule_param);\r\n            this.handleSearch();\r\n            if(result.isUsed){\r\n                this.notifySuccess(this.$t(\"ruleCommand.ruleCommandMessage1\"));\r\n            }else{\r\n                this.notifySuccess(response.message);\r\n            }\r\n            this.dialogVisible = false;\r\n            }\r\n      });\r\n    },\r\n    submitForm() {\r\n      this.$refs.dataForm.validate(valid => {\r\n        if (valid) {\r\n          let back;\r\n          if (this.form.id) {\r\n            back = updateRuleCommand(this.form);\r\n          } else {\r\n            back = addRuleCommand(this.form);\r\n          }\r\n          back.then(response => {\r\n            var result = response.data;\r\n            if(result.type = \"success\"){\r\n              var keyId =\"CMD_\"+this.form.name;\r\n              var param = {\r\n                            obj:this.form,\r\n                            type:'CMD',\r\n                            item:''\r\n                            };\r\n              var rule_param = {\r\n                                key:keyId,\r\n                                type:\"CMD\"\r\n                               };\r\n              this.$store.dispatch(\"ruleCache/updateRuleCache\",param);\r\n              this.$store.dispatch(\"ruleMessage/updateRuleMessage\",rule_param);\r\n              this.handleSearch();\r\n              if(result.isUsed){\r\n                  this.notifySuccess(this.$t(\"ruleCommand.ruleCommandMessage1\"));\r\n              }else{\r\n                  this.notifySuccess(response.message);\r\n              }\r\n              this.dialogVisible = false;\r\n            }else{\r\n              this.$message.error(this.$t(\"ruleCommand.ruleCommandMessage\"));\r\n            }\r\n            \r\n          });\r\n        }\r\n      });\r\n    },\r\n    resetForm() {\r\n      this.form = {\r\n        //表单数据与后端实体类对应\r\n        id: \"\", //主键ID\r\n        name: \"\",\r\n        commandType: \"\",\r\n        commType: \"1\",\r\n        valid: \"1\",\r\n        functionType: \"0\",\r\n        paraType:\"\",\r\n        resultType:\"\"\r\n      };\r\n    },\r\n    ruleMessage(row) {\r\n      this.keyId = \"CMD_\" + row.name;\r\n      this.msgType = \"CMD\";\r\n      this.dialogVisible_msg = true;\r\n    },\r\n    chooseOne(row) {\r\n      this.radio = row.id;\r\n    },\r\n    commandTypeFormat(row, column) {\r\n      let code = row.commandType;\r\n      let stateValue = \"\";\r\n      this.commandTypeArray.forEach((st, index) => {\r\n        if (st.code === code) {\r\n          stateValue = st.codename;\r\n        }\r\n      });\r\n      return stateValue;\r\n    },\r\n    paraTypeFormat(row, column) {\r\n      let code = row.paraType;\r\n      let stateValue = \"\";\r\n      this.paraTypeArray.forEach((st, index) => {\r\n        if (st.code === code) {\r\n          stateValue = st.codename;\r\n        }\r\n      });\r\n      return stateValue;\r\n    },\r\n    resultTypeFormat(row, column) {\r\n      let code = row.resultType;\r\n      let stateValue = \"\";\r\n      this.resultTypeArray.forEach((st, index) => {\r\n        if (st.code === code) {\r\n          stateValue = st.codename;\r\n        }\r\n      });\r\n      return stateValue;\r\n    },\r\n    commTypeFormat(row, column) {\r\n      let code = row.commType;\r\n      let stateValue = \"\";\r\n      this.commTypeArray.forEach((st, index) => {\r\n        if (st.code === code) {\r\n          stateValue = st.codename;\r\n        }\r\n      });\r\n      return stateValue;\r\n    },\r\n    validFormat(row, column) {\r\n      let code = row.valid;\r\n      let stateValue = \"\";\r\n      this.validArray.forEach((st, index) => {\r\n        if (st.code === code) {\r\n          stateValue = st.codename;\r\n        }\r\n      });\r\n      return stateValue;\r\n    },\r\n    goBack() {\r\n      this.dialogVisible = false;\r\n    },\r\n    goBackInfo_msg() {\r\n      this.dialogVisible_msg = false;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"],"sourceRoot":"src/views/ibrms/objectManagement/command"}]}