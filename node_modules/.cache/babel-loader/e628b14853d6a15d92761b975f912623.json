ect>\r\n            </el-form-item>\r\n          </cus-col>\r\n          <cus-btn-wrapper>\r\n            <el-button\r\n              size=\"mini\"\r\n              v-waves\r\n              @click=\"handleSearch\"\r\n              icon=\"el-icon-search\"\r\n              type=\"primary\"\r\n            >{{ $t('ruleAttribute.query') }}</el-button>\r\n            <el-button size=\"mini\" @click=\"handleApply\" type=\"primary\" v-waves>\r\n              <svg-icon icon-class=\"apply\" />\r\n              {{ $t('ruleAttribute.apply') }}\r\n            </el-button>\r\n          </cus-btn-wrapper>\r\n        </el-row>\r\n        <el-row :gutter=\"10\">\r\n          <cus-col :span=\"6\">\r\n            <el-form-item :label=\"$t('ruleAttribute.state')\" prop>\r\n              <cus-select :codetype=\"'rulestate'\" v-model=\"params.state\"></cus-select>\r\n            </el-form-item>\r\n          </cus-col>\r\n          <cus-col :span=\"6\">\r\n            <el-form-item :label=\"$t('ruleAttribute.performType')\" prop=\"performType\">\r\n              <cus-select :codetype=\"'performType'\" v-model=\"params.performType\"></cus-select>\r\n            </el-form-item>\r\n          </cus-col>\r\n          <cus-col :span=\"6\">\r\n            <el-form-item :label=\"$t('ruleAttribute.ruleType')\" prop=\"ruleType\">\r\n              <cus-radio :codetype=\"'ruleType'\" :isRequired=\"true\" v-model=\"params.ruleType\"></cus-radio>\r\n            </el-form-item>\r\n          </cus-col>\r\n        </el-row>\r\n      </el-form>\r\n    </cus-filter-wraper>\r\n    <cus-table-title :label=\"$t('ruleAttribute.tableName')\"></cus-table-title>\r\n    <el-table\r\n      :data=\"configs\"\r\n      size=\"mini\"\r\n      fit\r\n      element-loading-text=\"Loading\"\r\n      border\r\n      highlight-current-row\r\n      align=\"center\"\r\n      class=\"wraper-table\"\r\n      ref=\"page\"\r\n    >\r\n      <el-table-column :label=\"$t('table.select')\" width=\"50\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-radio :label=\"scope.row.id\" @change=\"chooseOne(scope.row)\" v-model=\"radio\">{{''}}</el-radio>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" sortable type=\"index\" width=\"60px\">\r\n          <template v-slot=\"scope\">\r\n                              {{ initTableIndex('page', scope.$index) }}\r\n          </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('ruleTest.ruleInfo')\" width=\"75px\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button  size=\"mini\" type=\"text\" @click=\"detail(scope.row)\">\r\n            <i class=\"el-icon-d-arrow-right\"></i>\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('ruleAttribute.code')\" prop=\"code\" align=\"center\"></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleAttribute.ruleBussModu')\"\r\n        prop=\"ruleBussModu\"\r\n        :formatter=\"businessFormat\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <!-- <el-table-column\r\n        :label=\"$t('ruleAttribute.ruleAscrCls')\"\r\n        prop=\"ruleAscrCls\"\r\n        :formatter=\"classFormat\"\r\n        align=\"center\"\r\n      ></el-table-column>-->\r\n      <el-table-column\r\n        :label=\"$t('ruleAttribute.ruleProperties')\"\r\n        prop=\"ruleAttr\"\r\n        :formatter=\"ruleAttrFormat\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleAttribute.performType')\"\r\n        prop=\"performType\"\r\n        :formatter=\"performTypeFormat\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleAttribute.node')\"\r\n        prop=\"node\"\r\n        :formatter=\"nodeFormat\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleAttribute.ruleType')\"\r\n        prop=\"ruleType\"\r\n        :formatter=\"ruleTypeFormat\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleAttribute.state')\"\r\n        prop=\"state\"\r\n        :formatter=\"stateFormat\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column :label=\"$t('ruleAttribute.description')\" prop=\"description\" align=\"center\"></el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <cus-pagination\r\n      ref=\"page\"\r\n      url=\"/rule-attribute/list\"\r\n      :pageSize=\"10\"\r\n      :params=\"params\"\r\n      v-model=\"configs\"\r\n    />\r\n     <el-dialog :visible.sync=\"dialogVisible\" v-dialog-drag width=\"70%\">\r\n       <cus-page-title-back @backTo=\"goBack\" :label=\"$t('ruleAttribute.ruleInfo')\" :isDialog=\"true\"></cus-page-title-back>\r\n      <div style=\"height:500px; overflow: auto;\" class=\"scrollbar\">\r\n          <el-scrollbar style=\"height:100%;\">\r\n            <template v-if=\"ruleType == '1'&& pageState && performType == '1'\">\r\n              <rule-make :ruleCode.sync=\"ruleCode\" :flag=\"ruleFlag\" :ruleQuery=\"ruleQuery\"  :performType=\"performType\"></rule-make>\r\n            </template>\r\n            <template v-if=\" ruleType == '2' && pageState && performType == '1'\">\r\n              <rule-calculator-make :ruleCode.sync=\"ruleCode\" :flag=\"ruleFlag\" :ruleQuery=\"ruleQuery\" :performType=\"performType\"></rule-calculator-make>\r\n            </template>\r\n            <template v-if=\"performType == '2' && pageState\">\r\n                <rule-sql-definition\r\n                  :ruleCode.sync=\"ruleCode\"\r\n                  :flag.sync=\"ruleFlag\"\r\n                  :ruleQuery=\"ruleQuery\"\r\n                  :ruleType ='ruleType'\r\n                  :performType='performType'\r\n                ></rule-sql-definition>\r\n            </template>\r\n          </el-scrollbar>\r\n       </div>\r\n    </el-dialog>\r\n  </cus-wraper>\r\n  \r\n</template>\r\n<script>\r\nimport { mapGetters } from \"vuex\";\r\nimport { closeVisitTag } from \"@/utils/tagsView\";\r\nimport ruleCalculatorMake from \"@/views/ibrms/ruleManagement/ruleCalculatorMake\";\r\nimport ruleSqlDefinition from \"@/views/ibrms/ruleManagement/ruleSqlDefinition\";\r\nimport ruleMake from \"@/views/ibrms/ruleManagement/ruleMake\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      //表格数据\r\n      configs: [],\r\n      //查询参数\r\n      params: {\r\n        code: \"\",\r\n        ruleBussModu: \"\",\r\n        ruleBussModuDeta: \"\",\r\n        state: \"7\",\r\n        ruleType: \"1\",\r\n        performType: \"\",\r\n        pdEdition:'',\r\n        pdSlChanl:[]\r\n      },\r\n      rules: {\r\n        pdCode: [\r\n          {\r\n            pattern: /^[A-Za-z0-9]{1,10}$/,\r\n            message: \"必须由数字和字母组成，且长度不超过10\",\r\n            trigger: \"blur\"\r\n          }\r\n        ],\r\n        pdEdition: [\r\n          {\r\n            pattern: /^[0-9]{1,8}$/,\r\n            message: \"必须由数字组成，且长度不超过8\",\r\n            trigger: \"blur\"\r\n          }\r\n        ]\r\n      },\r\n      radio: false,\r\n      stateArray: [],\r\n      businessArray: [],\r\n      ruleTypeArray: [],\r\n      nodeArray: [],\r\n      performTypeArray: [],\r\n      ibrmsstateFilter: \"\",\r\n      nodeName: \"ruleDefinition\",\r\n      finelinesRegionFilter: \"\",\r\n      dialogVisible: false,\r\n      radio: false,\r\n      ruleQuery: \"\",\r\n      ruleType: \"\",\r\n      ruleFlag: \"\",\r\n      performType:\"1\",\r\n      pageState:false\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"ldcodes\"])\r\n  },\r\n  components: {\r\n    ruleCalculatorMake,\r\n    ruleMake,\r\n    ruleSqlDefinition\r\n  },\r\n  created() {\r\n    let ldcodes = this.$store.state.ldcode.ldcodes;\r\n    this.stateArray = ldcodes[\"ibrmsstate\"];\r\n    this.businessArray = ldcodes[\"ruleBusinessModule\"];\r\n    this.ruleAttrArray = ldcodes[\"ruleAttr\"];\r\n    this.validArray = ldcodes[\"ibrmsvalid\"];\r\n    this.ruleTypeArray = ldcodes[\"ruleBussModuDeta\"];\r\n    this.nodeArray = ldcodes[\"ruleNode\"];\r\n    this.performTypeArray = ldcodes[\"performType\"];\r\n    //初始化状态下拉\r\n    this.ibrmsstateFilter = \"1$0$2\";\r\n  },\r\n  methods: {\r\n    handleSearch() {\r\n      this.$refs.page.reload();\r\n    },\r\n    chooseOne(row) {\r\n      this.radio = row.id;\r\n      var state = row.state;\r\n      if (state == undefined) {\r\n          state = \"0\";\r\n      }\r\n      if(!row.pdSlChanl){\r\n          row.pdSlChanl = [];\r\n      }\r\n      this.cacheJsonData(\"ruleFlag\", state);\r\n      this.cacheJsonData(\"attributeObj\", row);\r\n      this.cacheJsonData(\"ruleCode\", row.code);\r\n      this.cacheJsonData(\"menuPage\", true);\r\n      this.cacheJsonData(\"nodeName\", this.nodeName);\r\n      closeVisitTag();\r\n      this.$router.push({\r\n        name: \"规则定义界面\",\r\n        params: {\r\n          ruleFlag: state,\r\n          attributeObj: row,\r\n          ruleCode: row.code,\r\n          menuPage: true,\r\n          nodeName: this.nodeName\r\n        }\r\n      });\r\n    },\r\n    stateFormat(row, column) {\r\n      let state = row.state;\r\n      if (!state || typeof state == \"undefined\" || state == null) {\r\n        state = \"7\";\r\n      }\r\n      let stateValue = \"\";\r\n      this.stateArray.forEach((st, index) => {\r\n        if (st.code === state) {\r\n          stateValue = st.codename;\r\n        }\r\n      });\r\n      return stateValue;\r\n    },\r\n    ruleTypeFormat(row, column) {\r\n      let type = row.ruleType;\r\n      let typeValue = \"\";\r\n      this.ruleTypeArray.forEach((t, index) => {\r\n        if (t.code === type) {\r\n          typeValue = t.codename;\r\n        }\r\n      });\r\n      return typeValue;\r\n    },\r\n    businessFormat(row, column) {\r\n      let business = row.ruleBussModu;\r\n      let businessValue = \"\";\r\n      this.businessArray.forEach((bus, index) => {\r\n        if (bus.code === business) {\r\n          businessValue = bus.codename;\r\n        }\r\n      });\r\n      return businessValue;\r\n    },\r\n    ruleAttrFormat(row, column) {\r\n      let ruleAttr = row.ruleAttr;\r\n      let ruleAttrValue = \"\";\r\n      this.ruleAttrArray.forEach((attr, index) => {\r\n        if (attr.code === ruleAttr) {\r\n          ruleAttrValue = attr.codename;\r\n        }\r\n      });\r\n      return ruleAttrValue;\r\n    },\r\n    performTypeFormat(row, column) {\r\n      let performType = row.performType;\r\n      let performTypeValue = \"\";\r\n      this.performTypeArray.forEach((attr, index) => {\r\n        if (attr.code === performType) {\r\n          performTypeValue = attr.codename;\r\n        }\r\n      });\r\n      return performTypeValue;\r\n    },\r\n    nodeFormat(row, column) {\r\n      let ruleNode = row.node;\r\n      let ruleNodeValue = \"\";\r\n      this.nodeArray.forEach((node, index) => {\r\n        if (node.code === ruleNode && row.state == \"2\") {\r\n          ruleNodeValue = node.codename;\r\n        }\r\n      });\r\n\r\n      return ruleNodeValue;\r\n    },\r\n    handleApply() {\r\n      this.cacheJsonData(\"ruleFlag\", '0');\r\n      this.cacheJsonData(\"attributeObj\", this.params);\r\n      this.cacheJsonData(\"ruleCode\", \"\");\r\n      this.cacheJsonData(\"menuPage\", true);\r\n      this.cacheJsonData(\"nodeName\", this.nodeName);\r\n      closeVisitTag()\r\n      this.params.id = \"\";\r\n      this.$router.push({\r\n        name: \"规则定义界面\",\r\n        params: {\r\n          ruleFlag: \"0\",\r\n          attributeObj: this.params,\r\n          ruleCode: \"\",\r\n          menuPage: true,\r\n          nodeName: this.nodeName\r\n        }\r\n      });\r\n    },\r\n    changProductType() {\r\n      if (this.form.pdType == \"2\") {\r\n        this.form.riskType = \"\";\r\n        this.form.riskDetail 