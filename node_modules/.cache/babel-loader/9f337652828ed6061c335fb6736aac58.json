 align=\"center\" type=\"index\"></el-table-column>\r\n          <el-table-column :label=\"$t('claimTestDetails.compensationType')\" align=\"center\" prop=\"pdCode\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.policyCurrency')\" align=\"center\"\r\n                           prop=\"planCode\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.compensationAmount')\" align=\"center\"\r\n                           prop=\"planName\"/>\r\n        </el-table>\r\n\r\n\r\n      </div>\r\n      <!--赔付保单计算-->\r\n      <div v-if=\"configs7Flag\">\r\n        <cus-table-title :label=\"$t('claimTestDetails.paymentInformation')\"></cus-table-title>\r\n        <el-table :data=\"configs7\" align=\"center\" border class=\"wraper-table\" element-loading-text=\"Loading\" fit\r\n                  highlight-current-row size=\"mini\">\r\n          <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\"></el-table-column>\r\n          <el-table-column :label=\"$t('claimTestDetails.policyNumber')\" align=\"center\"\r\n                           prop=\"planCode\"/>\r\n          <el-table-column :label=\"$t('rule.product')\" align=\"center\"\r\n                           prop=\"paymentCode\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.compensationType')\" align=\"center\"\r\n                           prop=\"paymentName\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.policyCurrency')\" align=\"center\" prop=\"currency\">\r\n            <template v-slot=\"scope\">{{ scope.row.currency | filterCurrency }}</template>\r\n          </el-table-column>\r\n          <el-table-column :label=\"$t('claimTestDetails.adjustedAmount')\" align=\"center\" prop=\"premium\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.compensationAmount')\" align=\"center\"\r\n                           prop=\"premiumCalMethod\">\r\n            <template v-slot=\"scope\">{{ scope.row.premiumCalMethod | filterCalMethod }}</template>\r\n          </el-table-column>\r\n\r\n          <el-table-column :label=\"$t('claimTestDetails.controlFormula')\" align=\"center\"\r\n                           prop=\"payEndDate\"/>\r\n\r\n          <el-table-column :label=\"$t('claimTestDetails.statusAfterPayment')\" align=\"center\"\r\n                           prop=\"payStartDate\"/>\r\n        </el-table>\r\n      </div>\r\n\r\n      <!--赔付保障计算-->\r\n      <div v-if=\"configs8Flag\">\r\n        <cus-table-title :label=\"$t('claimTestDetails.compensationProtectionCalc')\"></cus-table-title>\r\n        <el-table :data=\"configs8\" align=\"center\" border class=\"wraper-table\" element-loading-text=\"Loading\" fit\r\n                  highlight-current-row size=\"mini\">\r\n          <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\"></el-table-column>\r\n          <el-table-column :label=\"$t('i18nView.pdCode')\" align=\"center\" prop=\"pdCode\"/>\r\n          <el-table-column :label=\"$t('i18nView.guarantee')\"\r\n                           prop=\"benefitCode\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.sumAssured')\" align=\"center\" prop=\"amount\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.protectionPeriodStart')\" align=\"center\"\r\n                           prop=\"starBnftDate\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.protectionPeriodEnd')\" align=\"center\"\r\n                           prop=\"endBnftDate\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.adjustedAmount')\" align=\"center\"\r\n                           prop=\"startDateOfCollection\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.policyCurrency')\" align=\"center\" prop=\"currency\">\r\n            <template v-slot=\"scope\">{{ scope.row.currency | filterCurrency }}</template>\r\n          </el-table-column>\r\n          <el-table-column :label=\"$t('claimTestDetails.compensationAmount')\" align=\"center\"\r\n                           prop=\"premiumCalMethod\">\r\n          </el-table-column>\r\n          <el-table-column :label=\"$t('claimTestDetails.endDateOfCollection')\" align=\"center\"\r\n                           prop=\"collectionDeadline\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.statusAfterPayment')\" align=\"center\"\r\n                           prop=\"payStartDate\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.controlFormula')\" align=\"center\"\r\n                           prop=\"payEndDate\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button class=\"filter-button\" icon=\"el-icon-info\" size=\"mini\" type=\"primary\"\r\n                         @click=\"\">\r\n              </el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </div>\r\n\r\n\r\n      <!--豁免计算-->\r\n      <div v-if=\"configs9Flag\">\r\n        <cus-table-title :label=\"$t('claimTestDetails.exemptCalculation')\"/>\r\n        <el-table :data=\"configs9\" align=\"center\" border class=\"wraper-table\" element-loading-text=\"Loading\" fit\r\n                  highlight-current-row size=\"mini\">\r\n          <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\"></el-table-column>\r\n          <el-table-column :label=\"$t('i18nView.pdCode')\" align=\"center\" prop=\"\"/>\r\n          <el-table-column :label=\"$t('i18nView.guarantee')\" prop=\"\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.exemptPolicy')\" align=\"center\" prop=\"\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.exemptedProducts')\" align=\"center\" prop=\"\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.exemptedPaymentItems')\" align=\"center\" prop=\"\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.premiumWaived')\" align=\"center\" prop=\"\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.exemptedStart')\" align=\"center\" prop=\"\"/>\r\n          <el-table-column :label=\"$t('claimTestDetails.exemptedEnd')\" align=\"center\" prop=\"\"/>\r\n        </el-table>\r\n      </div>\r\n    </div>\r\n\r\n    <!--测试点结论-->\r\n    <cus-table-title :label=\"$t('productTestingNewDetails.testPointConclusion')\"/>\r\n    <el-table :data=\"concForm.funcConcData\" align=\"center\" border class=\"wraper-table\" fit highlight-current-row>\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\" width=\"50px\"/>\r\n      <el-table-column :label=\"$t('productTestingNewDetails.testPoint')\" align=\"center\"\r\n                       prop=\"pdTestFuncDesc\" width=\"130px\"/>\r\n      <el-table-column :label=\"$t('productTestingNewDetails.testConclusion')\" align=\"center\"\r\n                       width=\"200px\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.row.pdTestConc === '1' ? '不通过' : '通过' }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('productTestingNewDetails.conclusionDetails')\" align=\"center\"\r\n                       prop=\"pdTestConcDesc\"/>\r\n    </el-table>\r\n\r\n\r\n    <!--测试案例详情弹窗-->\r\n    <el-dialog v-dialog-drag :close-on-click-modal=\"false\" :destroy-on-close=\"true\" :show-close=true :visible.sync=\"dialogTableVisible2\"\r\n               center top=\"8vh\" width=\"1200px\">\r\n      <div>\r\n      <span style=\"cursor: pointer\" @click=\"back2\">\r\n        <i class=\"el-icon-back\" style=\"margin-right: 5px\">{{ $t('table.back') }}</i>\r\n      </span>\r\n        <el-divider direction=\"vertical\"></el-divider>\r\n        <label>{{ $t('productTestingNewDetails.testCaseDetails') }}</label>\r\n      </div>\r\n      <hr>\r\n      <br/>\r\n      <casereadonly v-if=\"dialogTableVisible2\" :caseDef=\"caseDefRow\"></casereadonly>\r\n    </el-dialog>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\nimport {queryPdFuncConc,} from '@/api/pdtest/productClaimTest'\r\n\r\nimport casereadonly from '@/views/product-testing/claim-test/casereadonly'\r\n\r\nlet _this\r\nexport default {\r\n  name: 'productClaimTestCaseRecord',\r\n  filters: {\r\n    filter_Status(code) {\r\n      return _this.testConcStatus[code]\r\n    },\r\n    filter_CaseStatus(code) {\r\n      return _this.testCaseStatus[code]\r\n    },\r\n    filterCaseType(code) {\r\n      return _this.testCaseType[code]\r\n    },\r\n    filterCaseDefinition(code) {\r\n      return _this.testCaseDefinition[code]\r\n    },\r\n    filterCurrency(code) {\r\n      return _this.getCodeName('currency2', code, true)\r\n    }, filterCalMethod(code) {\r\n      return _this.getCodeName('pd_prem_cal_method', code, true)\r\n    }\r\n    ,\r\n    filterBnftType(code) {\r\n      return _this.getCodeName('guaranteeDetails', code, true)\r\n    },\r\n    filterFreqType(code) {\r\n      return _this.getCodeName('pd_prem_freq', code, true)\r\n    }\r\n\r\n  },\r\n  components: {\r\n    casereadonly\r\n  },\r\n  data() {\r\n    _this = this\r\n    return {\r\n      testConcStatus: {'0': '未测试', '1': '不通过', '2': '通过'},\r\n      testCaseStatus: {'0': '未测试', '1': '已测试'},\r\n      testCaseType: {'1': '自动化测试', '2': '自定义测试'},\r\n      testCaseDefinition: {'1': '页面定义', '2': '上传定义'},\r\n      props: {},\r\n      chooseConfigs4Mark: false,\r\n      configs2: [],\r\n      selectConfigs2: '',\r\n      selectedId: '',\r\n      configs3: [],\r\n      configs5: [],\r\n      selectConfigs5: '',\r\n      configs6: [],\r\n      selectConfigs6: '',\r\n      resultIcon: 'el-icon-arrow-down',\r\n      configs7: [],\r\n      configs8: [],\r\n      configs9: [],\r\n      configs10: [],\r\n      configs11: [],\r\n      configs5Flag: false,\r\n      configs7Flag: false,\r\n      configs6Flag: false,\r\n      configs8Flag: false,\r\n      configs9Flag: false,\r\n      concForm: {'funcConcData': []},\r\n      params: {},\r\n      params3: {'pdTestInfoId': '0'},\r\n      CaseDetails: {},\r\n      selecteConfigs4: {},\r\n      selecteConfigs5: {},\r\n      selecteConfigs6: {},\r\n      dialogTableVisible2: false,\r\n      resultProductTable: false,\r\n      resultPlanTable: false,\r\n      resultPaymentTable: false,\r\n      resultGuaranteeTable: false,\r\n      caseDefRow: {}\r\n    }\r\n  },\r\n  computed: {},\r\n  created() {\r\n    let pdInfo = this.getJsonData('pdTestInfo')\r\n  },\r\n  methods: {\r\n    filterBnftDetailType(code, sencode) {\r\n      return this.getCodeName('guaranteeDetails.' + code, sencode, true)\r\n    },\r\n    caseDefinition() {\r\n      this.$router.push({path: '/product-testing/claim-test/claim-test-casedef'})\r\n    },\r\n    testCaseDetails(row) {\r\n      this.caseDefRow = row\r\n      this.dialogTableVisible2 = true\r\n    },\r\n    testCaseFuncConc(row) {\r\n      // this.chooseConfigs4Mark = true\r\n      // this.resultIcon = 'el-icon-arrow-up'\r\n      this.selectedId = row.id\r\n      this.testCaseResultDetails(row)\r\n      this.concForm.funcConcData = []\r\n      queryPdFuncConc(this.buildRequestData({'pdTestCaseId': row.id})).then(res => {\r\n        this.concForm.funcConcData = res.data\r\n      })\r\n    },\r\n    testCaseResultDetails(row) {\r\n\r\n      //获取测试功能点 根据功能点展示不同的表格\r\n      let funcCodes = row.pdFuncDesc.map(data => data.pd_test_func_code).toString()\r\n      if (funcCodes.indexOf('RN01') >= 0) {\r\n        this.resultProductTable = true\r\n        this.resultPaymentTable = true\r\n      }\r\n\r\n      if (funcCodes.indexOf('RN02') >= 0) {\r\n        this.resultProductTable = true\r\n        this.resultPlanTable = true\r\n        this.resultPaymentTable = true\r\n        this.resultGuaranteeTable = true\r\n      }\r\n      //组装测试结果\r\n      // this.buildTestResult(row.pdTestCaseData)\r\n    },\r\n    buildTestResult(pdTestCaseData) {\r\n      let caseData = JSON.parse(pdTestCaseData)\r\n      let result = caseData.testResult\r\n      this.configs5 = result.products\r\n      this.testData = result\r\n      this.configs6 = result.plans\r\n      this.configs7 = result.payments\r\n      this.configs8 = result.bnfts\r\n    },\r\n    buildPlanData(row) {\r\n      this.configs6 = this.testData.plans.filter(plan => plan.pdCode === row.pdCode)\r\n    },\r\n    buildPaymentAndBnftData(row) {\r\n      this.configs7 = this.testData.payments.filter(payment => payment.planCode === row.planCode)\r\n      this.configs8 = this.testData.bnfts.filter(payment => payment.planCode === row.planCode)\r\n    },\r\n    hideTestCaseResult() {\r\n      //显示结果\r\n      if (this.resultIcon === 'el-icon-arrow-down') {\r\n        this.chooseConfigs4Mark = true\r\n        this.resultIcon = 'el-icon-arrow-up'\r\n      } else if (this.resultIcon === 'el-icon-arrow-up') {\r\n        this.chooseConfigs4Mark = false\r\n        this.resultIcon = 'el-icon-arrow-down'\r\n      }\r\n    },\r\n    back2() {\r\n      this.dialogTableVisible2 = false\r\n    },\r\n    backPage() {\r\n      this.$router.push({path: '/product-testing/claim-test/claim-test-detail'})\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n"],"sourceRoot":"src/views/product-testing/claim-test/caserecord"}]}