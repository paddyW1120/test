\n\r\n    <cus-page-title-back :label=\"$t('productCaseTestRecord.pageTitle')\" @backTo=\"backPage\"/>\r\n    <!-- 商品信息 -->\r\n    <cus-commodity-info></cus-commodity-info>\r\n\r\n\r\n    <!--测试案例-->\r\n    <cus-table-title :label=\"$t('productTestingNewDetails.testCase')\"></cus-table-title>\r\n    <el-table :data=\"configs3\" align=\"center\" border class=\"wraper-table\" element-loading-text=\"Loading\" fit\r\n              highlight-current-row\r\n              size=\"mini\">\r\n      <el-table-column align=\"center\" width=\"50\">\r\n        <template slot-scope=\"scope\">\r\n          <el-radio-group v-model=\"selectConfigs2\">\r\n                  <span>\r\n                <el-radio :disabled=\"scope.row.pdTestStatus === '0'\" :label=\"scope.row\"\r\n                          @change=\"testCaseFuncConc\">{{ '' }}</el-radio>\r\n                  </span>\r\n          </el-radio-group>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\"></el-table-column>\r\n      <el-table-column :label=\"$t('productTestingNewDetails.testCaseDetails')\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button class=\"filter-button\" icon=\"el-icon-info\" size=\"mini\" type=\"primary\"\r\n                     @click=\"testCaseDetails(scope.row)\">\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('productTestingNewDetails.testCaseCode')\" align=\"center\"\r\n                       prop=\"pdTestCaseCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('productTestingNewDetails.testCaseType')\" align=\"center\"\r\n                       prop=\"pdTestCaseType\">\r\n        <template v-slot=\"scope\">{{ scope.row.pdTestCaseType | filterCaseType }}</template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('productTestingNewDetails.testType')\" align=\"center\"\r\n                   