item>\r\n          </cus-col>\r\n\r\n\r\n          <cus-condition @show=\"moreCondition=!moreCondition\">\r\n            <el-button v-waves class=\"filter-button\" icon=\"el-icon-search\" size=\"small\" type=\"primary\" @click=\"handleSearch\">\r\n              {{$t('table.search')}}\r\n            </el-button>\r\n            <el-button v-auth=\"'sys-user:add'\" v-waves class=\"filter-button\" icon=\"el-icon-plus\" size=\"small\" type=\"primary\" @click=\"handleCreate\">{{                  $t('table.add') }}\r\n            </el-button>\r\n          </cus-condition>\r\n        </el-row>\r\n        <el-row :gutter=\"10\" v-show=\"moreCondition\">\r\n          <el-col :span=\"7\">\r\n            <el-form-item :label=\"$t('userView.table_userName')\">\r\n              <el-input v-model=\"params.userName\" :placeholder=\"$t('form.please_enter')\" class=\"filter-item\"\r\n                        clearable></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"7\">\r\n            <el-form-item :label=\"$t('userView.table_state')\">\r\n              <cus-select v-model=\"params.userState\" :codetype=\"'userstate'\">\r\n              </cus-select>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n\r\n    <cus-table-title :label=\"$t('userView.userList')\"></cus-table-title>\r\n    <el-table :data=\"configs\" align=\"center\" border class=\"wraper-table\" element-loading-text=\"Loading\"\r\n              fit highlight-current-row size=\"mini\">>\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" sortable type=\"index\"></el-table-column>\r\n      <el-table-column :label=\"$t('userView.table_userName')\" align=\"center\" prop=\"userName\"></el-table-column>\r\n      <el-table-column :label=\"$t('userView.table_userCode')\" align=\"center\" prop=\"userCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('userView.table_gender')\" align=\"center\" prop=\"gender\">\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ scope.row.gender == 1 ? $t('userView.table_man') : $t('userView.table_woman') }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('userView.table_phone')\" align=\"center\" prop=\"phone\"></el-table-column>\r\n      <el-table-column :label=\"$t('userView.table_mail')\" align=\"center\" prop=\"email\"></el-table-column>\r\n<!--      <el-table-column :label=\"$t('userView.table_avatar')\" align=\"center\" prop=\"avatar\">-->\r\n<!--        <template slot-scope=\"scope\">-->\r\n<!--              <span>-->\r\n<!--                <img :src=\"scope.row.avatar\" alt=\"\" style=\"width: 50px;height: 50px\">-->\r\n<!--              </span>-->\r\n<!--        </template>-->\r\n<!--      </el-table-column>-->\r\n      <el-table-column :label=\"$t('userView.table_state')\" align=\"center\" prop=\"userState\">\r\n        <template slot-scope=\"scope\">\r\n          <el-switch v-model=\"scope.row.userState\" @change=\"updateState($event,scope.row)\" active-value=\"1\" :active-text=\"$t('userView.table_enable')\" :inactive-text=\"$t('userView.table_disable') \"\r\n                     inactive-value=\"0\"></el-switch>\r\n\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.actions')\" align=\"center\" fixed=\"right\" width=\"300\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button v-auth=\"'sys-user:resetPwd'\" class=\"filter-button\" icon=\"el-icon-warning\" size=\"mini\"\r\n                     type=\"primary\" @click=\"handleResetPwd(scope.row)\">\r\n            {{ $t('userView.resetPassword') }}\r\n          </el-button>\r\n          <el-button v-if=\"isSuperAdmin\" v-auth=\"'sys-user:updatetenant'\" class=\"filter-button\"\r\n                     icon=\"el-icon-edit\" size=\"mini\" type=\"primary\" @click=\"handleUpdateTenant(scope.row.id)\">\r\n            {{ $t('userView.editTenant') }}\r\n          </el-button>\r\n          <cus-del-btn v-auth=\"'sys-user:delete'\" @ok=\"handleDelete(scope.row.id)\"/>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <cus-pagination ref=\"page\" v-model=\"configs\" :pageSize=\"10\" :params=\"params\" url=\"/sys-user-admin/base/listAll\"/>\r\n\r\n    <!---新增修改dialog---->\r\n\r\n    <el-dialog v-dialog-drag  :visible.sync=\"dialogVisible\" @close=\"dialogVisible=false\">\r\n      <cus-page-title-back :isDialog=\"true\" @backTo=\"dialogVisible=false\" :label=\"titleMap[dialogStatus]\" />\r\n      <el-form ref=\"dataForm\" :model=\"form\" :rules=\"rules\" label-position=\"right\" style=\"margin-left: 10%;width: 100%\"\r\n               label-width=\"100px\">\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item :label=\"$t('userView.table_userCode')+':'\" prop=\"userCode\">\r\n              <el-input v-model=\"form.userCode\"\r\n                        :placeholder=\"$t('userView.input')+$t('userView.table_userCode')\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item :label=\"$t('userView.table_userName')+':'\" prop=\"userName\">\r\n              <el-input v-model=\"form.userName\"\r\n                        :placeholder=\"$t('userView.input')+$t('userView.table_userName')\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item :label=\"$t('userView.table_password')+':'\" prop=\"pwd\">\r\n              <el-input v-model=\"form.pwd\" :placeholder=\"$t('userView.input')+$t('userView.table_password')\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item :label=\"$t('userView.table_gender')+':'\" prop=\"gender\">\r\n              <el-select v-model=\"form.gender\" :placeholder=\"$t('i18nView.selectPlaceholder')+$t('userView.table_gender')\"\r\n                         class=\"filter-item\">\r\n                <el-option v-for=\"item in genderList\" :key=\"item.key\" :label=\"item.name\"\r\n                           :value=\"item.key\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item :label=\"$t('userView.table_phone')+':'\" prop=\"phone\">\r\n              <el-input v-model=\"form.phone\" :placeholder=\"$t('userView.input')+$t('userView.table_phone')\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item :label=\"$t('userView.table_mail')+':'\" prop=\"email\">\r\n              <el-input v-model=\"form.email\" :placeholder=\"$t('userView.input')+$t('userView.table_mail')\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"12\">\r\n            <el-form-item :label=\"$t('userView.weComId')+':'\" prop=\"weComId\">\r\n              <el-input v-model=\"form.weComId\" :placeholder=\"$t('userView.input')+$t('userView.weComId')\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"20\"><el-col :span=\"12\">\r\n          <el-form-item :label=\"$t('userView.table_state')+':'\" prop=\"userState\">\r\n            <el-select v-model=\"form.userState\" :placeholder=\"$t('i18nView.selectPlaceholder')+$t('userView.table_state')\"\r\n                       class=\"filter-item\">\r\n              <el-option v-for=\"item in stateList\" :key=\"item.key\" :label=\"item.name\"\r\n                         :value=\"item.key\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col></el-row>\r\n        <el-row :gutter=\"20\"><el-col :span=\"12\">\r\n          <el-form-item :label=\"$t('userView.table_avatar')+':'\" prop=\"avatar\">\r\n            <el-upload :auto-upload=\"false\" :before-upload=\"beforeAvatarUpload\" :on-change='changeUpload'\r\n                       :show-file-list=\"false\"\r\n                       action=\"#\" class=\"avatar-uploader\">\r\n              <img v-if=\"form.avatar\" :src=\"form.avatar\" class=\"avatar\">\r\n              <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n            </el-upload>\r\n          </el-form-item>\r\n        </el-col></el-row>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button size=\"small\" icon='el-icon-circle-close' type=\"primary\" @click=\"dialogVisible = false\"> {{ $t('table.cancel') }} </el-button>\r\n          <el-button v-auth=\"'sys-user:add'\" class=\"el-icon-success\" size=\"small\" type=\"primary\" @click=\"submitForm\"> {{\r\n              $t('table.confirm')\r\n            }} </el-button>\r\n        </span>\r\n    </el-dialog>\r\n\r\n\r\n    <el-dialog\r\n      v-dialog-drag\r\n      :visible.sync=\"showEditTenant\"\r\n      @close=\"showEditTenant=false\">\r\n      <div>\r\n        <cus-page-title-back :isDialog=\"true\" @backTo=\"showEditTenant=false\" :label=\"titleMap[dialogStatus]\" />\r\n        <el-table ref=\"multipleTenantTable\" :data=\"tenantConfigs\" :row-key=\"getRowKey\"\r\n                  align=\"center\" border class=\"wraper-table\"\r\n                  element-loading-text=\"Loading\"\r\n                  fit highlight-current-row size=\"mini\"\r\n                  @selection-change=\"handleTenantSelectionChange\">>\r\n          <el-table-column :reserve-selection=\"true\" type=\"selection\" width=\"50\"></el-table-column>\r\n          <el-table-column :label=\"$t('sysTenant.comCode')\" align=\"center\" prop=\"comCode\"></el-table-column>\r\n          <el-table-column :label=\"$t('sysTenant.name')\" align=\"center\" prop=\"name\"></el-table-column>\r\n          <el-table-column :label=\"$t('roleView.table_roleName')\" align=\"center\" prop=\"roleList\">\r\n            <template slot-scope=\"scope\">\r\n              <el-select v-model=\"scope.row.userRoleIds\" filterable multiple clearable :placeholder=\"$t('form.please_select')\"\r\n                         @change=\"roleChange($event,scope.row.id)\">\r\n                <el-option\r\n                  v-for=\"(item,index) in scope.row.roleList\"\r\n                  :key=\"index\"\r\n                  :label=\"item.roleName\"\r\n                  :value=\"item.id\">\r\n                </el-option>\r\n              </el-select>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n        <!-- 分页 -->\r\n        <cus-pagination ref=\"roleTenantPage\" v-model=\"tenantConfigs\" :pageSize=\"10\" :params=\"tenantParams\"\r\n                        url=\"/sys-user-admin/base/tenant-user-role\"/>\r\n      </div>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button size=\"small\" type=\"primary\" icon='el-icon-circle-close' @click=\"showEditTenant = false\">取 消</el-button>\r\n        <el-button size=\"small\" type=\"primary\" class=\"el-icon-success\" @click=\"commitUserTenant\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n\r\n\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\n\r\n\r\nimport {addWithoutTenant, updateState,deleteUser, selectTenantByUserId, updateUserTenantAndRole,selectTenants,resetPwd} from '@/api/system/user-admin'\r\n\r\n\r\nexport default {\r\n  name: 'User',\r\n  data() {\r\n    return {\r\n      //静态数据\r\n      genderList: [{key: '1', name: this.$t('userView.table_man')}, {\r\n        key: '0',\r\n        name: this.$t('userView.table_woman')\r\n      }],\r\n      stateList: [{key: '1', name: this.$t('userView.table_enable')}, {\r\n        key: '0',\r\n        name: this.$t('userView.table_disable')\r\n      }],\r\n      moreCondition:false,\r\n      //表格数据\r\n      configs: [],\r\n      roleConfigs: [],// 角色表格数据\r\n      tenantConfigs: [],// 租户表格数据\r\n      multipleTenantSelection: [],// 租户表格选中项\r\n      //查询参数可以是模糊或者等值 like_  eq_\r\n      userRoleList: [],// 用户对应的角色\r\n      userId: null, // 角色表格所对应的用户Id\r\n      isSuperAdmin: false, // 用户是否为超级管理员\r\n      tableKey: 0,\r\n      params: {\r\n        tenantId: '',\r\n        userState: '',\r\n        userName: '',\r\n        userCode: '',\r\n        phone: '',\r\n      },\r\n      tenantList: [],\r\n      optionsConfig: [],\r\n      Roleparams: {\r\n        like_roleName: '',\r\n        like_roleCode: ''\r\n      },\r\n      tenantParams: {\r\n        userId: '',\r\n        like_name: '',\r\n        like_shortName: ''\r\n      },\r\n      selectRoleParams: [],\r\n      roleParams: [], // 角色表提交参数\r\n      form: {  //表单数据与后端实体类对应\r\n        id: '', //主键ID\r\n        userCode: '', //账号\r\n        userName: '', //账号\r\n        pwd: '', //登录密码\r\n        gender: '', //性别 0:女 1:男\r\n        phone: '', //手机号码\r\n        email: '', //邮箱\r\n        avatar: '', //头像\r\n        userState: '', //状态\r\n        qqOpenId: '', //qq登录id\r\n        wxOpenId: '', //wx登录id\r\n        weComId: '' //wx登录id\r\n      },\r\n      rules: {\r\n        userCode: [\r\n          {required: true, pattern: /^[0-9a-zA-Z]{1,8}$/, message: this.$t('userView.ruleuserCode')},\r\n          {required: true, message: this.$t('userView.input') + this.$t('userView.table_userCode'), trigger: 'blur'},\r\n        ],\r\n        pwd: [\r\n          {required: true, pattern: /^(\\w){6,50}$/, message: this.$t('userView.rulepwd')}\r\n        ],\r\n        userName: [\r\n          {required: true, message: this.$t('userView.input') + this.$t('userView.table_userName'), trigger: 'blur'},\r\n        ],\r\n        userState: [\r\n          {\r\n            required: true,\r\n            message: this.$t('i18nView.selectPlaceholder') + this.$t('userView.table_state'),\r\n            trigger: 'blur'\r\n          },\r\n        ]\r\n      },\r\n      showEditTenant: false,\r\n      dialogVisible: false, //dialog显示还是隐藏\r\n      dialogStatus: '', //表单状态是新增还是修改\r\n      titleMap: {\r\n        update: this.$t('userView.update'),\r\n        create: this.$t('userView.add'),\r\n        editRole: this.$t('userView.editRole'),\r\n        editTenant: this.$t('userView.editTenant')\r\n      },\r\n    }\r\n  },\r\n  computed: {},\r\n  created() {\r\n    let rolecodes = this.$store.getters.rolecodes\r\n    if (rolecodes.indexOf('superAdmin', 0) != -1) {\r\n      this.isSuperAdmin = true;\r\n    }\r\n    this.selectTenants()\r\n  },\r\n  methods: {\r\n    selectTenants() {\r\n      selectTenants().then(res =>{\r\n           this.tenantList = res.data\r\n      })\r\n    },handleSearch() {\r\n      this.$refs.page.reload()\r\n    },\r\n    roleChange(val, id) {\r\n        for (let i = 0; i < this.multipleTenantSelection.length; i++) {\r\n            let selected =  this.multipleTenantSelection[i]\r\n            if (selected.id === id){\r\n               selected.userRoleIds = val\r\n            }\r\n        }\r\n    },\r\n    handleCreate() {\r\n      this.resetForm()\r\n      this.dialogStatus = 'create'\r\n      this.dialogVisible = true\r\n    },\r\n    handleResetPwd(row) {\r\n      this.confirmWarning(\"请确认是否重置密码?\").then(()=>{\r\n        resetPwd(row).then(res =>{\r\n            this.notifySuccess('重置完毕')\r\n        })\r\n      })\r\n    },\r\n    updateState(val, row) {\r\n      updateState(row).then(res => {\r\n          this.notifySuccess('状态更新成功')\r\n      })\r\n    },\r\n    handleUpdateTenant(id) {\r\n      this.tenantParams.userId = id\r\n      this.showEditTenant = true;\r\n      this.dialogStatus = 'editTenant'\r\n      this.userId = id;\r\n      this.multipleSelection = [];\r\n      selectTenantByUserId(id).then(res => {\r\n        this.multipleTenantSelection = res.data\r\n        this.$refs.multipleTenantTable.clearSelection()\r\n        res.data.forEach(row => {\r\n          this.$refs.multipleTenantTable.toggleRowSelection(row, true);\r\n        })\r\n        this.$refs.roleTenantPage.reload()\r\n      })\r\n    },\r\n    beforeAvatarUpload(file) {\r\n      const isJPG = file.type === 'image/jpeg'\r\n      const isLt2M = file.size / 1024 < 100\r\n\r\n      if (!isJPG) {\r\n        this.$message.error('上传头像图片只能是 JPG 格式!')\r\n      }\r\n      if (!isLt2M) {\r\n        this.$message.error('上传头像图片大小不能超过 100KB!')\r\n      }\r\n      return isJPG && isLt2M\r\n    },\r\n    changeUpload: function (file, fileList) {\r\n      let reader = new FileReader()\r\n      reader.onload = async (e) => {\r\n        this.form.avatar = e.target.result\r\n      }\r\n      reader.readAsDataURL(file.raw)\r\n    },\r\n    getRowKey(row) {\r\n      return row.id\r\n    },\r\n    handleTenantSelectionChange(val) {\r\n      this.multipleTenantSelection = val;\r\n    },\r\n    commitUserTenant() {\r\n      let vm = this;\r\n      let tmp = []\r\n      let tempRoles = []\r\n      let flag = true\r\n      for (let i = 0; i < vm.multipleTenantSelection.length; i++) {\r\n        tmp.push(vm.multipleTenantSelection[i].id)\r\n        tempRoles.push(vm.multipleTenantSelection[i].userRoleIds)\r\n      }\r\n\r\n      if (tempRoles.length === 0){\r\n         this.notifyError('请选择租户！')\r\n         return\r\n      }\r\n\r\n      tempRoles.forEach(t => {\r\n        if (t.length === 0) {\r\n          flag = false;\r\n        }\r\n      })\r\n      if (flag) {\r\n        let myset = new Set(tmp)\r\n        let userTenants = []\r\n        for (let val of myset) {\r\n          let userTenant = { \"userId\": vm.userId, \"tenantId\": val }\r\n          userTenants.push(userTenant)\r\n        }\r\n\r\n        let userRoles = []\r\n        for (let val of tempRoles) {\r\n          for (let i = 0; i < val.length; i++) {\r\n            let userRole = { \"userId\": vm.userId, \"roleId\": val[i] }\r\n            userRoles.push(userRole)\r\n          }\r\n        }\r\n       let data =  this.buildRequestData({\"userTenants\":userTenants,\"userRoles\":userRoles,\"userId\":this.userId})\r\n        updateUserTenantAndRole(data).then(() => {\r\n            this.notifySuccess(\"租户配置完成\")\r\n            this.handleSearch()\r\n            vm.showEditTenant = false;\r\n        })\r\n      } else {\r\n        this.notifyError('选中行角色下拉选不能为空！')\r\n      }\r\n    },\r\n    handleDelete(id) {\r\n      deleteUser(id).then(response => {\r\n        this.handleSearch()\r\n        this.notifySuccess(response.message)\r\n        this.dialogVisible = false\r\n      })\r\n    },\r\n    submitForm() {\r\n      this.$refs.dataForm.validate(valid => {\r\n        if (valid) {\r\n          let back;\r\n          if (this.form.id) {\r\n            back = updateUser(this.form)\r\n          } else {\r\n            back = addWithoutTenant(this.form)\r\n          }\r\n          back.then(response => {\r\n            this.handleSearch()\r\n            this.notifySuccess(response.message)\r\n            this.dialogVisible = false\r\n          })\r\n        }\r\n      })\r\n    },\r\n    resetForm() {\r\n      this.form = {  //表单数据与后端实体类对应\r\n        id: '', //主键ID\r\n        userCode: '', //账号\r\n        userName: '', //账号\r\n        pwd: '', //登录密码\r\n        gender: '', //性别 0:女 1:男\r\n        phone: '', //手机号码\r\n        email: '', //邮箱\r\n        avatar: '', //头像\r\n        userState: '', //状态\r\n        qqOpenId: '', //qq登录id\r\n        wxOpenId: '', //wx登录id\r\n        weComId: '', //wx登录id\r\n      }\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.spacing {\r\n  margin-bottom: 22px !important;\r\n}\r\n\r\n.avatar-uploader\r\n.el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n.avatar-uploader\r\n.el-upload:hover {\r\n  border-color: #409eff;\r\n}\r\n\r\n.avatar-uploader-icon {\r\n  border: 1px dashed #d9d9d9;\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 90px;\r\n  height: 90px;\r\n  line-height: 90px;\r\n  text-align: center;\r\n}\r\n\r\n.avatar {\r\n  width: 90px;\r\n  height: 90px;\r\n  display: block;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/system/admin-user"}]}