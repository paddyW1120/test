AAc,CAACC,UAAf,CAA0B,YAA1B,E,CAEA;;AACA,IAAMC,YAAY,GAAG,SAAfA,YAAe;AAAA,SAAM,IAAIjB,kBAAJ,CAAW;AACpCkB,IAAAA,IAAI,EAAE,SAD8B;AACnB;AACjBC,IAAAA,cAAc,EAAE;AAAA,aAAO;AAACC,QAAAA,CAAC,EAAE;AAAJ,OAAP;AAAA,KAFoB;AAGpCC,IAAAA,MAAM,EAAEpB,iBAAiB,CAACqB,MAAlB,CAAyBR,oBAAzB;AAH4B,GAAX,CAAN;AAAA,CAArB;;AAKA,IAAMS,MAAM,GAAGN,YAAY,EAA3B;;AAEO,SAASO,WAAT,GAAuB;AAC5B,MAAMC,SAAS,GAAG,IAAIzB,kBAAJ,CAAW;AAC3BkB,IAAAA,IAAI,EAAE,SADqB;AACV;AACjBC,IAAAA,cAAc,EAAE;AAAA,aAAO;AAACC,QAAAA,CAAC,EAAE;AAAJ,OAAP;AAAA,KAFW;AAG3BC,IAAAA,MAAM,EAAEpB,iBAAiB,CAACqB,MAAlB,CAAyBR,oBAAzB;AAHmB,GAAX,CAAlB;AAKAS,EAAAA,MAAM,CAACG,OAAP,GAAiBD,SAAS,CAACC,OAA3B,CAN4B,CAMO;AACpC;;eAEcH,M","sourcesContent":["import Vue from 'vue'\r\nimport Router from 'vue-router'\r\nimport Layout from '@/layout'\r\nimport {setInit} from '@/utils/auth'\r\n\r\nVue.use(Router)\r\n// 静态路由，没有权限限制\r\nexport const constantRoutesTop = [\r\n  {\r\n    path: '/redirect',\r\n    component: Layout,\r\n    hidden: true,\r\n    children: [\r\n      {\r\n        path: '/redirect/:path*',\r\n        component: () => import('@/views/redirect/index')\r\n      }\r\n    ]\r\n  },\r\n  {\r\n    path: '/weComLogin',\r\n    component: () => import('@/views/wecom-login/index'),\r\n    hidden: true\r\n  },\r\n  {\r\n    path: '/weComDashboard',\r\n    component: () => import('@/views/wecom-login/wecom-dashboard/main'),\r\n    hidden: true,\r\n    meta: {noCache: true}\r\n  },\r\n  {\r\n    path: '/process',\r\n    component: () => import('@/views/wecom-login/wecom-dashboard/wecomlist/process'),\r\n    hidden: true,\r\n  },\r\n  {\r\n    path: '/assign',\r\n    component: () => import('@/views/wecom-login/wecom-dashboard/wecomlist/assign'),\r\n    hidden: true\r\n  },\r\n  {\r\n    path: '/login',\r\n    component: () => import('@/views/login/index'),\r\n    hidden: true\r\n  },\r\n  {\r\n    path: '/auth-redirect',\r\n    component: () => import('@/views/login/auth-redirect'),\r\n    hidden: true\r\n  },\r\n  {\r\n    path: '/404',\r\n    component: () => import('@/views/error-page/404'),\r\n    hidden: true\r\n  },\r\n  {\r\n    path: '/401',\r\n    component: () => import('@/views/error-page/401'),\r\n    hidden: true\r\n  },\r\n  {\r\n    path: '/',\r\n    component: Layout,\r\n    redirect: '/dashboard',\r\n    children: [\r\n      {\r\n        path: 'dashboard',\r\n        component: () => import('@/views/dashboard/index'),\r\n        name: 'Dashboard',\r\n        meta: {title: 'dashboard', icon: 'dashboard', affix: true}\r\n      }\r\n    ]\r\n  },\r\n  {\r\n    path: '/profile',\r\n    component: Layout,\r\n    redirect: '/profile/index',\r\n    hidden: true,\r\n    children: [\r\n      {\r\n        path: 'index',\r\n        component: () => import('@/views/profile/index'),\r\n        name: 'Profile',\r\n        meta: {title: 'profile', icon: 'user', noCache: true}\r\n      }\r\n    ]\r\n  }\r\n]\r\n\r\nexport const constantRoutesBottom = [\r\n  {\r\n    path: '/issue',\r\n    component: Layout,\r\n    hidden: true,\r\n    children: [\r\n      {\r\n        path: 'list',\r\n        component: () => import('@/views/dashboard/notepad/index'),\r\n        name: '问题记录',\r\n        meta: {title: 'problemRecord', icon: 'theme'}\r\n      }\r\n    ]\r\n  },\r\n  {\r\n    path: '/theme',\r\n    hidden: true,\r\n    component: Layout,\r\n    children: [\r\n      {\r\n        path: 'index',\r\n        component: () => import('@/views/theme/index'),\r\n        name: 'Theme',\r\n        meta: {title: 'theme', icon: 'theme'}\r\n      }\r\n    ]\r\n  },\r\n  {\r\n    path: '/i18n',\r\n    component: Layout,\r\n    hidden: true,\r\n    children: [\r\n      {\r\n        path: 'index',\r\n        component: () => import('@/views/i18n-demo/index'),\r\n        name: 'I18n',\r\n        meta: {title: 'i18n', icon: 'international'}\r\n      }\r\n    ]\r\n  },\r\n  {\r\n    path: '/ceiling',\r\n    component: Layout,\r\n    name: '测试',\r\n    hidden: true,\r\n    meta: {title: 'test', icon: 'test'},\r\n    children: [\r\n      {\r\n        path: 'test',\r\n        component: () => import('@/views/test/ceiling'),\r\n        name: 'test',\r\n        meta: {title: 'controlTest', icon: 'test'}\r\n      },\r\n      {\r\n        path: 'style',\r\n        component: () => import('@/views/test/newstyle'),\r\n        name: 'newstyle',\r\n        meta: {title: 'styleTest', icon: 'test'}\r\n      },\r\n      {\r\n        path: 'workflow',\r\n        component: () => import('@/views/test/workflow'),\r\n        name: 'workflow',\r\n        meta: {title: 'workflowTest', icon: 'test'}\r\n      },\r\n      {\r\n        path: 'funtionstyle',\r\n        component: () => import('@/views/test/funtionstyle'),\r\n        name: 'funtionstyle',\r\n        meta: { title: '功能样式测试', icon: 'international' }\r\n      },\r\n      {\r\n        path: 'draggable ',\r\n        component: () => import('@/views/test/draggable/Home'),\r\n        name: 'draggable',\r\n        meta: {title: 'dragTest', icon: 'test'}\r\n      },\r\n      {\r\n        path: 'file-comparison',\r\n        component: () => import('@/views/test/file-comparison'),\r\n        name: 'comparison',\r\n        meta: {title: '发布文件对比', icon: 'test'}\r\n      },\r\n    ]\r\n  },\r\n  // 404 page must be placed at the end !!!\r\n  {path: '*', redirect: '/404', hidden: true}\r\n]\r\n\r\nsetInit(\"false\")\r\nsessionStorage.removeItem(\"dialogOnly\")\r\n\r\n// 先取出根节点，没有父id的就是根节点\r\nconst createRouter = () => new Router({\r\n  mode: 'history', // require service support\r\n  scrollBehavior: () => ({y: 0}),\r\n  routes: constantRoutesTop.concat(constantRoutesBottom)\r\n})\r\nconst router = createRouter()\r\n\r\nexport function resetRouter() {\r\n  const newRouter = new Router({\r\n    mode: 'history', // require service support\r\n    scrollBehavior: () => ({y: 0}),\r\n    routes: constantRoutesTop.concat(constantRoutesBottom)\r\n  })\r\n  router.matcher = newRouter.matcher // reset router\r\n}\r\n\r\nexport default router\r\n"]}]}