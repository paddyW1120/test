r\n          <el-form-item :label=\"$t('i18nView.formulaCode')\">\r\n              <el-input v-model=\"params.pdCalcCode\" clearable></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n        <cus-btn-wrapper>\r\n          <el-button  size=\"small\" icon=\"el-icon-search\" type=\"primary\" @click=\"handleSearch\">\r\n            {{$t('table.search')}}\r\n          </el-button>\r\n        </cus-btn-wrapper>\r\n      </el-row>\r\n    </el-form>\r\n    <el-button  size=\"small\" icon=\"el-icon-circle-plus el-icon--left\" type=\"primary\" @click=\"toAddFormula\">\r\n      {{$t('rule.toAddFormula')}}\r\n    </el-button>\r\n    <cus-table-title :label=\"$t('i18nView.calcFormula')\"/>\r\n    <el-table :data=\"tableData_formula\" border element-loading-text=\"Loading\" fit highlight-current-row\r\n              size=\"mini\" @row-click=\"rowClick\">\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\" width=\"50\"></el-table-column>\r\n      <el-table-column :label=\"$t('table.detail')\" align=\"center\" width=\"80\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button  size=\"small\" type=\"text\" @click=\"showFormula(scope.row.pdCalcCode)\">>></el-button>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('i18nView.cmpnType')\" prop=\"pdBussDetailType\">\r\n        <template v-slot=\"scope\">\r\n          {{ scope.row.pdCmpnType | filter_pdCmpnType }}\r\n        </template>\r\n      </el-table-column>\r\n     <!-- <el-table-column align=\"center\" label=\"计算公式类型\" prop=\"pdBussDetailType\">\r\n        <template v-slot=\"scope\">\r\n          {{ scope.row.pdBussDetailType | filter_pdBussDetailType }}\r\n        </template>\r\n      </el-table-column>-->\r\n      <el-table-column align=\"center\" :label=\"$t('rule.pdCalcCode')\" prop=\"pdCalcCode\"></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('rule.pdCalcDesc')\"prop=\"pdCalcDesc\"></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('rule.ruleAttr')\">\r\n        <template v-slot=\"scope\">\r\n          {{ scope.row.ruleAttr === '1' ? $t('rule.general') : (scope.row.ruleAttr === '2' ? $t('rule.individual') : '') }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.actions')\" align=\"center\" width=\"100\">\r\n        <template v-slot=\"scope\">\r\n          <el-button v-auth=\"'clmCtrlFormulaSelect:associate'\" icon=\"el-icon-connection\"\r\n                     size=\"mini\" type=\"primary\" @click=\"handleSelectFormula(scope.row)\">\r\n            {{$t('form.select')}}\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <cus-pagination ref=\"page\" v-model=\"tableData_formula\" :autoBuild=\"false\" :pageSize=\"10\"\r\n                    :params=\"params\" url=\"/pdgenl-cmpn-formulaSelect/base/allFormulaToAssociate\"/>\r\n    <br/>\r\n    <el-form style=\"text-align: right\">\r\n      <el-form-item>\r\n        <el-button  size=\"small\" icon=\"el-icon-refresh\" type=\"primary\" @click=\"refresh\">\r\n          {{$t('table.refresh')}}\r\n        </el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n    <!-- 规则引擎公式定义弹窗 -->\r\n    <cus-rule-definition :attributeObj=\"attributeObj\" :ruleDefinitionClosed=\"ruleModuleClosed\"\r\n                         :visible=\"formulaVisible\"></cus-rule-definition>\r\n    <!-- 公式查看弹窗 -->\r\n    <cus-rule-modify :flag=\"formulaView.flag\" :ruleCode=\"formulaView.ruleCode\" :ruleModifyClosed=\"ruleMakeClosed\"\r\n                     :ruleType=\"'2'\" :visible=\"formulaView.visible\"></cus-rule-modify>\r\n\r\n  </cus-wraper>\r\n</template>\r\n\r\n<script>\r\n  import {getCalcCode,associate} from \"@/api/basicinfo/pdgenlDiscountFormulaSelect\"\r\n\r\n  let _this;\r\nexport default {\r\n  name: 'pdgenlDiscountFormulaSelect',\r\n  props: {\r\n    generalDiscount: {\r\n      type: Object,\r\n      required: true,\r\n      default:{}\r\n    },\r\n    pdBussDetailType: {\r\n      type: String,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    _this = this\r\n    return {\r\n      params: {\r\n        generalDiscount:this.generalDiscount,\r\n        pdCmpnType:'',\r\n        pdBussDetailType: '',\r\n        pdCalcCode: '',\r\n      },\r\n      form_disCountSelect:{\r\n        cmpnType:''\r\n      },\r\n      attributeObj: {},\r\n      formulaVisible: false,\r\n      formulaView: {\r\n        flag: '',\r\n        ruleCode: '',\r\n        ruleQuery: '',\r\n        visible: false\r\n      },\r\n      tableData_formula: [],\r\n      productOptionList: [],//产品数组\r\n      clmCtrlOptions: [],//赔付控制项数组\r\n      selectedFormula: {},\r\n      columnDetails: {}\r\n    }\r\n  },\r\n  filters: {\r\n    filter_pdBussDetailType(code) {\r\n      return _this.getCodeName('ruleBusinessDetailClass', code)\r\n    },\r\n    filter_pdCmpnType(code) {\r\n      return _this.getCodeName('yhtype', code)\r\n    }\r\n  },\r\n  created() {\r\n    this.form_disCountSelect.cmpnType=this.generalDiscount.cmpnType\r\n    this.params.pdBussDetailType = this.pdBussDetailType;\r\n    this.$nextTick(() => {\r\n      this.$refs.page.reload()\r\n    })\r\n  },\r\n  methods: {\r\n    ruleModuleClosed() {\r\n      this.formulaVisible = false;\r\n      this.handleSearch()\r\n    },\r\n    ruleMakeClosed() {\r\n      this.formulaView.visible = false\r\n    },\r\n    back() {\r\n      this.$emit('changeDialogVisible_GenDiscountFormulaSelect', false)\r\n    },\r\n    showFormula(code) {\r\n      this.formulaView.flag = '1';\r\n      this.formulaView.ruleCode = code;\r\n      this.formulaView.ruleQuery = true\r\n      this.formulaView.visible = true\r\n    },\r\n     handleSearch() {\r\n      this.$nextTick(() => {\r\n        this.$refs.page.reload()\r\n      })\r\n    },\r\n    toAddFormula() {\r\n      const param = {\r\n        pdBussType: '1',\r\n        pdBussDetailType: this.pdBussDetailType\r\n      }\r\n      getCalcCode(this.buildRequestData(param)).then(response => {\r\n        this.attributeObj = {\r\n          code: response.data,\r\n          ruleBussModu: '1',\r\n          ruleBussModuDeta: this.pdBussDetailType,\r\n          ruleType: '2',\r\n          ruleAttr: '1',\r\n          pdType: '1',\r\n          cmpnType:this.generalDiscount.cmpnType,\r\n          riskType: '',\r\n          riskDetail: '',\r\n          pdCode: '000000',\r\n          pdEdition: '1'\r\n        }\r\n        this.formulaVisible = true\r\n      })\r\n    },\r\n    rowClick(row, column, event) {\r\n      if (event.target.nodeName === \"INPUT\") {\r\n        if (row.id === this.productBnftForm.id) {\r\n          this.columnDetails = row;\r\n        }\r\n      }\r\n    },\r\n    handleSelectFormula(row) {\r\n      /*let PdtempPdCalc = {\r\n        pdBussType: '1',\r\n        pdBussDetailType: this.pdBussDetailType,\r\n        pdBussId: '',\r\n        pdStructureType: '',\r\n        pdCalcCode: row.pdCalcCode,\r\n        pdCalcDesc: row.pdCalcDesc,\r\n      }*/\r\n      const param = {\r\n        selectedFormula: row,\r\n        generalDiscount: this.generalDiscount\r\n      }\r\n      associate(this.buildRequestData(param)).then(response => {\r\n        console.log(response)\r\n        let pdgenlCmpnCalc = response.data\r\n        this.$emit('bringInCalcCode', this.generalDiscount.cmpnType, pdgenlCmpnCalc.pdCalcCode)\r\n        /*let successFlag = response.data.flag\r\n        let pdtempPdCalc = response.data.pdtempPdCalc\r\n        if (successFlag === true) {\r\n          this.notifySuccess(this.$t('rule.associationSuccess'))\r\n          this.handleSearch()\r\n          this.$emit('bringInCalcCode', '', pdtempPdCalc.pdCalcCode)\r\n        } else {\r\n          this.$confirm(String(this.$t('underwritingAmountFormulaSelect.confirm_overwrite_formula')), String(this.$t('confirmMessage.tip')), {\r\n            confirmButtonText: String(this.$t('confirmMessage.confirm')),\r\n            cancelButtonText: String(this.$t('confirmMessage.cancel')),\r\n            type: 'warning'\r\n          }).then(() => {\r\n            param.needChecking = false\r\n            associate(this.buildRequestData(param)).then(response => {\r\n              let pdtempPdCalc = response.data.pdtempPdCalc\r\n              this.notifySuccess(this.$t('rule.associationSuccess'))\r\n              this.handleSearch()\r\n              this.$emit('bringInCalcCode', '', pdtempPdCalc.pdCalcCode)\r\n            })\r\n          }).catch(() => {\r\n            this.messageInfo(this.$t('underwritingAmountFormulaSelect.canceled'))\r\n          })\r\n        }*/\r\n      })\r\n    },\r\n    refresh() {\r\n      this.params.pdBussDetailType = this.pdBussDetailType\r\n      this.$nextTick(() => {\r\n        this.$refs.page.reload()\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/business-general-conf/pdgenlcmpninfo/discount-formula-select"}]}