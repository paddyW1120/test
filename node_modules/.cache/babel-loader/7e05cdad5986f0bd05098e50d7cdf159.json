\n          <el-row :gutter=\"10\">\r\n            <cus-col :span=\"6\">\r\n              <el-form-item :label=\"$t('ruleRelease.ruleType')\" prop=\"ruleType\">\r\n                <cus-radio :codetype=\"'ruleType'\" :isRequired=\"true\" v-model=\"params.ruleType\" ></cus-radio>\r\n              </el-form-item>\r\n            </cus-col>\r\n          </el-row>\r\n        </el-form>\r\n      </div>\r\n    </cus-filter-wraper>\r\n    <div style=\"height:500px\">\r\n    <cus-table-title :label=\"$t('ruleRelease.tableName')\"></cus-table-title>\r\n    <el-table\r\n      :data=\"configs\"\r\n      size=\"mini\"\r\n      fit\r\n      element-loading-text=\"Loading\"\r\n      border\r\n      highlight-current-row\r\n      align=\"center\"\r\n      class=\"wraper-table\"\r\n      ref=\"page\"\r\n    >\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" sortable type=\"index\" width=\"60px\">\r\n         <template v-slot=\"scope\">\r\n                {{ initTableIndex('page', scope.$index) }}\r\n           </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('ruleRelease.ruleInfo')\" width=\"75px\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button  size=\"mini\" type=\"text\" @click=\"detail(scope.row)\">\r\n            <i class=\"el-icon-d-arrow-right\"></i>\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('ruleRelease.ruleCode')\" prop=\"code\" align=\"center\"></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleRelease.businessModule')\"\r\n        prop=\"ruleBussModu\"\r\n        :formatter=\"businessFormat\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleRelease.ruleClass')\"\r\n        prop=\"ruleAscrCls\"\r\n        :formatter=\"classFormat\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleAttribute.performType')\"\r\n        prop=\"performType\"\r\n        :formatter=\"performTypeFormat\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleRelease.ruleType')\"\r\n        prop=\"ruleType\"\r\n        :formatter=\"ruleTypeFormat\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleRelease.state')\"\r\n        prop=\"state\"\r\n        :formatter=\"stateFormat\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column :label=\"$t('ruleRelease.description')\" prop=\"description\" align=\"center\"></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('table.actions')\" fixed=\"right\" width=\"350\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"handleRuleRelease(scope.row)\"\r\n            class=\"filter-button\"\r\n          >\r\n            <svg-icon icon-class=\"release\" />\r\n            {{ $t('ruleRelease.ruleRelease') }}\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <cus-pagination\r\n      ref=\"page\"\r\n      url=\"/rule-test/list\"\r\n      :pageSize=\"10\"\r\n      :params=\"params\"\r\n      v-model=\"configs\"\r\n    />\r\n    </div>\r\n    <el-dialog :visible.sync=\"dialogVisible\" v-dialog-drag  width=\"70%\">\r\n      <cus-page-title-back @backTo=\"goBack\" :label=\"$t('ruleExamine.ruleInfo')\" :isDialog=\"true\"></cus-page-title-back>\r\n      <div style=\"height:500px; overflow: auto;\" class=\"scrollbar\">\r\n          <el-scrollbar style=\"height:100%;\">\r\n            <template v-if=\"ruleType == '1'&& pageState && performType == '1'\">\r\n              <rule-make :ruleCode.sync=\"ruleCode\" :flag=\"ruleFlag\" :ruleQuery=\"ruleQuery\"  :performType=\"performType\"></rule-make>\r\n            </template>\r\n            <template v-if=\" ruleType == '2' && pageState && performType == '1'\">\r\n              <rule-calculator-make :ruleCode.sync=\"ruleCode\" :flag=\"ruleFlag\" :ruleQuery=\"ruleQuery\" :performType=\"performType\"></rule-calculator-make>\r\n            </template>\r\n            <template v-if=\"performType == '2' && pageState\">\r\n                <rule-sql-definition\r\n                  :ruleCode.sync=\"ruleCode\"\r\n                  :flag.sync=\"ruleFlag\"\r\n                  :ruleQuery=\"ruleQuery\"\r\n                  :ruleType ='ruleType'\r\n                  :performType='performType'\r\n                ></rule-sql-definition>\r\n            </template>\r\n          </el-scrollbar>\r\n       </div>\r\n    </el-dialog>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\nimport { doRuleRelease } from \"@/api/ibrms/ruleRelease\";\r\nimport { mapGetters } from \"vuex\";\r\nimport Select from \"../../../../customComponents/CodeSelect/index\";\r\nimport ruleCalculatorMake from \"@/views/ibrms/ruleManagement/ruleCalculatorMake\";\r\nimport ruleMake from \"@/views/ibrms/ruleManagement/ruleMake\";\r\nimport ruleSqlDefinition from \"@/views/ibrms/ruleManagement/ruleSqlDefinition\";\r\nimport { closeVisitTag } from \"@/utils/tagsView\";\r\nimport { updateRuleState, findRuleMainByCode } from \"@/api/ibrms/ruleMain\";\r\n\r\nexport default {\r\n  name: \"ruleRelease\",\r\n  filters: {\r\n    stateFilter(type) {}\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"ldcodes\"])\r\n  },\r\n  components: {\r\n    Select,\r\n    ruleCalculatorMake,\r\n    ruleMake,\r\n    ruleSqlDefinition\r\n  },\r\n  props: [\"ruleCode\", \"ruleModule\"],\r\n  data() {\r\n    return {\r\n      //表格数据\r\n      configs: [],\r\n      bntType: false,\r\n      //查询参数\r\n      params: {\r\n        code: \"\",\r\n        state: \"4\",\r\n        ruleBussModu: \"\",\r\n        ruleType: \"1\"\r\n      },\r\n      form: {\r\n        //表单数据与后端实体类对应\r\n        code: \"\", //主键ID\r\n        xxxx: \"\" //\r\n      },\r\n      active: 0,\r\n      //检验规则\r\n      rules: {\r\n        userCode: [\r\n          { required: true, message: this.$t(\"ruleMain.xxx\"), trigger: \"blur\" }\r\n        ]\r\n      },\r\n      stateArray: [],\r\n      businessArray: [],\r\n      classArray: [],\r\n      validArray: [],\r\n      ruleTypeArray: [],\r\n      performTypeArray: [],\r\n      ruleQuery: \"\",\r\n      ruleType: \"\",\r\n      ruleFlag: \"\",\r\n      dialogVisible: false,\r\n      pageState: true,\r\n      radio: false,\r\n      performType:\"1\",\r\n      nodeName: \"rulePublished\"\r\n    };\r\n  },\r\n  created() {\r\n    if (this.ruleModule && typeof this.ruleModule != \"undefined\") {\r\n      this.params.code = this.ruleCode;\r\n      this.params.ruleType = \"\";\r\n      this.bntType = this.ruleModule;\r\n    }\r\n    this.init();\r\n  },\r\n  methods: {\r\n    init() {\r\n      this.stateArray = this.ldcodes[\"ibrmsstate\"];\r\n      this.businessArray = this.ldcodes[\"ruleBusinessModule\"];\r\n      this.classArray = this.ldcodes[\"ruleAscrCls\"];\r\n      this.validArray = this.ldcodes[\"ibrmsvalid\"];\r\n      this.ruleTypeArray = this.ldcodes[\"ruleBussModuDeta\"];\r\n      this.performTypeArray = this.ldcodes[\"performType\"];\r\n    },\r\n    stateFormat(row, column) {\r\n      let state = row.state;\r\n      let stateValue = \"\";\r\n      this.stateArray.forEach((st, index) => {\r\n        if (st.code === state) {\r\n          stateValue = st.codename;\r\n        }\r\n      });\r\n      return stateValue;\r\n    },\r\n    businessFormat(row, column) {\r\n      let business = row.ruleBussModu;\r\n      let businessValue = \"\";\r\n      this.businessArray.forEach((bus, index) => {\r\n        if (bus.code === business) {\r\n          businessValue = bus.codename;\r\n        }\r\n      });\r\n      return businessValue;\r\n    },\r\n    classFormat(row, column) {\r\n      let clasz = row.ruleAscrCls;\r\n      let claszValue = \"\";\r\n      this.classArray.forEach((cla, index) => {\r\n        if (cla.code === clasz) {\r\n          claszValue = cla.codename;\r\n        }\r\n      });\r\n      return claszValue;\r\n    },\r\n    validFormat(row, column) {\r\n      let va = row.valid;\r\n      let validValue = \"\";\r\n      this.validArray.forEach((valid, index) => {\r\n        if (valid.code === va) {\r\n          validValue = valid.codename;\r\n        }\r\n      });\r\n      return validValue;\r\n    },\r\n    ruleTypeFormat(row, column) {\r\n      let type = row.ruleType;\r\n      let typeValue = \"\";\r\n      this.ruleTypeArray.forEach((t, index) => {\r\n        if (t.code === type) {\r\n          typeValue = t.codename;\r\n        }\r\n      });\r\n      return typeValue;\r\n    },\r\n    handleSearch() {\r\n      this.$refs.page.reload();\r\n    },\r\n    handleRuleRelease(row) {\r\n      let ruleId = row.id;\r\n      this.$confirm(\"是否发布当前规则？\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\"\r\n      })\r\n        .then(() => {\r\n          doRuleRelease(ruleId).then(response => {\r\n            console.log(response.data);\r\n            if (response.code == 200) {\r\n              this.notifySuccess(\"发布成功\");\r\n              this.$refs.page.reload();\r\n            }\r\n          });\r\n        })\r\n        .catch(() => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: \"已取消发布\"\r\n          });\r\n        });\r\n    },\r\n    resetForm() {\r\n      this.form = {\r\n        //表单数据与后端实体类对应\r\n        id: \"\", //主键ID\r\n        xxxx: \"\"\r\n      };\r\n    },\r\n    detail(row) {\r\n      //通过规则类型查看规则明细\r\n      this.ruleCode = row.code;\r\n      this.ruleFlag = \"1\";\r\n      this.ruleQuery = true;\r\n      if(row.performType && row.performType != \"\"){\r\n         this.performType=row.performType;\r\n      }\r\n      this.ruleType = row.ruleType;\r\n      this.pageState = fal