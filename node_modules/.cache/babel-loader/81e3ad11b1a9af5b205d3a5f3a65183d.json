AAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,eAAA,GAAA,IAAA;AACA,OAHA;AAIA,KA3CA;;AA6CA;AACA,IAAA,cA9CA,0BA8CA,EA9CA,EA8CA;AAAA;;AACA,WAAA,YAAA,GAAA,IAAA;AACA,UAAA,IAAA,GAAA;AACA,oBAAA;AADA,OAAA;AAGA,2CAAA,KAAA,gBAAA,CAAA,IAAA,CAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,OAAA;AACA;AACA,OAJA;AAKA,KAxDA;AAyDA,IAAA,aAzDA,2BAyDA;AACA,WAAA,eAAA,GAAA,KAAA;AACA,KA3DA;AA4DA,IAAA,YA5DA,0BA4DA;AACA,WAAA,YAAA,GAAA,KAAA;AACA,KA9DA;;AA+DA;AACA,IAAA,YAhEA,wBAgEA,GAhEA,EAgEA,MAhEA,EAgEA,KAhEA,EAgEA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,QAAA,KAAA,OAAA,EAAA;AACA,YAAA,GAAA,CAAA,EAAA,KAAA,KAAA,mBAAA,CAAA,EAAA,EAAA;AACA,eAAA,mBAAA,GAAA,EAAA;AACA,eAAA,iBAAA,GAAA,EAAA;AACA,SAHA,MAGA;AACA,eAAA,mBAAA,GAAA,GAAA;AACA,eAAA,iBAAA,GAAA,GAAA,CAAA,WAAA;AACA,eAAA,cAAA,CAAA,SAAA,GAAA,GAAA,CAAA,SAAA,CAHA,CAGA;;AACA,eAAA,cAAA,CAAA,QAAA,GAAA,GAAA,CAAA,EAAA,CAJA,CAIA;;AACA,eAAA,cAAA,CAAA,MAAA,GAAA,GAAA,CAAA,MAAA,CALA,CAKA;;AACA,eAAA,cAAA,CAAA,OAAA,GAAA,GAAA,CAAA,OAAA,CANA,CAMA;AACA;AACA;AACA;AA9EA;AApEA,C","sourcesContent":["<template>\r\n  <cus-wraper>\r\n    <cus-table-title :label=\"$t('rollBackReleInfo.tipMsg1')\"/>\r\n    <el-table :data=\"configs\" border element-loading-text=\"Loading\" fit highlight-current-row size=\"mini\" @row-click=\"chooseOneRow\">\r\n      <el-table-column :label=\"$t('pdtempPdBnft.choose')\" align=\"center\" width=\"55\">\r\n        <template slot-scope=\"scope\">\r\n          <el-radio v-model=\"selectedReleComboVo\" :label=\"scope.row\">\r\n            <span>{{ '' }}</span>\r\n          </el-radio>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\"></el-table-column>\r\n      <!--:label=\"$t('table.id')\"-->\r\n      <el-table-column :label=\"$t('i18nView.ppCode')\" align=\"center\"\r\n                       prop=\"ppCode\"></el-table-column>      <!--:label=\"$t('table.id')\"-->\r\n      <el-table-column :label=\"$t('i18nView.pdName')\" align=\"center\"\r\n                       prop=\"ppName\"></el-table-column>  <!--:label=\"$t('pdtempPdFile.column1')\"-->\r\n      <el-table-column align=\"center\" :label=\"$t('i18nView.pdAuditDoneDate')\" prop=\"pdAuditDate\"></el-table-column>\r\n      <!--:label=\"$t('pdtempPdFile.column3')\"-->\r\n      <el-table-column align=\"center\" :label=\"$t('i18nView.testResult')\" prop=\"pdAuditTimes\">\r\n        <el-button size=\"small\" style=\"width: 100px\" type=\"text\" @click=\"dialogTableVisible = true\"><i\r\n          class=\"el-icon-d-arrow-right\"></i></el-button>\r\n      </el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('i18nView.auditRes')\" prop=\"pdAuditTimes\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button size=\"small\" style=\"width: 100px\" type=\"text\" @click=\"reviewHistory(scope.row)\"><i\r\n            class=\"el-icon-d-arrow-right\"></i></el-button>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('i18nView.pdAuditConc')\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button size=\"small\" style=\"width: 100px\" type=\"text\" @click=\"searchPathName(scope.row.id)\"><i\r\n            class=\"el-icon-d-arrow-right\"></i></el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <cus-pagination ref=\"page\" v-show=\"false\" v-model=\"configs\" :pageSize=\"10\" :params=\"params\"\r\n                    url=\"/comboRelease/base/searchPPInRelease\"/>\r\n\r\n    <el-form :ref=\"PdtempPdVerRls\" :model=\"PdtempPdVerRls\" :rules=\"rules\" label-width=\"auto\">\r\n      <el-row style=\"margin-top: 20px\">\r\n        <el-col :span=\"24\">\r\n          <el-form-item :label=\"$t('i18nView.rebackNote')\" prop=\"pdReleDetails\">\r\n            <el-input v-model=\"PdtempPdVerRls.pdReleDetails\" style=\"width: 300px;\"></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n      <div align=\"right\">\r\n        <el-button v-auth=\"'pdtemp-pd-ver-rls:returnToAudit'\" size=\"small\" type=\"primary\"\r\n                   @click=\"returnToAudit(PdtempPdVerRls)\">{{$t('i18nView.returnBack')}}\r\n        </el-button>\r\n      </div>\r\n    </el-form>\r\n\r\n    <!-- 审核结论 -->\r\n    <el-dialog v-if=\"showAuditResult\" v-dialog-drag :close-on-click-modal=\"false\"\r\n               :visible.sync=\"showAuditResult\" append-to-body center width=\"75%\">\r\n      <cus-page-title-back :label=\"$t('i18nView.auditRes')\" @backTo=\"backAuditPage\"></cus-page-title-back>\r\n      <el-table :data=\"auditData\" border element-loading-text=\"Loading\" fit highlight-current-row size=\"mini\">\r\n        <el-table-column align=\"center\" :label=\"$t('table.id')\" type=\"index\" width=\"55px\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.ppCode')\"\r\n                         prop=\"pdCode\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.auditRes')\" align=\"center\" prop=\"pdAuditConc\">\r\n          <template v-slot=\"scope\">{{ scope.row.pdAuditConc == 'N' ? '不通过' : '通过' }}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.auditOper')\" align=\"center\"\r\n                         prop=\"pdAuditStaff\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.auditDate')\" align=\"center\"\r\n                         prop=\"pdAuditDate\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.pdAuditIdea')\" align=\"center\"\r\n                         prop=\"pdAuditIdea\"></el-table-column>\r\n      </el-table>\r\n    </el-dialog>\r\n    <!-- 发布环境 -->\r\n    <el-dialog v-if=\"showRelePath\" v-dialog-drag :close-on-click-modal=\"false\"\r\n               :visible.sync=\"showRelePath\" append-to-body center width=\"75%\">\r\n      <cus-page-title-back :label=\"$t('rollBackReleInfo.relePath')\" @backTo=\"backPathPage\"></cus-page-title-back>\r\n      <el-form style=\"margin-left: 10%\">\r\n        <el-form-item :label=\"$t('i18nView.pdReleSysname')\">\r\n          <span>{{ pathName }}</span>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-dialog>\r\n\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\n\r\n\r\nimport {returnToAudit, searchPathName} from \"@/api/basicinfo/pdtemppprelease\";\r\nimport {reviewHistory} from \"@/api/basicinfo/comboAuditInfo\";\r\n\r\nlet count = 0;\r\nexport default {\r\n  name: 'reback',\r\n  filters: {\r\n    getPdAuditConc(state) {\r\n      return state == 'N' ? '不通过' : '通过';\r\n    },\r\n    getPdReturnFuncNode(node) {\r\n      let mnode = JSON.parse(node);\r\n      let pdReturnFuncNode = '';\r\n      for (let i = 0; i < mnode.length; i++) {\r\n        if (i == mnode.length - 1) {\r\n          pdReturnFuncNode += mnode[i] + \"\"\r\n        } else {\r\n          pdReturnFuncNode += mnode[i] + \";\"\r\n        }\r\n      }\r\n      return pdReturnFuncNode;\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      pathName: '',\r\n      processInstanceId: '',\r\n      selectedReleComboVo: {},\r\n      //表格数据\r\n      configs: [],\r\n      //查询参数\r\n      params: {\r\n        eq_pdCode: '',\r\n        eq_pdEdition: '',\r\n        like_pdName: ''\r\n      },\r\n      showRelePath: false,\r\n      showAuditResult: false,\r\n      PdtempPdVerRls: {\r\n        pdReleWay: '',      //发布方式\r\n        pdEdition: '',  //  套餐版本\r\n        pdType: '',  //  套餐类型\r\n        pdInfoId: '',  //  套餐id\r\n        pdCode: '',  //  产品编码\r\n        pdReleOrder: '',  //  发布序号\r\n        pdReleType: '',  //  发布类型 : 全量发布 增量发布\r\n        pdReleSys: '',  //  发至系统\r\n        pdReleEnv: '',  //  发至环境\r\n        pdReleFilePath: '',  //  发布文件地址\r\n        pdReleStu: '',  //  发布状态 : 发布成功 发布失败\r\n        pdReleContent: '',  //  发布内容\r\n        pdReleDetails: '',  //  发布说明\r\n        pdReleFile: '',  //  生成文件名\r\n      },\r\n      auditData: [],\r\n\r\n      pdAuditDetailInfo: {\r\n        pdCode: '',\r\n        pdInfoId: ''\r\n      },\r\n\r\n      //检验规则\r\n      rules: {\r\n        pdReleDetails: [{required: true, message: this.$t('rollBackReleInfo.retDet'), trigger: 'blur'}],\r\n      },\r\n    }\r\n  },\r\n  computed: {},\r\n  created() {\r\n  },\r\n  mounted() {\r\n  },\r\n  methods: {\r\n\r\n    /* 父组件刷新子组件表格 */\r\n    refreshChildTable() {\r\n      this.$refs.page.reload();\r\n      this.selectedReleComboVo = {};\r\n      this.processInstanceId = '';\r\n    },\r\n\r\n    /*退回*/\r\n    returnToAudit(form) {\r\n      if (Object.keys(this.selectedReleComboVo).length > 0) {\r\n        this.$refs[form].validate((valid) => {\r\n          if (valid) {\r\n            let data = {\r\n              'processInstanceId': this.processInstanceId,\r\n              'PdtempPdVerRls': this.PdtempPdVerRls,\r\n              'comboReleaseInfoVo': this.selectedReleComboVo\r\n            }\r\n            returnToAudit(this.buildRequestData(data)).then(() => {\r\n              this.$refs.page.reload();\r\n              this.$emit('callBack');\r\n              this.$store.dispatch('product/storagePackages')\r\n              this.notifySuccess('退回成功');\r\n              this.PdtempPdVerRls.pdReleDetails = ''\r\n            })\r\n          }\r\n        })\r\n      } else {\r\n        this.notifyWarning(\"请选择要退回的套餐！\")\r\n      }\r\n    },\r\n\r\n    /*  审核结论  */\r\n    reviewHistory(row) {\r\n      let data = {\r\n        pdAuditDetailInfoId: row.id,\r\n        pdVerId: row.pdVerId\r\n      }\r\n      reviewHistory(this.buildRequestData(data)).then(res => {\r\n        this.auditData = res.data\r\n        this.showAuditResult = true;\r\n      })\r\n    },\r\n\r\n    /*发布环境查询*/\r\n    searchPathName(id) {\r\n      this.showRelePath = true;\r\n      let data = {\r\n        'pdInfoId': id,\r\n      }\r\n      searchPathName(this.buildRequestData(data)).then(res => {\r\n        if (res.code === 200) {\r\n          this.pathName = res.message;\r\n        }\r\n      })\r\n    },\r\n    backAuditPage() {\r\n      this.showAuditResult = false\r\n    },\r\n    backPathPage() {\r\n      this.showRelePath = false\r\n    },\r\n    /*选择表格数据*/\r\n    chooseOneRow(row, column, event) {\r\n      if (event.target.nodeName === \"INPUT\") {\r\n        if (row.id === this.selectedReleComboVo.id) {\r\n          this.selectedReleComboVo = {};\r\n          this.processInstanceId = '';\r\n        } else {\r\n          this.selectedReleComboVo = row;\r\n          this.processInstanceId = row.pdProcessId;\r\n          this.PdtempPdVerRls.pdEdition = row.pdEdition; //  套餐版本\r\n          this.PdtempPdVerRls.pdInfoId = row.id; //  套餐id\r\n          this.PdtempPdVerRls.pdCode = row.ppCode; //  套餐编码\r\n          this.PdtempPdVerRls.pdVerId = row.pdVerId; //  套餐编码\r\n        }\r\n      }\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n.sbut {\r\n  width: 100px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/commodity-release/package-release/reback"}]}