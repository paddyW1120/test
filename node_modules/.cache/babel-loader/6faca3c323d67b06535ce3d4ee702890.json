MApHA,EAoHA;AACA,UAAA,WAAA,GAAA,GAAA,CAAA,WAAA;AACA,UAAA,gBAAA,GAAA,EAAA;AACA,WAAA,gBAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,YAAA,IAAA,CAAA,IAAA,KAAA,WAAA,EAAA;AACA,UAAA,gBAAA,GAAA,IAAA,CAAA,QAAA;AACA;AACA,OAJA;AAKA,aAAA,gBAAA;AACA;AA7HA;AAjEA,C","sourcesContent":["<template>\r\n  <cus-wraper>\r\n    <cus-filter-wraper>\r\n       <el-form label-width=\"auto\" :model=\"params\" style=\"margin: 20px 0 0 0\">\r\n      <el-row :gutter=\"10\">\r\n        <cus-col :span=\"6\">\r\n          <el-form-item :label=\"$t('ruleExamine.ruleCode')\" prop=\"code\">\r\n              <el-input clearable  v-model=\"params.code\"></el-input>\r\n            </el-form-item>\r\n        </cus-col>\r\n        <cus-col :span=\"6\">\r\n            <el-form-item :label=\"$t('ruleExamine.businessModule')\" prop>\r\n              <cus-select\r\n                :codetype=\"'ruleBusinessModule'\"\r\n                placeholder\r\n                v-model=\"params.ruleBussModu\"\r\n              ></cus-select>\r\n            </el-form-item>\r\n        </cus-col>\r\n        <cus-col :span=\"6\">\r\n           <el-form-item :label=\"$t('ruleExamine.performType')\" prop>\r\n              <cus-select :codetype=\"'performType'\" placeholder v-model=\"params.performType\"></cus-select>\r\n            </el-form-item>\r\n        </cus-col>\r\n          <cus-btn-wrapper>\r\n              <el-button  size=\"mini\" @click=\"handleSearch\" icon=\"el-icon-search\" type=\"primary\">\r\n              {{ $t('table.search') }}\r\n            </el-button>\r\n          </cus-btn-wrapper>\r\n        </el-row>\r\n        <el-row :gutter=\"10\">\r\n        <cus-col :span=\"6\">\r\n            <el-form-item :label=\"$t('ruleExamine.ruleType')\" prop=\"ruleType\">\r\n              <cus-radio :codetype=\"'ruleType'\" :isRequired=\"true\" v-model=\"params.ruleType\" ></cus-radio>\r\n            </el-form-item>\r\n          </cus-col>\r\n      </el-row>\r\n    </el-form>\r\n    </cus-filter-wraper>\r\n    <cus-table-title :label=\"$t('ruleExamine.tableName')\"></cus-table-title>\r\n    <el-table\r\n      :data=\"configs\"\r\n      size=\"mini\"\r\n      fit\r\n      element-loading-text=\"Loading\"\r\n      border\r\n      highlight-current-row\r\n      align=\"center\"\r\n      class=\"wraper-table\"\r\n      ref=\"page\"\r\n    >\r\n      <el-table-column :label=\"$t('table.select')\" width=\"50\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-radio :label=\"scope.row.id\" @change=\"chooseOne(scope.row)\" v-model=\"radio\">{{''}}</el-radio>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" sortable type=\"index\"  width=\"60px\">\r\n          <template v-slot=\"scope\">\r\n                {{ initTableIndex('page', scope.$index) }}\r\n           </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('ruleTest.ruleInfo')\" width=\"75px\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button  size=\"mini\" type=\"text\" @click=\"detail(scope.row)\">\r\n            <i class=\"el-icon-d-arrow-right\"></i>\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('ruleExamine.ruleCode')\" prop=\"code\" align=\"center\"></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleExamine.businessModule')\"\r\n        prop=\"ruleBussModu\"\r\n        :formatter=\"businessFormat\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleExamine.ruleClass')\"\r\n        prop=\"ruleAscrCls\"\r\n        :formatter=\"classFormat\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleAttribute.performType')\"\r\n        prop=\"performType\"\r\n        :formatter=\"performTypeFormat\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleExamine.ruleType')\"\r\n        prop=\"ruleType\"\r\n        :formatter=\"ruleTypeFormat\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n      :label=\"$t('ruleExamine.state')\"\r\n      prop=\"state\"\r\n      :formatter=\"stateFormat\"\r\n      align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column :label=\"$t('ruleExamine.description')\" prop=\"description\" align=\"center\"></el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <cus-pagination\r\n      ref=\"page\"\r\n      url=\"/rule-test/list\"\r\n      :pageSize=\"10\"\r\n      :params=\"params\"\r\n      v-model=\"configs\"\r\n    />\r\n    <br />\r\n    <br />\r\n    <template v-if=\"show\">\r\n      <el-form label-width=\"auto\" ref=\"dataForm\" :rules=\"rules\" :model=\"form\">\r\n        <el-row>\r\n          <el-form-item :label=\"$t('ruleExamine.auditConclusion')\" prop=\"state\">\r\n            <el-radio-group v-model=\"form.state\">\r\n              <el-radio label=\"1\">{{$t('ruleExamine.auditThrough')}}</el-radio>\r\n              <el-radio label=\"2\">{{$t('ruleExamine.auditNotThrough')}}</el-radio>\r\n            </el-radio-group>\r\n          </el-form-item>\r\n        </el-row>\r\n        <el-row>\r\n          <el-form-item :label=\"$t('ruleExamine.auditIdea')\" prop=\"auditIdea\">\r\n               <el-input  type=\"textarea\" :rows=\"4\" placeholder=\"请输入内容\"\r\n                  :autosize=\"{ minRows:4, maxRows:10}\" style=\"width:700px\" maxlength=\"200\"\r\n                  show-word-limit v-model=\"form.auditIdea\" ></el-input>\r\n          </el-form-item>\r\n        </el-row>\r\n        <el-row style=\"margin-top: 35px\">\r\n          <cus-col :span=\"12\" align=\"right\">\r\n            <el-button  size=\"mini\"\r\n              type=\"primary\"\r\n              @click=\"submitForm\"\r\n            ><svg-icon icon-class=\"audit\"/> {{$t('ruleExamine.audit')}}</el-button>\r\n            <el-button  size=\"mini\" @click=\"Refresh()\" type=\"warning\">\r\n              <i class=\"el-icon-refresh el-icon--left\"></i>{{$t('ruleExamine.refresh')}}\r\n            </el-button>\r\n          </cus-col>\r\n        </el-row>\r\n      </el-form>\r\n    </template>\r\n    <el-dialog :visible.sync=\"dialogVisible\" v-dialog-drag width=\"70%\">\r\n       <cus-page-title-back @backTo=\"goBack\" :label=\"$t('ruleExamine.ruleInfo')\" :isDialog=\"true\"></cus-page-title-back>\r\n      <div style=\"height:500px; overflow: auto;\" class=\"scrollbar\">\r\n          <el-scrollbar style=\"height:100%;\">\r\n            <template v-if=\"ruleType == '1'&& pageState && performType == '1'\">\r\n              <rule-make :ruleCode.sync=\"ruleCode\" :flag=\"ruleFlag\" :ruleQuery=\"ruleQuery\"  :performType=\"performType\"></rule-make>\r\n            </template>\r\n            <template v-if=\" ruleType == '2' && pageState && performType == '1'\">\r\n              <rule-calculator-make :ruleCode.sync=\"ruleCode\" :flag=\"ruleFlag\" :ruleQuery=\"ruleQuery\" :performType=\"performType\"></rule-calculator-make>\r\n            </template>\r\n            <template v-if=\"performType == '2' && pageState\">\r\n                <rule-sql-definition\r\n                  :ruleCode.sync=\"ruleCode\"\r\n                  :flag.sync=\"ruleFlag\"\r\n                  :ruleQuery=\"ruleQuery\"\r\n                  :ruleType ='ruleType'\r\n                  :performType='performType'\r\n                ></rule-sql-definition>\r\n            </template>\r\n          </el-scrollbar>\r\n       </div>\r\n    </el-dialog>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\nimport { ruleExamineCross } from \"@/api/ibrms/ruleExamine\";\r\nimport { mapGetters } from \"vuex\";\r\nimport Select from \"../../../../customComponents/CodeSelect/index\";\r\nimport ruleCalculatorMake from \"@/views/ibrms/ruleManagement/ruleCalculatorMake\";\r\nimport ruleSqlDefinition from \"@/views/ibrms/ruleManagement/ruleSqlDefinition\";\r\nimport ruleMake from \"@/views/ibrms/ruleManagement/ruleMake\";\r\nimport { closeVisitTag } from \"@/utils/tagsView\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      //表格数据\r\n      configs: [],\r\n      //查询参数\r\n      params: {\r\n        code: \"\",\r\n        state: \"3\",\r\n        ruleBussModu: \"\",\r\n        ruleType:\"1\",\r\n        performType:\"\"\r\n      },\r\n      show: false,\r\n      form: {\r\n        //表单数据与后端实体类对应\r\n        auditIdea: \"\",\r\n        state: \"1\"\r\n      },\r\n      //检验规则\r\n      rules: {\r\n        state: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleExamine.stateMessage\"),\r\n            trigger: \"blur\"\r\n          }\r\n        ],\r\n        auditIdea: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleExamine.auditIdeaMessage\"),\r\n            trigger: \"blur\"\r\n          }\r\n        ]\r\n      },\r\n      active: 0,\r\n      ruleId: \"\",\r\n      stateArray: [],\r\n      businessArray: [],\r\n      classArray: [],\r\n      validArray: [],\r\n      ruleTypeArray: [],\r\n      performTypeArray: [],\r\n      dialogVisible: false,\r\n      radio: false,\r\n      ruleQuery: \"\",\r\n      ruleType: \"\",\r\n      ruleFlag: \"\",\r\n      performType:\"1\",\r\n      pageState:false\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"ldcodes\"])\r\n  },\r\n  components: {\r\n    Select,\r\n    ruleCalculatorMake,\r\n    ruleMake,\r\n    ruleSqlDefinition\r\n  },\r\n  created() {\r\n    this.init();\r\n  },\r\n  methods: {\r\n    init() {\r\n      this.stateArray = this.ldcodes[\"ibrmsstate\"];\r\n      this.businessArray = this.ldcodes[\"ruleBusinessModule\"];\r\n      this.classArray = this.ldcodes[\"ruleAscrCls\"];\r\n      this.validArray = this.ldcodes[\"ibrmsvalid\"];\r\n      this.ruleTypeArray = this.ldcodes[\"ruleBussModuDeta\"];\r\n      this.performTypeArray = this.ldcodes[\"performType\"];\r\n    },\r\n    stateFormat(row, column) {\r\n      let state = row.state;\r\n      let stateValue = \"\";\r\n      this.stateArray.forEach((st, index) => {\r\n        if (st.code === state) {\r\n           stateValue = st.codename;\r\n        }\r\n      });\r\n      return stateValue;\r\n    },\r\n    businessFormat(row, column) {\r\n      let business = row.ruleBussModu;\r\n      let businessValue = \"\";\r\n      this.businessArray.forEach((bus, index) => {\r\n        if (bus.code === business) {\r\n          businessValue = bus.codename;\r\n        }\r\n      });\r\n      return businessValue;\r\n    },\r\n    classFormat(row, column) {\r\n      let clasz = row.ruleAscrCls;\r\n      let claszValue = \"\";\r\n      this.classArray.forEach((cla, index) => {\r\n        if (cla.code === clasz) {\r\n          claszValue = cla.codename;\r\n        }\r\n      });\r\n      return claszValue;\r\n    },\r\n    validFormat(row, column) {\r\n      let va = row.valid;\r\n      let validValue = \"\";\r\n      this.validArray.forEach((valid, index) => {\r\n        if (valid.code === va) {\r\n          validValue = valid.codename;\r\n        }\r\n      });\r\n      return validValue;\r\n    },\r\n    ruleTypeFormat(row, column) {\r\n      let type = row.ruleType;\r\n      let typeValue = \"\";\r\n      this.ruleTypeArray.forEach((t, index) => {\r\n        if (t.code === type) {\r\n          typeValue = t.codename;\r\n        }\r\n      });\r\n      return typeValue;\r\n    },\r\n    handleSearch() {\r\n      this.show = false;\r\n      this.radio = false;\r\n      this.$refs.page.reload();\r\n    },\r\n    detail(row) {\r\n      //通过规则类型查看规则明细\r\n      this.ruleCode = row.code;\r\n      this.ruleFlag = \"1\";\r\n      this.ruleQuery = true;\r\n      this.ruleType = row.ruleType;\r\n      if(row.performType && row.performType != \"\"){\r\n         this.performType=row.performType;\r\n      }\r\n      this.dialogVisible = true;\r\n      this.pageState = false;\r\n      this.$nextTick(() => (this.pageState = true));\r\n    },\r\n    chooseOne(row) {\r\n      this.show = true;\r\n      this.radio = row.id;\r\n      this.ruleId = row.id;\r\n      this.form = {};\r\n    },\r\n    Refresh() {\r\n      this.form = {};\r\n    },\r\n    submitForm() {\r\n      this.$refs.dataForm.validate(valid => {\r\n        if (valid) {\r\n          let vvo = {\r\n            ruleId: this.ruleId,\r\n            isCross: \"\",\r\n            auditIdea: this.form.auditIdea\r\n          };\r\n          if (this.form.state == \"1\") {\r\n            vvo.isCross = true;\r\n          } else {\r\n            vvo.isCross = false;\r\n          }\r\n          ruleExamineCross(this.buildRequestData(vvo)).then(response => {\r\n            if (response.code == 200) {\r\n              this.notifySuccess(\"审批成功\");\r\n              this.show = false;\r\n              this.handleSearch();\r\n            } else {\r\n              this.$message.error(response.message);\r\n            }\r\n          });\r\n        } else {\r\n          callback();\r\n        }\r\n      });\r\n    },\r\n    goBack() {\r\n        this.dialogVisible= false;\r\n    },\r\n    performTypeFormat(row, column) {\r\n      let performType = row.performType;\r\n      let performTypeValue = \"\";\r\n      this.performTypeArray.forEach((attr, index) => {\r\n        if (attr.code === performType) {\r\n          performTypeValue = attr.codename;\r\n        }\r\n      });\r\n      return performTypeValue;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.step-row {\r\n  min-width: 500px;\r\n  margin-bottom: 12px;\r\n  margin-top: 12px;\r\n}\r\n  .el-dialog__footer {\r\n  text-align: left;\r\n}\r\n.el-scrollbar__wrap {  // 必要的\r\n    overflow: auto !important;\r\n    width: 110%;  //隐藏侧边默认滚动条 （达不到效果可以自己试着微调） 如解决不了可尝试用 `margin-right -60px`\r\n    height: 100%;  //隐藏底部滚动条  （100%如果达不到效果可以试着用110%）\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/ibrms/ruleManagement/ruleExamine"}]}