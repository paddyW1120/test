der=\"\" clearable v-model=\"params1.pdCode\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('customComponents.pdEdition')\" prop=\"\">\r\n              <el-input placeholder=\"\" clearable v-model=\"params1.pdEdition\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdName')\">\r\n              <el-input clearable  clearable v-model=\"params1.pdName\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.taskName')\" prop=\"\">\r\n              <el-select v-model=\"params1.taskName\" clearable filterable>\r\n                <el-option  v-for=\"item in testTaskOptions\" :label=\"item.value\" :value=\"item.text\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n            <el-button  size=\"small\" type=\"primary\" @click=\"handleSearch\" icon=\"el-icon-search\" v-waves class=\"filter-button\">{{$t('i18nView.query')}}</el-button>\r\n          </el-form>\r\n        </cus-filter-wraper>\r\n        <cus-table-title :label=\"$t('i18nView.waitThingclaim')\"/>\r\n        <el-table :data=\"configs1\" align=\"center\" border class=\"wraper-table\" element-loading-text=\"Loading\" fit\r\n                  highlight-current-row\r\n                  size=\"mini\">>\r\n          <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\"></el-table-column>\r\n          <el-table-column align=\"center\" :label=\"$t('i18nView.pdCode')\" prop=\"pdCode\"></el-table-column>\r\n          <el-table-column align=\"center\" :label=\"$t('i18nView.pdName')\" prop=\"pdName\"></el-table-column>\r\n          <el-table-column align=\"center\" :label=\"$t('customComponents.pdEdition')\" prop=\"pdEdition\"></el-table-column>\r\n          <el-table-column :label=\"$t('i18nView.taskName')\" align=\"center\" prop=\"taskName\"></el-table-column>\r\n          <el-table-column align=\"center\" :label=\"$t('i18nView.defType')\" prop=\"defType\">\r\n            <template v-slot=\"scope\">{{filterCodeTypeHandler('pdAuditType',scope.row.defType )}}</template>\r\n          </el-table-column>\r\n          <el-table-column align=\"center\" :label=\"$t('pdppInfo.updateTime')\" prop=\"changeDate\"></el-table-column>\r\n\r\n          <el-table-column  align=\"center\" :label=\"$t('table.actions')\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button class=\"filter-button\" icon=\"el-icon-thumb\" @click=\"claimTask(scope.row.taskId)\" size=\"mini\" type=\"primary\">认领\r\n              </el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n        <!-- 分页 -->\r\n        <cus-pagination :pageSize=\"5\" :params=\"params1\" ref=\"page\" url=\"/workflow/tasklist/queryUnAssignTask\" v-model=\"configs1\"/>\r\n\r\n\r\n      </div>\r\n    </el-dialog>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\n\r\n  import process from '@/views/basicinfo/pdprocess/index'\r\n\r\n  import cardlist from '@/views/dashboard/cradlist/index'\r\n\r\n  import {\r\n    claimTask\r\n  } from '@/api/workflow/workflow'\r\n\r\n  import {\r\n    indexData\r\n  } from '@/api/system/index'\r\n\r\n  import { mapGetters } from 'vuex'\r\n\r\n  export default {\r\n    name: 'normaluser',\r\n    computed: {\r\n      ...mapGetters([\r\n        'rolecodes'\r\n      ])\r\n    },\r\n    components:{\r\n      process,\r\n      cardlist\r\n    },\r\n    data() {\r\n      return {\r\n        filterHandler(value, row, column) {\r\n          const property = column['property']\r\n          return row[property] === value\r\n        },\r\n        cardData:{},\r\n        cardShow: false,\r\n        radio: '',\r\n        showUnAssign: false,\r\n        processShow: false,\r\n        pdInfoShow: false,\r\n        opShow: false,\r\n        checkShow: false,\r\n        pdProcessId: '',\r\n        moduleType: '',\r\n        roleCode: '',\r\n        rolesName: '',\r\n        systemVersion: '',\r\n        tenantName: '',\r\n        testTaskOptions:[\r\n            {text: '新单测试', value: '新单测试'},\r\n            {text: '续期续保测试', value: '续期续保测试'},\r\n      {text: '保全测试', value: '保全测试'},\r\n      {text: '理赔测试', value: '理赔测试'},\r\n      {text: '规则测试', value: '规则测试'}\r\n    ],\r\n      taskOptions: [{text: '产品框架定义', value: '产品框架定义'},\r\n        {text: '产品销售定义', value: '产品销售定义'},\r\n        {text: '产品核赔定义', value: '产品核赔定义'},\r\n        {text: '产品保全定义', value: '产品保全定义'},\r\n        {text: '产品续期续保定义', value: '产品续期续保定义'},\r\n        {text: '产品核保定义', value: '产品核保定义'},\r\n        {text: '产品承保定义', value: '产品承保定义'},\r\n        {text: '产品审核', value: '产品审核'},\r\n        {text: '产品发布', value: '产品发布'}],\r\n        configs1: [],\r\n        configs2: [],\r\n        params1: {\r\n          pdCode: '',\r\n          pdName: '',\r\n          pdEdition: '',\r\n          taskName: ''\r\n        }, params2: {\r\n          pdCode: '',\r\n          pdEdition: '',\r\n          taskName: ''\r\n        },\r\n        moreCondition:false\r\n      }\r\n    },\r\n    created() {\r\n      this.taskOptions = this.taskOptions.concat(this.testTaskOptions)\r\n      this.roleCode = this.rolecodes\r\n      this.getIndexData()\r\n    },\r\n    mounted() {\r\n    },\r\n    methods: {\r\n      getIndexData() {\r\n        indexData(this.buildRequestData({})).then(res => {\r\n\r\n          let data = res.data\r\n          this.tenantName = data.tenantName\r\n          this.rolesName = data.rolesName\r\n          this.systemVersion = data.systemVersion\r\n          this.cardData ={ 'taskCount':data.taskCount,'issueCount':data.issueCount }\r\n          this.cardShow = true\r\n\r\n        })\r\n      },\r\n      filterCodeTypeHandler(codetype, value) {\r\n        return this.getCodeName(codetype,value,true);\r\n      },\r\n      handleSearch() {\r\n        this.$refs.page.reload()\r\n      }, handleSearch2() {\r\n        this.$refs.page2.reload()\r\n      },\r\n      doTask(row){\r\n        let path  =  this.menuNameToPath(row.taskName)\r\n        if(path){\r\n          this.cacheJsonData(path,row)\r\n          this.$router.push({path: path});\r\n        }else {\r\n            this.messageWarning('该节点路径尚未配置!')\r\n        }\r\n      },\r\n      taskStatus(row){\r\n        this.radio = row.id + row.taskName\r\n        this.pdProcessId = row.pdProcessId\r\n        this.cacheCurrPagePdData(row)\r\n         this.processShow =false\r\n        if (row.taskName === '产品框架定义'){\r\n          this.moduleType = 'pdInfo'\r\n        }else if(row.taskName === '产品审核'){\r\n          this.moduleType = 'examine'\r\n        }else if(row.taskName === '产品发布'){\r\n          this.moduleType = 'release'\r\n        }else if(row.taskName.indexOf('测试')>0){\r\n          this.moduleType = 'test'\r\n        }else {\r\n          this.moduleType = 'operational'\r\n        }\r\n        if (this.moduleType){\r\n          this.processShow = true\r\n        }\r\n\r\n      },\r\n      claimTask(taskId) {\r\n        this.confirmWarning('确定要认领当前任务吗？').then(() => {\r\n          let data = { 'taskId': taskId }\r\n          claimTask(data).then(response => {\r\n            this.handleSearch()\r\n            this.handleSearch2()\r\n            this.notifySuccess(response.message)\r\n          })\r\n        }).catch(() => {\r\n\r\n        })\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n  .panel-group {\r\n    margin-top: 18px;\r\n\r\n    .card-panel-col {\r\n      margin-bottom: 32px;\r\n    }\r\n\r\n    .card-panel {\r\n      height: 108px;\r\n      cursor: pointer;\r\n      font-size: 12px;\r\n      position: relative;\r\n      overflow: hidden;\r\n      color: #666;\r\n      background: #fff;\r\n      box-shadow: 4px 4px 40px rgba(0, 0, 0, .05);\r\n      border-color: rgba(0, 0, 0, .05);\r\n\r\n      & {\r\n        .card-panel-icon-wrapper {\r\n          color: #fff !important;\r\n        }\r\n\r\n        .icon-people {\r\n          background: #40c9c6;\r\n        }\r\n\r\n        .ic