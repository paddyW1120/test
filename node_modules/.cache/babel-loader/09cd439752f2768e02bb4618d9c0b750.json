in taskOptions\" :label=\"item.value\" :value=\"item.text\"></el-option>\r\n             </el-select>\r\n           </el-form-item>\r\n         </el-col>\r\n\r\n         <cus-condition @show=\"moreCondition=!moreCondition\">\r\n           <el-button  size=\"small\" type=\"primary\" @click=\"handleSearch2\" icon=\"el-icon-search\" v-waves class=\"filter-button\">{{$t('table.search')}}</el-button>\r\n           <el-button  size=\"small\" type=\"primary\" @click=\"showUnAssign = true\" icon=\"el-icon-thumb\" v-waves class=\"filter-button\">\r\n             {{ $t('i18nView.taskClaim') }}</el-button>\r\n         </cus-condition>\r\n       </el-row>\r\n\r\n\r\n      <el-row :gutter=\"10\"  v-if=\"moreCondition\">\r\n        <el-col :span=\"5\">\r\n        <el-form-item :label=\"$t('i18nView.pdCode')\" prop=\"\">\r\n          <el-input placeholder=\"\" clearable v-model=\"params2.pdCode\"></el-input>\r\n        </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"5\">\r\n        <el-form-item :label=\"$t('customComponents.pdEdition')\" prop=\"\">\r\n          <el-input placeholder=\"\" clearable v-model=\"params2.pdEdition\"></el-input>\r\n        </el-form-item>\r\n        </el-col>\r\n\r\n      </el-row>\r\n\r\n\r\n    </el-form>\r\n\r\n\r\n    <cus-table-title :label=\"$t('i18nView.waitThing')\"/>\r\n    <el-table :data=\"configs2\" align=\"center\" border class=\"wraper-table\" element-loading-text=\"Loading\" fit\r\n              highlight-current-row\r\n              size=\"mini\">>\r\n      <el-table-column :label=\"$t('table.select')\" align=\"center\" width=\"60px\">\r\n        <template slot-scope=\"scope\">\r\n          <el-radio :label=\"scope.row.id+scope.row.taskName\" @change=\"taskStatus(scope.row)\" v-model=\"radio\">{{''}}</el-radio>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\"></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('i18n