  <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('ruleMain.description')\" prop=\"description\">\r\n            <el-input :rows=\"1\" type=\"textarea\" v-model=\"form.description\"></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n      <el-form-item>\r\n        <el-button  size=\"mini\" type=\"primary\" @click=\"submitForm('form')\">下一步</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\nimport {\r\n  addRuleMain,\r\n  deleteRuleMain,\r\n  updateRuleMain\r\n} from \"@/api/ibrms/ruleMain\";\r\n\r\nexport default {\r\n  name: \"ruleMain\",\r\n  data() {\r\n    return {\r\n      //表格数据\r\n      configs: [],\r\n      //查询参数\r\n      params: {\r\n        like_xxxxx: \"\",\r\n        eq_xxxx: \"\"\r\n      },\r\n      form: {\r\n        //表单数据与后端实体类对应\r\n        id: \"\", //主键ID\r\n        ruleType: \"\", //\r\n        state: \"0\"\r\n      },\r\n      //检验规则\r\n      rules: {\r\n        userCode: [\r\n          { required: true, message: this.$t(\"ruleMain.xxx\"), trigger: \"blur\" }\r\n        ]\r\n      },\r\n      dialogVisible: false, //dialog显示还是隐藏\r\n      dialogStatus: \"\", //表单状态是新增还是修改\r\n      titleMap: {\r\n        update: this.$t(\"ruleMain.update\"),\r\n        create: this.$t(\"ruleMain.add\")\r\n      }\r\n    };\r\n  },\r\n  computed: {},\r\n  created() {},\r\n  methods: {\r\n    handleSearch() {\r\n      this.$refs.page.reload();\r\n    },\r\n    handleCreate() {\r\n      this.resetForm();\r\n      this.dialogStatus = \"create\";\r\n      this.dialogVisible = true;\r\n    },\r\n    handleUpdate(row) {\r\n      this.form = Object.assign({}, row);\r\n      this.dialogStatus = \"update\";\r\n      this.dialogVisible = true;\r\n    },\r\n    handleDelete(id) {\r\n      deleteRuleMain(id).then(response => {\r\n        this.handleSearch();\r\n        this.notifySuccess(response.message);\r\n        this.dialogVisible = false;\r\n      });\r\n    },\r\n    submitForm() {\r\n      this.$refs.dataForm.validate(valid => {\r\n        if (valid) {\r\n          if (this.form.ruleType == \"0\") {\r\n            sessionStorage.setItem(\"keyId\", \"1\");\r\n            this.$router.push({ name: \"校验型规则\", params: {} });\r\n          }\r\n        }\r\n      });\r\n    },\r\n    resetForm() {\r\n      this.form = {\r\n        //表单数据与后端实体类对应\r\n        id: \"\", //主键ID\r\n        xxxx: \"\"\r\n      };\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.el-select {\r\n  width: 100%;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/ibrms/ruleManagement/ruleInfo"}]}