"checkProcess('release')\"\r\n               shadow=\"always\">\r\n\r\n      </el-card>\r\n    </div>\r\n\r\n    <!--产品框架定义-->\r\n    <div class=\"cusSubCard cusProcess\">\r\n      <div class=\"sub\" v-for=\"item1 in  this.statusData.children\">\r\n        <div class=\"wraper\">\r\n          <div :class=\"{'loss_no' : item1.moduleStatus ==='0'}\" class=\"loss\"></div>\r\n          <div :class=\"{'tag_no' : item1.moduleStatus ==='0'}\" class=\"tag\">{{item1.moduleName}}</div>\r\n          <div :class=\"{'triangle_no' : item1.moduleStatus ==='0'}\" class=\"triangle\"></div>\r\n        </div>\r\n        <div style=\"margin-top: 23px\">\r\n          <el-steps direction=\"vertical\" process-status=\"success\" v-for=\"item2 in  item1.children\">\r\n            <el-step :icon=\"item2.moduleStatus=='1'?'el-icon-circle-check': 'el-icon-circle-close'\"\r\n                     :status=\"item2.moduleStatus=='1'? 'finish' : 'wait'\" :title=\"item2.moduleName\"></el-step>\r\n          </el-steps>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"triangle-up\">\r\n\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\n\r\n  import {getStatus} from '@/api/basicinfo/pdtemppdmodulestatus'\r\n\r\n  export default {\r\n    name: 'newstyle',\r\n    props: {\r\n      pdProcessId: {\r\n        type: String,\r\n        required: false\r\n      },moduleType: {\r\n        type: String,\r\n        required: false,\r\n        default: 'pdInfo'\r\n      },\r\n      pdInfoShow: {\r\n        type: Boolean,\r\n        required: false,\r\n        default: true\r\n      },\r\n      operationalShow: {\r\n        type: Boolean,\r\n        required: false,\r\n        default: true\r\n      },\r\n      testShow: {\r\n        type: Boolean,\r\n        required: false,\r\n        default: true\r\n      },\r\n      checkShow: {\r\n        type: Boolean,\r\n        required: false,\r\n        default: true\r\n      },\r\n      releaseShow: {\r\n        type: Boolean,\r\n        required: false,\r\n        default: true\r\n      }\r\n    },\r\n    data() {\r\n      return {\r\n        moduleNode: 'pdInfo',\r\n        processShow: 'pdInfo',\r\n        statusData: {},\r\n        form: {\r\n          referenceProducts: []\r\n        }\r\n      }\r\n    },\r\n    watch: {\r\n      pdProcessId(val,oldVal) {\r\n        this.pdProcessId = val\r\n        this.checkProcess(this.moduleType)\r\n      }\r\n    },\r\n    created() {\r\n      let data = this.getPdData()\r\n      this.pdProcessId = data.pdProcessId\r\n      this.checkProcess(this.moduleType)\r\n    },\r\n    mounted() {\r\n\r\n    },\r\n    methods: {\r\n      checkProcess(type) {\r\n        this.moduleNode = type\r\n        getStatus(this.buildRequestData({\"pdProcessId\": this.pdProcessId, \"moduleNode\": this.moduleNode})).then(res => {\r\n          this.statusData = res.data[0]\r\n        })\r\n      }\r\n    },\r\n    destroyed() {\r\n\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/basicinfo/pdprocess"}]}