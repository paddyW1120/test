[]\r\n      }\r\n    },\r\n    definingAmount: {\r\n      type: Object,\r\n      required: false,\r\n      default: () => {\r\n        return {}\r\n      }\r\n    }\r\n  },\r\n  filters: {\r\n    filter_pdUwSiType(codeArray) {\r\n      if (codeArray) {\r\n        return _this.getCascaderCodeName('underwritingAmountType', codeArray)\r\n      } else {\r\n        return ''\r\n      }\r\n    }\r\n  },\r\n  data() {\r\n    _this = this\r\n    return {\r\n      ruleView: {\r\n        visible: false,\r\n        performType: '',\r\n        flag: '',\r\n        ruleCode: '',\r\n      },\r\n      pdUwSiType: '',\r\n      selectedFormula: {},\r\n      params_current: {\r\n        pdInfoId: undefined,\r\n        currentBussId: this.definingAmount.id\r\n      },\r\n      params_brief: {\r\n        product: undefined,\r\n        pdBussId: this.definingAmount.id,\r\n        productDetailType: '',\r\n        pdBussDetailType: this.pdUwSiTypeForFormula.startsWith('FX') ? '41' : '42'\r\n      },\r\n      tableData_current: [],\r\n      tableData_brief: [],\r\n      attributeObj: {},\r\n      formulaVisible: false,\r\n      product: {\r\n        id: undefined,\r\n        bType: '',\r\n        cType: '',\r\n        pdCode: '',\r\n        pdEdition: '',\r\n        pdVerId: undefined,\r\n        pdSalechnl: '',\r\n        pdName: '',\r\n        pdShortname: '',\r\n        pdEnName: '',\r\n        pdShortEnName: '',\r\n        operator: '',\r\n        pdApplDate: undefined,\r\n        pdProcessId: undefined,\r\n        createTime: undefined,\r\n        updateTime: undefined\r\n      },\r\n      filters_pdCode: []\r\n    }\r\n  },\r\n  watch: {\r\n    tableData_brief() {\r\n      this.filters_pdCode = []\r\n      this.tableData_brief.forEach(val => {\r\n        if (!this._.includes(this._.map(this.filters_pdCode, 'value'), val.pdCode)) {\r\n          this.filters_pdCode.push({\r\n            text: val.pdCode + ':' + val.pdName,\r\n            value: val.pdCode\r\n          })\r\n        }\r\n      })\r\n    }\r\n  },\r\n  mounted() {\r\n  },\r\n  created() {\r\n    this.initProductInfo()\r\n    // 初始化核保保额类型\r\n    this.pdUwSiType = this.pdUwSiTypeForFormula\r\n    this.$nextTick(() => {\r\n      this.$refs.page_current.reload()\r\n      this.$refs.page_brief.reload()\r\n    })\r\n  },\r\n  methods: {\r\n    initProductInfo() {\r\n      let pdtempPdInfo = this.getPdData();\r\n      if (Object.keys(pdtempPdInfo).length > 0) {\r\n        Object.assign(this.product, pdtempPdInfo)\r\n        this.params_current.pdInfoId = this.product.id\r\n        this.params_brief.product = this.product\r\n        this.params_brief.productDetailType = this.product.cType\r\n      } else {\r\n        this.notifyError(this.$t('message.invalidProductInfo'))\r\n      }\r\n    },\r\n    deleteFormula(row) {\r\n      deleteAmountFormula(row).then(() => {\r\n        this.notifySuccess(this.$t('successMessage.delete'))\r\n        this.$refs.page_current.reload()\r\n        this.$refs.page_brief.reload()\r\n      })\r\n    },\r\n    back() {\r\n      this.$emit('changeDialogVisible_formula', false)\r\n    },\r\n    toFormulaSelect() {\r\n      if (!this.pdUwSiType) {\r\n        this.notifyError(this.$t('pdtempPdUnderwritingAmountFormula.pleaseSelectPdUwSiType'))\r\n        return\r\n      }\r\n      this.$emit('changeDialogVisible_formulaSelect', true, this.pdUwSiType)\r\n    },\r\n    rowClick(row, column, event) {\r\n      if (event.target.nodeName === \"INPUT\") {\r\n        if (row.id === this.selectedFormula.id) {\r\n          this.selectedFormula = {}\r\n        }\r\n      }\r\n    },\r\n    ruleModuleClosed() {\r\n      this.formulaVisible = false\r\n      this.$refs.page_brief.reload()\r\n    },\r\n    formulaViewClosed() {\r\n      this.ruleView.visible = false\r\n      this.selectedFormula = {}\r\n    },\r\n    selectedFormulaChanged() {\r\n      this.ruleView.flag = this.selectedFormula.ruleAttr === '1' ? '1' : '2'\r\n      this.ruleView.performType = this.selectedFormula.performType\r\n      this.ruleView.ruleCode = this.selectedFormula.pdCalcCode\r\n      this.ruleView.visible = true\r\n    },\r\n    toAddFormula() {\r\n      const param = {\r\n        pdBussType: '8',\r\n        pdBussDetailType: this.pdUwSiTypeForFormula.startsWith('FX') ? '41' : '42'\r\n      }\r\n      getCalcCode(this.buildRequestData(param)).then(response => {\r\n        this.attributeObj = {\r\n          code: response.data,\r\n          ruleBussModu: '8',\r\n          ruleBussModuDeta: param.pdBussDetailType,\r\n          ruleType: '2',\r\n          ruleAttr: '2',\r\n          pdType: '1',\r\n          riskType: this.product.bType,\r\n          riskDetail: this.product.cType,\r\n          pdCode: this.product.pdCode,\r\n          pdEdition: this.product.pdEdition\r\n        }\r\n        this.formulaVisible = true\r\n      })\r\n    },\r\n    showFormula(row) {\r\n      this.ruleView.flag = '1'\r\n      this.ruleView.performType = row.performType\r\n      this.ruleView.ruleCode = row.pdCalcCode\r\n      this.ruleView.visible = true\r\n    },\r\n    associate(row) {\r\n      const param = {\r\n        product: this.product,\r\n        formula: row,\r\n        amount: this.definingAmount,\r\n        needChecking: true\r\n      }\r\n      associate(this.buildRequestData(param)).then(response => {\r\n        let flag = response.data.flag\r\n        if (flag === true) {\r\n          this.notifySuccess(this.$t('rule.associationSuccess'))\r\n          this.$refs.page_current.reload()\r\n          this.$refs.page_brief.reload()\r\n        } else {\r\n          this.$confirm(String(this.$t('underwritingAmountFormulaSelect.confirm_overwrite_formula')), String(this.$t('confirmMessage.tip')), {\r\n            confirmButtonText: String(this.$t('confirmMessage.confirm')),\r\n            cancelButtonText: String(this.$t('confirmMessage.cancel')),\r\n            type: 'warning'\r\n          }).then(() => {\r\n            param.needChecking = false\r\n            associate(this.buildRequestData(param)).then(() => {\r\n              this.notifySuccess(this.$t('rule.associationSuccess'))\r\n              this.$refs.page_current.reload()\r\n              this.$refs.page_brief.reload()\r\n            })\r\n          }).catch(() => {\r\n            this.messageInfo(this.$t('underwritingAmountFormulaSelect.canceled'))\r\n          })\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/product-definition/pd-operation-def/pd-underwriting-def/underwritingAmountFormula"}]}