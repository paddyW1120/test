2BA0CA,IA1CA,EA0CA;AACA,UAAA,cAAA,GAAA,KAAA,gBAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,GAAA,cAAA,GAAA,GAAA,GAAA,KAAA,UAAA,GAAA,QAAA;AACA,UAAA,KAAA,GAAA,KAAA,UAAA,CAAA,IAAA,CAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;;AACA,WAAA,aAAA,GAAA,UAAA,OAAA;AAAA,eAAA,OAAA,CAAA,MAAA,CAAA,EAAA,EAAA;AAAA,iBAAA,OAAA,CAAA,OAAA,mBAAA,KAAA,EAAA,CAAA;AAAA,SAAA,CAAA;AAAA,OAAA;AACA,KAhDA;AAiDA,IAAA,gBAjDA,4BAiDA,IAjDA,EAiDA;AACA,WAAA,iBAAA,GAAA,KAAA;AACA,WAAA,aAAA,GAAA,KAAA;AACA,WAAA,WAAA,GAAA,KAAA;;AACA,cAAA,IAAA;AACA,aAAA,sBAAA;AACA,eAAA,iBAAA,GAAA,IAAA;AACA,iBAAA,8EAAA;;AACA,aAAA,WAAA;AACA,eAAA,aAAA,GAAA,IAAA;AACA,iBAAA,6EAAA;;AACA,aAAA,SAAA;AACA,eAAA,WAAA,GAAA,IAAA;AACA,iBAAA,mEAAA;;AACA;AACA,iBAAA,EAAA;AAXA;AAaA,KAlEA;AAmEA,IAAA,QAnEA,oBAmEA,GAnEA,EAmEA;AACA,WAAA,OAAA,GAAA,GAAA;AACA,KArEA;AAsEA,IAAA,UAtEA,wBAsEA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,yEAAA;AAAA,QAAA,MAAA,EAAA;AAAA,OAAA;AACA,KAxEA;AAyEA,IAAA,QAzEA,oBAyEA,GAzEA,EAyEA;AACA,UAAA,IAAA,GAAA,GAAA,CAAA,IAAA;AACA,WAAA,mBAAA,CAAA,IAAA;AACA,KA5EA;AA6EA,IAAA,UA7EA,wBA6EA;AACA,UAAA,SAAA,GACA,MAAA,CAAA,WAAA,IACA,QAAA,CAAA,eAAA,CAAA,SADA,IAEA,QAAA,CAAA,IAAA,CAAA,SAHA;AAIA,WAAA,OAAA,GAAA,SAAA,GAAA,KAAA,SAAA;AACA;AAnFA;AAxDA,C","sourcesContent":["<template>\r\n  <cus-wraper>\r\n    <div class=\"nav-container\">\r\n      <cus-page-title-back :label=\"$t('route.productUnderwritingDefinition')\" @backTo=\"backToMenu\"/>\r\n\r\n      <cus-product-info></cus-product-info>\r\n\r\n      <van-sticky :offset-top=\"0\">\r\n      <el-collapse id=\"boxFixed\" v-model=\"active\" :class=\"{'is_fixed' : isFixed}\" accordion\r\n                   class=\"collapseCus collapseCusProductUnderWriting\">\r\n        <el-collapse-item name=\"hunderwriting\" title=\"产品承保定义\">\r\n          <div class=\"stepDiv\" style=\"height: 97px;\">\r\n            <el-steps :active=\"activeName\" direction=\"vertical\" finish-status=\"wait\" process-status=\"finish \">\r\n              <el-step icon=\"el-icon-info\" title=\"承保信息定义\"\r\n                       @click.native=\"handleHunderwriting('hunderwritingdefinfo')\"></el-step>\r\n              <el-step icon=\"el-icon-info\" title=\"投保规则定义\"\r\n                       @click.native=\"handleHunderwriting('insurance')\"></el-step>\r\n              <el-step icon=\"el-icon-info\" title=\"产品页面定义\"\r\n                       @click.native=\"handleHunderwriting('pagedef')\"></el-step>\r\n            </el-steps>\r\n          </div>\r\n        </el-collapse-item>\r\n      </el-collapse>\r\n      </van-sticky>\r\n\r\n\r\n      <operationbutton :procInsId=\"pdProcessId\" :type=\"type\"></operationbutton>\r\n\r\n      <el-tabs v-model=\"activeTabName\" type=\"border-card\" @tab-click=\"clickTab\">\r\n        <div :class=\"{ 'showDiv': showPageFlag === '1' }\"></div>\r\n        <el-tab-pane :label=\"$t('route.pdtemppdnbinfo_list')\" name=\"hunderwritingdefinfo\">\r\n          <component :is=\"componentName\" v-if=\"hunderwritingFlag\" @childFn=\"parentFn\" @hook:mounted=\"finishPage\"/>\r\n        </el-tab-pane>\r\n        <el-tab-pane :label=\"$t('i18nView.cusRuleConfiguration')\" name=\"insurance\">\r\n          <component :is=\"componentName\" v-if=\"insuranceFlag\" @childFn=\"parentFn\" @hook:mounted=\"finishPage\"/>\r\n        </el-tab-pane>\r\n        <el-tab-pane :label=\"$t('i18nView.pdPageDef')\" name=\"pagedef\">\r\n          <component :is=\"componentName\" v-if=\"pagedefFlag\" @childFn=\"parentFn\" @hook:mounted=\"finishPage\"/>\r\n        </el-tab-pane>\r\n      </el-tabs>\r\n    </div>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\nimport operationbutton from '@/views/basicinfo/operationend/index'\r\nimport {mapGetters} from 'vuex'\r\n\r\nexport default {\r\n  name: 'productUnderwritingDefinition',\r\n  computed: {\r\n    ...mapGetters(['tenantCode'])\r\n  },\r\n  data() {\r\n    return {\r\n      activeName: 0,\r\n      activedNames: ['pdtemppdnbinfo'],\r\n      type: 'productCoverFlag',\r\n      showPageFlag: '',\r\n      active: 'hunderwriting',\r\n      isFixed: false,\r\n      isSaved: false,\r\n      offsetTop: 20,\r\n      componentName: '',\r\n      hunderwritingFlag: false,\r\n      insuranceFlag: false,\r\n      pagedefFlag: false,\r\n      nowPosition: 'hunderwritingdefinfo',\r\n      activeTabName: 'hunderwritingdefinfo',\r\n      stepOrder: {'hunderwritingdefinfo': 0, 'insurance': 1, 'pagedef': 2},\r\n      product: {\r\n        id: undefined,\r\n        pdCode: '',\r\n        pdEdition: '',\r\n        pdVerId: undefined,\r\n        pdSalechnl: '',\r\n        pdName: '',\r\n        pdShortname: '',\r\n        pdEnName: '',\r\n        pdShortEnName: '',\r\n        operator: '',\r\n        pdApplDate: undefined,\r\n        pdProcessId: undefined,\r\n        createTime: undefined,\r\n        updateTime: undefined\r\n      },\r\n      pdProcessId: ''\r\n    }\r\n  },\r\n  async created() {\r\n    this.showPageFlag = this.$route.query.showPageFlag\r\n    await this.initProductInfo()\r\n    this.switchComponent('hunderwritingdefinfo')\r\n  },\r\n  components: {\r\n    operationbutton\r\n  },\r\n  mounted() {\r\n    window.addEventListener('scroll', this.initHeight)\r\n    this.$nextTick(() => {\r\n      this.offsetTop = document.querySelector('#boxFixed').offsetTop\r\n    })\r\n    this.$emit('finish')\r\n  },\r\n  methods: {\r\n    finishPage(){\r\n      this.$emit('finish')\r\n    },\r\n    async initProductInfo() {\r\n      let pdtempPdInfo = this.getPdData()\r\n      if (pdtempPdInfo) {\r\n        Object.assign(this.product, pdtempPdInfo)\r\n        this.pdProcessId = pdtempPdInfo.pdProcessId\r\n      }\r\n    },\r\n    handleHunderwriting(type) {\r\n      let index = this.stepOrder[type]\r\n      if (this.nowPosition !== type) {\r\n        if (this.isSaved) {\r\n          this.activeTabName = type\r\n          this.isSaved = false\r\n          this.activeName = index\r\n          this.nowPosition = type\r\n          this.switchComponent(type)\r\n        } else {\r\n          this.$confirm(\r\n            this.$t('productMenu.messageTitle'),\r\n            this.$t('productMenu.messageTip'),\r\n            {\r\n              confirmButtonText: this.$t('productMenu.messageButton2'),\r\n              cancelButtonText: this.$t('productMenu.messageButton1'),\r\n              type: 'warning'\r\n            }\r\n          ).then(() => {\r\n            this.activeTabName = type\r\n            this.activeName = index\r\n            this.nowPosition = type\r\n            window.scrollTo(0, 0)\r\n            this.switchComponent(type)\r\n          }).catch(() => {\r\n            this.activeTabName = this.nowPosition\r\n          })\r\n        }\r\n\r\n      }\r\n    },\r\n    switchComponent(type) {\r\n      let moduleLocation = this.getComponentPath(type)\r\n      const path = moduleLocation + '/' + this.tenantCode + '/index'\r\n      let path1 = this.isInAllVue(path)\r\n      console.log(path1)\r\n      this.componentName = resolve => require.ensure([], () => resolve(require(`@/views/${path1}`)))\r\n    },\r\n    getComponentPath(type) {\r\n      this.hunderwritingFlag = false\r\n      this.insuranceFlag = false\r\n      this.pagedefFlag = false\r\n      switch (type) {\r\n        case  'hunderwritingdefinfo':\r\n          this.hunderwritingFlag = true\r\n          return 'product-definition/pd-operation-def/pd-acceptance-def/pd-acceptance-info-def'\r\n        case  'insurance':\r\n          this.insuranceFlag = true\r\n          return 'product-definition/pd-operation-def/pd-acceptance-def/pd-insurance-rule-def'\r\n        case  'pagedef':\r\n          this.pagedefFlag = true\r\n          return 'product-definition/pd-operation-def/pd-acceptance-def/pd-page-def'\r\n        default :\r\n          return ''\r\n      }\r\n    },\r\n    parentFn(val) {\r\n      this.isSaved = val\r\n    },\r\n    backToMenu() {\r\n      this.$router.push({path: '/product-definition/pd-operation-def/pd-acceptance-def/acceptance-apply', params: {}})\r\n    },\r\n    clickTab(val) {\r\n      let type = val.name\r\n      this.handleHunderwriting(type)\r\n    },\r\n    initHeight() {\r\n      let scrollTop =\r\n        window.pageYOffset ||\r\n        document.documentElement.scrollTop ||\r\n        document.body.scrollTop\r\n      this.isFixed = scrollTop > this.offsetTop\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.showDiv {\r\n  width: 100%;\r\n  height: 100%;\r\n  margin-top: -40px;\r\n  margin-left: -40px;\r\n  position: absolute;\r\n  background: #ffffff00;\r\n  z-index: 999;\r\n}\r\n\r\n\r\n.collapseCusProductUnderWriting {\r\n  top: 129px !important;\r\n  right: 15px !important;\r\n  width: 150px !important;\r\n\r\n}\r\n\r\n\r\n.is_fixed {\r\n  position: fixed;\r\n  top: 0px !important;\r\n  z-index: 99;\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/product-definition/pd-operation-def/pd-acceptance-def/main"}]}