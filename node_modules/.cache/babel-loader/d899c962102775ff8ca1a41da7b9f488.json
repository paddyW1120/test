\n    return {\r\n      pdId: '',\r\n      chart1Show: false,\r\n      chartShow: false,\r\n      chart1: null,\r\n      chart1xData: [],\r\n      chart1yData: [],\r\n      chart2Show: false,\r\n      chart2: null,\r\n      chart2xData: [],\r\n      chart2yData: [],\r\n      barCount: '',\r\n      releasePdCount: '',\r\n      activeName: 'product',\r\n      title1: this.$t('monitorUser.productDefStatistics'),\r\n      tabTitle1: '',\r\n      title2: this.$t('monitorUser.productOperationStatistics'),\r\n      processShow: 'pdInfo',\r\n      statusData: {},\r\n      form: {\r\n        referenceProducts: []\r\n      },\r\n      pdform: {\r\n        pdCode: '',\r\n        pdEdition: '',\r\n        pdName: ''\r\n      }\r\n    }\r\n  },\r\n\r\n  created() {\r\n    let pdtempPdInfo = this.getJsonData('phone-pdInfo')\r\n    if (pdtempPdInfo) {\r\n      this.pdId = pdtempPdInfo.id\r\n      this.pdform.pdCode = pdtempPdInfo.pdCode\r\n      this.pdform.pdEdition = pdtempPdInfo.pdEdition\r\n      this.pdform.pdName = pdtempPdInfo.pdName\r\n    }\r\n    this.getIndexData()\r\n  },\r\n  mounted() {\r\n\r\n  },\r\n  methods: {\r\n    onClick(name) {\r\n\r\n\r\n    },\r\n    onClickLeft() {\r\n      this.$router.go(-1)\r\n    },\r\n    getIndexData() {\r\n      indexData(this.buildRequestData({})).then(res => {\r\n        let data = res.data\r\n        this.releasePdCount = data.releasePdCount\r\n        this.barData = data.barData\r\n        this.chart1xData = data.barData.xData\r\n        this.chart1yData = data.barData.yData\r\n        this.chart2xData = data.barData.productOperating.xData\r\n        this.chart2yData = data.barData.productOperating.yData\r\n\r\n        this.barCount = this.chart1yData.reduce(function(prev, curr, idx, arr) {\r\n          return prev + curr\r\n        })\r\n\r\n        this.tabTitle1 = \"产品(\"+this.barCount+\")\"\r\n\r\n        this.cardData = { 'taskCount': data.taskCount,monitorShow: this.monitorShow}\r\n        this.cardShow = true\r\n        this.initChart()\r\n      })\r\n    },\r\n    initChart() {\r\n      this.chart1 = 'chart1'\r\n      this.chart1Show = true\r\n\r\n      this.chart2 = 'chart2'\r\n      this.title2 = this.$t('monitorUser.productOperationStatistics')\r\n      this.chart2Show = true\r\n\r\n    },\r\n    refreshChBar(name) {\r\n      if (name === '产品营运定义') {\r\n        this.title2 = this.$t('monitorUser.productOperationStatistics')\r\n        this.chart2xData = this.barData.productOperating.xData\r\n        this.chart2yData = this.barData.productOperating.yData\r\n\r\n      } else if (name === '产品测试') {\r\n        this.title2 = '险种测试统计'\r\n        this.chart2xData = this.barData.productTest.xData\r\n        this.chart2yData = this.barData.productTest.yData\r\n      }\r\n    },\r\n  },\r\n  destroyed() {\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/wecom-login/wecom-dashboard/wecomlist"}]}