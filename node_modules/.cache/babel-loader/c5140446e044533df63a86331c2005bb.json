,EAAA,KAAA,cAAA;AACA;AApEA;AArCA,C","sourcesContent":["<template>\r\n  <cus-wraper>\r\n    <el-row>\r\n      <el-col :span=\"24\">\r\n        <el-button  size=\"small\" type=\"text\" @click=\"backToMenu\"><i class=\"el-icon-back\"></i>{{ $t('pdtempclmbnft.back') }}</el-button>\r\n        <label style=\"font-size:14px\">{{$t('i18nView.bill')}}</label>\r\n      </el-col>\r\n    </el-row>\r\n    <hr/>\r\n    <br/>\r\n    <el-form ref=\"selectForm1\" :model=\"paymentForm\" label-width=\"120px\" style=\"width:98%;\">\r\n      <el-row :gutter=\"10\" class=\"row-bg\" justify=\"left\" type=\"flex\">\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('pdtempclmbnft.finelinesRegion')\">\r\n            <cus-select v-model=\"paymentForm.productTypeDetail\" :codetype=\"'ProductCategory'\" @change=\"findProducts\">\r\n            </cus-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('pdtempclmbnft.product')\">\r\n            <el-select v-model=\"paymentForm.product\" :placeholder=\"$t('pdtempclmbnft.pleaseChoose')\" clearable\r\n                       filterable>\r\n              <el-option v-for=\"item in productOptions\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('pdtempclmbnft.pdMedBillType')\">\r\n            <cus-select v-model=\"paymentForm.pdBnftDetailType\"\r\n                        :codetype=\"'medBillType'\"></cus-select>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row>\r\n        <el-form-item align=\"right\">\r\n          <!-- 搜索 -->\r\n          <el-button  size=\"small\" icon=\"el-icon-search\" type=\"primary\" @click=\"selectPdBnft\" dialog-disable=\"disable\">{{ $t('pdtempclmbnft.search') }}\r\n          </el-button>\r\n        </el-form-item>\r\n      </el-row>\r\n    </el-form>\r\n    <cus-table-title :label=\"$t('pdtempclmbnft.choosePdClmBnft')\"/>\r\n    <el-table ref=\"bnftTable\" :data=\"bnftConfigs\" align=\"center\" border class=\"wraper-table\"\r\n              element-loading-text=\"Loading\"\r\n              fit highlight-current-row size=\"mini\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\" width=\"50\"></el-table-column>\r\n      <el-table-column :label=\"$t('table.detail')\" align=\"center\" style=\"width: 50%\" type=\"expand\">\r\n        <template slot-scope=\"props\">\r\n          <el-form class=\"demo-table-expand\" inline label-position=\"left\">\r\n            <el-form-item :label=\"$t('pdtempclmbnft.pdClmBnftCode')\" style=\"width: 50%\">\r\n              {{ props.row.pdClmBnftCode }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('pdtempclmbnft.pdMedBillType')\" style=\"width: 50%\">\r\n              {{ props.row.pdMedBillTypeName }}\r\n            </el-form-item>\r\n          </el-form>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('pdtempclmbnft.pdClmBnftCode')\" align=\"center\"\r\n                       prop=\"pdClmBnftCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('pdtempclmbnft.pdMedBillType')\" align=\"center\" prop=\"pdMedBillType\">\r\n      </el-table-column>\r\n    </el-table>\r\n    <cus-pagination ref=\"page\" v-model=\"bnftConfigs\" :pageSize=\"5\"\r\n                    :params=\"paymentForm\" url=\"/pdtemp-pd-claim-verification-rule/base/selectPdClmBnft\"/>\r\n    <br/>\r\n    <el-form>\r\n      <el-form-item style=\"text-align: right\">\r\n        <el-button  size=\"small\" v-auth=\"'select:AllPdAccount'\" icon=\"el-icon-circle-plus-outline\" type=\"primary\" @click=\"addPdProg\" dialog-disable=\"disable\">\r\n          {{ $t('pdtempclmbnft.add') }}\r\n        </el-button>\r\n        <el-button  size=\"small\" icon=\"el-icon-refresh\" type=\"primary\" @click=\"researchData\" dialog-disable=\"disable\">\r\n          {{ $t('pdtempclmbnft.refresh') }}\r\n        </el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n    <cus-table-title :label=\"$t('pdtempclmbnft.AssociatedPlan')\"/>\r\n    <el-table ref=\"paymentTable\" :data=\"paymentConfigs\" border element-loading-text=\"Loading\" fit highlight-current-row\r\n              max-height=\"300px\" size=\"mini\">\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\" width=\"50\"></el-table-column>\r\n      <el-table-column :label=\"$t('table.detail')\" align=\"center\" style=\"width: 50%\" type=\"expand\">\r\n        <template slot-scope=\"props\">\r\n          <el-form class=\"demo-table-expand\" inline label-position=\"left\">\r\n            <el-form-item :label=\"$t('pdtempclmbnft.pdClmBnftCode')\" style=\"width: 50%\">\r\n              {{ props.row.pdClmBnftCode }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('pdtempclmbnft.pdMedBillType')\" style=\"width: 50%\">\r\n              {{ props.row.pdMedBillTypeName }}\r\n            </el-form-item>\r\n          </el-form>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('pdtempclmbnft.pdClmBnftCode')\" align=\"center\"\r\n                       prop=\"pdClmBnftCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('pdtempclmbnft.pdMedBillType')\" align=\"center\"\r\n                       prop=\"pdMedBillType\"></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('pdtempclmbnft.actions')\" width=\"60px\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button   size=\"mini\" type=\"danger\" @click=\"deleteRela(scope.row)\" dialog-disable=\"disable\">-</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\nimport {findProductsByProductType} from \"@/api/basicinfo/pdtemppdclmbnft\";\r\nimport {findCtrlRela} from \"@/api/basicinfo/productCompensation\"\r\n\r\nexport default {\r\n  filters: {\r\n    filter_pdclmbnfttype2(code) {\r\n      return _this.getCodeName('pdclmbnfttype2', code);\r\n    },\r\n  },\r\n  name: \"pdtemppdbnftinfo\",\r\n  props: {\r\n    moreType: {\r\n      type: String,\r\n      default: ''\r\n    },\r\n    pdBnftCtrlCode: {\r\n      type: String,\r\n      default: ''\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      pdtempPdInfo: {},\r\n      paymentForm: {\r\n        productTypeDetail: '',\r\n        product: '',\r\n        pdBnftDetailType: '',\r\n      },\r\n      paymentConfigs: [],\r\n      bnftConfigs: [],\r\n      productOptions: [],\r\n      chooseBnft: [],\r\n      chooseProg: []\r\n\r\n    };\r\n  },\r\n  created() {\r\n    this.pdtempPdInfo = this.getPdData();\r\n    this.initPdClmBnft();\r\n  },\r\n  methods: {\r\n    //初始化关联账单信息\r\n    initPdClmBnft() {\r\n      if (this.pdBnftCtrlCode) {\r\n        findCtrlRela(this.pdBnftCtrlCode).then(response => {\r\n          this.paymentConfigs = response.data\r\n        })\r\n        /*const params = {\r\n          pdInfoId: this.pdtempPdInfo.id\r\n        }\r\n        initPdClmBnft(this.buildRequestData(params)).then(res => {\r\n          this.paymentConfigs = res.data\r\n        })*/\r\n      }\r\n    },\r\n    //保存产品方案\r\n    addPdProg() {\r\n      for (const item of this.chooseProg) {\r\n        let duplicated = false\r\n        for (const selectedItem of this.paymentConfigs) {\r\n          if (selectedItem.id === item.id) {\r\n            setTimeout(() => {\r\n              this.notifyError(item.pdClmBnftCode + ':' + item.pdMedBillType + '已添加，请勿重复添加')\r\n            }, 1)\r\n            duplicated = true\r\n            break\r\n          }\r\n        }\r\n        if (!duplicated) {\r\n          this.paymentConfigs.push(item)\r\n        }\r\n      }\r\n    },\r\n    //产品搜索\r\n    selectPdBnft() {\r\n      this.$refs.page.reload()\r\n    },\r\n\r\n    findProducts(val) {\r\n      let data = {\r\n        pdCls2: val\r\n      }\r\n      findProductsByProductType(this.buildRequestData(data)).then(res => {\r\n        this.productOptions = [];\r\n        res.data.forEach(product => {\r\n          if (product.id != this.pdtempPdInfo.id) {\r\n            this.productOptions.push({\r\n              value: product.pdCode,\r\n              label: product.pdCode + \":\" + product.pdName\r\n            });\r\n          }\r\n        });\r\n      })\r\n    },\r\n    researchData() {\r\n      this.paymentForm.productTypeDetail = ''\r\n      this.paymentForm.product = ''\r\n      this.bnftConfigs = []\r\n      this.$refs.page.reload()\r\n    },\r\n    deleteRela(row) {\r\n      this.paymentConfigs.splice(this.paymentConfigs.indexOf(row), 1)\r\n    },\r\n    handleSelectionChange(row) {\r\n      this.chooseProg = row;\r\n    },\r\n    backToMenu() {\r\n      this.$emit('backToFeeBill', this.moreType, this.paymentConfigs)\r\n    },\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.divider {\r\n  color: black;\r\n  background-color: black;\r\n  width: 1px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/product-definition/pd-framework-def/pd-structure-def/guarantee-def/general-medical/pdtemppdclmbnftinfo"}]}