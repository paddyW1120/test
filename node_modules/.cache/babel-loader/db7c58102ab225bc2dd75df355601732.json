,\r\n    hideCode: {\r\n      type: Boolean,\r\n      required: false,\r\n      default: false\r\n    },\r\n    secondLevel: {\r\n      type: Boolean,\r\n      required: false,\r\n      default: false\r\n    },\r\n    filter: {\r\n      type: String,\r\n      required: false\r\n    },\r\n    disabled: {\r\n      type: Boolean,\r\n      required: false\r\n    },\r\n    value: {\r\n      type: String\r\n    },\r\n    disabledCodeArray: {\r\n      type: Array,\r\n      required: false,\r\n      default: () => {\r\n        return []\r\n      }\r\n    },\r\n    placeholder: {\r\n      type: String,\r\n      required: false,\r\n      default: '请选择'\r\n    }\r\n  },\r\n  mounted() {\r\n    this.buildSelectOptions()\r\n    this.selectValue = this.value\r\n    if (this.disabled) {\r\n      this.isDisabled = this.disabled\r\n    }\r\n  },\r\n  methods: {\r\n    buildSelectOptions() {\r\n      if (!this.secondLevel) {\r\n        this.options = this.ldcodes[this.codetype]\r\n        if (this.filter) {\r\n          this.options = this.filterOptions(this.filter, this.options)\r\n        }\r\n      } else {\r\n        if (this.filter) {\r\n          let secondKey = this.codetype + \".\" + this.filter\r\n          this.options = this.ldcodes[secondKey]\r\n        }\r\n      }\r\n    },\r\n    doChange(val) {\r\n      this.$emit('change', val)\r\n    },\r\n    setDisabled(code) {\r\n      return !!this._.includes(this.disabledCodeArray, code)\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      options: [],\r\n      selectValue: '',\r\n      isDisabled: false\r\n    }\r\n  },\r\n  watch: {\r\n    value(val, oldVal) {\r\n      this.selectValue = val\r\n    },\r\n    selectValue(val, oldVal) {\r\n      if (val != oldVal) {\r\n        this.$emit('input', this.selectValue)\r\n      }\r\n    },\r\n    disabled(val, oldVal) {\r\n      if (val != oldVal) {\r\n        this.isDisabled = val\r\n      }\r\n    },\r\n    filter(val, oldVal) {\r\n      if (val != oldVal) {\r\n        this.selectValue = ''\r\n        this.buildSelectOptions()\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapGetters([\r\n      'ldcodes'\r\n    ])\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n"],"sourceRoot":"src/customComponents/CodeSelect"}]}