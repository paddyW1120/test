r\n  created() {\r\n    if (this.content) {\r\n      this.content_ = this.content\r\n    }\r\n    if (this.buttonText) {\r\n      this.buttonText_ = this.buttonText\r\n    }\r\n  },\r\n  mounted() {\r\n      this.onlyDisabeldTableButton()\r\n  },\r\n  methods: {\r\n    handleConfirm() {\r\n      this.$emit('confirm');\r\n      this.visible = false\r\n    },\r\n    handleCancel() {\r\n      this.$emit('cancel');\r\n      this.visible = false\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped></style>\r\n"],"sourceRoot":"src/customComponents/Popover"}]}