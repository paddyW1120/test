;AA7DA;AAvCA,C","sourcesContent":["<template>\r\n  <cus-wraper>\r\n\r\n<!--    <label>{{ $t('commonPa.paConfig') }}</label>-->\r\n<!--    <hr>\r\n    <el-button  size=\"small\" @click=\"dialogVisible.paDef = true\" type=\"primary\">{{ $t('commonPa.paDef') }}</el-button>-->\r\n    <el-button  size=\"small\" @click=\"dialogVisible.infoItemDef = true\" icon=\"el-icon-monitor el-icon-left\" type=\"primary\">{{ $t('commonPa.paItemDef') }}</el-button>\r\n    <cus-table-title :label=\"$t('commonPa.paList')\"></cus-table-title>\r\n    <el-table :data=\"tableDataPa\" @row-click=\"rowClick_pa\" border element-loading-text=\"Loading\" fit\r\n              highlight-current-row size=\"mini\">\r\n      <el-table-column :label=\"$t('table.select')\" align=\"center\" width=\"50px\">\r\n        <template slot-scope=\"scope\">\r\n          <el-radio-group @change=\"selectedPaChanged\" v-model=\"selectedPa\">\r\n            <el-radio :label=\"scope.row\">{{''}}</el-radio>\r\n          </el-radio-group>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" width=\"50\">\r\n        <template v-slot=\"scope\">\r\n          {{ initTableIndex('page_pa', scope.$index) }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('commonPa.pdPaCode')\" align=\"center\" prop=\"pdPaCode\" sortable></el-table-column>\r\n      <el-table-column :label=\"$t('commonPa.pdPaName')\" align=\"center\" prop=\"pdPaName\" sortable></el-table-column>\r\n      <el-table-column :label=\"$t('commonPa.pdPaAttributes')\" align=\"center\" prop=\"pdAttr\" sortable>\r\n        <template v-slot=\"scope\">\r\n          {{ scope.row.pdAttr | filter_pdPaAttributes }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('commonPa.pdPaType')\" align=\"center\"  sortable>\r\n        <template v-slot=\"scope\">\r\n            <span v-for=\"list in scope.row.pdgenlPdPaDesTypeList\" v-if=\"scope.row.pdgenlPdPaDesTypeList.length > 0\">\r\n              {{ list.pdDesType | filter_pdPaType }}\r\n              <span v-if=\"scope.row.pdgenlPdPaDesTypeList.indexOf(list) !== scope.row.pdgenlPdPaDesTypeList.length - 1\">,</span>\r\n            </span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.actions')\" align=\"center\" width=\"150\">\r\n        <template slot-scope=\"scope\">\r\n          <cus-popover v-auth=\"'common-pd-pa-info:judgeDelet'\" :content=\"$t('confirmMessage.delete')\"\r\n                       icon=\"el-icon-delete\"\r\n                       size=\"mini\" type=\"danger\" @confirm=\"deletePa(scope.row)\"></cus-popover>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <cus-pagination :pageSize=\"10\" :params=\"undefined\" ref=\"page_pa\" url=\"/common-pd-pa/base/list\"\r\n                    v-model=\"tableDataPa\"/>\r\n   <cus-form-title :display=\"false\" :label=\"$t('commonPa.paItemBaseDef')\" :showArrow=\"true\"\r\n                      @show=\"toShowPdBnft\"></cus-form-title>\r\n    <div v-show=\"showState\">\r\n       <commonpadef @dialogClosed=\"dialogClosed\" :selectedPaItem.sync=\"selectedPa\" ref=\"commonpadefData\"></commonpadef>\r\n    </div>\r\n   <cus-form-title :display=\"false\" :label=\"$t('route.commonPaInfoItemDef')\"  :showArrow=\"true\"\r\n                      @show=\"toShowPdCofigBnft\"></cus-form-title>\r\n  <div v-show=\"showCofigState\">\r\n    <el-row style=\"margin: 10px 0 0 0\">\r\n      <el-button  size=\"small\" @click=\"toRelate\" icon=\"el-icon-connection el-icon-left\" type=\"primary\">{{ $t('commonPa.paItemConfig') }}</el-button>\r\n    </el-row>\r\n    <cus-table-title :label=\"$t('commonPa.paItem')\"></cus-table-title>\r\n    <el-table :data=\"tableDataPaInfoItem\" border element-loading-text=\"Loading\" fit highlight-current-row\r\n              max-height=\"500\" size=\"mini\"  style=\"width: 80%\">\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\"></el-table-column>\r\n      <el-table-column :label=\"$t('commonPa.paItem')\" align=\"center\" prop=\"pdPaMessName\" sortable></el-table-column>\r\n     <!-- <el-table-column :label=\"$t('commonPa.pdPaMsExplanation')\" align=\"center\" prop=\"pdPaMessDesc\"\r\n                       sortable></el-table-column> -->\r\n      <el-table-column :label=\"$t('commonPa.pdPaMsType')\" align=\"center\" prop=\"pdPaMessType\" sortable>\r\n        <template v-slot=\"scope\">\r\n          {{ scope.row.pdPaMessType | filter_pdPaMsType }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('commonPa.pdPaMsOptions')\" align=\"center\" prop=\"ldcodeList\">\r\n        <template v-slot=\"scope\">\r\n          <span v-if=\"scope.row.pdPaMessType === '2'\">\r\n            <span v-for=\"ldcode in scope.row.ldcodeList\" v-if=\"scope.row.ldcodeList.length > 0\">\r\n              {{ ldcode.code+':'+ldcode.codeName }}\r\n              <span v-if=\"scope.row.ldcodeList.indexOf(ldcode) !== scope.row.ldcodeList.length - 1\">,</span>\r\n            </span>\r\n          </span>\r\n          <span v-else>\r\n            {{ scope.row.pdPaMessInputType | filter_pdPaMsInputType }}\r\n          </span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.actions')\" align=\"center\" width=\"150\">\r\n        <template slot-scope=\"scope\">\r\n          <cus-popover :content=\"$t('confirmMessage.delete')\" @confirm=\"deletePaInfo(scope.row)\" icon=\"el-icon-delete\"\r\n                       size=\"mini\" type=\"danger\" v-auth=\"'common-pd-pa:deleteInfoItemByPaIdAndItemId'\"></cus-popover>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n   </div>\r\n    <!-- 保全项目定义弹窗\r\n    <el-dialog :visible.sync=\"dialogVisible.paDef\" @close=\"dialogClosed\" v-dialog-drag v-if=\"dialogVisible.paDef\"\r\n               :close-on-click-modal=\"false\"   width=\"75%\">\r\n      <commonpadef @dialogClosed=\"dialogClosed\"></commonpadef>\r\n    </el-dialog> -->\r\n    <!-- 信息项定义弹窗 -->\r\n    <el-dialog :visible.sync=\"dialogVisible.infoItemDef\" @close=\"dialogClosed\" v-dialog-drag :close-on-click-modal=\"false\"\r\n               v-if=\"dialogVisible.infoItemDef\"\r\n               width=\"75%\">\r\n      <commonpainfoitemdef @dialogClosed=\"dialogClosed\"></commonpainfoitemdef>\r\n    </el-dialog>\r\n    <!-- 保全项目信息项关联弹窗 -->\r\n    <el-dialog :visible.sync=\"dialogVisible.relate\" @close=\"relateClosed\" v-dialog-drag v-if=\"dialogVisible.relate\"\r\n               :close-on-click-modal=\"false\"   width=\"75%\">\r\n      <commonpaitemrelate :pdPaInfo=\"selectedPa\" @dialogClosed=\"relateClosed\"\r\n                          @selectedPaChanged=\"selectedPaChanged\"></commonpaitemrelate>\r\n    </el-dialog>\r\n\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\n\r\n  import {deleteInfoItemByPaIdAndItemId, findAllPaInfoItemVoByPaId} from \"@/api/basicinfo/commonpa\"\r\n  import commonpadef from \"@/views/business-general-conf/commonpa/commonpa-item-def/index\"\r\n  import commonpainfoitemdef from \"@/views/business-general-conf/commonpa/commonpa-info-item-def/index\"\r\n  import commonpaitemrelate from \"@/views/business-general-conf/commonpa/commonpa-item-relate/index\"\r\n\r\n  let _this\r\n\r\n  export default {\r\n    name: \"commonPa\",\r\n    components: {commonpadef, commonpainfoitemdef, commonpaitemrelate},\r\n    filters: {\r\n      filter_pdPaAttributes(code) {\r\n        return _this.getCodeName('pdPaAttributes', code)\r\n      },\r\n      filter_pdPaType(code) {\r\n        return _this.getCodeName('ProductDesignType', code)\r\n      },\r\n      filter_pdPaMsType(code) {\r\n        return _this.getCodeName('pdPaMsType', code)\r\n      },\r\n      filter_pdPaMsInputType(code) {\r\n        return _this.getCodeName('pdPaMsInputType', code)\r\n      }\r\n    },\r\n    data() {\r\n      _this = this\r\n      return {\r\n        tableDataPa: [],\r\n        tableDataPaInfoItem: [],\r\n        selectedPa: {},\r\n        showState:false,\r\n        showCofigState:false,\r\n        dialogVisible: {\r\n          paDef: false,\r\n          infoItemDef: false,\r\n          relate: false\r\n        }\r\n      }\r\n    },\r\n    created() {\r\n\r\n    },\r\n    mounted() {\r\n\r\n    },\r\n    computed: {},\r\n    methods: {\r\n      selectedPaChanged() {\r\n        findAllPaInfoItemVoByPaId(this.selectedPa.id).then(response => {\r\n          this.tableDataPaInfoItem = response.data\r\n        })\r\n      },\r\n      rowClick_pa(row, column, event) {\r\n        if (event.target.nodeName === \"INPUT\") {\r\n          if (row.id === this.selectedPa.id) {\r\n            this.selectedPa = {}\r\n            this.tableDataPaInfoItem = []\r\n          }\r\n        }\r\n      },\r\n      deletePaInfo(row) {\r\n        const param = {\r\n          paId: this.selectedPa.id,\r\n          itemId: row.id\r\n        }\r\n        deleteInfoItemByPaIdAndItemId(this.buildRequestData(param)).then(() => {\r\n          this.notifySuccess(this.$t('successMessage.delete'))\r\n          this.selectedPaChanged()\r\n        })\r\n      },\r\n      refreshPaInfo(){\r\n         this.$refs.page_pa.reload();\r\n         this.selectedPa = {}\r\n         this.tableDataPaInfoItem = []\r\n      },\r\n      dialogClosed() {\r\n        this.dialogVisible.paDef = false\r\n        this.dialogVisible.infoItemDef = false\r\n        this.dialogVisible.relate = false\r\n        this.showCofigState =false\r\n        this.showState=false\r\n        this.$refs.page_pa.reload()\r\n        this.selectedPa = {}\r\n        this.tableDataPaInfoItem = []\r\n      },\r\n      relateClosed() {\r\n        this.dialogVisible.relate = false\r\n      },\r\n      toRelate() {\r\n        if (Object.keys(this.selectedPa).length === 0) {\r\n          this.notifyError(this.$t('commonPa.pleaseSelectPa'))\r\n          return\r\n        }\r\n        this.dialogVisible.relate = true\r\n      },\r\n      toShowPdBnft() {\r\n        this.$nextTick(() => {\r\n           this.showState = !this.showState;\r\n        })\r\n      },\r\n        toShowPdCofigBnft() {\r\n          this.$nextTick(() => {\r\n            this.showCofigState = !this.showCofigState;\r\n          })\r\n      },\r\n      deletePa(row) {\r\n        this.$refs.commonpadefData.deletePaInfo(row);\r\n      },\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"css\">\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/business-general-conf/commonpa"}]}