ase',\r\n    method: 'post',\r\n    data\r\n  })\r\n}\r\n\r\n/*发布退回*/\r\nexport function returnToAudit(data) {\r\n  return request({\r\n    url: '/pdtemp-pd-ver-rls/returnToAudit',\r\n    method: 'post',\r\n    data\r\n  })\r\n}\r\n\r\n/*查询发布环境*/\r\nexport function searchPathName(data) {\r\n  return request({\r\n    url: '/pdtemp-pd-ver-rls/base/searchPathName',\r\n    method: 'post',\r\n    data\r\n  })\r\n}\r\n\r\n/*查询发布规则*/\r\nexport function searchPdRules(data) {\r\n  return request({\r\n    url: '/pdtemp-pd-ver-rls/base/searchPdRules',\r\n    method: 'post',\r\n    data\r\n  })\r\n}\r\n\r\n/*查询发布公式*/\r\nexport function searchPdCalc(data) {\r\n  return request({\r\n    url: '/pdtemp-pd-ver-rls/base/searchPdCalc',\r\n    method: 'post',\r\n    data\r\n  })\r\n}\r\n\r\n/*查询待发布的产品*/\r\nexport function searchPDInRelease(data) {\r\n  return request({\r\n    url: '/pdtemp-pd-ver-rls/base/searchPDInRelease',\r\n    method: 'post',\r\n    data\r\n  })\r\n}\r\n\r\n/*查询待发布的产品资料*/\r\nexport function getPdFiles(data) {\r\n  return request({\r\n    url: '/pdtemp-pd-ver-rls/base/getPdFiles',\r\n    method: 'post',\r\n    data\r\n  })\r\n}\r\n\r\nexport function pdReviewHistory(data) {\r\n  return request({\r\n    url: '/pdtemp-pd-ver-rls/base/reviewHistory',\r\n    method: 'post',\r\n    data\r\n  })\r\n}\r\n"]}]}