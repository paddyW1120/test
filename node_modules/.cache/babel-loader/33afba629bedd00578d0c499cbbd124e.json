nction,\r\n        required: false\r\n      }\r\n    },\r\n    mounted() {\r\n      this.options = this.ldcodes[this.codetype]\r\n      if (this.filter) {\r\n        this.options = this.filterOptions(this.filter, this.options)\r\n      }\r\n      this.radioSelectValue = this.value\r\n      if (this.disabled) {\r\n        this.isDisabled = this.disabled\r\n      }\r\n    },\r\n    methods: {\r\n      radioClick(e) {\r\n        if (this.isDisabled === true) return\r\n        if (!this.isRequired) {\r\n          e === this.radioSelectValue ? (this.radioSelectValue = '') : (this.radioSelectValue = e)\r\n        } else {\r\n          this.radioSelectValue = e;\r\n        }\r\n        if (!(this.radioSelectValue === '')) {\r\n          if (this.changeMethod) {\r\n            this.changeMethod(this.radioSelectValue);\r\n          }\r\n        }\r\n\r\n      },\r\n      doChange(val) {\r\n        this.$emit('change', val)\r\n      },\r\n      handleCancel(value) {\r\n        this.radioSelectValue = value === this.radioSelectValue ? '' : value\r\n      }\r\n    },\r\n    data() {\r\n      return {\r\n        options: [],\r\n        radioSelectValue: '',\r\n        isDisabled: false\r\n      }\r\n    },\r\n    watch: {\r\n      value(val, oldVal) {\r\n        this.radioSelectValue = val\r\n      },\r\n      radioSelectValue(val, oldVal) {\r\n        if (val != oldVal) {\r\n          this.$emit('input', this.radioSelectValue);\r\n        }\r\n      },\r\n      disabled(val, oldVal) {\r\n        if (val != oldVal) {\r\n          this.isDisabled = val\r\n        }\r\n      }\r\n    },\r\n    computed: {\r\n      ...mapGetters([\r\n        'ldcodes'\r\n      ])\r\n    }\r\n  }\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n"],"sourceRoot":"src/customComponents/RadioSelect"}]}