r\n      <el-table-column :label=\"$t('i18nView.testModule')\" prop=\"pdTestModule\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.testProgress')\" prop=\"pdTestState\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.testTime')\" prop=\"pdTestLastTime\" align=\"center\"></el-table-column>\r\n    </el-table>\r\n\r\n    <div v-if=\"showTestScheduleFlag\">\r\n      <cus-form-title :first=\"true\" :label=\"$t('pdTestResult.testFormalDesc')\"/>\r\n      <cus-table-title :label=\"$t('i18nView.testProgress')\"></cus-table-title>\r\n      <el-table :data=\"productTestPoint\" size=\"mini\"\r\n                fit element-loading-text=\"Loading\" border align=\"center\" class=\"wraper-table\">\r\n        <el-table-column type=\"index\" :label=\"$t('table.id')\" align=\"center\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.testPoint')\" align=\"center\" prop=\"pdTestFuncDesc\">\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.allTestConc')\" align=\"center\" prop=\"pdTestConc\">\r\n            <template v-slot=\"scope\">{{ scope.row.pdTestConc | filter_Status }}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.allTestPoinConctDesc')\"  align=\"center\" prop=\"pdTestConcDesc\">\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\n\r\n  import { queryAuditTestState, queryPdTestRes } from \"@/api/basicinfo/pdauditinfo\";\r\n\r\n  let _this;\r\n  export default {\r\n    name: 'pdAuditRecords',\r\n    filters: {\r\n      filter_Status(code){\r\n      \treturn _this.testConcStatus[code]\r\n      }\r\n    },\r\n    props: {\r\n      pdInfo:{\r\n      \ttype: Object,\r\n        default: {}\r\n      }\r\n    },\r\n    data() {\r\n      _this = this;\r\n      return {\r\n      \ttestConcStatus: {'0': '未测试', '1': '不通过', '2': '通过'},\r\n        //表格数据\r\n        productTestSchedule: [],\r\n        productTestPoint: [],\r\n        chooseRow: {},\r\n        chooseNumberRow: {},\r\n        pdAuditConc: '',\r\n        pdAuditDetailInfo: {\r\n          id: '',\r\n          pdAuditConc: '',\r\n          pdReturnFuncNode: [],\r\n          pdReleContent: '',\r\n          pdAuditIdea: ''\r\n        },\r\n        showTestScheduleFlag: false,\r\n        editionOptionList: [],\r\n        productOptionList: [],\r\n        params: {\r\n          pdRegionType: '',\r\n          pdCode: '',\r\n          pdEdition: '',\r\n          pdAuditStartDate: undefined,\r\n          pdAuditEndDate: undefined\r\n        },\r\n        numberParams: {\r\n          pdInfoId: ''\r\n        },\r\n      }\r\n    },\r\n    computed: {},\r\n    created() {\r\n      this.auditRecordsList();\r\n    },\r\n    mounted() {\r\n    },\r\n    watch: {\r\n      auditDateValue(val) {\r\n        if (val) {\r\n          this.params.pdAuditStartDate = val[0];\r\n          this.params.pdAuditEndDate = val[1];\r\n        } else {\r\n          this.params.pdAuditStartDate = '';\r\n          this.params.pdAuditEndDate = ''\r\n        }\r\n      },\r\n    },\r\n    methods: {\r\n      auditRecordsList() {\r\n        let params = {};\r\n        params.pdInfoId = this.pdInfo.id;\r\n        queryAuditTestState(this.buildRequestData(params)).then(res => {\r\n          this.productTestSchedule = res.data\r\n          console.log(this.productTestSchedule);\r\n        })\r\n      },\r\n\r\n      //选择表格数据\r\n      chooseTestModule(row, column, event) {\r\n        this.showTestScheduleFlag = true;\r\n        console.log(row);\r\n        let param = {\r\n          'pdInfoId': this.pdInfo.id,\r\n          'moduleCode': row.pdTestModuleCode\r\n        }\r\n        queryPdTestRes(this.buildRequestData(param)).then(res => {\r\n          this.productTestPoint = res.data;\r\n          console.log(this.productTestPoint);\r\n        });\r\n      },\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/commodity-audit/product-audit/pdTestResult"}]}