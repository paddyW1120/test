steps>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!--产品测试详情-->\r\n      <cus-form-title style=\"clear: both\" :label=\"$t('productSelect.test')\"/>\r\n      <div class=\"cusSubCard cusProcess\">\r\n        <div class=\"sub\" v-for=\"item1 in  this.statusData3.children\">\r\n          <el-tooltip effect=\"light\" :content=\"$t('productSelect.processTip')\"\r\n                      placement=\"top-end\">\r\n            <div class=\"wraper\" @click=\"handleGoPage3(item1.moduleName)\">\r\n              <div class=\"loss\" :class=\"{'loss_no' : item1.moduleStatus ==='0'}\"></div>\r\n              <div class=\"tag\" :class=\"{'tag_no' : item1.moduleStatus ==='0'}\">{{item1.moduleName}}</div>\r\n              <div class=\"triangle\" :class=\"{'triangle_no' : item1.moduleStatus ==='0'}\"></div>\r\n            </div>\r\n          </el-tooltip>\r\n          <div style=\"margin-top: 23px\">\r\n            <el-steps direction=\"vertical\" process-status=\"success\" v-for=\"item2 in  item1.children\">\r\n              <el-step :icon=\"item2.moduleStatus=='1'?'el-icon-circle-check': 'el-icon-circle-close'\"\r\n                       :status=\"item2.moduleStatus=='1'? 'finish' : 'wait'\" :title=\"item2.moduleName\"></el-step>\r\n            </el-steps>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!--产品审核详情-->\r\n      <cus-form-title style=\"clear: both\" :label=\"$t('productSelect.examine')\"/>\r\n      <div class=\"cusSubCard cusProcess\">\r\n        <div class=\"sub\" v-for=\"item1 in  this.statusData4.children\">\r\n          <el-tooltip effect=\"light\" :content=\"$t('productSelect.processTip')\"\r\n                      placement=\"top-end\">\r\n            <div class=\"wraper\" @click=\"handleGoPage('产品审核')\">\r\n              <div class=\"loss\" :class=\"{'loss_no' : item1.moduleStatus ==='0'}\"></div>\r\n              <div class=\"tag\" :class=\"{'tag_no' : item1.moduleStatus ==='0'}\">{{item1.moduleName}}</div>\r\n              <div class=\"triangle\" :class=\"{'triangle_no' : item1.moduleStatus ==='0'}\"></div>\r\n            </div>\r\n          </el-tooltip>\r\n          <div style=\"margin-top: 23px\">\r\n            <el-steps direction=\"vertical\" process-status=\"success\" v-for=\"item2 in  item1.children\">\r\n              <el-step :icon=\"item2.moduleStatus=='1'?'el-icon-circle-check': 'el-icon-circle-close'\"\r\n                       :status=\"item2.moduleStatus=='1'? 'finish' : 'wait'\" :title=\"item2.moduleName\"></el-step>\r\n            </el-steps>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!--产品发布详情-->\r\n      <cus-form-title style=\"clear: both\" :label=\"$t('productSelect.release')\"/>\r\n      <div class=\"cusSubCard cusProcess\">\r\n        <div class=\"sub\" v-for=\"item1 in  this.statusData5.children\">\r\n          <el-tooltip effect=\"light\" :content=\"$t('productSelect.processTip')\"\r\n                      placement=\"top-end\">\r\n            <div class=\"wraper\" @click=\"handleGoPage('item1.moduleName')\">\r\n              <div class=\"loss\" :class=\"{'loss_no' : item1.moduleStatus ==='0'}\"></div>\r\n              <div class=\"tag\" :class=\"{'tag_no' : item1.moduleStatus ==='0'}\">{{item1.moduleName}}</div>\r\n              <div class=\"triangle\" :class=\"{'triangle_no' : item1.moduleStatus ==='0'}\"></div>\r\n            </div>\r\n          </el-tooltip>\r\n          <div style=\"margin-top: 23px\">\r\n            <el-steps direction=\"vertical\" process-status=\"success\" v-for=\"item2 in  item1.children\">\r\n              <el-step :icon=\"item2.moduleStatus=='1'?'el-icon-circle-check': 'el-icon-circle-close'\"\r\n                       :status=\"item2.moduleStatus=='1'? 'finish' : 'wait'\" :title=\"item2.moduleName\"></el-step>\r\n            </el-steps>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <el-dialog v-if=\"showQueryTestResult\" v-dialog-drag :close-on-click-modal=\"false\" :visible.sync=\"showQueryTestResult\" append-to-body center width=\"80%\">\r\n      <cus-page-title-back @backTo=\"showQueryTestResult=false\" :label=\"testDialogTitle\" :isDiablg=\"true\" />\r\n      <query-test-result :pdInfoId=\"pdInfoId\" :testResultType=\"queryType\"></query-test-result>\r\n    </el-dialog>\r\n  </cus-wraper>\r\n</template>\r\n\r\n<script>\r\n  import {productSelectIndexData, getStatus} from '@/api/basicinfo/productInfoSelect'\r\n\r\n  import queryTestResult from '@/views/commodity-query/queryTestResult'\r\n\r\n  let _this\r\n  export default {\r\n    name: 'productSelect',\r\n    filters: {\r\n      filter_pdProcess(code) {\r\n        return _this.pdProcessS[code]\r\n      },\r\n      filterPdCategory(val) {\r\n        return _this.getCodeName('ProductCategory',val)\r\n      }\r\n\r\n    },\r\n    data() {\r\n      _this = this;\r\n      return {\r\n        filterCommodityBreakdown: [],\r\n        queryType: '',\r\n        //初始化表格\r\n        productSelectTable: [],\r\n        tableData: [],\r\n        //初始化下拉选\r\n        productList:[],\r\n        pdEditionList: [],\r\n        productOriginalList: [],\r\n        selectedPd:{},\r\n        detailTypeList: [],\r\n\t      applyDateValue: [],\r\n        missionDateValue: [],\r\n\t      releaseDateValue: [],\r\n\t      stopDateValue: [],\r\n        testDialogTitle: '',\r\n        showMore: false,\r\n        showQueryTestResult: false,\r\n        url: \"/productSelect/base/indexData\",\r\n        selectOperation: {},\r\n        selectPd: {},\r\n        statusData: {},\r\n        statusData2: {},\r\n        statusData3: {},\r\n        statusData4: {},\r\n        statusData5: {},\r\n        pdInfoId: '',\r\n        pdProcessId: '',\r\n        moduleNode: '',\r\n        pdProcessCode: [\r\n        \t{text: '定义中', value: '1'},\r\n          {text: '测试中', value: '2'},\r\n\t        {text: '审核中', value: '3'},\r\n          {text: '待发布', value: '4'},\r\n          {text: '已发布', value: '5'},\r\n          {text: '已停售', value: '6'},\r\n          {text:'规则订正中', value:'7'},\r\n          {text:'公式订正中', value:'8'}],\r\n        pdProcessS: {'1': '定义中', '2': '测试中', '3': '审核中', '4': '待发布', '5': '已发布', '6': '已停售','7': '规则订正中','8': '公式订正中'},\r\n        pickerOptions: {\r\n          shortcuts: [{\r\n            text: '最近一周',\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 7);\r\n              picker.$emit('pick', [start, end]);\r\n            }\r\n          }, {\r\n            text: '最近一个月',\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 30);\r\n              picker.$emit('pick', [start, end]);\r\n            }\r\n          }, {\r\n            text: '最近三个月',\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 90);\r\n              picker.$emit('pick', [start, end]);\r\n            }\r\n          }]\r\n        },\r\n        params: {\r\n          productCategory: '',\r\n          pdCode: '',\r\n          pdName: '',\r\n          pdEdition: '',\r\n          defType: '',\r\n\t        pdProcess: '',\r\n          productApplyStartDate: undefined,\r\n          productApplyEndDate: undefined,\r\n          missionDistributeStartDate: undefined,\r\n\t        missionDistributeEndDate: undefined,\r\n          releaseStartDate: undefined,\r\n          releaseEndDate: undefined,\r\n          stopStartDate: undefined,\r\n          stopEndDate: undefined\r\n        },\r\n        selecteConfigs2: {},\r\n        //产品营运监控定义完毕传参\r\n        data: {\r\n          productTestFlag: '1',\r\n          processInstanceId: ''\r\n        },\r\n        //动态显示\r\n        showOperationProgress: false,\r\n\r\n      }\r\n    },\r\n    watch: {\r\n\t    applyDateValue(val) {\r\n        if (val) {\r\n          this.params.productApplyStartDate = val[0];\r\n          this.params.productApplyEndDate = val[1]\r\n        } else {\r\n          this.params.productApplyStartDate = '';\r\n          this.params.productApplyEndDate = ''\r\n        }\r\n      },\r\n\t    missionDateValue(val){\r\n\t    \tif(val){\r\n\t    \t\tthis.params.missionDistributeStartDate = val[0];\r\n\t    \t\tthis.params.missionDistributeEndDate = val[1];\r\n        }else{\r\n\t\t\t    this.params.missionDistributeStartDate = '';\r\n\t\t\t    this.params.missionDistributeEndDate = '';\r\n        }\r\n      },\r\n\t    releaseDateValue(val){\r\n\t    \tif(val){\r\n\t    \t\tthis.params.releaseStartDate = val[0];\r\n\t    \t\tthis.params.releaseEndDate = val[1];\r\n        }else{\r\n\t\t\t    this.params.releaseStartDate = '';\r\n\t\t\t    this.params.releaseEndDate = '';\r\n        }\r\n      },\r\n\t    stopDateValue(val){\r\n\t    \tif(val){\r\n\t    \t\tthis.params.stopStartDate = val[0];\r\n\t    \t\tthis.params.stopEndDate = val[1];\r\n        }else{\r\n\t\t\t    this.params.stopStartDate = '';\r\n\t\t\t    this.params.stopEndDate = '';\r\n        }\r\n      }\r\n    },\r\n    components: {\r\n    \tqueryTestResult\r\n    },\r\n    created() {\r\n      this.initProductQueryData()\r\n      let row = this.getJsonData(\"operationSelect\")\r\n      if (row.id != undefined) {\r\n        this.selectOperation = row.id\r\n        this.pdInfoId = row.id\r\n        // this.showOperationProgress = true\r\n        this.checkProcess()\r\n      }\r\n    },\r\n    methods: {\r\n      doSearch() {\r\n        this.dataReload()\r\n        this.selectOperation = {}\r\n        this.showOperationProgress = false\r\n      },\r\n      filterHandler(codetype, value) {\r\n        return this.getCodeName(codetype, value);\r\n      },\r\n      filterPdProcessHandler(value, row, column) {\r\n        const property = column['property']\r\n        return row[property] === value\r\n      },\r\n      filterPdTypeHandler(value, row, column) {\r\n        const property = column['property']\r\n        return row[property] === value\r\n      },\r\n      dataReload() {\r\n        //console.log(this.$refs.dataPage);\r\n        if (this.$refs.dataPage) {\r\n          this.$refs.dataPage.reload();\r\n        }\r\n      },\r\n      initProductQueryData(){\r\n         this.filterProductByNode(this)\r\n      },\r\n      selectedDetailTypeChanged(val) {\r\n           val === ''?  this.initProductQueryData() : this.filterProductByCategory(this,val)\r\n      },\r\n      selectedPdCodeChanged(product) {\r\n        this.filterEditionByProduct(this,product)\r\n      },\r\n      rowClick_Operation(row, column, event) {\r\n        if (event.target.nodeName === \"INPUT\") {\r\n          if (row.id === this.selectOperation) {\r\n            this.selectOperation = {}\r\n            this.showOperationProgress = false\r\n            this.cacheJsonData('operationSelect', this.selectOperation)\r\n          } else {\r\n            this.selectPd = row\r\n            this.cacheJsonData('operationSelect', row)\r\n            this.pdProcessId = row.pdProcessId\r\n            this.cacheJsonData('productQueryRow', row)\r\n            this.showOperationProgress = true\r\n            this.checkProcess()\r\n          }\r\n        }\r\n      },\r\n      checkProcess() {\r\n        getStatus(this.buildRequestData({\"pdProcessId\": this.pdProcessId, \"moduleNode\": 'pdInfo'})).then(res => {\r\n          this.statusData = res.data[0]\r\n        })\r\n        getStatus(this.buildRequestData({\"pdProcessId\": this.pdProcessId, \"moduleNode\":'operational'})).then(res => {\r\n          this.statusData2 = res.data[0]\r\n        })\r\n        getStatus(this.buildRequestData({\"pdProcessId\": this.pdProcessId, \"moduleNode\": 'test'})).then(res => {\r\n          this.statusData3 = res.data[0]\r\n          console.log(res);\r\n        })\r\n        getStatus(this.buildRequestData({\"pdProcessId\": this.pdProcessId, \"moduleNode\": 'examine'})).then(res => {\r\n          this.statusData4 = res.data[0]\r\n        })\r\n        getStatus(this.buildRequestData({\"pdProcessId\": this.pdProcessId, \"moduleNode\": 'release'})).then(res => {\r\n          this.statusData5 = res.data[0]\r\n        })\r\n      },\r\n      handleGoPage(type) {\r\n        let path = this.menuNameToPath(type)\r\n        this.cachePdData(path,this.selectPd)\r\n        this.$router.push({path: path});\r\n      },\r\n      handleGoPage3(type){\r\n      \tlet path = this.menuNameToPath(type)\r\n      \tconsole.log(type);\r\n\t      let row  = this.getJsonData('productQueryRow');\r\n\t      this.cacheJsonData('pdTestInfo', row);\r\n      \tthis.pdInfoId = row.id;\r\n      \tthis.queryType = type;\r\n      \tthis.testDialogTitle = type;\r\n      \tthis.showQueryTestResult = true;\r\n\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/commodity-query/product-query"}]}