           <el-select clearable filterable v-model=\"params.pdEdition\">\r\n              <el-option :key=\"edition\" :label=\"edition\" :value=\"edition\"\r\n                         v-for=\"edition in pdEditionList\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </cus-col>\r\n        <cus-condition @show=\"showMore=!showMore\"></cus-condition>\r\n      </el-row>\r\n      <div>\r\n        <el-collapse-transition>\r\n          <div v-if=\"showMore\">\r\n            <el-row :gutter=\"10\">\r\n              <cus-col>\r\n                <el-form-item :label=\"$t('productSelect.pdApplyDate')\">\r\n                  <el-date-picker v-model=\"applyDateValue\" type=\"daterange\" unlink-panels\r\n                                  value-format=\"yyyy-MM-dd\"\r\n                                  :range-separator=\"$t('productSelect.to')\"\r\n                                  :start-placeholder=\"$t('productSelect.productApplyStartDate')\"\r\n                                  :end-placeholder=\"$t('productSelect.productApplyEndDate')\"\r\n                                  :picker-options=\"pickerOptions\">\r\n                  </el-date-picker>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col>\r\n                <el-form-item :label=\"$t('productSelect.pdProcess')\">\r\n                  <el-select clearable filterable v-model=\"params.pdProcess\">\r\n                    <el-option v-for=\"pdProcess in pdProcessCode\"\r\n                          :key=\"pdProcess.text\" :label=\"pdProcess.text\" :value=\"pdProcess.value\"></el-option>\r\n                  </el-select>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col>\r\n                <el-form-item :label=\"$t('productSelect.defType')\">\r\n                  <cus-select v-model=\"params.defType\" clearable filterable :codetype=\"'pdAuditType'\"></cus-select>\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n            <el-row>\r\n              <cus-col>\r\n                <el-form-item :label=\"$t('productSelect.missionDate')\">\r\n                  <el-date-picker v-model=\"missionDateValue\" type=\"daterange\" unlink-panels\r\n                                  :range-separator=\"$t('productSelect.to')\" value-format=\"yyyy-MM-dd\"\r\n                                  :start-placeholder=\"$t('productSelect.productApplyStartDate')\"\r\n                                  :end-placeholder=\"$t('productSelect.productApplyEndDate')\"\r\n                                  :picker-options=\"pickerOptions\">\r\n                  </el-date-picker>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col>\r\n                <el-form-item :label=\"$t('productSelect.pdRelDate')\">\r\n                  <el-date-picker v-model=\"releaseDateValue\" type=\"daterange\" unlink-panels\r\n                                  :range-separator=\"$t('productSelect.to')\" value-format=\"yyyy-MM-dd\"\r\n                                  :start-placeholder=\"$t('productSelect.productApplyStartDate')\"\r\n                                  :end-placeholder=\"$t('productSelect.productApplyEndDate')\"\r\n                                  :picker-options=\"pickerOptions\">\r\n                  </el-date-picker>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col>\r\n                <el-form-item :label=\"$t('productSelect.pdDisDate')\">\r\n                  <el-date-picker v-model=\"stopDateValue\" type=\"daterange\" unlink-panels\r\n                                  :range-separator=\"$t('productSelect.to')\" value-format=\"yyyy-MM-dd\"\r\n                                  :start-placeholder=\"$t('productSelect.productApplyStartDate')\"\r\n                                  :end-placeholder=\"$t('productSelect.productApplyEndDate')\"\r\n                                  :picker-options=\"pickerOptions\">\r\n                  </el-date-picker>\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n          </div>\r\n        </el-collapse-transition>\r\n      </div>\r\n      <el-row >\r\n        <cus-col :span=\"24\">\r\n            <el-button style=\"float: right\"  size=\"small\" type=\"primary\"  icon=\"el-icon-search\" @click=\"doSearch\">\r\n              {{$t('productSelect.search')}}\r\n            </el-button>\r\n        </cus-col>\r\n      </el-row>\r\n    </el-form>\r\n    <cus-table-title :label=\"$t('productSelect.productSelect')\"></cus-table-title>\r\n    <el-table :data=\"productSelectTable\" size=\"mini\" @row-click=\"rowClick_Operation\" highlight-current-row\r\n              fit element-loading-text=\"Loading\" border align=\"center\" class=\"wraper-table\">\r\n      <el-table-column width=\"50\" :label=\"$t('productSelect.selection')\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-radio-group v-model=\"selectOperation\">\r\n            <el-radio :label=\"scope.row.id\">{{''}}</el-radio>\r\n          </el-radio-group>\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <el-table-column :label=\"$t('table.id')\" type=\"index\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('productSelect.pdDetailType')\" prop=\"cType\" align=\"center\">\r\n        <template v-slot=\"scope\">{{scope.row.cType | filterPdCategory}}</template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.pdCode')\" prop=\"pdCode\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.pdName')\" prop=\"pdName\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('customComponents.pdEdition')\" prop=\"pdEdition\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('productSelect.pdProcess')\" prop=\"pdProcess\" align=\"center\"\r\n                       :filters=\"[{text: '定义中', value: '1'}, {text: '测试中', value: '2'},\r\n                       {text: '审核中', value: '3'}, {text: '待发布', value: '4'}, {text: '已发布', value: '5'}, {text: '已停售', value: '6'},{value:'7', text:'规则订正中'},{value:'8', text:'公式订正中'}]\"\r\n                       :filter-method=\"filterPdProcessHandler\" column-key=\"pdProcess\" >\r\n        <template v-slot=\"scope\">{{scope.row.pdProcess | filter_pdProcess}}</template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('productSelect.defType')\" prop=\"defType\" align=\"center\"\r\n                       :filters=\"[{text: '新产品', value: '1'}, {text: '订正产品', value: '2'}, {text: '停售产品', value: '3'}]\"\r\n                       :filter-method=\"filterPdTypeHandler\" column-key=\"defType\" >\r\n        <template v-slot=\"scope\">{{filterHandler('pdAuditType',scope.row.defType )}}</template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('productSelect.pdApplyDate')\" prop=\"pdApplDate\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('productSelect.pdDisDate')\" prop=\"pdStopDate\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('productSelect.operator')\" prop=\"operator\" align=\"center\"></el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <cus-pagination ref=\"dataPage\" :url=\"url\" :pageSize=\"10\" :params=\"params\" v-model=\"productSelectTable\" :autoBuild=\"false\"/>\r\n    <br>\r\n    <!--产品营运定义进度-->\r\n    <div v-if=\"showOperationProgress\">\r\n\r\n\r\n      <!--产品框架定义详情-->\r\n      <cus-form-title  :label=\"$t('productSelect.pdInfo')\"/>\r\n      <div class=\"cusSubCard cusProcess\">\r\n        <div class=\"sub\" v-for=\"item1 in  this.statusData.children\">\r\n          <el-tooltip effect=\"light\" :content=\"$t('productSelect.processTip')\"\r\n                      placement=\"top-end\">\r\n            <div class=\"wraper\" @click=\"handleGoPage('产品框架定义')\">\r\n              <div class=\"loss\" :class=\"{'loss_no'