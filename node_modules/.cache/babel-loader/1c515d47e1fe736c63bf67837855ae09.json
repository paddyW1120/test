KAAA,UAAA,CAAA,IAAA,CAAA;;AACA,WAAA,aAAA,GAAA,UAAA,OAAA;AAAA,eAAA,OAAA,CAAA,MAAA,CAAA,EAAA,EAAA;AAAA,iBAAA,OAAA,CAAA,OAAA,mBAAA,KAAA,EAAA,CAAA;AAAA,SAAA,CAAA;AAAA,OAAA;AACA,KAxCA;AAyCA,IAAA,gBAzCA,4BAyCA,IAzCA,EAyCA;AACA,WAAA,iBAAA,GAAA,KAAA;AACA,WAAA,iBAAA,GAAA,KAAA;;AACA,cAAA,IAAA;AACA,aAAA,kBAAA;AACA,eAAA,iBAAA,GAAA,IAAA;AACA,iBAAA,6EAAA;;AACA,aAAA,oBAAA;AACA,eAAA,iBAAA,GAAA,IAAA;AACA,iBAAA,kFAAA;;AACA;AACA,iBAAA,EAAA;AARA;AAUA,KAtDA;AAuDA,IAAA,QAvDA,oBAuDA,GAvDA,EAuDA;AACA,WAAA,OAAA,GAAA,GAAA;AACA,KAzDA;AA0DA;AACA;AACA;AACA;AACA;AACA,IAAA,mBA/DA,iCA+DA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,iGADA;AAEA,QAAA,MAAA,EAAA;AAFA,OAAA;AAIA,KApEA;AAqEA,IAAA,QArEA,oBAqEA,GArEA,EAqEA;AACA,UAAA,IAAA,GAAA,GAAA,CAAA,IAAA;AACA,WAAA,kBAAA,CAAA,IAAA;AACA,KAxEA;AAyEA,IAAA,UAzEA,wBAyEA;AACA,UAAA,SAAA,GACA,MAAA,CAAA,WAAA,IACA,QAAA,CAAA,eAAA,CAAA,SADA,IAEA,QAAA,CAAA,IAAA,CAAA,SAHA;AAIA,WAAA,OAAA,GAAA,SAAA,GAAA,KAAA,SAAA;AACA;AA/EA;AAnDA,C","sourcesContent":["<template>\r\n  <cus-wraper>\r\n\r\n    <div class=\"package-compensation-container\">\r\n      <!--      <div v-if=\"comboHePeiCalcMarkFlag\">-->\r\n      <!--      <cus-page-title-back v-if=\"showPageFlag!='1'\" @backTo=\"backToMenu\" :label=\"$t('compensationDefinitionMain.packageUnderWritingMenu')\"/>-->\r\n      <!--      <cus-page-title-back v-if=\"showPageFlag=='1'\" @backTo=\"backToSaleStop\" :label=\"$t('route.comboStop')\"/>-->\r\n      <!--      </div>-->\r\n      <div>\r\n        <cus-page-title-back :label=\"$t('route.comboCalcRevision')\" @backTo=\"backToComboCalcMark\"/>\r\n      </div>\r\n      <!-- 产品套餐信息 -->\r\n      <el-form :model=\"form\" ref=\"form\">\r\n        <el-row>\r\n          <el-form-item :label=\"$t('underwritingDefinitionMain.productMeat')\">\r\n            <el-tag type=\"info\">{{ form.ppCode + ' : ' + form.ppName }}</el-tag>\r\n          </el-form-item>\r\n        </el-row>\r\n      </el-form>\r\n      <el-collapse v-model=\"active\" accordion class=\"collapseCus collapseCusPPUnderWriting\" id=\"boxFixed\"\r\n                   :class=\"{'is_fixed' : isFixed}\">\r\n        <el-collapse-item :title=\"$t('underwritingDefinitionMain.packageUnderWritingMenu')\" name=\"packageUnderwriting\">\r\n          <div style=\"height: 64px;\" class=\"stepDiv\">\r\n            <el-steps process-status=\"finish \" finish-status=\"wait\" direction=\"vertical\" :active=\"activeName\">\r\n              <el-step icon=\"el-icon-info\" :title=\"$t('compensationDefinitionMain.stepunderwritingRule')\"\r\n                       @click.native=\"handleUnderwriting('underwritingRule')\"></el-step>\r\n              <el-step icon=\"el-icon-info\" :title=\"$t('compensationDefinitionMain.steprenewInsuranceRule')\"\r\n                       @click.native=\"handleUnderwriting('renewInsuranceRule')\"></el-step>\r\n            </el-steps>\r\n          </div>\r\n        </el-collapse-item>\r\n      </el-collapse>\r\n\r\n      <el-tabs v-model=\"activeTabName\" @tab-click=\"clickTab\" type=\"border-card\">\r\n        <el-tab-pane :label=\"$t('compensationDefinitionMain.underwritingRule')\" name=\"underwritingRule\">\r\n          <div :class=\"{ 'showDiv': this.showPageFlag == 1 }\"></div>\r\n          <component @childFn=\"parentFn\" :is=\"componentName\" v-if=\"compensationFlag1\" @hook:mounted=\"finishPage\"/>\r\n        </el-tab-pane>\r\n        <el-tab-pane :label=\"$t('compensationDefinitionMain.renewInsuranceRule')\" name=\"renewInsuranceRule\">\r\n          <div :class=\"{ 'showDiv': this.showPageFlag == 1 }\"></div>\r\n          <component @childFn=\"parentFn\" :is=\"componentName\" v-if=\"compensationFlag2\" @hook:mounted=\"finishPage\"/>\r\n        </el-tab-pane>\r\n      </el-tabs>\r\n      <operationbutton :type=\"type\" :procInsId=\"pdProcessId\" clazz=\"packageFinishDefWithNav\"></operationbutton>\r\n    </div>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\nimport operationbutton from '@/views/basicinfo/operationend/index'\r\nimport {mapGetters} from 'vuex'\r\n\r\nexport default {\r\n  name: 'compensationDefinitionMain',\r\n  computed: {\r\n    ...mapGetters(['tenantCode'])\r\n  },\r\n  data() {\r\n    return {\r\n      type: 'packageHePeiFlag',\r\n      showPageFlag: '',\r\n      activeName: 0,\r\n      activeTabName: 'underwritingRule',\r\n      active: 'packageUnderwriting',\r\n      isFixed: false,\r\n      offsetTop: 20,\r\n      componentName: '',\r\n      nowPosition: 'underwritingRule',\r\n      stepOrder: {'underwritingRule': 0, 'renewInsuranceRule': 1},\r\n      form: {\r\n        ppCode: '',\r\n        ppName: ''\r\n      },\r\n      pageFlag: '',\r\n      comboInfo: {},\r\n      pdProcessId: '',\r\n      compensationFlag1: false,\r\n      compensationFlag2: false,\r\n      comboHePeiCalcMarkFlag: true\r\n    }\r\n  },\r\n  async created() {\r\n    this.showPageFlag = this.$route.query.showPageFlag\r\n    this.pageFlag = this.$route.query.pageFlag\r\n    this.comboHePeiCalcMarkFlag = this.$route.query.comboHePeiCalcMarkFlag;\r\n    if (this.comboHePeiCalcMarkFlag == undefined) {\r\n      this.comboHePeiCalcMarkFlag = true\r\n    }\r\n    let pdInfoData = this.getPdData();\r\n    this.comboInfo = pdInfoData\r\n    this.form.ppCode = pdInfoData.ppCode\r\n    this.form.ppName = pdInfoData.ppName\r\n    this.pdProcessId = pdInfoData.pdProcessId\r\n    this.switchComponent('underwritingRule')\r\n  },\r\n  components: {operationbutton},\r\n  mounted() {\r\n    window.addEventListener(\"scroll\", this.initHeight);\r\n    this.$nextTick(() => {\r\n      this.offsetTop = document.querySelector(\"#boxFixed\").offsetTop;\r\n    });\r\n    this.$emit('finish')\r\n  },\r\n  methods: {\r\n    finishPage() {\r\n      this.$emit('finish')\r\n    },\r\n    handleUnderwriting(type) {\r\n      let index = this.stepOrder[type]\r\n      if (this.nowPosition !== type) {\r\n        if (this.isSaved) {\r\n          this.isSaved = false\r\n          this.activeName = index\r\n          this.activeTabName = type\r\n          this.nowPosition = type\r\n          this.switchComponent(type)\r\n        } else {\r\n          this.$confirm(\r\n            this.$t('productMenu.messageTitle'),\r\n            this.$t('productMenu.messageTip'),\r\n            {\r\n              confirmButtonText: this.$t('productMenu.messageButton2'),\r\n              cancelButtonText: this.$t('productMenu.messageButton1'),\r\n              type: 'warning'\r\n            }\r\n          ).then(() => {\r\n            this.activeName = index\r\n            this.activeTabName = type\r\n            this.nowPosition = type\r\n            window.scrollTo(0, 0)\r\n            this.switchComponent(type)\r\n          }).catch(() => {\r\n            this.activeTabName = this.nowPosition\r\n          })\r\n        }\r\n\r\n      }\r\n    },\r\n    switchComponent(type) {\r\n      let moduleLocation = this.getComponentPath(type);\r\n      const path = moduleLocation + '/' + this.tenantCode + '/index';\r\n      let path1 = this.isInAllVue(path)\r\n      this.componentName = resolve => require.ensure([], () => resolve(require(`@/views/${path1}`)))\r\n    },\r\n    getComponentPath(type) {\r\n      this.compensationFlag1 = false\r\n      this.compensationFlag2 = false\r\n      switch (type) {\r\n        case  'underwritingRule':\r\n          this.compensationFlag1 = true\r\n          return 'product-package-definition/pp-operation-def/pp-compensation-def/combo-quota';\r\n        case  'renewInsuranceRule':\r\n          this.compensationFlag2 = true\r\n          return 'product-package-definition/pp-operation-def/pp-compensation-def/combo-deductible';\r\n        default :\r\n          return ''\r\n      }\r\n    },\r\n    parentFn(val) {\r\n      this.isSaved = val\r\n    },\r\n    // backToMenu() {\r\n    //   this.$router.push({ name: '套餐发布', params: {} })    },\r\n    // backToSaleStop(){\r\n    //   this.$router.push({ name: '套餐发布', query:{comboInfo:this.comboInfo,pageFlag:this.pageFlag}})\r\n    // },\r\n    backToComboCalcMark() {\r\n      this.$router.push({\r\n        path: '/product-package-definition/pp-operation-def/pp-compensation-def/pp-compensation-def/comp-apply',\r\n        params: {}\r\n      })\r\n  },\r\n  clickTab(val) {\r\n    let type = val.name\r\n    this.handleUnderwriting(type)\r\n  },\r\n  initHeight() {\r\n    let scrollTop =\r\n      window.pageYOffset ||\r\n      document.documentElement.scrollTop ||\r\n      document.body.scrollTop;\r\n    this.isFixed = scrollTop > this.offsetTop;\r\n  }\r\n}\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.showDiv {\r\n  width: 100%;\r\n  height: 100%;\r\n  margin-top: -40px;\r\n  margin-left: -40px;\r\n  position: absolute;\r\n  background: #ffffff00;\r\n  z-index: 999;\r\n}\r\n\r\n\r\n.collapseCusPPUnderWriting {\r\n  top: 130px;\r\n  right: 0px !important;\r\n  width: 170px !important;\r\n}\r\n\r\n.package-compensation-container {\r\n  width: calc(100% - 160px);\r\n}\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/product-package-definition/pp-operation-def/pp-compensation-def/comp-main"}]}