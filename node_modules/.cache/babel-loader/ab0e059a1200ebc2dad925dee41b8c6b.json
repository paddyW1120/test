"changeDate\"></el-table-column>\r\n        <el-table-column align=\"center\" :label=\"$t('table.actions')\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button class=\"filter-button\" icon=\"el-icon-s-promotion\" @click=\"doTask(scope.row)\" size=\"mini\"\r\n                       type=\"primary\">{{$t('i18nView.transcation')}}\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <!-- 分页 -->\r\n      <cus-pagination :pageSize=\"5\" :params=\"params1\" ref=\"page1\" url=\"/workflow/tasklist/queryOwnTask\"\r\n                      v-model=\"configs1\"/>\r\n\r\n\r\n    </div>\r\n\r\n\r\n    <br/>\r\n    <el-divider v-if=\"processShow\" content-position=\"left\">{{$t('i18nView.pdState')}}</el-divider>\r\n    <process v-if=\"processShow\" :moduleType=\"moduleType\" :pdProcessId=\"pdProcessId\" :pdInfoShow=\"true\"\r\n             :operationalShow=\"true\"></process>\r\n\r\n    <el-dialog  v-dialog-drag :close-on-click-modal=\"false\" :visible.sync=\"dialogVisible\"\r\n                @close=\"dialogVisible=false\" width=\"60%\">\r\n\r\n      <cus-page-title-back :isDialog=\"true\" @backTo=\"dialogVisible=false\" :label=\"$t('i18nView.userOption')\"></cus-page-title-back>\r\n      <el-form style=\"margin-top: 30px\" label-width=\"90px\">\r\n        <el-row :gutter=\"10\">\r\n          <el-col :span=\"5\">\r\n            <el-form-item :label=\"$t('i18nView.userName')\">\r\n              <el-input clearable v-model=\"params3.userName\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"5\">\r\n            <el-form-item :label=\"$t('i18nView.userAccount')\" prop=\"\">\r\n              <el-input placeholder=\"\" clearable v-model=\"params3.userCode\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <cus-btn-wrapper>\r\n            <el-button size=\"small\" type=\"primary\" @click=\"handleSearch3\" icon=\"el-icon-search\" v-waves>{{$t('table.search')}}\r\n            </el-button>\r\n          </cus-btn-wrapper>\r\n        </el-row>\r\n      </el-form>\r\n\r\n      <el-table :data=\"configs3\" align=\"center\" border class=\"wraper-table\" element-loading-text=\"Loading\" fit\r\n                highlight-current-row\r\n                size=\"mini\">>\r\n        <el-table-column :label=\"$t('table.select')\" align=\"center\" width=\"60px\">\r\n          <template slot-scope=\"scope\">\r\n            <el-radio :label=\"scope.row.userCode\" @change=\"selectUser(scope.row)\" v-model=\"radio3\">\r\n              {{ '' }}\r\n            </el-radio>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.id')\" align=\"center\" width=\"50px\">\r\n          <template v-slot=\"scope\">\r\n            {{ initTableIndex('page3', scope.$index) }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column align=\"center\" :label=\"$t('i18nView.userName')\" prop=\"userName\"></el-table-column>\r\n        <el-table-column align=\"center\" :label=\"$t('i18nView.userCode')\" prop=\"userCode\"></el-table-column>\r\n        <el-table-column align=\"center\" :label=\"$t('userView.table_phone')\" prop=\"phone\"></el-table-column>\r\n        <el-table-column  align=\"center\" :label=\"$t('userView.table_mail')\" prop=\"email\"></el-table-column>\r\n      </el-table>\r\n      <!-- 分页 -->\r\n      <cus-pagination :pageSize=\"5\" :params=\"params3\" ref=\"page3\" url=\"/workflow/getAllUsers\"  v-model=\"configs3\"/>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button size=\"small\" icon='el-icon-circle-close' @click=\"dialogVisible = false\"> {{ $t('table.cancel') }} </el-button>\r\n          <el-button size=\"small\" type=\"primary\" class=\"el-icon-success\" @click=\"submitForm\"> {{ $t('table.confirm') }} </el-button>\r\n        </span>\r\n    </el-dialog>\r\n\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\n\r\nimport process from '@/views/basicinfo/pdprocess/index'\r\nimport cradlist from '@/views/dashboard/cradlist'\r\n\r\nimport {assignTask, claimTask, getAllUsers} from '@/api/workflow/workflow'\r\n\r\nimport {indexData} from '@/api/system/index'\r\n\r\nimport {mapGetters} from 'vuex'\r\n\r\nimport BarChart from '@/views/dashboard/admin/components/BarChart'\r\nimport cardlist from '@/views/dashboard/cradlist/index'\r\n\r\nexport default {\r\n  name: 'normaluser',\r\n  computed: {\r\n    ...mapGetters([\r\n      'rolecodes'\r\n    ])\r\n  },\r\n  components: {\r\n    cardlist,\r\n    process,\r\n    cradlist,\r\n    BarChart\r\n  },\r\n  data() {\r\n    return {\r\n      filterHandler(value, row, column) {\r\n        const property = column['property']\r\n        return row[property] === value\r\n      },\r\n      barData: {},\r\n      cardData: {},\r\n      cardShow: false,\r\n      activeName: 'product',\r\n      title1: this.$t('monitorUser.productDefStatistics'),\r\n      title2: this.$t('monitorUser.productOperationStatistics'),\r\n      chartShow: true,\r\n      taskShow: true,\r\n      unDoTaskShow: true,\r\n      chart1Show: false,\r\n      chart1: null,\r\n      chart1xData: [],\r\n      chart1yData: [],\r\n      chart2Show: false,\r\n      chart2: null,\r\n      chart2xData: [],\r\n      chart2yData: [],\r\n      radio: '',\r\n      radio3: '',\r\n      dialogVisible: false,\r\n      processShow: false,\r\n      pdInfoShow: false,\r\n      opShow: false,\r\n      pdProcessId: '',\r\n      moduleType: '',\r\n      barCount: '',\r\n      releasePdCount: '',\r\n      roleCode: '',\r\n      rolesName: '',\r\n      systemVersion: '',\r\n      tenantName: '',\r\n      testTaskOptions: [\r\n        { text: '新单测试', value: '新单测试' },\r\n        { text: '续期续保测试', value: '续期续保测试' },\r\n        { text: '保全测试', value: '保全测试' },\r\n        { text: '理赔测试', value: '理赔测试' },\r\n        { text: '规则测试', value: '规则测试' }\r\n      ],\r\n      taskOptions: [{ text: '产品框架定义', value: '产品框架定义' },\r\n        { text: '产品销售定义', value: '产品销售定义' },\r\n        { text: '产品核赔定义', value: '产品核赔定义' },\r\n        { text: '产品保全定义', value: '产品保全定义' },\r\n        { text: '产品续期续保定义', value: '产品续期续保定义' },\r\n        { text: '产品核保定义', value: '产品核保定义' },\r\n        { text: '产品承保定义', value: '产品承保定义' },\r\n        { text: '产品审核', value: '产品审核' },\r\n        { text: '产品发布', value: '产品发布' }],\r\n      configs1: [],\r\n      configs2: [],\r\n      configs3: [],\r\n      params1: {\r\n        pdCode: '',\r\n        pdName: '',\r\n        pdEdition: '',\r\n        taskName: ''\r\n      }, params2: {\r\n        userCode: '',\r\n        pdCode: '',\r\n        pdEdition: '',\r\n        taskName: ''\r\n      }, params3: {\r\n        userCode: '',\r\n        userName: ''\r\n      },\r\n      moreCondition: false,\r\n      moreCondition2: false,\r\n      form: {\r\n        userCode: '',\r\n        taskId: '',\r\n        taskName: '',\r\n        pdCode: '',\r\n        pdEdition: '',\r\n        pdName: '',\r\n        weComId: ''\r\n      },\r\n      userOptions: [],\r\n      rules: {\r\n        userCode: [\r\n          {\r\n            required: true,\r\n            message: this.$t('userView.input') + this.$t('userView.table_userCode'),\r\n            trigger: 'change'\r\n          }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.taskOptions = this.taskOptions.concat(this.testTaskOptions)\r\n    this.roleCode = this.rolecodes\r\n    this.getIndexData()\r\n  },\r\n  mounted() {\r\n\r\n  },\r\n  methods: {\r\n    refreshChBar(name) {\r\n      if (name === '产品营运定义') {\r\n        this.title2 = this.$t('monitorUser.productOperationStatistics')\r\n        this.chart2xData = this.barData.productOperating.xData\r\n        this.chart2yData = this.barData.productOperating.yData\r\n\r\n      } else if (name === '产品测试') {\r\n        this.title2 = '险种测试统计'\r\n        this.chart2xData = this.barData.productTest.xData\r\n        this.chart2yData = this.barData.productTest.yData\r\n      }\r\n\r\n    },\r\n    getIndexData() {\r\n      indexData(this.buildRequestData({})).then(res => {\r\n        let data = res.data\r\n        this.releasePdCount = data.releasePdCount\r\n        this.barData = data.barData\r\n        this.chart1xData = data.barData.xData\r\n        this.chart1yData = data.barData.yData\r\n        this.chart2xData = data.barData.productOperating.xData\r\n        this.chart2yData = data.barData.productOperating.yData\r\n\r\n        this.barCount = this.chart1yData.reduce(function(prev, curr, idx, arr) {\r\n          return prev + curr\r\n        })\r\n\r\n        this.cardData = { 'taskCount': data.taskCount, 'issueCount': data.issueCount }\r\n        this.cardShow = true\r\n\r\n        this.initChart()\r\n      })\r\n    },\r\n    initChart() {\r\n      this.chart1 = 'chart1'\r\n      this.chart1Show = true\r\n\r\n      this.chart2 = 'chart2'\r\n      this.title2 = this.$t('monitorUser.productOperationStatistics')\r\n      this.chart2Show = true\r\n\r\n    },\r\n    submitForm() {\r\n      assignTask(this.form).then(response => {\r\n        this.handleSearch2()\r\n        this.notifySuccess(response.message)\r\n        this.dialogVisible = false\r\n      })\r\n    }\r\n    , assignTask(row) {\r\n      this.radio3 = row.assignName\r\n      this.form.userCode = row.assignName\r\n      this.form.taskId = row.taskId\r\n      this.form.taskName = row.taskName\r\n      this.form.pdCode = row.pdCode\r\n      this.form.pdEdition = row.pdEdition\r\n      this.form.pdName = row.pdName\r\n      this.dialogVisible = true\r\n    },\r\n    getAllUsers() {\r\n      getAllUsers().then(res => {\r\n        this.userOptions = []\r\n        let users = res.data\r\n        users.forEach(user => {\r\n          this.userOptions.push({ 'key': user.userCode, 'label': user.userName })\r\n        })\r\n      })\r\n    },\r\n    filterCodeTypeHandler(codetype, value) {\r\n      return this.getCodeName(codetype, value, true)\r\n    },\r\n    handleSearch() {\r\n      this.$refs.page1.reload()\r\n    },\r\n    handleSearch2() {\r\n      this.$refs.page2.reload()\r\n    },\r\n    handleSearch3() {\r\n      this.$refs.page3.reload()\r\n    },\r\n    selectUser(row) {\r\n      this.form.userCode  = row.userCode\r\n    },\r\n    handleSetLine(type) {\r\n      switch (type) {\r\n        case 'newProduct':\r\n          this.$router.push({ path: '/productDefinition/basicInfoDefinition' })\r\n          break\r\n        case 'issue':\r\n          this.$router.push({ path: '/issue/list' })\r\n          break\r\n      }\r\n    },\r\n    doTask(row) {\r\n      let path = this.menuNameToPath(row.taskName)\r\n      this.cacheJsonData(path,row)\r\n      this.$router.push({ path: path})\r\n    },\r\n    taskStatus(row) {\r\n      this.radio = row.id + row.taskName\r\n\r\n      this.moduleType = 'pdInfo'\r\n\r\n      this.pdProcessId = row.pdProcessId\r\n\r\n      this.cacheCurrPagePdData(row)\r\n      this.processShow = false\r\n\r\n      if (this.moduleType) {\r\n        this.processShow = true\r\n      }\r\n\r\n    },\r\n    claimTask(taskId) {\r\n      this.confirmWarning('确定要认领当前任务吗？').then(() => {\r\n        let data = { 'taskId': taskId }\r\n        claimTask(data).then(response => {\r\n          this.handleSearch()\r\n          this.handleSearch2()\r\n          this.notifySuccess(response.message)\r\n        })\r\n      }).catch(() => {\r\n\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.panel-group {\r\n  margin-top: 18px;\r\n\r\n  .card-panel-col {\r\n    margin-bottom: 32px;\r\n  }\r\n\r\n  .card-panel {\r\n    height: 108px;\r\n    cursor: pointer;\r\n    font-size: 12px;\r\n    position: relative;\r\n    overflow: hidden;\r\n    color: #666;\r\n    background: #fff;\r\n    box-shadow: 4px 4px 40px rgba(0, 0, 0, .05);\r\n    border-color: rgba(0, 0, 0, .05);\r\n\r\n    & {\r\n      .card-panel-icon-wrapper {\r\n        color: #fff !important;\r\n      }\r\n\r\n      .icon-people {\r\n        background: #40c9c6;\r\n      }\r\n\r\n      .icon-message {\r\n        background: #36a3f7;\r\n      }\r\n\r\n      .icon-money {\r\n        background: #f4516c;\r\n      }\r\n\r\n      .icon-shopping {\r\n        background: #34bfa3\r\n      }\r\n    }\r\n\r\n    .icon-people {\r\n      color: #40c9c6;\r\n    }\r\n\r\n    .icon-message {\r\n      color: #36a3f7;\r\n    }\r\n\r\n    .icon-money {\r\n      color: #f4516c;\r\n    }\r\n\r\n    .icon-shopping {\r\n      color: #34bfa3\r\n    }\r\n\r\n    .card-panel-icon-wrapper {\r\n      float: left;\r\n      margin: 14px 0 0 14px;\r\n      padding: 16px;\r\n      transition: all 0.38s ease-out;\r\n      border-radius: 6px;\r\n    }\r\n\r\n    .card-panel-icon {\r\n      float: left;\r\n      font-size: 48px;\r\n    }\r\n\r\n    .card-panel-description {\r\n      float: right;\r\n      font-weight: bold;\r\n      margin: 26px;\r\n      margin-left: 0px;\r\n\r\n      .card-panel-text {\r\n        line-height: 18px;\r\n        color: rgba(0, 0, 0, 0.45);\r\n        font-size: 16px;\r\n        margin-bottom: 12px;\r\n        margin-right: 8px;\r\n      }\r\n\r\n      .card-panel-num {\r\n        font-size: 20px;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n@media (max-width: 550px) {\r\n  .card-panel-description {\r\n    display: none;\r\n  }\r\n\r\n  .card-panel-icon-wrapper {\r\n    float: none !important;\r\n    width: 100%;\r\n    height: 100%;\r\n    margin: 0 !important;\r\n\r\n    .svg-icon {\r\n      display: block;\r\n      margin: 14px auto !important;\r\n      float: none !important;\r\n    }\r\n  }\r\n}\r\n\r\n.el-badge__content.is-fixed {\r\n  right: 4px;\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/dashboard/monitoruser"}]}