,KAAA,CAAA,YAAA,EAAA,IAAA;AACA,KATA;AAUA,IAAA,QAVA,oBAUA,CAVA,EAUA;AACA,UAAA,KAAA,GAAA,CAAA,CAAA,MAAA,CAAA,KAAA,CAAA,IAAA,EAAA;AADA,UAEA,IAFA,GAEA,IAFA,CAEA,IAFA;;AAGA,UAAA,CAAA,KAAA,EAAA;AACA,aAAA,UAAA,CAAA;AACA,UAAA,IAAA,EAAA;AADA,SAAA;AAGA,OAJA,MAIA,IAAA,KAAA,OAAA,EAAA;AACA,aAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,IADA;AAEA,UAAA,KAAA,EAAA;AAFA,SAAA;AAIA,aAAA,OAAA,GAAA,KAAA;AACA;AACA,KAxBA;AAyBA,IAAA,UAzBA,sBAyBA,CAzBA,EAyBA;AACA,MAAA,CAAA,CAAA,MAAA,CAAA,KAAA,GAAA,KAAA,IAAA,CAAA,IAAA;AACA,WAAA,OAAA,GAAA,KAAA;AACA;AA5BA;AAxBA,C","sourcesContent":["<template>\r\n  <li :class=\"{ completed: todo.done, editing: editing }\" class=\"todo\">\r\n    <div class=\"view\">\r\n      <input\r\n        :checked=\"todo.done\"\r\n        class=\"toggle\"\r\n        type=\"checkbox\"\r\n        @change=\"toggleTodo( todo)\"\r\n      >\r\n      <label @dblclick=\"editing = true\" v-text=\"todo.text\" />\r\n      <button class=\"destroy\" @click=\"deleteTodo( todo )\" />\r\n    </div>\r\n    <input\r\n      v-show=\"editing\"\r\n      v-focus=\"editing\"\r\n      :value=\"todo.text\"\r\n      class=\"edit\"\r\n      @keyup.enter=\"doneEdit\"\r\n      @keyup.esc=\"cancelEdit\"\r\n      @blur=\"doneEdit\"\r\n    >\r\n  </li>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'Todo',\r\n  directives: {\r\n    focus(el, { value }, { context }) {\r\n      if (value) {\r\n        context.$nextTick(() => {\r\n          el.focus()\r\n        })\r\n      }\r\n    }\r\n  },\r\n  props: {\r\n    todo: {\r\n      type: Object,\r\n      default: function() {\r\n        return {}\r\n      }\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      editing: false\r\n    }\r\n  },\r\n  methods: {\r\n    deleteTodo(todo) {\r\n      this.$emit('deleteTodo', todo)\r\n    },\r\n    editTodo({ todo, value }) {\r\n      this.$emit('editTodo', { todo, value })\r\n    },\r\n    toggleTodo(todo) {\r\n      this.$emit('toggleTodo', todo)\r\n    },\r\n    doneEdit(e) {\r\n      const value = e.target.value.trim()\r\n      const { todo } = this\r\n      if (!value) {\r\n        this.deleteTodo({\r\n          todo\r\n        })\r\n      } else if (this.editing) {\r\n        this.editTodo({\r\n          todo,\r\n          value\r\n        })\r\n        this.editing = false\r\n      }\r\n    },\r\n    cancelEdit(e) {\r\n      e.target.value = this.todo.text\r\n      this.editing = false\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views/dashboard/admin/components/TodoList"}]}