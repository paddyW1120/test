n pdProgOptions\"\r\n                         :key=\"item.id\"\r\n                         :label=\"item.pdProgCode+':'+item.pdProgName\"\r\n                         :value=\"item.pdProgCode\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col>\r\n          <cus-btn-wrapper>\r\n            <el-button dialog-disable=\"disable\"  size=\"small\" @click=\"selectPdProg\" icon=\"el-icon-search\" type=\"primary\">\r\n              {{ $t('table.search') }}\r\n            </el-button>\r\n        </cus-btn-wrapper>\r\n      </el-row>\r\n    </el-form>\r\n    <cus-table-title :label=\"$t('pdtempProgInfo.chooseProg')\"/>\r\n    <el-table ref=\"bnftTable\" :data=\"bnftConfigs\" border element-loading-text=\"Loading\" fit highlight-current-row\r\n              size=\"mini\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\" width=\"50\"></el-table-column>\r\n      <el-table-column :label=\"$t('table.detail')\" align=\"center\" style=\"width: 50%\" type=\"expand\">\r\n        <template slot-scope=\"props\">\r\n          <el-form class=\"demo-table-expand\" inline label-position=\"left\">\r\n            <el-form-item :label=\"$t('i18nView.pdCode')\" style=\"width: 50%\">\r\n              {{ props.row.pdCode }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('customComponents.pdEdition')\" style=\"width: 50%\">\r\n              {{ props.row.pdEdition }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdName')\" style=\"width: 50%\">\r\n              {{ props.row.pdName }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.schemeCode')\" style=\"width: 50%\">\r\n              {{ props.row.pdProgCode }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.programName')\" style=\"width: 50%\">\r\n              {{ props.row.pdProgName }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdOptFlag')\" style=\"width: 50%\">\r\n              {{ props.row.pdOptFlag | filter_pdOptFlag }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdAddAmntFlag')\" style=\"width: 50%\">\r\n              {{ props.row.pdAddAmntFlag | filter_pdAddAmntFlag }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdAddAmntFlag')\" style=\"width: 50%\">\r\n              {{ props.row.pdAmntSaleType | filter_pdAmntSaleType }}\r\n            </el-form-item>\r\n          </el-form>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.pdCode')\" align=\"center\" prop=\"pdCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.pdName')\" align=\"center\" prop=\"pdName\"></el-table-column>\r\n      <el-table-column :label=\"$t('customComponents.pdEdition')\" align=\"center\"\r\n                       prop=\"pdEdition\"></el-table-column>\r\n      <el-table-column :label=\"$t('pdtempProgInfo.pdProgCode')\" align=\"center\"\r\n                       prop=\"pdProgCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('pdtempProgInfo.pdProgName')\" align=\"center\"\r\n                       prop=\"pdProgName\"></el-table-column>\r\n    </el-table>\r\n    <cus-pagination ref=\"page\" v-model=\"bnftConfigs\" :pageSize=\"5\" :params=\"paymentForm\"\r\n                    url=\"/pdtemp-pd-claim-verification-rule/base/selectPdProg\"/>\r\n    <br/>\r\n    <el-form>\r\n      <el-form-item style=\"text-align: right\">\r\n        <el-button dialog-disable=\"disable\"  size=\"small\" v-auth=\"'select:AllPdAccount'\" icon=\"el-icon-circle-plus-outline\" type=\"primary\" @click=\"addPdProg\">\r\n          {{$t('i18nView.add')}}\r\n        </el-button>\r\n        <el-button dialog-disable=\"disable\"  size=\"small\" icon=\"el-icon-refresh\" type=\"primary\" @click=\"researchData\">\r\n          {{$t('table.refresh')}}\r\n        </el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n    <cus-table-title :label=\"$t('pdtempProgInfo.AssociatedPlan')\" />\r\n    <el-table ref=\"paymentTable\" :data=\"paymentConfigs\" border element-loading-text=\"Loading\" fit highlight-current-row\r\n              max-height=\"300px\" size=\"mini\">\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\" width=\"50\"></el-table-column>\r\n      <el-table-column :label=\"$t('table.detail')\" align=\"center\" style=\"width: 50%\" type=\"expand\">\r\n        <template slot-scope=\"props\">\r\n          <el-form class=\"demo-table-expand\" inline label-position=\"left\">\r\n            <el-form-item :label=\"$t('i18nView.pdCode')\" style=\"width: 50%\">\r\n              {{ props.row.pdCode }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('customComponents.pdEdition')\" style=\"width: 50%\">\r\n              {{ props.row.pdEdition }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdName')\" style=\"width: 50%\">\r\n              {{ props.row.pdName }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.schemeCode')\" style=\"width: 50%\">\r\n              {{ props.row.pdProgCode }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.programName')\" style=\"width: 50%\">\r\n              {{ props.row.pdProgName }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdOptFlag')\" style=\"width: 50%\">\r\n              {{ props.row.pdOptFlag | filter_pdOptFlag }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdAddAmntFlag')\" style=\"width: 50%\">\r\n              {{ props.row.pdAddAmntFlag | filter_pdAddAmntFlag }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdAmntSaleType')\" style=\"width: 50%\">\r\n              {{ props.row.pdAmntSaleType | filter_pdAmntSaleType }}\r\n            </el-form-item>\r\n          </el-form>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.pdCode')\" align=\"center\" prop=\"pdCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.pdName')\" align=\"center\" prop=\"pdName\"></el-table-column>\r\n      <el-table-column :label=\"$t('customComponents.pdEdition')\" align=\"center\"\r\n                       prop=\"pdEdition\"></el-table-column>\r\n      <el-table-column :label=\"$t('pdtempProgInfo.pdProgCode')\" align=\"center\"\r\n                       prop=\"pdProgCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('pdtempProgInfo.pdProgName')\" align=\"center\"\r\n                       prop=\"pdProgName\"></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('table.actions')\" width=\"100\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button dialog-disable=\"disable\"   size=\"mini\" type=\"danger\" @click=\"deleteRela(scope.row)\">-</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\nimport {findProductsByProductType,initProg} from \"@/api/basicinfo/pdtemppdclmbnft\";\r\nimport {findBusinessItemByCtrlCode} from \"@/api/basicinfo/productCompensation\"\r\n\r\nlet _this\r\n\r\nexport default {\r\n  name: \"pdtemppdbnftinfo\",\r\n  props: {\r\n    pdBnftCtrlCode: {\r\n      required: false,\r\n      type: String,\r\n      default: ''\r\n    }\r\n  },\r\n  filters: {\r\n    filter_pdAddAmntFlag(code) {\r\n      return _this.getCodeName(\"reckonIn\", code)\r\n    },\r\n    filter_pdOptFlag(code) {\r\n      return _this.getCodeName(\"pdOptFlag\", code)\r\n    },\r\n    filter_pdAmntSaleType(code) {\r\n      return _this.getCodeName(\"pdAmntSaleType\", code)\r\n    }\r\n  },\r\n  watch:{\r\n    'paymentForm.productTypeDetail'(){\r\n      this.paymentForm.product = []\r\n    },\r\n    'paymentForm.product'(){\r\n      this.paymentForm.pdProg = [],\r\n      this.initProg1();\r\n    }\r\n  },\r\n  data() {\r\n    _this = this\r\n    return {\r\n      pdtempPdInfo: {},\r\n      paymentForm: {\r\n        productTypeDetail: '',\r\n        product: '',\r\n        pdProg:''\r\n      },\r\n      paymentConfigs: [],\r\n      bnftConfigs: [],\r\n      productOptions: [],\r\n      pdProgOptions: [],\r\n      chooseBnft: [],\r\n      chooseProg: []\r\n    }\r\n  },\r\n  created() {\r\n    this.pdtempPdInfo = this.getPdData()\r\n    this.initPdog()\r\n  },\r\n  methods: {\r\n    initProg1(){\r\n      const params = {\r\n        product: this.paymentForm.product\r\n      }\r\n      initProg(this.buildRequestData(params)).then((response) => {\r\n        this.pdProgOptions= response.data\r\n      })\r\n    },\r\n    //初始化关联方案信息\r\n    initPdog() {\r\n      if (this.pdBnftCtrlCode) {\r\n        findBusinessItemByCtrlCode(this.pdBnftCtrlCode).then(response => {\r\n          this.paymentConfigs = response.data\r\n        })\r\n      }\r\n    },\r\n    //保存关联方案\r\n    addPdProg() {\r\n      for (const item of this.chooseProg) {\r\n        let duplicated = false\r\n        for (const selectedItem of this.paymentConfigs) {\r\n          if (selectedItem.id === item.id) {\r\n            setTimeout(() => {\r\n              this.notifyError(item.pdProgCode + ':' + item.pdProgName + '已添加，请勿重复添加')\r\n            }, 1)\r\n            duplicated = true\r\n            break\r\n          }\r\n        }\r\n        if (!duplicated) {\r\n          this.paymentConfigs.push(item)\r\n        }\r\n      }\r\n    },\r\n    //方案搜索\r\n    selectPdProg() {\r\n      this.$refs.page.reload()\r\n    },\r\n    findProducts(val) {\r\n      let data = {\r\n        pdCls2: val\r\n      }\r\n      findProductsByProductType(this.buildRequestData(data)).then(res => {\r\n        this.productOptions = [];\r\n        res.data.forEach(product => {\r\n          this.productOptions.push({\r\n            value: product.pdCode,\r\n            label: product.pdCode + \":\" + product.pdName,\r\n            key: product.id\r\n          });\r\n        });\r\n      })\r\n    },\r\n    researchData() {\r\n      this.paymentForm.productTypeDetail = ''\r\n      this.paymentForm.product = ''\r\n      this.bnftConfigs = []\r\n      this.$refs.page.reload()\r\n    },\r\n    deleteRela(row) {\r\n      this.paymentConfigs.splice(this.paymentConfigs.indexOf(row), 1)\r\n    },\r\n    handleSelectionChange(row) {\r\n      this.chooseProg = row;\r\n    },\r\n    backToMenu() {\r\n      this.$emit('backToFeeBill', 'prog', this.paymentConfigs)\r\n    },\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.divider {\r\n  color: black;\r\n  background-color: black;\r\n  width: 1px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/product-definition/pd-operation-def/pd-compensation-def/main/pdtempproginfo"}]}