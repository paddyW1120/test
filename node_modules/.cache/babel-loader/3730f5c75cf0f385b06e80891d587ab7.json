   let labelWidth = config.labelWidth ? `${config.labelWidth}px` : null\r\n    return (\r\n      <cus-row span={config.span} class={className}\r\n               nativeOnClick={event => {\r\n                 activeItem(element);\r\n                 event.stopPropagation()\r\n               }}>\r\n        <el-form-item label-width={labelWidth}\r\n                      label={element.label} required={config.required}>\r\n          <render key={config.renderKey} conf={element} onInput={event => {\r\n            this.$set(config, 'defaultValue', event)\r\n          }}/>\r\n        </el-form-item>\r\n        {components.itemBtns.apply(this, arguments)}\r\n      </cus-row>\r\n    )\r\n  }\r\n}\r\n\r\n\r\nfunction layoutIsNotFound() {\r\n  throw new Error(`没有与${this.element.__config__.layout}匹配的layout`)\r\n}\r\n\r\nexport default {\r\n  components: {\r\n    render,\r\n    draggable\r\n  },\r\n  props: [\r\n    'element',\r\n    'index',\r\n    'drawingList',\r\n    'formConf'\r\n  ],\r\n  render(h) {\r\n    const layout = layouts[this.element.config.layout]\r\n\r\n    if (layout) {\r\n      return layout.call(this, h, this.element, this.index, this.drawingList)\r\n    }\r\n    return layoutIsNotFound.call(this)\r\n  }\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views/product-definition/pd-operation-def/pd-acceptance-def/pd-page-def/draggable"}]}