                             prop=\"pdReleIp\"></el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.pdRelePort')\" align=\"center\"\r\n                             prop=\"pdRelePort\"></el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.pdUsername')\" align=\"center\"\r\n                             prop=\"pdUsername\"></el-table-column>\r\n          </el-table>\r\n        </el-row>\r\n        <el-row :gutter=\"10\" class=\"row-bg\" style=\"margin-top: 15px\" type=\"flex\">\r\n          <el-col :span=\"24\">\r\n            <el-form-item :label=\"$t('i18nView.pdReleFileName')\">\r\n              <el-input v-model=\"PdtempPdVerRls.pdReleFile\" readonly style=\"width: 588px\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n\r\n      <!--发布选项  ： Excel文件-->\r\n      <div v-if=\"showRadio\">\r\n        <el-row :gutter=\"10\" class=\"row-bg\" justify=\"center\" type=\"flex\">\r\n          <el-col :span=\"24\">\r\n            <el-form-item :label=\"$t('i18nView.pdAuditConc')\" prop=\"pdAuditConc\">\r\n              <el-radio-group v-model=\"PdtempPdVerRls.pdAuditConc\" @change=\"showPathOne\">\r\n                <el-radio v-for=\"path in pathtableDataBc\" :key=\"path.id\" :label=\"path\">\r\n                  {{ path.pdReleSysname }}\r\n                </el-radio>\r\n              </el-radio-group>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row v-if=\"tableflag\">\r\n          <el-table ref=\"sizeForm3.tableData2\" :data=\"pathtableData\" border class=\"wraper-table\" fit\r\n                    highlight-current-row>\r\n            <el-table-column align=\"center\" :label=\"$t('table.id')\" type=\"index\" width=\"55px\"></el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.currency')\" align=\"center\"\r\n                             prop=\"pdReleSysname\"></el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.pdReleIp')\" align=\"center\"\r\n                             prop=\"pdReleIp\"></el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.pdRelePort')\" align=\"center\"\r\n                             prop=\"pdRelePort\"></el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.pdUsername')\" align=\"center\"\r\n                             prop=\"pdUsername\"></el-table-column>\r\n          </el-table>\r\n        </el-row>\r\n        <el-row :gutter=\"10\" class=\"row-bg\" style=\"margin-top: 15px\" type=\"flex\">\r\n          <el-col :span=\"24\">\r\n            <el-form-item :label=\"$t('i18nView.pdReleFileName')\">\r\n              <el-input v-model=\"PdtempPdVerRls.pdReleFile\" readonly style=\"width: 588px\"></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n      <!--发布确认按钮-->\r\n      <el-row>\r\n        <el-col :span=\"24\" align=\"right\">\r\n          <el-button :disabled=\"releButton\" icon=\"el-icon-success\" size=\"small\" type=\"primary\" @click=\"submitReleInfo\">\r\n            {{ $t('ppReleaseInfoAll.releBut') }}\r\n          </el-button>\r\n          <el-button :disabled=\"releButton\" icon=\"el-icon-refresh\" size=\"small\" type=\"primary\" @click=\"refreshPage\">\r\n            {{ $t('ppReleaseInfoAll.releRefresh') }}\r\n          </el-button>\r\n        </el-col>\r\n      </el-row>\r\n    </el-form>\r\n    <el-dialog v-if=\"dialogTableVisible\" v-dialog-drag :close-on-click-modal=\"false\"\r\n               :visible.sync=\"dialogTableVisible\" append-to-body center width=\"75%\">\r\n      <cus-page-title-back :label=\"$t('ppReleaseInfoAll.pdaudith')\" @backTo=\"backAuditPage\"></cus-page-title-back>\r\n      <el-table :data=\"tableData\" border element-loading-text=\"Loading\" fit highlight-current-row size=\"mini\">\r\n        <el-table-column align=\"center\" :label=\"$t('table.id')\" type=\"index\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.details')\" type=\"expand\">\r\n          <template slot-scope=\"props\">\r\n            <el-form class=\"demo-table-expand\" inline label-position=\"left\">\r\n              <el-form-item :label=\"$t('i18nView.ppCode')\">\r\n                <span>{{ props.row.pdCode }}</span>\r\n              </el-form-item>\r\n              <el-form-item :label=\"$t('i18nView.auditRes')\">\r\n                <span>{{ props.row.pdAuditConc === 'N' ? '不通过' : '通过' }}</span>\r\n              </el-form-item>\r\n              <el-form-item :label=\"$t('i18nView.auditOper')\">\r\n                <span>{{ props.row.pdAuditStaff }}</span>\r\n              </el-form-item>\r\n              <el-form-item :label=\"$t('i18nView.auditDate')\">\r\n                <span>{{ props.row.pdAuditDate }}</span>\r\n              </el-form-item>\r\n              <el-form-item :label=\"$t('i18nView.returnFunctionPoints')\" style=\"width: 100%\">\r\n                <template v-slot=\"scope\"><span>{{ props.row.pdReturnFuncNode | getPdReturnFuncNode }}</span></template>\r\n              </el-form-item>\r\n              <br>\r\n              <el-form-item :label=\"$t('i18nView.auditContent')\">\r\n                <span>{{ props.row.pdAuditIdea }}</span>\r\n              </el-form-item>\r\n            </el-form>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.ppCode')\" align=\"center\" prop=\"pdCode\">\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.auditRes')\" align=\"center\" prop=\"pdAuditConc\">\r\n          <template v-slot=\"scope\">{{ scope.row.pdAuditConc | getPdAuditConc }}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.auditOper')\" align=\"center\" prop=\"pdAuditStaff\">\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.auditDate')\" align=\"center\" prop=\"pdAuditDate\">\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-dialog>\r\n\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\n\r\n\r\nimport {reviewHistory} from \"@/api/basicinfo/comboAuditInfo\";\r\nimport {comboRelease, getAllPath, searchPPInRelease} from \"@/api/basicinfo/pdtemppprelease\";\r\n\r\nlet count = 0;\r\nexport default {\r\n  name: 'releaseAll',\r\n  filters: {\r\n    /*审核结论*/\r\n    getPdAuditConc(state) {\r\n      return state === 'N' ? '不通过' : '通过';\r\n    },\r\n    /*退回功能点*/\r\n    getPdReturnFuncNode(node) {\r\n      let mnode = JSON.parse(node);\r\n      let returnText = '';\r\n      for (let i = 0; i < mnode.length; i++) {\r\n        switch (mnode[i]) {\r\n          case 'productRuleFlag':\r\n            returnText = returnText + '套餐规则订正;';\r\n            break;\r\n          case 'productFormulaFlag':\r\n            returnText = returnText + '套餐公式订正;'\r\n            break;\r\n          case 'productInfoFlag':\r\n            returnText = returnText + '套餐框架定义;'\r\n            break;\r\n          case 'productDistributionFlag':\r\n            returnText = returnText + '套餐销售定义;'\r\n            break;\r\n          case 'productCoverFlag':\r\n            returnText = returnText + '套餐承保定义;'\r\n            break;\r\n          case 'productUnderwritingFlag':\r\n            returnText = returnText + '套餐核保定义;'\r\n            break;\r\n          case 'productHePeiFlag':\r\n            returnText = returnText + '套餐核赔定义;'\r\n            break;\r\n        }\r\n      }\r\n      return returnText;\r\n    },\r\n  },\r\n  watch: {\r\n    'PdtempPdVerRls.pdAuditConc'(val) {\r\n      this.envList = val\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      distinguishFieldPresentFlag: 'Y',\r\n      //表格多选\r\n      multipleSelection: [],\r\n      checkId: [],\r\n      PdtempPdVerRls: {\r\n        pdReleWay: '',      //发布方式\r\n        pdEdition: '',  //  套餐版本\r\n        pdType: '',  //  套餐类型\r\n        pdInfoId: '',  //  套餐id\r\n        pdVerId: '',  //  版别id\r\n        pdCode: '',  //  套餐编码\r\n        pdReleOrder: '',  //  发布序号\r\n        pdReleType: '',  //  发布类型 : 全量发布 增量发布\r\n        pdReleSys: '',  //  发至系统\r\n        pdReleEnv: '',  //  发至环境\r\n        pdReleFilePath: '',  //  发布文件地址\r\n        pdReleStu: '',  //  发布状态 : 发布成功 发布失败\r\n        pdReleContent: '',  //  发布内容\r\n        pdReleDetails: '',  //  发布说明\r\n        pdReleFile: '',  //  生成文件名\r\n        pdAuditConc: []  //  发布环境\r\n      },\r\n      releButton: true,\r\n      showRele: false,\r\n      tableflag: false,\r\n      showRadio: false,\r\n      showCheck: false,\r\n      showComboDate: false,\r\n      pathtableDataBc: [],\r\n      pathtableData: [],\r\n      envList: [],\r\n      checkPath: [],\r\n\r\n      tableData: [],\r\n\r\n      readyReleaseInfo: false,\r\n      dialogTableVisible: false,\r\n      pdOptions: [],\r\n      selectRow: [],\r\n      pdInfo: {},\r\n\r\n      pdAuditDetailInfo: {\r\n        pdType: '1',\r\n        pdInfoId: '',\r\n        pdCode: '',\r\n        pdAuditOrderNum: '1',\r\n        pdAuditInfoId: '',\r\n        pdAuditConc: '',\r\n        pdAuditStaff: '',\r\n        pdAuditDate: '',\r\n        pdAuditIdea: '',\r\n        pdReturnFuncNode: [],\r\n      },\r\n\r\n      //表格数据\r\n      configs: [],\r\n      //查询参数\r\n      params: {\r\n        pdReleType: '1'\r\n      },\r\n      //检验规则\r\n      rules: {\r\n        pdReleWay: [{required: true, message: '请选择发布方式', trigger: 'change'}],\r\n        pdReleDetails: [{required: true, message: '请输入发布说明', trigger: 'change'}],\r\n        pdAuditConc: [{required: true, message: '请选择发布环境', trigger: 'change'}]\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n  },\r\n  created() {\r\n  },\r\n  methods: {\r\n    /* 父组件刷新子组件表格 */\r\n    refreshChildTable() {\r\n      searchPPInRelease(this.buildRequestData(this.params)).then(response => {\r\n        this.configs = response.data.records\r\n      })\r\n      this.multipleSelection = [];\r\n      this.checkId = [];\r\n    },\r\n\r\n    /*  多选待发布数据，获取并准备 发布数据  */\r\n    handleSelectionChange(val) {\r\n      this.multipleSelection = [];\r\n      this.checkId = [];\r\n      if (val) {\r\n        this.showRele = val.length > 0;\r\n        for (const valElement of val) {\r\n          this.checkId.push(valElement.id)\r\n        }\r\n        this.multipleSelection = val\r\n      }\r\n    },\r\n    backAuditPage() {\r\n      this.dialogTableVisible = false\r\n    },\r\n    /*刷新*/\r\n    refreshPage() {\r\n      if (this.$refs.PdtempPdVerRlsForm) {\r\n        this.$refs.PdtempPdVerRlsForm.resetFields();\r\n      }\r\n    },\r\n    /*  发布确认 */\r\n    submitReleInfo() {\r\n      this.$refs.PdtempPdVerRlsForm.validate(pass => {\r\n        if (pass) {\r\n          let params = {\r\n            \"comboReleaseInfoVoList\": this.multipleSelection,\r\n            \"pdtempPdReleSyspathList\": Array.isArray(this.envList) ? this.envList : Array(this.envList),\r\n            \"pdReleWay\": this.PdtempPdVerRls.pdReleWay,\r\n            \"pdReleDetails\": this.PdtempPdVerRls.pdReleDetails,\r\n            \"releType\": \"1\",\r\n            \"releTypeDet\": \"0\",\r\n            \"distinguishFieldPresentFlag\": this.distinguishFieldPresentFlag\r\n          }\r\n          comboRelease(this.buildRequestData(params)).then(res => {\r\n            if (res.data.message) {\r\n              this.notifyError(res.data.message)\r\n            } else {\r\n              this.notifySuccess('发布成功')\r\n              /*this.PdtempPdVerRls.pdReleFile = '';\r\n              for (const pdoperPdVerRls of res.data.pdoperPdVerRlsList) {\r\n                this.PdtempPdVerRls.pdReleFile += pdoperPdVerRls.pdReleFileName + \"; \";\r\n                let filePath = pdoperPdVerRls.pdReleFilePath + \"//\" + pdoperPdVerRls.pdReleFileName;\r\n                let fileName = pdoperPdVerRls.pdReleFileName;\r\n                let data = {\"filePath\": filePath}\r\n                downloadReleFile(this.buildRequestData(data)).then(res => {\r\n                  this.downloadResponseFile(res, fileName)\r\n                })\r\n              }\r\n              this.PdtempPdVerRls.pdReleFile = this.PdtempPdVerRls.pdReleFile.substring(0, this.PdtempPdVerRls.pdReleFile.length - 2)*/\r\n              if (this.PdtempPdVerRls.pdReleWay === '2') {\r\n                this.$emit('parentByClick', \"已发布\" + this.PdtempPdVerRls.pdReleEnv);\r\n              }\r\n              this.$emit('initTableData')\r\n            }\r\n          })\r\n        } else {\r\n          this.focusItem()\r\n        }\r\n      })\r\n    },\r\n    /*  待发布--审核结论  */\r\n    reviewHistory(row) {\r\n      let data = {\r\n        pdAuditDetailInfoId: row.id,\r\n        pdVerId: row.pdVerId\r\n      }\r\n      reviewHistory(this.buildRequestData(data)).then(res => {\r\n        this.tableData = res.data\r\n        this.dialogTableVisible = true;\r\n      })\r\n    },\r\n    /*  发布多选-显示环境  */\r\n    showPath(val) {\r\n      this.PdtempPdVerRls.pdReleEnv = ''\r\n      for (const argument of val) {\r\n        this.PdtempPdVerRls.pdReleEnv += argument.pdReleSysname + \";\"\r\n      }\r\n      this.pathtableData = []\r\n      this.pathtableData = val\r\n      this.tableflag = this.pathtableData.length !== 0\r\n    },\r\n    /*  发布单选-显示环境  */\r\n    showPathOne(val) {\r\n      this.pathtableData = [];\r\n      this.pathtableData.push(val)\r\n      this.tableflag = this.pathtableData.length !== 0;\r\n    },\r\n    /*  根据发布选项获取环境  */\r\n    getSysPath(val) {\r\n      this.PdtempPdVerRls.pdAuditConc = []\r\n      this.PdtempPdVerRls.pdReleFile = ''\r\n      if (!val) {\r\n        this.releButton = true;\r\n        this.$emit('parentByClick', \"0\", this.checkId);\r\n        this.showCheck = false;\r\n        this.showRadio = false;\r\n      } else {\r\n        if (val !== '2') {\r\n          this.PdtempPdVerRls.pdReleFile = '';\r\n          this.showCheck = true;\r\n          this.showRadio = false;\r\n          this.envList = [];\r\n          this.tableflag = false;\r\n          this.PdtempPdVerRls.pdReleEnv = '';\r\n          this.pathtableData = [];\r\n          this.$emit('parentByClick', \"1\", this.checkId);\r\n        } else {\r\n          this.envList = [];\r\n          this.showRadio = true;\r\n          this.showCheck = false;\r\n          this.PdtempPdVerRls.pdReleEnv = '';\r\n          this.tableflag = false;\r\n          this.pathtableData = [];\r\n        }\r\n        this.releButton = false;\r\n        this.checkPath = [];\r\n        getAllPath().then(res => {\r\n          this.pathtableDataBc = res.data\r\n          for (const argument of res.data) {\r\n            this.checkPath.push(argument.pdReleSysname)\r\n          }\r\n        })\r\n      }\r\n      this.$nextTick(() => {\r\n        this.$refs.PdtempPdVerRlsForm.clearValidate()\r\n      })\r\n    },\r\n    /*待发布内容  按钮组   显示*/\r\n    readyReleaseInfoClick(row) {\r\n      this.readyReleaseInfo = !this.readyReleaseInfo;\r\n      this.selectRow = row\r\n    },\r\n    /*页面跳转按钮组*/\r\n    getPdDesignPage() {\r\n      this.$router.push({name: '产品套餐框架定义主页', query: {showPageFlag: '0'},params:this.selectRow})\r\n    },\r\n    getPdSalePage() {\r\n      this.$router.push({name: '产品套餐销售定义主页', query: {showPageFlag: '1'},params:this.selectRow})\r\n    },\r\n    getPdUWPage() {\r\n      this.$router.push({name: '产品套餐承保定义主页', query: {showPageFlag: '1'},params:this.selectRow})\r\n    },\r\n    getPdAUPage() {\r\n      this.$router.push({name: '产品套餐核保定义主页', query: {showPageFlag: '1'},params:this.selectRow})\r\n    },\r\n    getPdCLPage() {\r\n      this.$router.push({name: '产品套餐核赔定义主页', query: {showPageFlag: '1'},params:this.selectRow})\r\n    },\r\n    getPdTSPage() {\r\n      this.$router.push({name: '', query: {showPageFlag: '1'}})\r\n    }\r\n\r\n  },\r\n\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.el-button {\r\n  text-align: center;\r\n  width: 100px;\r\n}\r\n\r\n.swbu {\r\n  width: 135px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/commodity-release/package-release/releaseall"}]}