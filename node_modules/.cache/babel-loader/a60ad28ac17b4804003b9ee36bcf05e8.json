Rules\">\r\n        <div :class=\"{ 'showDiv': showPageFlag == 1 }\"></div>\r\n        <underwritingRules v-if=\"activeName==='renewalUnderwritingRules'\" :showTabFlag=\"11\"\r\n                           @childFn=\"parentFn\"></underwritingRules>\r\n      </el-tab-pane>\r\n      <el-tab-pane :label=\"$t('underwritingRulesMain.securityUnderwritingRules')\" name=\"securityUnderwritingRules\">\r\n        <div :class=\"{ 'showDiv': showPageFlag == 1 }\"></div>\r\n        <underwritingRules v-if=\"activeName==='securityUnderwritingRules'\" :showTabFlag=\"12\"\r\n                           @childFn=\"parentFn\"></underwritingRules>\r\n      </el-tab-pane>\r\n      <el-tab-pane :label=\"$t('underwritingRulesMain.claimUnderwritingRules')\" name=\"claimUnderwritingRules\">\r\n        <div :class=\"{ 'showDiv': showPageFlag == 1 }\"></div>\r\n        <underwritingRules v-if=\"activeName==='claimUnderwritingRules'\" :showTabFlag=\"13\"\r\n                           @childFn=\"parentFn\"></underwritingRules>\r\n      </el-tab-pane>\r\n    </el-tabs>\r\n\r\n  </cus-wraper>\r\n</template>\r\n\r\n<script>\r\n\r\nimport underwritingRules\r\n  from '@/views/product-definition/pd-operation-def/pd-underwriting-def/underwriting-rule-public-page'\r\nimport {closeVisitTag} from \"@/utils/tagsView\"\r\n\r\nlet _this\r\nexport default {\r\n  name: \"underwritingRulesMain\",\r\n\r\n  filters: {},\r\n  data() {\r\n    _this = this\r\n    return {\r\n      page: undefined,\r\n      pageSize: undefined,\r\n      ruleRevisionFlag: false,\r\n      showPageFlag: 0,\r\n      activeName: \"acceptInsuranceRules\",\r\n      activatedNames: [\"acceptInsuranceRules\"]\r\n    }\r\n  },\r\n  components: {underwritingRules},\r\n  created() {\r\n    this.page = this.$route.query.page\r\n    this.pageSize = this.$route.query.pageSize\r\n    this.ruleRevisionFlag = this.$route.query.ruleRevisionFlag === 'true'\r\n    this.showPageFlag = this.$route.query.showPageFlag\r\n  },\r\n  methods: {\r\n    back2() {\r\n      this.$router.push({name: '产品公式订正主页', params: {}})\r\n    },\r\n    back() {\r\n      this.$router.push({name: '产品规则订正主页', params: {path: this.$route.path, page: this.page, pageSize: this.pageSize}})\r\n    },\r\n    backToMenu() {\r\n      closeVisitTag()\r\n      this.$router.push({name: '产品核保定义', params: {}})\r\n    },\r\n    parentFn(val) {\r\n      this.activeName = val\r\n    },\r\n    clickTab(val) {\r\n      let flag = false\r\n      this.activatedNames.forEach(value => {\r\n        if (value === val.name) {\r\n          flag = true\r\n        }\r\n      })\r\n      if (flag === false) {\r\n        this.activatedNames.push(val.name)\r\n      }\r\n    },\r\n    isAlreadyActived(val) {\r\n      let flag = false\r\n      this.activatedNames.forEach(value => {\r\n        if (val === value) {\r\n          flag = true\r\n        }\r\n      })\r\n      return flag\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.showDiv {\r\n  width: 100%;\r\n  height: 100%;\r\n  margin-top: -40px;\r\n  margin-left: -40px;\r\n  position: absolute;\r\n  background: #ffffff00;\r\n  z-index: 999\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/product-definition/pd-operation-def/pd-underwriting-def/underwriting-rule"}]}