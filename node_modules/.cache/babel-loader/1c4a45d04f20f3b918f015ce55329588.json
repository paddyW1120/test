  });\r\n      return validValue;\r\n    },\r\n    ruleTypeFormat(row, column) {\r\n      let type = row.ruleType;\r\n      let typeValue = \"\";\r\n      this.ruleTypeArray.forEach((t, index) => {\r\n        if (t.code === type) {\r\n          typeValue = t.codename;\r\n        }\r\n      });\r\n      return typeValue;\r\n    },\r\n    handleSearch() {\r\n      this.$refs.page.reload();\r\n    },\r\n    handleRuleModify(row) {\r\n       //通过规则类型查看规则明细\r\n      this.ruleCode = row.code;\r\n      this.ruleFlag = \"2\";\r\n      this.ruleQuery = true;\r\n      this.ruleType = row.ruleType;\r\n      this.pageState = false;\r\n      this.$nextTick(() => (this.pageState = true));\r\n      this.dialogVisible = true;\r\n    },\r\n    handleRuleModify1(row) {\r\n      row.code = \"rule205548486\";\r\n      row.id = \"\";\r\n      this.cacheJsonData(\"ruleFlag\", \"0\");\r\n      this.cacheJsonData(\"attributeObj\", row);\r\n      this.cacheJsonData(\"menuPage\", false);\r\n      this.cacheJsonData(\"nodeName\", \"\");\r\n      this.dialogVisible = true;\r\n    },\r\n     handleRuleModify3(row) {\r\n       row.id = \"\";\r\n       row.code = \"rule2020072399\";\r\n       row.description ='nnnnn';\r\n       row.ruleType ='2';\r\n\r\n    },\r\n    resetForm() {\r\n      this.form = {\r\n        //表单数据与后端实体类对应\r\n        id: \"\", //主键ID\r\n        xxxx: \"\"\r\n      };\r\n    },\r\n    goBack(){\r\n      this.dialogVisible = false;\r\n    },\r\n    handleRuleModify5(row){\r\n      let ruleMain ={\r\n        ruleCode:\"FA536000000000001848\",\r\n        dynamicTable:'{\"paramArr\":[{\"prop\":\"BOM_BOMBqCont,LINK_LINK_OF,BOMItem_BOMBqCont_Amnt,CMD_CMD_number_not_large\",\"keyId\":\"BOM_BOMBqCont,LINK_LINK_OF,BOMItem_BOMBqCont_Amnt,CMD_CMD_number_not_large\",\"commandType\":\"Number\",\"column\":\"COLUMN0\",\"options\":[]},{\"prop\":\"RESULT_BOMDTTABLE_DTRATE\",\"keyId\":\"RESULT_BOMDTTABLE_DTRATE\",\"commandType\":\"RESULT\",\"column\":\"RESULT\"},{\"prop\":\"RESULT_RES_MULTMSG