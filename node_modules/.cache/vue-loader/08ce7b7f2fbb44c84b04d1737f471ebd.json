"file":"newstyle.vue","sourceRoot":"src/views/test","sourcesContent":["<template>\r\n  <div class=\"box\">\r\n    <cus-wraper>\r\n      <cus-filter-wraper>\r\n        <div>\r\n          <el-collapse-transition>\r\n            <div v-show=\"quickshow\">\r\n              <el-form ref=\"form\" :model=\"form\" label-width=\"80px\" style=\"width:98%;\">\r\n                <el-row>\r\n                  <el-col :span=\"12\">\r\n                    <el-form-item :label=\"$t('i18nView.referenceProduct')\">\r\n                      <el-select v-model=\"form.referenceProducts\" :placeholder=\"$t('form.please_select')\">\r\n                        <el-option v-for=\"item in options\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\">\r\n                        </el-option>\r\n                      </el-select>\r\n                      <el-button  size=\"small\">{{$t('i18nView.productReplication')}}</el-button>\r\n                    </el-form-item>\r\n                  </el-col>\r\n                </el-row>\r\n              </el-form>\r\n            </div>\r\n          </el-collapse-transition>\r\n          <el-collapse-transition>\r\n            <div v-if=\"excelshow\">\r\n              <my-template></my-template>\r\n            </div>\r\n          </el-collapse-transition>\r\n        </div>\r\n\r\n\r\n      </cus-filter-wraper>\r\n\r\n\r\n      <el-collapse v-model=\"activeName\" accordion class=\"collapseCus\" id=\"boxFixed\" :class=\"{'is_fixed' : isFixed}\">\r\n        <el-collapse-item title=\"产品属性定义\" name=\"attribute\">\r\n          <div style=\"height: 80px;\" class=\"stepDiv\">\r\n            <el-steps process-status=\"finish \" finish-status=\"wait\" direction=\"vertical\" :active=\"attrActive\">\r\n              <el-step icon=\"el-icon-circle-check\" title=\"产品基本属性定义\"\r\n                       @click.native=\"handleProductAttr(0,'base')\"></el-step>\r\n              <el-step icon=\"el-icon-circle-check\" title=\"监管属性定义\"\r\n                       @click.native=\"handleProductAttr(1,'supervise')\"></el-step>\r\n            </el-steps>\r\n          </div>\r\n        </el-collapse-item>\r\n        <br/>\r\n        <el-collapse-item title=\"产品结构定义\" name=\"structure\">\r\n          <div style=\"height: 160px;\" class=\"stepDiv\">\r\n            <el-steps process-status=\"finish \" finish-status=\"wait\" direction=\"vertical\" :active=\"struActive\">\r\n              <el-step icon=\"el-icon-circle-check\" title=\"缴费定义\" @click.native=\"handleProductStru(0,'pay')\"></el-step>\r\n              <el-step icon=\"el-icon-circle-check\" title=\"给付定义\"\r\n                       @click.native=\"handleProductStru(1,'payment')\"></el-step>\r\n              <el-step icon=\"el-icon-circle-check\" title=\"方案定义\" @click.native=\"handleProductStru(2,'scheme')\"></el-step>\r\n              <el-step icon=\"el-icon-circle-check\" title=\"账户定义\" @click.native=\"handleProductStru(3,'account')\"></el-step>\r\n            </el-steps>\r\n          </div>\r\n        </el-collapse-item>\r\n        <br/>\r\n        <el-collapse-item title=\"产品现价定义\" name=\"currprice\">\r\n          <div style=\"height: 80px;\" class=\"stepDiv\">\r\n            <el-steps process-status=\"finish \" finish-status=\"wait\" direction=\"vertical\" :active=\"currActive\">\r\n              <el-step icon=\"el-icon-circle-check\" title=\"现价定义1\" @click.native=\"handleProductCurr(0,'curr1')\"></el-step>\r\n              <el-step icon=\"el-icon-circle-check\" title=\"现价定义2\" @click.native=\"handleProductCurr(1,'curr1')\"></el-step>\r\n            </el-steps>\r\n          </div>\r\n        </el-collapse-item>\r\n        <br/>\r\n        <el-collapse-item title=\"产品定义进度\" name=\"process\">\r\n          <div style=\"height: 80px;\" class=\"stepDiv\">\r\n            <el-steps process-status=\"finish \" finish-status=\"wait\" direction=\"vertical\" :active=\"procActive\">\r\n              <el-step icon=\"el-icon-circle-check\" title=\"进度概述\" @click.native=\"handleProductProc(0,'summary')\"></el-step>\r\n              <el-step icon=\"el-icon-circle-check\" title=\"进度详情\" @click.native=\"handleProductProc(1,'detail')\"></el-step>\r\n            </el-steps>\r\n          </div>\r\n        </el-collapse-item>\r\n      </el-collapse>\r\n\r\n      <div class=\"container\">\r\n        <!--产品框架定义-->\r\n        <pdtemp-pd-base-definition v-if=\"isActived('base')\" @childFn=\"parentFn\"></pdtemp-pd-base-definition>\r\n        <pdtemp-pd-rpt-info v-if=\"isActived('supervise')\" @childFn=\"parentFn\"></pdtemp-pd-rpt-info>\r\n\r\n        <!--产品结构定义-->\r\n        <pay v-if=\"isActived('pay')\" @childFn=\"parentFn\"></pay>\r\n\r\n\r\n        <!--产品进度-->\r\n\r\n         <process v-if=\"isActived('detail')\" @childFn=\"parentFn\"></process>\r\n\r\n\r\n\r\n\r\n      </div>\r\n\r\n      <!--      <el-dialog  :visible.sync=\"showTemplate\" @close=\"showTemplate = false\">-->\r\n      <!--        <my-template></my-template>-->\r\n      <!--      </el-dialog>-->\r\n\r\n\r\n      <div class=\"footer-step\">\r\n        <el-button  size=\"small\" v-if=\"stepShow('back')\" icon=\"back_step\" type=\"text\" @click=\"clickStep('back')\" circle>\r\n          <svg-icon icon-class=\"back_step\"/>\r\n          {{$t('i18nView.upStep')}}\r\n        </el-button>\r\n        <el-button  size=\"small\" v-if=\"stepShow('next')\" icon=\"next_step\" type=\"text\"  @click=\"clickStep('next')\" circle>\r\n          <svg-icon icon-class=\"next_step\"/>\r\n          {{$t('i18nView.downStep')}}\r\n        </el-button>\r\n      </div>\r\n\r\n    </cus-wraper>\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\n\r\n  import pdtempPdRptInfo from '../product-definition/pd-framework-def/pd-regulatory-def/index'\r\n  import pdtempPdBaseDefinition from '../product-definition/pd-framework-def/pd-attr-def/base-attr-def/index'\r\n\r\n  import pay from '../product-definition/pd-framework-def/pd-structure-def/payment-def/index'\r\n\r\n  import process from '../basicinfo/pdprocess/index'\r\n\r\n  export default {\r\n    name: 'newstyle',\r\n    data() {\r\n      return {\r\n        isFixed: false,\r\n        offsetTop: 20,\r\n        quickicon: 'el-icon-arrow-down',\r\n        excelicon: 'el-icon-arrow-down',\r\n        quickshow: false,\r\n        excelshow: false,\r\n        currentPage: 'detail',\r\n        stepsAttr: {\r\n          attribute: [{ 'base': 1 }, { 'supervise': 2 }],\r\n          structure: [{ 'pay': 3 }, { 'payment': 4 },{\"scheme\":5},{\"account\":6}],\r\n          currprice: [{ 'curr1': 7 }, { 'curr2': 8 }],\r\n          process: [{ 'summary': 9 }, { 'detail': 10 }],\r\n        },\r\n        attrActive: 0,\r\n        struActive: -1,\r\n        currActive: -1,\r\n        procActive: -1,\r\n        activeName: 'attribute',\r\n        activedNames: ['attribute'],\r\n        showTemplate: false,\r\n        options: [{\r\n          value: '选项1',\r\n          label: 'true'\r\n        }, {\r\n          value: '选项2',\r\n          label: '2'\r\n        }, {\r\n          value: '选项3',\r\n          label: '3'\r\n        }, {\r\n          value: '选项4',\r\n          label: '4'\r\n        }, {\r\n          value: '选项5',\r\n          label: '5'\r\n        }],\r\n        form: {\r\n          referenceProducts: []\r\n        }\r\n      }\r\n    },\r\n    components: {\r\n      pdtempPdRptInfo,\r\n      pdtempPdBaseDefinition,\r\n      pay,\r\n      process\r\n    },\r\n    mounted() {\r\n      window.addEventListener('scroll', this.initHeight)\r\n      this.$nextTick(() => {\r\n        this.offsetTop = document.querySelector('#boxFixed').offsetTop\r\n      })\r\n    },\r\n    methods: {\r\n      clickStep(type){\r\n        let result =  this.findCurrPageIndex()\r\n        let index = result.currIndex\r\n        let nextIndex ;\r\n        if (type == 'back') {\r\n          nextIndex = index - 1;\r\n        }else {\r\n          nextIndex = index + 1;\r\n        }\r\n        for (let key in  this.stepsAttr){\r\n          let lists  =   this.stepsAttr[key]\r\n          for (let i = 0; i < lists.length; i++) {\r\n            let data = lists[i];\r\n            for (let key2 in data) {\r\n              let index =  data[key2]\r\n              if(nextIndex == index){\r\n                this.handleProduct(key,i,key2)\r\n                break\r\n              }\r\n            }\r\n          }\r\n        }\r\n      },\r\n      stepShow(type) {\r\n        let result =  this.findCurrPageIndex()\r\n        if (type == 'back') {\r\n          return   result.currIndex != 1\r\n        } else {\r\n          return   result.currIndex != result.total\r\n        }\r\n      },\r\n      findCurrPageIndex(){\r\n        let result = {'total' : 0, 'currIndex': 0 }\r\n        for (let key in  this.stepsAttr){\r\n           let lists  =   this.stepsAttr[key]\r\n           result.total = parseInt(result.total) + lists.length\r\n        }\r\n        for (let key in  this.stepsAttr){\r\n          let lists  =   this.stepsAttr[key]\r\n          for (let i = 0; i < lists.length; i++) {\r\n            let data = lists[i];\r\n            let currIndex =  data[this.currentPage]\r\n            if(!currIndex){continue;}\r\n            result.currIndex = currIndex\r\n            return result\r\n          }\r\n        }\r\n\r\n\r\n\r\n      },\r\n      isShowQuick(type) {\r\n        if (type == 'excel') {\r\n          this.quickshow = false\r\n          this.quickicon = 'el-icon-arrow-down'\r\n          if (!this.excelshow) {\r\n            this.excelicon = 'el-icon-arrow-up'\r\n          } else {\r\n            this.excelicon = 'el-icon-arrow-down'\r\n          }\r\n          return this.excelshow = !this.excelshow\r\n        } else {\r\n          this.excelshow = false\r\n          this.excelicon = 'el-icon-arrow-down'\r\n          if (!this.quickshow) {\r\n            this.quickicon = 'el-icon-arrow-up'\r\n          } else {\r\n            this.quickicon = 'el-icon-arrow-down'\r\n          }\r\n          return this.quickshow = !this.quickshow\r\n        }\r\n      },\r\n      handleProduct(type, index, page) {\r\n\r\n        this.currentPage = page\r\n        this.activeName = type\r\n        if (type == 'attribute') { //产品属性定义\r\n          this.attrActive = parseInt(index)\r\n        } else if (type == 'structure') { //产品结构定义\r\n          this.struActive = parseInt(index)\r\n        } else if (type == 'currprice') { //产品现价定义\r\n          this.currActive = parseInt(index)\r\n        } else if (type == 'process') { //产品进度\r\n          this.procActive = parseInt(index)\r\n        }\r\n      },\r\n      handleProductAttr(data, page) {\r\n        this.handleProduct('attribute', data, page)\r\n      }, handleProductStru(data, page) {\r\n        this.handleProduct('structure', data, page)\r\n      }, handleProductCurr(data,page) {\r\n        this.handleProduct('currprice', data, page)\r\n      }, handleProductProc(data,page) {\r\n        this.handleProduct('process', data, page)\r\n      },\r\n      parentFn(val) {\r\n        this.activeName = val\r\n      },\r\n      clickTab(val) {\r\n        let flag = false\r\n        this.activedNames.forEach(value => {\r\n          if (value == val.name) {\r\n            flag = true\r\n          }\r\n        })\r\n        if (flag == false) {\r\n          this.activedNames.push(val.name)\r\n        }\r\n      },\r\n      isActived(flag) {\r\n        return this.currentPage == flag\r\n      },\r\n      initHeight() {\r\n        let scrollTop = window.pageYOffset || document.documentElement.scrollTop || document.body.scrollTop\r\n        this.isFixed = scrollTop > this.offsetTop ? true : false\r\n      }\r\n    },\r\n    destroyed() {\r\n      window.removeEventListener('scroll', this.handleScroll)\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n\r\n  .collapseCus {\r\n    z-index: 99;\r\n    top: 20px;\r\n    right: 10px;\r\n    position: absolute;\r\n    border-top: none;\r\n    border-bottom: none;\r\n    margin-left: 50px;\r\n    width: 120px;\r\n\r\n    .is-active {\r\n      color: #fff;\r\n      background-color: #348CE7 !important;\r\n    }\r\n\r\n    .el-collapse-item__content {\r\n      padding-bottom: 0px !important;\r\n    }\r\n\r\n    .el-collapse-item__header {\r\n      background-color: #ebeef5;\r\n      height: 35px;\r\n      line-height: 35px;\r\n      /*margin-bottom: 10px;*/\r\n      /*border-radius: 4px;*/\r\n      /*border: 1px solid #ebeef5;*/\r\n      /*box-shadow: 0 2px 12px 0 rgba(0,0,0,.1);*/\r\n      font-size: 12px;\r\n      text-align: center;\r\n      display: block;\r\n      cursor: pointer;\r\n    }\r\n\r\n    .el-collapse-item__arrow {\r\n      margin-left: 10px;\r\n    }\r\n\r\n    .el-collapse-item__wrap {\r\n      border-bottom: none;\r\n    }\r\n  }\r\n\r\n\r\n  .stepDiv {\r\n\r\n    padding-top: 10px;\r\n\r\n    .el-step__icon-inner[class*=el-icon]:not(.is-status) {\r\n      font-size: 20px;\r\n    }\r\n\r\n    .el-step__title {\r\n      color: #000000;\r\n      background-color: #ebeef5;\r\n      font-weight: normal;\r\n      font-size: 10px;\r\n      /*border-radius: 4px;*/\r\n      border: 1px solid #ebeef5;\r\n      /*box-shadow: 0 2px 12px 0 rgba(0,0,0,.1);*/\r\n      text-align: center;\r\n      height: 24px;\r\n      cursor: pointer;\r\n    }\r\n\r\n    .el-step__main .is-finish {\r\n      color: #fff;\r\n      background-color: #348CE7 !important;\r\n    }\r\n  }\r\n\r\n\r\n  .container {\r\n    width: 90%;\r\n  }\r\n\r\n\r\n  .footer-step {\r\n    text-align: center;\r\n    position: absolute;\r\n    bottom: 0;\r\n    right: 45%;\r\n  }\r\n\r\n  .is_fixed {\r\n    position: fixed;\r\n    top: 20px;\r\n    z-index: 99;\r\n  }\r\n\r\n  .cusProcess{\r\n     .el-card{\r\n       margin-bottom: 5px;\r\n       width: 130px;\r\n       cursor: pointer;\r\n       border-radius: 8px;\r\n     }\r\n    .el-card__body{\r\n       padding: 12px;\r\n       text-align: center;\r\n    }\r\n  }\r\n\r\n</style>\r\n"]}]}