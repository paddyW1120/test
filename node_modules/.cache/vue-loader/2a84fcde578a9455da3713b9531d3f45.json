r\n    name: \"comboStopSelling\",\r\n    filters: {\r\n      capitalize:function(value){\r\n        switch(value) {\r\n          case '0':\r\n            return '全量发布'\r\n          case '1' :\r\n            return '套餐发布'\r\n          case 2 :\r\n            return '校验规则发布'\r\n          case 3 :\r\n            return '公式发布'\r\n          case 4 :\r\n            return '停售发布'\r\n        }\r\n      },\r\n      getPdAuditConc(state) {\r\n        if (state == 'N') {\r\n          state = '不通过'\r\n        } else {\r\n          state = '通过'\r\n        }\r\n        return state\r\n      },\r\n      filter_ppType(code) {\r\n        return _this.getCodeName('ppType', code)\r\n      }\r\n    },\r\n    data() {\r\n      _this = this;\r\n      return {\r\n        showStopDetailsFlag: false,\r\n        stopDateValue:'',\r\n        comboStopOptions:[],\r\n        comboPPOptions:[],\r\n        configs: [],\r\n        configs1: [],\r\n        configs2: [],\r\n        configs3: [],\r\n        showPageFlag:'',\r\n        showDetailsFlag:false,\r\n        pageFlag:'',\r\n        selectedDef: {},//表格的单选按钮\r\n        selectedDef1: {},// 初始化页面表格的单选按钮\r\n        dialogTableVisible1: false, /*审核记录*/\r\n        dialogTableVisible2: false, /*发布记录*/\r\n        dialogTableVisible3: false, /*新增停售*/\r\n        PageMark:{\r\n          showPageTitle:false,\r\n          showPageFlag:'1'\r\n        },\r\n        comboInfo: {},\r\n        addNewFrom: {\r\n          pdVerId:'',\r\n          ppCode:'',\r\n          ppName:'',\r\n          ppType:'',\r\n          pdProcessId:'',\r\n          comboStopDate:'',\r\n          comboStopReason:''\r\n        },\r\n        updateFrom: {\r\n          pdVerId:'',\r\n          ppCode:'',\r\n          ppName:'',\r\n          ppType:'',\r\n          pdProcessId:'',\r\n          comboStopDate:'',\r\n          comboStopReason:''\r\n        },\r\n        params: {\r\n          pdCode: '',\r\n          stopStartDate: '',\r\n          stopEndDate: ''\r\n        },\r\n        comboStopSellingForm: {\r\n          ppCode:'',\r\n          ppType:''\r\n        },\r\n        comboStopDetailsForm: {\r\n          ppCode: '',\r\n          ppName: '',\r\n          stopDate: '',\r\n          stopReason: '',\r\n        },\r\n        pdStopSelForm: {\r\n          stopDate: '',\r\n          stopReason: ''\r\n        },\r\n        pickerOptions: {\r\n          shortcuts: [{\r\n            text: '最近一周',\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 7);\r\n              picker.$emit('pick', [start, end]);\r\n            }\r\n          }, {\r\n            text: '最近一个月',\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 30);\r\n              picker.$emit('pick', [start, end]);\r\n            }\r\n          }, {\r\n            text: '最近三个月',\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 90);\r\n              picker.$emit('pick', [start, end]);\r\n            }\r\n          }]\r\n        },\r\n        rules: {\r\n          stopDate: [{required: true, message: this.$t('comboStopSelling.stopDateRules'), trigger: 'change'}],\r\n          stopReason: [{required: true, message: this.$t('comboStopSelling.opinionRules'), trigger: 'blur'}]\r\n        },\r\n        details_Rules: {\r\n          stopDate: [{required: true, message: this.$t('comboStopSelling.stopDateRules'), trigger: 'change'}],\r\n          stopReason: [{required: true, message: this.$t('comboStopSelling.opinionRules'), trigger: 'blur'}]\r\n        }\r\n      }\r\n    },\r\n    created() {\r\n      this.initSelect()\r\n      let comboInfo = this.$route.query.comboInfo\r\n      let pageFlag = this.$route.query.pageFlag\r\n      if(comboInfo.id!=undefined &&comboInfo.id!=''){\r\n        if(pageFlag=='1'){\r\n          this.selectedDef1=comboInfo\r\n          this.queryRow(comboInfo)\r\n        }\r\n       else if(pageFlag=='2'){\r\n          this.selectedDef=comboInfo\r\n          this.showDetailsFlag=true\r\n          this.newAddDiscontinuation()\r\n          this.findOne(comboInfo)\r\n        }\r\n      }\r\n    },\r\n    watch: {\r\n      'configs'(){\r\n        this.comboStopOptions=this.configs\r\n      },\r\n      stopDateValue(val) {\r\n        if (val) {\r\n          this.params.stopStartDate = val[0];\r\n          this.params.stopEndDate = val[1]\r\n        } else {\r\n          this.params.stopStartDate = '';\r\n          this.params.stopEndDate = ''\r\n        }\r\n      }\r\n    },\r\n    methods: {\r\n      //根据key值查value\r\n      filterHandler(codetype, value) {\r\n        return this.getCodeName(codetype, value);\r\n      },\r\n      queryTableData() {\r\n        this.$refs.page_comboAuditBack.reload();\r\n      },\r\n      showDetailsPage(){\r\n        this.showStopDetailsFlag = false;\r\n        this.selectedDef1={}\r\n      },\r\n      /*新增停售*/\r\n      newAddDiscontinuation() {\r\n        this.dialogTableVisible3 = true\r\n        this.pageFlag='2'\r\n      },\r\n      /*审核记录*/\r\n      auditRecord(row) {\r\n        this.dialogTableVisible1 = true;\r\n        let params={\r\n          \"pdInfoId\":row.id,\r\n          \"pdVerId\":row.pdVerId\r\n        };\r\n        queryAudit(this.buildRequestData(params)).then(res => {\r\n          this.configs2 = res.data\r\n        })\r\n      },\r\n      /*发布记录*/\r\n      releaseRecord(row) {\r\n        this.dialogTableVisible2 = true;\r\n        let params={\r\n          \"pdInfoId\":row.id,\r\n          \"pdVerId\":row.pdVerId\r\n        };\r\n        queryRelease(this.buildRequestData(params)).then(res => {\r\n          this.configs3 = res.data\r\n          console.log(this.configs3)\r\n        })\r\n      },\r\n      /*初始化*/\r\n      initSelect() {\r\n        queryData(this.buildRequestData({})).then(res => {\r\n          this.comboPPOptions =res.data\r\n        })\r\n      },\r\n      /*查询表格数据*/\r\n      queryTable() {\r\n        this.$refs.page_combo.reload()\r\n      },\r\n      /*页面初始化表格选中当前行*/\r\n      queryRow(row) {\r\n        this.comboInfo=row\r\n        this.showStopDetailsFlag = true;\r\n        this.comboStopDetailsForm.ppCode = row.ppCode;\r\n        this.comboStopDetailsForm.ppName = row.ppName;\r\n        this.comboStopDetailsForm.stopDate = row.comboStopDate;\r\n        this.comboStopDetailsForm.stopReason = row.comboStopReason;\r\n        this.updateFrom = row\r\n        this.pageFlag='1'\r\n      },\r\n      /*取消表格选择*/\r\n      rowClickUpdate(row, column, event) {\r\n        if (event.target.nodeName === \"INPUT\") {\r\n          if (row.id === this.selectedDef1.id) {\r\n            this.selectedDef1 = {};\r\n            this.showStopDetailsFlag = false\r\n          }\r\n        }\r\n      },\r\n      /*获取当前行数据*/\r\n      findOne(row) {\r\n        this.comboInfo=row\r\n        this.addNewFrom=row\r\n      },\r\n      /*取消表格选择*/\r\n      rowClickNewAdd(row, column, event) {\r\n        if (event.target.nodeName === \"INPUT\") {\r\n          if (row.id === this.selectedDef.id) {\r\n            this.selectedDef = {};\r\n            this.showDetailsFlag=false\r\n          }else{\r\n            this.showDetailsFlag=true\r\n          }\r\n        }\r\n      },\r\n      //保存按钮 工作流扭转到审核\r\n      saveDiscontinuation() {\r\n        this.$refs.pdStopSelForm.validate(valid => {\r\n          if (valid) {\r\n            if (Object.keys(this.selectedDef).length === 0) {\r\n              this.notifyError(this.$t('comboStopSelling.pleaseSelectPd'))\r\n              return\r\n            }\r\n            this.addNewFrom.comboStopDate = this.pdStopSelForm.stopDate;\r\n            this.addNewFrom.comboStopReason = this.pdStopSelForm.stopReason;\r\n            let map = {\r\n              \"comboStopCellVo\": this.addNewFrom,\r\n            };\r\n            addNew(this.buildRequestData(map)).then(res => {\r\n              if (res.code == 200) {\r\n                this.notifySuccess(res.message);\r\n                this.$refs.page_combo.reload();\r\n                this.dialogTableVisible3=false\r\n              }\r\n            })\r\n          }\r\n        })\r\n      },\r\n      refresh() {\r\n        this.pdStopSelForm.stopDate = '';\r\n        this.pdStopSelForm.stopReason = '';\r\n        //this.selectedDef={};\r\n        this.$nextTick(() => {\r\n          this.$refs.pdStopSelForm.clearValidate()\r\n        })\r\n      },\r\n      refreshStopDetails() {\r\n        this.comboStopDetailsForm.stopDate = '';\r\n        this.comboStopDetailsForm.stopReason = '';\r\n        this.$nextTick(() => {\r\n          this.$refs.comboStopDetailsForm.clearValidate()\r\n        })\r\n      },\r\n      saveUpdate() {\r\n        this.$refs.comboStopDetailsForm.validate(valid => {\r\n          if (valid) {\r\n            this.updateFrom.comboStopDate = this.comboStopDetailsForm.stopDate;\r\n            this.updateFrom.comboStopReason = this.comboStopDetailsForm.stopReason\r\n            let map = {\r\n              \"comboStopCellVo\": this.updateFrom,\r\n            };\r\n            update(this.buildRequestData(map)).then(res => {\r\n              if (res.code == 200) {\r\n                this.notifySuccess(res.message);\r\n                this.$refs.page_comboAuditBack.reload();\r\n                this.showStopDetailsFlag = false;\r\n              }\r\n            })\r\n          }\r\n        })\r\n      },\r\n      //页面跳转按钮\r\n      getPdDesignPage() {\r\n        if (this.comboInfo) {\r\n          this.$router.push({name: '产品套餐框架定义主页', query: {showPageFlag: '1',pageFlag:this.pageFlag},params:this.comboInfo})\r\n        }\r\n      },\r\n      getPdSalePage() {\r\n        if (this.comboInfo) {\r\n          this.$router.push({name: '产品套餐销售定义主页', query: {showPageFlag: '1',pageFlag:this.pageFlag},params:this.comboInfo})\r\n        }\r\n      },\r\n      getPdUWPage() {\r\n        if (this.comboInfo) {\r\n          this.$router.push({name: '产品套餐承保定义主页', query: {showPageFlag: '1',pageFlag:this.pageFlag},params:this.comboInfo})\r\n        }\r\n      },\r\n      getPdRWPage() {\r\n        if (this.comboInfo) {\r\n          this.$router.push({name: '产品套餐核保定义主页', query: {showPageFlag: '1',pageFlag:this.pageFlag},params:this.comboInfo})\r\n        }\r\n      },\r\n      getPdPVPage() {\r\n        if (this.comboInfo) {\r\n          this.$router.push({name: '产品套餐核赔定义主页', query: {showPageFlag: '1',pageFlag:this.pageFlag},params:this.comboInfo})\r\n        }\r\n      },\r\n      getTestPage() {\r\n        if (this.comboInfo) {\r\n          //this.$router.push({name: '', query: {showPageFlag: '1'}})\r\n        }\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n</style>\r\n\r\n\r\n"]}]}