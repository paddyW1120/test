r\n\r\n        <el-collapse  accordion class=\"collapseCus\"  v-model=\"activeName\">\r\n          <el-collapse-item v-for=\"(item1,i) in this.statusData.children\" :key=\"i\" :name=\"item1.moduleCode\" :title=\"item1.moduleName\">\r\n            <div class=\"stepDiv\" :style=\"{height: item1.children.length*32 +'px'}\">\r\n              <el-steps :active=\"activeTag[i]\" direction=\"vertical\" finish-status=\"wait\" process-status=\"finish \">\r\n                <el-step v-for=\"(item2,j) in item1.children\" :key=\"j\" @click.native=\"handleProduct(item1.moduleCode,j,item2.moduleCode)\" icon=\"el-icon-info\" :title=\"item2.moduleName\"></el-step>\r\n              </el-steps>\r\n            </div>\r\n          </el-collapse-item>\r\n          <el-collapse-item name=\"process\" title=\"产品定义进度\">\r\n            <div class=\"stepDiv\" style=\"height: 80px;\">\r\n              <el-steps :active=\"procActive\" direction=\"vertical\" finish-status=\"wait\" process-status=\"finish \">\r\n                <el-step @click.native=\"handleProductProc(0,'detail')\" icon=\"el-icon-info\" title=\"进度详情\"></el-step>\r\n              </el-steps>\r\n            </div>\r\n          </el-collapse-item>\r\n        </el-collapse>\r\n\r\n      </van-sticky>\r\n\r\n\r\n\r\n\r\n      <div class=\"product-container\">\r\n        <component v-if=\"currentPage!='detail'\" @childFn=\"parentFn\" :is=\"componentName\"  @hook:mounted=\"finishPage\"/>\r\n\r\n        <process @childFn=\"parentFn\" v-if=\"currentPage=='detail'\" :pdProcessId=\"workflow.processInstanceId\" :pdInfoShow=\"false\":operationalShow=\"false\" :testShow=\"false\"  :checkShow=\"false\"  :releaseShow=\"false\"></process>\r\n\r\n      </div>\r\n      <div class=\"footer-step\">\r\n        <el-button  size=\"small\" @click=\"backToApply()\" circle icon=\"el-icon-s-home\" type=\"text\" >\r\n          {{$t('route.dashboard') }}\r\n        </el-button>\r\n        <el-button  size=\"small\" @click=\"clickStep('back')\" circle icon=\"back_step\" type=\"text\" v-if=\"stepShow('back')\">\r\n          <svg-icon icon-class=\"back_step\" />\r\n          {{$t('others.previous') }}\r\n        </el-button>\r\n        <el-button  size=\"small\" @click=\"clickStep('next')\" circle icon=\"next_step\" type=\"text\" v-if=\"stepShow('next')\">\r\n          <svg-icon icon-class=\"next_step\" />\r\n          {{$t('others.nextStep') }}\r\n        </el-button>\r\n      </div>\r\n\r\n\r\n\r\n        <div class=\"collapseCus_finishDef is_fixed_finishDef\" v-if=\"showPageFlagFromRelease !== '1'\">\r\n          <el-form>\r\n            <el-form-item :label=\"$t('productMenu.node_selection')\">\r\n              <el-select :disabled=\"showPageFlag\" clearable filterable style=\"width: 150px;margin-bottom: 10px\" v-model=\"workflow.selectValue\">\r\n                <el-option :disabled=\"item.disabled\" :key=\"item.key\" :label=\"item.label\" :value=\"item.key\" v-for=\"item in workflow.options\"></el-option>\r\n              </el-select>\r\n              <el-button  size=\"small\" :disabled=\"showPageFlag\" @click=\"finishProductInfoDef\"  dialog-disable=\"disable\" style=\"width: 150px\" type=\"primary\">\r\n                <svg-icon icon-class=\"end\"></svg-icon> {{$t('productMenu.nextWorkFlow')}}\r\n              </el-button>\r\n            </el-form-item>\r\n          </el-form>\r\n        </div>\r\n\r\n\r\n    </cus-wraper>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { finishProductInfoDef } from \"@/api/workflow/workflow\";\r\nimport {getPdInfoModule} from '@/api/basicinfo/pdtempPdInfo'\r\nimport { closeVisitTag } from \"@/utils/tagsView\";\r\nimport process from \"@/views/basicinfo/pdprocess/index\";\r\nimport { mapGetters } from \"vuex\";\r\n\r\nimport Vue from 'vue'\r\nimport { Sticky } from 'vant';\r\nVue.use(Sticky);\r\n\r\n\r\nexport default {\r\n  name: \"ProductMain\",\r\n  computed: {\r\n    ...mapGetters([\"usercode\", \"tenantCode\"])\r\n  },\r\n  data() {\r\n    return {\r\n      pdInfo: {},\r\n      statusData: {},\r\n      componentName: \"\",\r\n      workflow: {\r\n        processInstanceId: \"\",\r\n        selectValue: \"productOperatingFlag\",\r\n        options: [\r\n          { key: \"productOperatingFlag\", label: \"产品营运定义\" },\r\n          { key: \"productTestFlag\", label: \"产品测试\" }\r\n          // {'key': 'productAuditFlag', 'label': '产品审核'}\r\n        ]\r\n      },\r\n      isSaved: false,\r\n      isFixed: false,\r\n      offsetTop: 20,\r\n      currentPage: \"base\",\r\n      stepsAttr: {\r\n        //产品框架定义步骤\r\n        attribute: [{ base: 1 }, { productaddtax: 2 }],\r\n        structure: [\r\n          { pay: 3 },\r\n          { payment: 4 },\r\n          { programme: 5 },\r\n          { inv: 6 },\r\n          { account: 7 }\r\n        ],\r\n        price: [{ price1: 8 }],\r\n        supervisedef: [{ superviseattr: 9 }],\r\n        process: [/*{summary: 9}, */ { detail: 10 }]\r\n      },\r\n      activeTag: [0, -1, -1, -1],\r\n      nowPosition: \"attribute-base\",\r\n      procActive: -1,\r\n      activeName: \"attribute\",\r\n      showPageFlagFromRelease: '0',\r\n      showPageFlag: false,\r\n      showTemplate: false\r\n    };\r\n  },\r\n  components: {\r\n    process\r\n  },\r\n  mounted() {\r\n\r\n  },\r\n  beforeDestroy() {\r\n    this.cacheStrData(\"showPageFlag\", \"0\");\r\n  },\r\n  async created() {\r\n    this.pdInfo = this.getPdData();\r\n    this.showPageFlagFromRelease = this.$route.query.showPageFlag\r\n    if (this.getStrData(\"showPageFlag\") === \"1\") {\r\n      this.showPageFlag = true;\r\n    }\r\n    this.workflow.processInstanceId = this.pdInfo.pdProcessId;\r\n    this.buildWorkOptions(this.pdInfo);\r\n    await this.checkProcess(\"pdInfo\");\r\n    this.switchComponent(\"base\");\r\n  },\r\n  methods: {\r\n    finishPage(){\r\n        this.$emit('finish')\r\n    },\r\n    async checkProcess(type) {\r\n      await getPdInfoModule(\r\n        this.buildRequestData({ moduleNode: type })\r\n      ).then(res => {\r\n        this.statusData = res.data[0];\r\n      });\r\n    },\r\n    buildWorkOptions(data) {\r\n      let backFlag = data.backFlag;\r\n      for (let i = 0; i < this.workflow.options.length; i++) {\r\n        if (\r\n          backFlag === \"1\" &&\r\n          this.workflow.options[i].key !== \"productOperatingFlag\"\r\n        ) {\r\n          this.workflow.options[i].disabled = true;\r\n        }\r\n      }\r\n    },\r\n    finishProductInfoDef() {\r\n      if (this.workflow.selectValue === \"\") {\r\n        this.messageError(this.$t(\"productMenu.tipError\"));\r\n        return;\r\n      }\r\n      this.$confirm(\r\n        this.$t(\"productMenu.warnMsg\"),\r\n        this.$t(\"productMenu.messageTip\"),\r\n        {\r\n          confirmButtonText: this.$t(\"productMenu.warnMsgButton2\"),\r\n          cancelButtonText: this.$t(\"productMenu.warnMsgButton1\"),\r\n          type: \"warning\"\r\n        }\r\n      ).then(() => {\r\n        let data = {\r\n          userCode: this.usercode,\r\n          productOperatingFlag: \"1\",\r\n          productTestFlag: \"1\",\r\n          processInstanceId: this.workflow.processInstanceId\r\n        };\r\n        data[this.workflow.selectValue] = \"0\";\r\n        finishProductInfoDef(this.buildRequestData(data)).then(res => {\r\n          //更新产品缓存\r\n          this.$store.dispatch('product/storageProducts')\r\n          this.notifySuccess(\"success\");\r\n          closeVisitTag();\r\n          this.backToApply();\r\n        });\r\n      });\r\n    },\r\n    backToApply(){\r\n      closeVisitTag()\r\n      this.$router.push({ path: \"/product-definition/pd-framework-def/apply\" });\r\n    },\r\n    clickStep(type) {\r\n      let result = this.findCurrPageIndex();\r\n      let index = result.currIndex;\r\n      let nextIndex;\r\n      if (type === \"back\") {\r\n        nextIndex = index - 1;\r\n      } else {\r\n        nextIndex = index + 1;\r\n      }\r\n      for (let key in this.stepsAttr) {\r\n        let lists = this.stepsAttr[key];\r\n        for (let i = 0; i < lists.length; i++) {\r\n          let data = lists[i];\r\n          for (let key2 in data) {\r\n            let index = data[key2];\r\n            if (nextIndex === index) {\r\n              this.handleProduct(key, i, key2);\r\n              break;\r\n            }\r\n          }\r\n        }\r\n      }\r\n    },\r\n    stepShow(type) {\r\n      let result = this.findCurrPageIndex();\r\n      if (type === \"back\") {\r\n        return result.currIndex !== 1;\r\n      } else {\r\n        return result.currIndex !== result.total;\r\n      }\r\n    },\r\n    findCurrPageIndex() {\r\n      let result = { total: 0, currIndex: 0 };\r\n      for (let key in this.stepsAttr) {\r\n        let lists = this.stepsAttr[key];\r\n        result.total = parseInt(result.total) + lists.length;\r\n      }\r\n      for (let key in this.stepsAttr) {\r\n        let lists = this.stepsAttr[key];\r\n        for (let i = 0; i < lists.length; i++) {\r\n          let data = lists[i];\r\n          let currIndex = data[this.currentPage];\r\n          if (!currIndex) {\r\n            continue;\r\n          }\r\n          result.currIndex = currIndex;\r\n          return result;\r\n        }\r\n      }\r\n    },\r\n    handleProduct(type, index, page) {\r\n      if (this.nowPosition != type + \"-\" + page) {\r\n        if (this.isSaved || this.showPageFlag === true) {\r\n          this.nowPosition = type + \"-\" + page;\r\n          this.isSaved = false;\r\n          this.currentPage = page;\r\n          this.activeName = type;\r\n          this.switchComponent(page);\r\n          if (type === \"attribute\") {\r\n            //产品属性定义\r\n            this.activeTag[0] = parseInt(index);\r\n            this.cleanOthersActive(\"attribute\");\r\n          } else if (type === \"structure\") {\r\n            //产品结构定义\r\n            this.activeTag[1] = parseInt(index);\r\n            this.cleanOthersActive(\"structure\");\r\n          } else if (type === \"price\") {\r\n            //产品现价定义\r\n            this.activeTag[2] = parseInt(index);\r\n            this.cleanOthersActive(\"price\");\r\n          } else if (type === \"supervisedef\") {\r\n            //产品监管定义\r\n            this.activeTag[3] = parseInt(index);\r\n            this.cleanOthersActive(\"supervisedef\");\r\n          } else if (type === \"process\") {\r\n            //产品进度\r\n            this.procActive = parseInt(index);\r\n            this.cleanOthersActive(\"process\");\r\n          }\r\n        } else {\r\n          this.$confirm(\r\n            this.$t(\"productMenu.messageTitle\"),\r\n            this.$t(\"productMenu.messageTip\"),\r\n            {\r\n              confirmButtonText: this.$t(\"productMenu.messageButton2\"),\r\n              cancelButtonText: this.$t(\"productMenu.messageButton1\"),\r\n              type: \"warning\"\r\n            }\r\n          )\r\n            .then(() => {\r\n              this.nowPosition = type + \"-\" + page;\r\n              this.currentPage = page;\r\n              this.activeName = type;\r\n              if (type === \"attribute\") {\r\n                //产品属性定义\r\n                this.activeTag[0] = parseInt(index);\r\n                this.cleanOthersActive(\"attribute\");\r\n              } else if (type === \"structure\") {\r\n                //产品结构定义\r\n                this.activeTag[1] = parseInt(index);\r\n                this.cleanOthersActive(\"structure\");\r\n              } else if (type === \"price\") {\r\n                //产品现价定义\r\n                this.activeTag[2] = parseInt(index);\r\n                this.cleanOthersActive(\"price\");\r\n              } else if (type === \"supervisedef\") {\r\n                //产品监管定义\r\n                this.activeTag[3] = parseInt(index);\r\n                this.cleanOthersActive(\"supervisedef\");\r\n              } else if (type === \"process\") {\r\n                //产品进度\r\n                this.procActive = parseInt(index);\r\n                this.cleanOthersActive(\"process\");\r\n              }\r\n              window.scrollTo(0, 0);\r\n              this.switchComponent(page);\r\n            })\r\n            .catch(() => {});\r\n        }\r\n      }\r\n    },\r\n    handleProductProc(data, page) {\r\n      this.handleProduct(\"process\", data, page);\r\n    },\r\n    cleanOthersActive(acitve) {\r\n      if (acitve === \"attribute\") {\r\n        this.activeTag[1] = -1;\r\n        this.activeTag[2] = -1;\r\n        this.activeTag[3] = -1;\r\n        this.procActive = -1;\r\n      }\r\n      if (acitve === \"structure\") {\r\n        this.activeTag[0] = -1;\r\n        this.activeTag[2] = -1;\r\n        this.activeTag[3] = -1;\r\n        this.procActive = -1;\r\n      }\r\n      if (acitve === \"price\") {\r\n        this.activeTag[0] = -1;\r\n        this.activeTag[1] = -1;\r\n        this.activeTag[3] = -1;\r\n        this.procActive = -1;\r\n      }\r\n      if (acitve === \"supervisedef\") {\r\n        this.activeTag[0] = -1;\r\n        this.activeTag[1] = -1;\r\n        this.activeTag[2] = -1;\r\n        this.procActive = -1;\r\n      }\r\n      if (acitve === \"process\") {\r\n        this.activeTag = [-1, -1, -1, -1];\r\n      }\r\n    },\r\n    parentFn(val) {\r\n      this.isSaved = val;\r\n    },\r\n    switchComponent(page) {\r\n      const tenantCode = this.tenantCode;\r\n      if (this.statusData) {\r\n        this.statusData.children.forEach(val1 => {\r\n          val1.children.forEach(val2 => {\r\n            if (val2.moduleCode == page) {\r\n              const path = val2.moduleLocation + \"/\" + tenantCode + \"/index\";\r\n              let path1 = this.isInAllVue(path);\r\n              // this.componentName = () => import(`@/views/${path1}`)\r\n              this.componentName = resolve =>\r\n                require.ensure([], () => resolve(require(`@/views/${path1}`)));\r\n            }\r\n          });\r\n        });\r\n      }\r\n    }\r\n  },\r\n  destroyed() {\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n\r\n.product-container {\r\n  width: calc(100% - 130px);\r\n}\r\n\r\n.collapseCus_finishDef {\r\n  z-index: 99;\r\n  bottom: 20px;\r\n  right: 0px;\r\n  position: absolute;\r\n  border-top: none;\r\n  border-bottom: none;\r\n  margin-left: 50px;\r\n  width: 140px;\r\n  transform: scale(0.85);\r\n}\r\n\r\n\r\n\r\n.is_fixed_finishDef {\r\n  position: fixed;\r\n  bottom: 1px;\r\n  z-index: 99;\r\n}\r\n\r\n\r\n\r\n</style>\r\n"]}]}