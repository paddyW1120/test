\n            </el-select>\r\n          </el-form-item>\r\n        </cus-col>\r\n        <cus-condition @show=\"showMore=!showMore\"></cus-condition>\r\n      </el-row>\r\n      <div>\r\n        <el-collapse-transition>\r\n          <div v-if=\"showMore\">\r\n            <el-row :gutter=\"10\">\r\n              <cus-col>\r\n                <el-form-item :label=\"$t('productSelect.pdApplyDate')\">\r\n                  <el-date-picker v-model=\"applyDateValue\" type=\"daterange\" unlink-panels\r\n                                  value-format=\"yyyy-MM-dd\"\r\n                                  :range-separator=\"$t('productSelect.to')\"\r\n                                  :start-placeholder=\"$t('productSelect.productApplyStartDate')\"\r\n                                  :end-placeholder=\"$t('productSelect.productApplyEndDate')\"\r\n                                  :picker-options=\"pickerOptions\">\r\n                  </el-date-picker>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col>\r\n                <el-form-item :label=\"$t('productSelect.pdProcess')\">\r\n                  <el-select clearable filterable v-model=\"params.pdProcess\">\r\n                    <el-option v-for=\"pdProcess in pdProcessCode\"\r\n                          :key=\"pdProcess.text\" :label=\"pdProcess.text\" :value=\"pdProcess.value\"></el-option>\r\n                  </el-select>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col>\r\n                <el-form-item :label=\"$t('productSelect.defType')\">\r\n                  <cus-select v-model=\"params.defType\" clearable filterable :codetype=\"'pdAuditType'\"></cus-select>\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n            <el-row>\r\n              <cus-col>\r\n                <el-form-item :label=\"$t('productSelect.missionDate')\">\r\n                  <el-date-picker v-model=\"missionDateValue\" type=\"daterange\" unlink-panels\r\n                                  :range-separator=\"$t('productSelect.to')\" value-format=\"yyyy-MM-dd\"\r\n                                  :start-placeholder=\"$t('productSelect.productApplyStartDate')\"\r\n                                  :end-placeholder=\"$t('productSelect.productApplyEndDate')\"\r\n                                  :picker-options=\"pickerOptions\">\r\n                  </el-date-picker>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col>\r\n                <el-form-item :label=\"$t('productSelect.pdRelDate')\">\r\n                  <el-date-picker v-model=\"releaseDateValue\" type=\"daterange\" unlink-panels\r\n                                  :range-separator=\"$t('productSelect.to')\" value-format=\"yyyy-MM-dd\"\r\n                                  :start-placeholder=\"$t('productSelect.productApplyStartDate')\"\r\n                                  :end-placeholder=\"$t('productSelect.productApplyEndDate')\"\r\n                                  :picker-options=\"pickerOptions\">\r\n                  </el-date-picker>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col>\r\n                <el-form-item :label=\"$t('productSelect.pdDisDate')\">\r\n                  <el-date-picker v-model=\"stopDateValue\" type=\"daterange\" unlink-panels\r\n                                  :range-separator=\"$t('productSelect.to')\" value-format=\"yyyy-MM-dd\"\r\n                                  :start-placeholder=\"$t('productSelect.productApplyStartDate')\"\r\n                                  :end-placeholder=\"$t('productSelect.productApplyEndDate')\"\r\n                                  :picker-options=\"pickerOptions\">\r\n                  </el-date-picker>\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n          </div>\r\n        </el-collapse-transition>\r\n      </div>\r\n      <el-row >\r\n        <cus-col :span=\"24\">\r\n            <el-button style=\"float: right\"  size=\"small\" type=\"primary\"  icon=\"el-icon-search\" @click=\"doSearch\">\r\n              {{$t('productSelect.search')}}\r\n            </el-button>\r\n        </cus-col>\r\n      </el-row>\r\n    </el-form>\r\n    <cus-table-title :label=\"$t('productSelect.productSelect')\"></cus-table-title>\r\n    <el-table :data=\"productSelectTable\" size=\"mini\" @row-click=\"rowClick_Operation\" highlight-current-row\r\n              fit element-loading-text=\"Loading\" border align=\"center\" class=\"wraper-table\">\r\n      <el-table-column width=\"50\" :label=\"$t('productSelect.selection')\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-radio-group v-model=\"selectOperation\">\r\n            <el-radio :label=\"scope.row.id\">{{''}}</el-radio>\r\n          </el-radio-group>\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <el-table-column :label=\"$t('table.id')\" type=\"index\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('productSelect.pdDetailType')\" prop=\"cType\" align=\"center\">\r\n        <template v-slot=\"scope\">{{scope.row.cType | filterPdCategory}}</template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.pdCode')\" prop=\"pdCode\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.pdName')\" prop=\"pdName\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('customComponents.pdEdition')\" prop=\"pdEdition\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('productSelect.pdProcess')\" prop=\"pdProcess\" align=\"center\"\r\n                       :filters=\"[{text: '定义中', value: '1'}, {text: '测试中', value: '2'},\r\n                       {text: '审核中', value: '3'}, {text: '待发布', value: '4'}, {text: '已发布', value: '5'}, {text: '已停售', value: '6'},{value:'7', text:'规则订正中'},{value:'8', text:'公式订正中'}]\"\r\n                       :filter-method=\"filterPdProcessHandler\" column-key=\"pdProcess\" >\r\n        <template v-slot=\"scope\">{{scope.row.pdProcess | filter_pdProcess}}</template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('productSelect.defType')\" prop=\"defType\" align=\"center\"\r\n                       :filters=\"[{text: '新产品', value: '1'}, {text: '订正产品', value: '2'}, {text: '停售产品', value: '3'}]\"\r\n                       :filter-method=\"filterPdTypeHandler\" column-key=\"defType\" >\r\n        <template v-slot=\"scope\">{{filterHandler('pdAuditType',scope.row.defType )}}</template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('productSelect.pdApplyDate')\" prop=\"pdApplDate\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('productSelect.pdDisDate')\" prop=\"pdStopDate\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('productSelect.operator')\" prop=\"operator\" align=\"center\"></el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <cus-pagination ref=\"dataPage\" :url=\"url\" :pageSize=\"10\" :params=\"params\" v-model=\"productSelectTable\" :autoBuild=\"false\"/>\r\n    <br>\r\n    <!--产品营运定义进度-->\r\n    <div v-if=\"showOperationProgress\">\r\n\r\n\r\n      <!--产品框架定义详情-->\r\n      <cus-form-title  :label=\"$t('productSelect.pdInfo')\"/>\r\n      <div class=\"cusSubCard cusProcess\">\r\n        <div class=\"sub\" v-for=\"item1 in  this.statusData.children\">\r\n          <el-tooltip effect=\"light\" :content=\"$t('productSelect.processTip')\"\r\n                      placement=\"top-end\">\r\n            <div class=\"wraper\" @click=\"handleGoPage('产品框架定义')\">\r\n              <div class=\"loss\" :class=\"{'loss_no' : item1.moduleStatus ==='0'}\"></div>\r\n              <div class=\"tag\" :class=\"{'tag_no' : item1.moduleStatus ==='0'}\">{{item1.moduleName}}</div>\r\n              <div class=\"triangle\" :class=\"{'triangle_no' : item1.moduleStatus ==='0'}\"></div>\r\n            </div>\r\n          </el-tooltip>\r\n          <div style=\"margin-top: 23px\">\r\n            <el-steps direction=\"vertical\" process-status=\"success\" v-for=\"item2 in  item1.children\">\r\n              <el-step :icon=\"item2.moduleStatus=='1'?'el-icon-circle-check': 'el-icon-circle-close'\"\r\n                       :status=\"item2.moduleStatus=='1'? 'finish' : 'wait'\" :title=\"item2.moduleName\"></el-step>\r\n            </el-steps>\r\n          </div>\r\n        </div>\r\n\r\n      </div>\r\n\r\n\r\n      <!--产品营运定义详情-->\r\n      <cus-form-title style=\"clear: both\" :label=\"$t('productSelect.operational')\"/>\r\n      <div class=\"cusSubCard cusProcess\">\r\n        <div class=\"sub\" v-for=\"item1 in  this.statusData2.children\">\r\n          <el-tooltip effect=\"light\" :content=\"$t('productSelect.processTip')\"\r\n                      placement=\"top-end\">\r\n            <div class=\"wraper\" @click=\"handleGoPage(item1.moduleName)\">\r\n              <div class=\"loss\" :class=\"{'loss_no' : item1.moduleStatus ==='0'}\"></div>\r\n              <div class=\"tag\" :class=\"{'tag_no' : item1.moduleStatus ==='0'}\">{{item1.moduleName}}</div>\r\n              <div class=\"triangle\" :class=\"{'triangle_no' : item1.moduleStatus ==='0'}\"></div>\r\n            </div>\r\n          </el-tooltip>\r\n          <div style=\"margin-top: 23px\">\r\n            <el-steps direction=\"vertical\" process-status=\"success\" v-for=\"item2 in  item1.children\">\r\n              <el-step :icon=\"item2.moduleStatus=='1'?'el-icon-circle-check': 'el-icon-circle-close'\"\r\n                       :status=\"item2.moduleStatus=='1'? 'finish' : 'wait'\" :title=\"item2.moduleName\"></el-step>\r\n            </el-steps>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!--产品测试详情-->\r\n      <cus-form-title style=\"clear: both\" :label=\"$t('productSelect.test')\"/>\r\n      <div class=\"cusSubCard cusProcess\">\r\n        <div class=\"sub\" v-for=\"item1 in  this.statusData3.children\">\r\n          <el-tooltip effect=\"light\" :content=\"$t('productSelect.processTip')\"\r\n                      placement=\"top-end\">\r\n            <div class=\"wraper\" @click=\"handleGoPage3(item1.moduleName)\">\r\n              <div class=\"loss\" :class=\"{'loss_no' : item1.moduleStatus ==='0'}\"></div>\r\n              <div class=\"tag\" :class=\"{'tag_no' : item1.moduleStatus ==='0'}\">{{item1.moduleName}}</div>\r\n              <div class=\"triangle\" :class=\"{'triangle_no' : item1.moduleStatus ==='0'}\"></div>\r\n            </div>\r\n          </el-tooltip>\r\n          <div style=\"margin-top: 23px\">\r\n            <el-steps direction=\"vertical\" process-status=\"success\" v-for=\"item2 in  item1.children\">\r\n              <el-step :icon=\"item2.moduleStatus=='1'?'el-icon-circle-check': 'el-icon-circle-close'\"\r\n                       :status=\"item2.moduleStatus=='1'? 'finish' : 'wait'\" :title=\"item2.moduleName\"></el-step>\r\n            </el-steps>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!--产品审核详情-->\r\n      <cus-form-title style=\"clear: both\" :label=\"$t('productSelect.examine')\"/>\r\n      <div class=\"cusSubCard cusProcess\">\r\n        <div class=\"sub\" v-for=\"item1 in  this.statusData4.children\">\r\n          <el-tooltip effect=\"light\" :content=\"$t('productSelect.processTip')\"\r\n                      placement=\"top-end\">\r\n            <div class=\"wraper\" @click=\"handleGoPage('产品审核')\">\r\n              <div class=\"loss\" :class=\"{'loss_no' : item1.moduleStatus ==='0'}\"></div>\r\n              <div class=\"tag\" :class=\"{'tag_no' : item1.moduleStatus ==='0'}\">{{item1.moduleName}}</div>\r\n              <div class=\"triangle\" :class=\"{'triangle_no' : item1.moduleStatus ==='0'}\"></div>\r\n            </div>\r\n          </el-tooltip>\r\n          <div style=\"margin-top: 23px\">\r\n            <el-steps direction=\"vertical\" process-status=\"success\" v-for=\"item2 in  item1.children\">\r\n              <el-step :icon=\"item2.moduleStatus=='1'?'el-icon-circle-check': 'el-icon-circle-close'\"\r\n                       :status=\"item2.moduleStatus=='1'? 'finish' : 'wait'\" :title=\"item2.moduleName\"></el-step>\r\n            </el-steps>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!--产品发布详情-->\r\n      <cus-form-title style=\"clear: both\" :label=\"$t('productSelect.release')\"/>\r\n      <div class=\"cusSubCard cusProcess\">\r\n        <div class=\"sub\" v-for=\"item1 in  this.statusData5.children\">\r\n          <el-tooltip effect=\"light\" :content=\"$t('productSelect.processTip')\"\r\n                      placement=\"top-end\">\r\n            <div class=\"wraper\" @click=\"handleGoPage('item1.moduleName')\">\r\n              <div class=\"loss\" :class=\"{'loss_no' : item1.moduleStatus ==='0'}\"></div>\r\n              <div class=\"tag\" :class=\"{'tag_no' : item1.moduleStatus ==='0'}\">{{item1.moduleName}}</div>\r\n              <div class=\"triangle\" :class=\"{'triangle_no' : item1.moduleStatus ==='0'}\"></div>\r\n            </div>\r\n          </el-tooltip>\r\n          <div style=\"margin-top: 23px\">\r\n            <el-steps direction=\"vertical\" process-status=\"success\" v-for=\"item2 in  item1.children\">\r\n              <el-step :icon=\"item2.moduleStatus=='1'?'el-icon-circle-check': 'el-icon-circle-close'\"\r\n                       :status=\"item2.moduleStatus=='1'? 'finish' : 'wait'\" :title=\"item2.moduleName\"></el-step>\r\n            </el-steps>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <el-dialog v-if=\"showQueryTestResult\" v-dialog-drag :close-on-click-modal=\"false\" :visible.sync=\"showQueryTestResult\" append-to-body center width=\"80%\">\r\n      <cus-page-title-back @backTo=\"showQueryTestResult=false\" :label=\"testDialogTitle\" :isDiablg=\"true\" />\r\n      <query-test-result :pdInfoId=\"pdInfoId\" :testResultType=\"queryType\"></query-test-result>\r\n    </el-dialog>\r\n  </cus-wraper>\r\n</template>\r\n\r\n<script>\r\n  import {productSelectIndexData, getStatus} from '@/api/basicinfo/productInfoSelect'\r\n\r\n  import queryTestResult from '@/views/commodity-query/queryTestResult'\r\n\r\n  let _this\r\n  export default {\r\n    name: 'productSelect',\r\n    filters: {\r\n      filter_pdProcess(code) {\r\n        return _this.pdProcessS[code]\r\n      },\r\n      filterPdCategory(val) {\r\n        return _this.getCodeName('ProductCategory',val)\r\n      }\r\n\r\n    },\r\n    data() {\r\n      _this = this;\r\n      return {\r\n        filterCommodityBreakdown: [],\r\n        queryType: '',\r\n        //初始化表格\r\n        productSelectTable: [],\r\n        tableData: [],\r\n        //初始化下拉选\r\n        productList:[],\r\n        pdEditionList: [],\r\n        productOriginalList: [],\r\n        selectedPd:{},\r\n        detailTypeList: [],\r\n\t      applyDateValue: [],\r\n        missionDateValue: [],\r\n\t      releaseDateValue: [],\r\n\t      stopDateValue: [],\r\n        testDialogTitle: '',\r\n        showMore: false,\r\n        showQueryTestResult: false,\r\n        url: \"/productSelect/base/indexData\",\r\n        selectOperation: {},\r\n        selectPd: {},\r\n        statusData: {},\r\n        statusData2: {},\r\n        statusData3: {},\r\n        statusData4: {},\r\n        statusData5: {},\r\n        pdInfoId: '',\r\n        pdProcessId: '',\r\n        moduleNode: '',\r\n        pdProcessCode: [\r\n        \t{text: '定义中', value: '1'},\r\n          {text: '测试中', value: '2'},\r\n\t        {text: '审核中', value: '3'},\r\n          {text: '待发布', value: '4'},\r\n          {text: '已发布', value: '5'},\r\n          {text: '已停售', value: '6'},\r\n          {text:'规则订正中', value:'7'},\r\n          {text:'公式订正中', value:'8'}],\r\n        pdProcessS: {'1': '定义中', '2': '测试中', '3': '审核中', '4': '待发布', '5': '已发布', '6': '已停售','7': '规则订正中','8': '公式订正中'},\r\n        pickerOptions: {\r\n          shortcuts: [{\r\n            text: '最近一周',\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 7);\r\n              picker.$emit('pick', [start, end]);\r\n            }\r\n          }, {\r\n            text: '最近一个月',\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 30);\r\n              picker.$emit('pick', [start, end]);\r\n            }\r\n          }, {\r\n            text: '最近三个月',\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 90);\r\n              picker.$emit('pick', [start, end]);\r\n            }\r\n          }]\r\n        },\r\n        params: {\r\n          productCategory: '',\r\n          pdCode: '',\r\n          pdName: '',\r\n          pdEdition: '',\r\n          defType: '',\r\n\t        pdProcess: '',\r\n          productApplyStartDate: undefined,\r\n          productApplyEndDate: undefined,\r\n          missionDistributeStartDate: undefined,\r\n\t        missionDistributeEndDate: undefined,\r\n          releaseStartDate: undefined,\r\n          releaseEndDate: undefined,\r\n          stopStartDate: undefined,\r\n          stopEndDate: undefined\r\n        },\r\n        selecteConfigs2: {},\r\n        //产品营运监控定义完毕传参\r\n        data: {\r\n          productTestFlag: '1',\r\n          processInstanceId: ''\r\n        },\r\n        //动态显示\r\n        showOperationProgress: false,\r\n\r\n      }\r\n    },\r\n    watch: {\r\n\t    applyDateValue(val) {\r\n        if (val) {\r\n          this.params.productApplyStartDate = val[0];\r\n          this.params.productApplyEndDate = val[1]\r\n        } else {\r\n          this.params.productApp