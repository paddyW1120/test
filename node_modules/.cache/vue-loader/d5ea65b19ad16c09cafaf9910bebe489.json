ble-column>\r\n            </el-table>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column align=\"center\" :label=\"$t('i18nView.pdBnftStartRule')\" prop=\"pdBnftFactorLimValue\">\r\n          <template v-slot=\"scope\">{{ scope.row.pdBnftFactorLimValue | filter_pdBnftFactorLimValue }}</template>\r\n        </el-table-column>\r\n        <el-table-column align=\"center\" :label=\"$t('i18nView.Obperiod')\" prop=\"pdBnftFactorValue\">\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdBnftFactorValue }}{{ scope.row.pdBnftFactorValueUnit | filter_pdBnftRuleObservationUnit }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column align=\"center\" :label=\"$t('i18nView.pdBnftCumType')\" prop=\"pdBnftCumType\">\r\n          <template v-slot=\"scope\">{{ scope.row.pdBnftCumType | filter_pdBnftCumType }}</template>\r\n        </el-table-column>\r\n        <el-table-column align=\"center\" :label=\"$t('i18nView.pdBnftCumCount')\" prop=\"pdBnftCumFrequency\">\r\n          <template v-slot=\"scope\">{{ scope.row.pdBnftCumFrequency | filter_pdBnftCumFrequency }}</template>\r\n        </el-table-column>\r\n        <el-table-column align=\"center\" :label=\"$t('table.actions')\" width=\"60px\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button   @click=\"deleteBnftForm(scope.row)\" icon=\"el-icon-delete\" size=\"mini\" type=\"danger\"></el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <cus-pagination ref=\"page_O\" v-model=\"periodConfigs\" :pageSize=\"5\" :params=\"params\"\r\n                      url=\"/product-compensation/base/pagePdtempClmCtrlLimVo1\"/>\r\n      <br />\r\n      <h4>\r\n        <el-divider class=\"divider\" direction=\"vertical\"></el-divider>\r\n        {{$t('i18nView.operiodDef')}}\r\n      </h4>\r\n      <el-row :gutter=\"10\" class=\"row-bg\" justify=\"left\" type=\"flex\">\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('i18nView.pdBnftCtrlLimLevel')\" prop=\"pdBnftCtrlLimLevel\">\r\n            <cus-select :codetype=\"'pdBnftRuleLimitLevel'\" v-model=\"bnftForm.pdBnftCtrlLimLevel\" @change=\"clearAll\"></cus-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\" v-if=\"bnftForm.pdBnftCtrlLimLevel==10\" >\r\n          <el-form-item :label=\"$t('i18nView.AssociatedBnftMoney')\" prop=\"pdBnftCtrlConCode\">\r\n              <el-select :disabled=\"bnftForm.pdBnftCtrlLimLevel!=2\" :placeholder=\"$t('pdtempPdBnft.pleaseChoose')\"\r\n                         clearable @change=\"changeOption\" value-key=\"id\"\r\n                         filterable  disabled multiple v-model=\"bnftForm.pdtempPdMedFeeBillList\">\r\n                <el-option :key=\"item.id\" :label=\"item.pdMedFeeCode+':'+item.pdMedFeeName\" :value=\"item\" v-for=\"item in medOptions\">\r\n                </el-option>\r\n              </el-select>\r\n              <!-- <el-button  size=\"small\" @click=\"openChooseDialog\" type=\"text\">{{ \"更多>>\" }}</el-button> -->\r\n          </el-form-item>\r\n        </el-col>\r\n        <cus-btn-wrapper  v-if=\"bnftForm.pdBnftCtrlLimLevel==10\">\r\n          <el-button size=\"small\" type=\"text\" @click=\"openChooseDialog\">{{$t('i18nView.more')}}</el-button>\r\n        </cus-btn-wrapper>\r\n        <!-- <el-col :span=\"8\" v-if=\"bnftForm.pdBnftCtrlLimLevel==9\">\r\n          <el-form-item label=\"单一账单费用\":label=\"$t('pdClaimVerificationRules.pdAssociatedBillMoney')\" >\r\n            <el-col>\r\n              <el-select  v-model=\"bnftForm.pdtempPdMedFeeBill\" clearable filterable value-key=\"id\">\r\n                <el-option v-for=\"item in PdMedFeeBillOptions\"\r\n                           :key=\"item.id\"\r\n                           :label=\"item.pdMedFeeCode+':'+item.pdMedFeeName\"\r\n                           :value=\"item\">\r\n                </el-option>\r\n              </el-select>\r\n            </el-col>\r\n          </el-form-item>\r\n        </el-col> -->\r\n      </el-row>\r\n      <el-row :gutter=\"10\" class=\"row-bg\" justify=\"center\" type=\"flex\">\r\n        <el-col :span=\"16\">\r\n          <el-form-item :label=\"$t('i18nView.pdBnftStartRule')\" prop=\"pdBnftFactorLimValue\">\r\n            <cus-radio :codetype=\"'pdObPeriodStartRule'\" v-model=\"bnftForm.pdBnftFactorLimValue\"></cus-radio>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"10\" class=\"row-bg\" justify=\"center\" type=\"flex\">\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('i18nView.Obperiod')\" prop=\"pdBnftFactorValue\">\r\n            <el-input v-model=\"bnftForm.pdBnftFactorValue\">\r\n              <cus-select :codetype=\"'pdsbnftdy'\" :filter=\"pdBnftRuleObservationUnit\" class=\"cst\" slot=\"append\" v-model=\"bnftForm.pdBnftFactorValueUnit\"></cus-select>\r\n            </el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('i18nView.pdBnftCumType')\" prop=\"pdBnftCumType\">\r\n            <cus-radio :codetype=\"'pdBnftCumType'\" v-model=\"bnftForm.pdBnftCumType\"></cus-radio>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('i18nView.pdBnftCumPeriod')\" prop=\"pdBnftCumFrequency\">\r\n            <cus-radio :codetype=\"'pdBnftCumFrequency'\" v-model=\"bnftForm.pdBnftCumFrequency\"></cus-radio>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n\r\n      <el-form-item align=\"right\">\r\n        <el-button  size=\"small\" @click=\"saveBnftForm('O')\" icon=\"el-icon-success\" type=\"primary\" v-show=\"!radio2\">\r\n          {{$t('table.save')}}\r\n        </el-button>\r\n        <el-button  size=\"small\" @click=\"updateBnftForm('O')\" icon=\"el-icon-success\" type=\"primary\" v-show=\"radio2\">\r\n          {{$t('table.update')}}\r\n        </el-button>\r\n        <el-button  size=\"small\" @click=\"cleanBnftForm\" icon=\"el-icon-refresh\" type=\"primary\">\r\n          {{$t('table.refresh')}}\r\n        </el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\nlet _this;\r\nimport {\r\n  deleteObBnftForm,\r\n  findperiodConfigs,\r\n  saveMedObBnftForm,\r\n  selectPdMedFeeBill,\r\n  selectPdMedFeeBillOptions,\r\n  selectpdtempPdMedFeeBills,\r\n  updateObBnftForm,\r\n  savePdtempClmCtrlLim,\r\n  findBusinessItemByCtrlCode1\r\n} from \"@/api/basicinfo/pdtemppdclmbnft\";\r\n\r\nimport {\r\n  selectPdBnftCode,\r\n  selectPdClmBnftCode,\r\n  selectPdMedFeeBillCode,\r\n  selectPdProgCode,\r\n} from \"@/api/basicinfo/pdClaimVerificationRules\"\r\n\r\nexport default {\r\n  name: \"pdtemppdmedobperiod\",\r\n  filters: {\r\n    filter_pdBnftCtrlLimLevel(code) {\r\n      if (code) {\r\n        return _this.getCodeName('pdBnftRuleLimitLevel', code);\r\n      } else {\r\n        return ''\r\n      }\r\n    },\r\n    filter_pdBnftRuleObservationUnit(code) {\r\n      return _this.getCodeName(\"pdsbnftdy\", code, true);\r\n    },\r\n    filter_pdBnftCumType(code) {\r\n      return _this.getCodeName(\"pdBnftCumType\", code);\r\n    },\r\n    filter_pdBnftCumFrequency(code) {\r\n      return _this.getCodeName(\"pdBnftCumFrequency\", code);\r\n    },\r\n    filter_pdBnftRuleLevel(code) {\r\n      return _this.getCodeName(\"pdBnftRuleLevel\", code);\r\n    },\r\n    filter_pdBnftFactorLimValue(code) {\r\n      return _this.getCodeName(\"pdObPeriodStartRule\", code);\r\n    },\r\n    filter_dataFlag(code) {\r\n      if (code == \"Y\") {\r\n        return code;\r\n      } else {\r\n        return \"N\";\r\n      }\r\n    },\r\n  },\r\n  props: {\r\n    bnftForm1: {\r\n      type: Object,\r\n      default: {},\r\n    },\r\n    configsForm: {\r\n      type: Object,\r\n      default: {},\r\n    },\r\n    pdBnftArr: {\r\n      type: Array,\r\n      default: () => {\r\n        return [];\r\n      },\r\n    },\r\n    pdClmBnftArr: {\r\n      type: Array,\r\n      default: [],\r\n    },\r\n    feeBillData: {\r\n      type: Object,\r\n      default: {},\r\n    },\r\n    showMedObPeriod: {\r\n      type: Boolean,\r\n      default: false,\r\n    },\r\n  },\r\n  data() {\r\n    _this = this;\r\n    return {\r\n      params: {\r\n        pdBnftCode: this.bnftForm1.pdBnftCode,\r\n        pdClmBnftCode: this.configsForm.pdClmBnftCode,\r\n        pdBnftCtrlType: 'O',\r\n        pdBnftCtrlLimLevelList:'7'\r\n      },\r\n      PdMedFeeBillOptions:[],\r\n      pdClmBnftOptions:[],\r\n      medOptions1: [],\r\n      pdtempPdInfo: {},\r\n      chooseFee: [],\r\n      radio2: null,\r\n      bnftForm: {\r\n        pdObPeriodStartRule: null,\r\n        pdBnftRuleObservation: null,\r\n        pdBnftFactorValueUnit: \"D\",\r\n        pdBnftCumType: null,\r\n        pdBnftCumFrequency:'2',\r\n        pdBnftRuleConCode: null,\r\n        pdtempPdMedBillVoList: [],\r\n        pdtempPdMedFeeBillList:[],\r\n        pdBnftCtrlLimLevel:null,\r\n        pdBnftCtrlCode:null\r\n      },\r\n      pdBnftCtrlLevel: \"1\",\r\n      pdBnftRuleObservationUnit: \"1$Y$M$D\",\r\n      feeConfigs: [],\r\n      pdMedFeeBillTypes: [],\r\n      periodConfigs: [],\r\n      Options: [],\r\n      medOptions: [],\r\n      medFeeOptions: [],\r\n      ClmBnftOptions: [],\r\n      cusObperiod: false,\r\n    };\r\n  },\r\n  watch: {\r\n    showMedObPeriod: function () {\r\n      if (this.showMedObPeriod == true) {\r\n        this.selectpdtempPdMedFeeBills(this.configsForm.pdClmBnftCode);\r\n        this.findMedPeriodConfigs();\r\n      }\r\n    },\r\n  },\r\n  created() {\r\n    let pdtempPdInfo = this.getPdData();\r\n    this.pdtempPdInfo = pdtempPdInfo;\r\n    this.bnftForm.pdBnftCtrlLimLevel = '9'\r\n    this.findOptions();\r\n    if (this.configsForm.pdClmBnftCode != null) {\r\n      this.selectpdtempPdMedFeeBills(this.configsForm.pdClmBnftCode);\r\n    }\r\n    if(this.$refs.page_O){\r\n      this.$refs.page_O.reload();\r\n    }\r\n  },\r\n  methods: {\r\n    selectpdtempPdMedFeeBills(code) {\r\n      selectpdtempPdMedFeeBills(code).then((res) => {\r\n        this.feeConfigs = res.data;\r\n        this.medOptions = [];\r\n        this.medFeeOptions = [];\r\n        this.feeConfigs.forEach((pdtempPdMedFeeBill) => {\r\n          let sameFlag = false;\r\n          if (this.medOptions.length > 0) {\r\n            this.medOptions.forEach((val) => {\r\n              if (val.value == pdtempPdMedFeeBill.pdMedBillType) {\r\n                sameFlag = true;\r\n              }\r\n            });\r\n          }\r\n          if (sameFlag == false) {\r\n            this.medOptions.push({\r\n              value: pdtempPdMedFeeBill.pdMedBillType,\r\n              label:\r\n                pdtempPdMedFeeBill.pdMedBillType +\r\n                \":\" +\r\n                this.getCodeName(\r\n                  \"medBillType\",\r\n                  pdtempPdMedFeeBill.pdMedBillType\r\n                ),\r\n            });\r\n          }\r\n          this.medFeeOptions.push({\r\n            value: pdtempPdMedFeeBill.pdMedFeeBillType,\r\n            label:\r\n              pdtempPdMedFeeBill.pdMedFeeBillType +\r\n              \":\" +\r\n              pdtempPdMedFeeBill.pdMedFeeName,\r\n          });\r\n        });\r\n      });\r\n    },\r\n    findMedPeriodConfigs() {\r\n      selectPdMedFeeBill(\r\n        this.buildRequestData({\r\n          pdClmBnftCode: this.configsForm.pdClmBnftCode,\r\n          pdMedBillType: this.feeBillData.pdMedBillType,\r\n        })\r\n      ).then((res) => {\r\n        let configs = res.data;\r\n        this.pdMedFeeBillTypes = configs;\r\n        let data = {\r\n          pdBnftCtrlType: \"O\",\r\n          pdBnftCtrlLevel: \"2\",\r\n          pdBnftCode: this.configsForm.pdClmBnftCode,\r\n          medFeeBillCodes: [],\r\n        };\r\n        configs.forEach((val) => {\r\n          data.medFeeBillCodes.push(val.pdMedFeeBillType);\r\n        });\r\n      });\r\n    },\r\n    findOptions() {\r\n      this.ClmBnftOptions = [];\r\n      this.pdClmBnftArr.forEach((data) => {\r\n        this.ClmBnftOptions.push({\r\n          value: data.pdClmBnftCode,\r\n          label: data.pdClmBnftCode + \":\" + data.pdClmBnftName,\r\n        });\r\n      });\r\n      this.bnftOptions = [];\r\n      this.pdBnftArr.forEach((data) => {\r\n        this.bnftOptions.push({\r\n          value: data.pdBnftCode,\r\n          label: data.pdBnftCode + \":\" + data.pdBnftName,\r\n        });\r\n      });\r\n    },\r\n    handleSelectionChange(row) {\r\n      this.chooseFee = row;\r\n    },\r\n    chooseone2(row, column, event) {\r\n      if (event.target.nodeName === \"INPUT\") {\r\n        if (row.id == this.radio2) {\r\n          this.radio2 = null;\r\n          this.$refs.periodTable.setCurrentRow();\r\n          this.cleanBnftForm();\r\n        } else {\r\n          this.radio2 = row.id;\r\n          this.bnftForm = JSON.parse(JSON.stringify(row));\r\n        }\r\n      }\r\n      findBusinessItemByCtrlCode1(this.bnftForm.pdBnftCtrlCode).then(response => {\r\n        switch (this.bnftForm.pdBnftCtrlLimLevel) {\r\n          // 单一账单\r\n          case \"7\":\r\n            this.bnftForm.pdtempPdMedBillVo = response.data[0]\r\n            break;\r\n          // 关联账单\r\n          case \"8\":\r\n            this.bnftForm.pdtempPdMedBillVoList = response.data\r\n            this.medOptions1 = response.data\r\n            break;\r\n          // 单一账单费用\r\n          case \"9\":\r\n            this.bnftForm.pdtempPdMedFeeBill = response.data[0]\r\n            break;\r\n          // 关联账单费用\r\n          case \"10\":\r\n            this.bnftForm.pdtempPdMedFeeBillList = response.data\r\n            this.medOptions = response.data\r\n            break;\r\n          default:\r\n            break;\r\n        }\r\n      })\r\n    },\r\n    changeOption() {\r\n      this.$forceUpda