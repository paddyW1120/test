A;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA","file":"index.vue","sourceRoot":"src/views/commodity-statistic/product-statistic","sourcesContent":["<template>\r\n  <cus-wraper>\r\n    <cus-page-title :label=\"$t('route.productOverview')\"/>\r\n    <el-form ref=\"selectForm1\" label-width=\"auto\" :model=\"productOverviewForm\">\r\n      <el-row :gutter=\"10\">\r\n        <!-- 产品细类下拉选 -->\r\n        <cus-col :span=\"7\">\r\n          <el-form-item :label=\"$t('rule.productDetailType')\" prop=\"ProductCategory\">\r\n            <cus-multi-select :codetype=\"'ProductCategory'\" :filter=\"productCategoryFilter\"\r\n                        v-model=\"productOverviewForm.productCategory\"></cus-multi-select>\r\n          </el-form-item>\r\n        </cus-col>\r\n        <!-- 产品形态下拉选 -->\r\n        <cus-col :span=\"7\">\r\n          <el-form-item :label=\"$t('i18nView.defType')\" prop=\"ProductForm\">\r\n            <cus-multi-select :codetype=\"'pdAuditType'\" v-model=\"productOverviewForm.productForm\"></cus-multi-select>\r\n          </el-form-item>\r\n        </cus-col>\r\n        <cus-col :span=\"7\">\r\n          <!-- 产品定义申请日期选项 -->\r\n          <el-form-item :label=\"$t('i18nView.defineApplicationDate')\">\r\n            <el-date-picker v-model=\"dateValue\" type=\"daterange\" unlink-panels\r\n                            :range-separator=\"$t('productOverview.to')\" value-format=\"yyyy-MM-dd\"\r\n                            :start-placeholder=\"$t('productOverview.productApplyStartDate')\"\r\n                            :end-placeholder=\"$t('productOverview.productApplyEndDate')\"\r\n                            :picker-options=\"pickerOptions\">\r\n            </el-date-picker>\r\n          </el-form-item>\r\n        </cus-col>\r\n        <cus-btn-wrapper>\r\n          <el-button size=\"small\" type=\"primary\" @click=\"showchart\" icon=\"el-icon-search\" v-waves class=\"filter-button\">\r\n            {{$t('table.search')}}\r\n          </el-button>\r\n        </cus-btn-wrapper>\r\n      </el-row>\r\n    </el-form>\r\n\r\n    <!-- 对应柱状图 -->\r\n    <el-row :gutter=\"32\">\r\n      <cus-col :span=\"12\">\r\n        <div class=\"chart-wrapper\">\r\n          <bar-chart :title=\"title1\" v-if=\"chart1Show\" :id=\"chart1\" :callBack=\"clickThisChart\"\r\n                     :style=\"{height:'450px',width:'100%'}\" :xData=\"chart1xData\" :yData=\"chart1yData\"/>\r\n        </div>\r\n      </cus-col>\r\n      <cus-col :span=\"12\">\r\n        <div class=\"chart-wrapper\">\r\n          <bar-chart :title=\"title3\" v-if=\"chart3Show\" :id=\"chart3\"\r\n                     :style=\"{height:'450px',width:'100%'}\" :xData=\"chart3xData\" :yData=\"chart3yData\"/>\r\n        </div>\r\n      </cus-col>\r\n    </el-row>\r\n\r\n\r\n    <!-- 点击页面所有图表中的x坐标可弹窗显示对应柱状图 -->\r\n    <el-dialog :close-on-click-modal=\"false\" :visible.sync=\"showThisDialog1\" center top=\"8vh\" v-if=\"showThisDialog1\"\r\n               v-dialog-drag width=\"1200px\">\r\n      <el-row :gutter=\"32\">\r\n        <el-col :xs=\"24\" :sm=\"24\" :lg=\"12\">\r\n          <div class=\"chart-wrapper\">\r\n            <bar-chart :title=\"title2\" v-if=\"chart2Show\" :id=\"chart2\" :callBack=\"clickThisChart2\"\r\n                       :style=\"{height:'500px',width:'200%'}\"\r\n                       :xData=\"chart2xData\" :yData=\"chart2yData\"/>\r\n          </div>\r\n        </el-col>\r\n\r\n      </el-row>\r\n    </el-dialog>\r\n    <!-- 点击页面所有图表中的柱状图都可弹窗显示对应表格 -->\r\n    <el-dialog :close-on-click-modal=\"false\" :visible.sync=\"showThisDialog2\" center top=\"8vh\" v-if=\"showThisDialog2\"\r\n               v-dialog-drag width=\"1200px\">\r\n      <cus-page-title-back :label=\"$t('productOverview.productList')\" @backTo=\"goBack\"/>\r\n      <cus-table-title :label=\"$t('productOverview.productTableName')\"/>\r\n      <el-table :data=\"configs\" fit element-loading-text=\"Loading\" border highlight-current-row align=\"center\"\r\n                class=\"wraper-table\">\r\n\r\n        <el-table-column :label=\"$t('table.id')\" type=\"index\" align=\"center\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.pdCode')\" prop=\"pdCode\" align=\"center\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.pdName')\" prop=\"pdName\" align=\"center\"></el-table-column>\r\n        <el-table-column :label=\"$t('customComponents.pdEdition')\" prop=\"pdEdition\" align=\"center\"></el-table-column>\r\n        <el-table-column :label=\"$t('rule.productDetailType')\" prop=\"cType\" align=\"center\">\r\n          <template v-slot=\"scope\">{{filterHandler('ProductCategory',scope.row.cType )}}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.defType')\" prop=\"defType\" align=\"center\">\r\n          <template v-slot=\"scope\">{{filterHandler('pdAuditType',scope.row.defType )}}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.pdDefApplyDate')\" prop=\"pdApplDate\"\r\n                         align=\"center\"></el-table-column>\r\n      </el-table>\r\n      <!-- 分页 -->\r\n      <cus-pagination ref=\"dataPage\" :url=\"url\" :pageSize=\"5\" :params=\"productOverviewForm\" v-model=\"configs\"/>\r\n\r\n    </el-dialog>\r\n\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\n\r\n\timport BarChart from '@/views/dashboard/admin/components/BarChart'\r\n\timport {\r\n\t\tproductOverviewIndexData,\r\n\t\tfwData,\r\n\t\topData,\r\n\t\ttestData,\r\n\t\trelease1Data,\r\n\t\trelease2Data\r\n\t} from '@/api/basicinfo/productOverview'\r\n\r\n\texport default {\r\n\t\tname: 'normaluser',\r\n\t\tcomponents: {\r\n\t\t\tBarChart\r\n\t\t},\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\tbarData: {},\r\n\t\t\t\ttitle1: '产品定义统计',\r\n\t\t\t\ttitle2: '',\r\n\t\t\t\ttitle3: '险种营运定义统计',\r\n\t\t\t\turl: '',\r\n\t\t\t\turl2: '',\r\n\t\t\t\tchart1Show: false,\r\n\t\t\t\tchart3Show: false,\r\n\t\t\t\tshowThisDialog1: false,\r\n\t\t\t\tshowThisDialog2: false,\r\n\t\t\t\tdateValue: [],\r\n\t\t\t\tchart1xData: [],\r\n\t\t\t\tchart1yData: [],\r\n\t\t\t\tchart1: null,\r\n\t\t\t\tchart3: null,\r\n\t\t\t\tchart2Show: false,\r\n\t\t\t\tchart2: null,\r\n\t\t\t\tchart2xData: [],\r\n\t\t\t\tchart2yData: [],\r\n\t\t\t\tchart3xData: [],\r\n\t\t\t\tchart3yData: [],\r\n\t\t\t\tproductCategoryFilter: '',\r\n\t\t\t\t//表格数据\r\n\t\t\t\tconfigs: [],\r\n\t\t\t\tpickerOptions: {\r\n\t\t\t\t\tshortcuts: [{\r\n\t\t\t\t\t\ttext: '最近一周',\r\n\t\t\t\t\t\tonClick(picker) {\r\n\t\t\t\t\t\t\tconst end = new Date();\r\n\t\t\t\t\t\t\tconst start = new Date();\r\n\t\t\t\t\t\t\tstart.setTime(start.getTime() - 3600 * 1000 * 24 * 7);\r\n\t\t\t\t\t\t\tpicker.$emit('pick', [start, end]);\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t}, {\r\n\t\t\t\t\t\ttext: '最近一个月',\r\n\t\t\t\t\t\tonClick(picker) {\r\n\t\t\t\t\t\t\tconst end = new Date();\r\n\t\t\t\t\t\t\tconst start = new Date();\r\n\t\t\t\t\t\t\tstart.setTime(start.getTime() - 3600 * 1000 * 24 * 30);\r\n\t\t\t\t\t\t\tpicker.$emit('pick', [start, end]);\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t}, {\r\n\t\t\t\t\t\ttext: '最近三个月',\r\n\t\t\t\t\t\tonClick(picker) {\r\n\t\t\t\t\t\t\tconst end = new Date();\r\n\t\t\t\t\t\t\tconst start = new Date();\r\n\t\t\t\t\t\t\tstart.setTime(start.getTime() - 3600 * 1000 * 24 * 90);\r\n\t\t\t\t\t\t\tpicker.$emit('pick', [start, end]);\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t}]\r\n\t\t\t\t},\r\n\t\t\t\t//查询参数\r\n\t\t\t\tparams: {},\r\n\r\n\t\t\t\tproductOverviewForm: {\r\n\t\t\t\t\tproductCategory: [],\r\n\t\t\t\t\tproductType: '',\r\n\t\t\t\t\tproductForm: [],\r\n\t\t\t\t\tproductApplyStartDate: '',\r\n\t\t\t\t\tproductApplyEndDate: ''\r\n\t\t\t\t},\r\n\t\t\t\tform: {  //表单数据与后端实体类对应\r\n\r\n\t\t\t\t},\r\n\t\t\t\t//检验规则\r\n\t\t\t\trules: {\r\n\t\t\t\t\tuserCode: [\r\n\t\t\t\t\t\t{required: true, message: this.$t('ldcode.xxx'), trigger: 'blur'},\r\n\t\t\t\t\t]\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t},\r\n\t\tcomputed: {},\r\n\t\twatch: {\r\n\t\t\tdateValue(val) {\r\n\t\t\t\tif (val) {\r\n\t\t\t\t\tthis.productOverviewForm.productApplyStartDate = val[0];\r\n\t\t\t\t\tthis.productOverviewForm.productApplyEndDate = val[1]\r\n\t\t\t\t} else {\r\n\t\t\t\t\tthis.productOverviewForm.productApplyStartDate = '';\r\n\t\t\t\t\tthis.productOverviewForm.productApplyEndDate = ''\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t},\r\n\t\tcreated() {\r\n\t\t\tthis.getIndexData()\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\t//下拉过滤1$对应编码$1\r\n\t\t\tproductTypeSelect(val) {\r\n\t\t\t\tif (val == 'L') {\r\n\t\t\t\t\tthis.productCategoryFilter = '1$01$02$03$1'\r\n\t\t\t\t} else if (val == 'R') {\r\n\t\t\t\t\tthis.productCategoryFilter = '1$04$05$1'\r\n\t\t\t\t} else if (val == 'H') {\r\n\t\t\t\t\tthis.productCategoryFilter = '1$06$07$08$09$1'\r\n\t\t\t\t} else if (val == 'A') {\r\n\t\t\t\t\tthis.productCategoryFilter = '1$10$1'\r\n\t\t\t\t} else {\r\n\t\t\t\t\tthis.productCategoryFilter = ''\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\t\tfilterHandler(codetype, value) {\r\n\t\t\t\treturn this.getCodeName(codetype, value);\r\n\t\t\t},\r\n\t\t\tdataReload() {\r\n\t\t\t\t//console.log(this.$refs.dataPage);\r\n\t\t\t\tif (this.$refs.dataPage) {\r\n\t\t\t\t\tthis.$refs.dataPage.reload();\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\t\tgetIndexData() {\r\n\t\t\t\tproductOverviewIndexData(this.buildRequestData()).then(res => {\r\n\t\t\t\t\tlet data = res.data\r\n\t\t\t\t\tconsole.log(data);\r\n\t\t\t\t\tthis.barData = data.barData\r\n\t\t\t\t\tthis.chart1xData = data.barData.xData\r\n\t\t\t\t\tthis.chart1yData = data.barData.yData\r\n          this.chart3xData = data.barData.productOperating.xData\r\n          this.chart3yData = data.barData.productOperating.yData\r\n\t\t\t\t\tthis.taskCount = this.chart1yData.reduce(function (prev, curr, idx, arr) {\r\n\t\t\t\t\t\treturn prev + curr;\r\n\t\t\t\t\t});\r\n\t\t\t\t\tthis.initChart()\r\n\t\t\t\t})\r\n\t\t\t},\r\n\t\t\tshowchart() {\r\n\t\t\t\tproductOverviewInd