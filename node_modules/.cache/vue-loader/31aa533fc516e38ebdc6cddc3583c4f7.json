AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;;AAGA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/views/ibrms/objectManagement/function","sourcesContent":["<template>\r\n  <cus-wraper>\r\n    <cus-filter-wraper>\r\n      <el-form style=\"margin: 20px 0 0 0\" label-width=\"auto\">\r\n        <el-row :gutter=\"10\">\r\n          <cus-col :span=\"6\">\r\n            <el-form-item :label=\"$t('ruleFunction.name')\" prop=\"name\">\r\n              <el-input placeholder clearable v-model=\"params.like_name\"></el-input>\r\n            </el-form-item>\r\n          </cus-col>\r\n          <cus-col :span=\"6\">\r\n            <el-form-item :label=\"$t('ruleFunction.commandType')\" prop=\"commandType\">\r\n              <cus-select\r\n                :codetype=\"'ibrmscommandtype'\"\r\n                placeholder\r\n                v-model=\"params.eq_commandType\"\r\n              ></cus-select>\r\n            </el-form-item>\r\n          </cus-col>\r\n          <cus-col :span=\"6\">\r\n            <el-form-item :label=\"$t('ruleFunction.state')\" prop=\"state\">\r\n              <cus-select :codetype=\"'functionvalid'\" placeholder v-model=\"params.eq_state\"></cus-select>\r\n            </el-form-item>\r\n          </cus-col>\r\n          <cus-btn-wrapper>\r\n             <el-button  size=\"mini\"\r\n                v-waves\r\n                type=\"primary\"\r\n                @click=\"handleSearch\"\r\n                class=\"filter-button\"\r\n                icon=\"el-icon-search\"\r\n              >{{$t('ruleFunction.search') }}</el-button>\r\n              <el-button  size=\"mini\"\r\n                v-waves\r\n                type=\"primary\"\r\n                v-auth=\"'rule-function:add'\"\r\n                @click=\"handleCreate\"\r\n                icon=\"el-icon-plus\"\r\n                class=\"filter-button\"\r\n              >\r\n                {{\r\n                $t('ruleFunction.add') }}\r\n              </el-button>\r\n\r\n          </cus-btn-wrapper>\r\n        </el-row>\r\n        <el-row :gutter=\"10\">\r\n          <cus-col :span=\"6\">\r\n            <el-form-item :label=\"$t('ruleFunction.paraType')\" prop=\"paraType\">\r\n              <cus-select :codetype=\"'ibrmsparatype'\" placeholder v-model=\"params.eq_paraType\"></cus-select>\r\n            </el-form-item>\r\n          </cus-col>\r\n        </el-row>\r\n      </el-form>\r\n    </cus-filter-wraper>\r\n    <cus-table-title :label=\"$t('ruleFunction.tableName')\"></cus-table-title>\r\n    <el-table\r\n      :data=\"configs\"\r\n      size=\"mini\"\r\n      fit\r\n      element-loading-text=\"Loading\"\r\n      border\r\n      highlight-current-row\r\n      align=\"center\"\r\n      class=\"wraper-table\"\r\n      ref=\"page\"\r\n    >\r\n      <el-table-column :label=\"$t('table.id')\"  align=\"center\" sortable type=\"index\" width=\"60px\">\r\n            <template v-slot=\"scope\">\r\n                  {{ initTableIndex('page', scope.$index) }}\r\n            </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('ruleFunction.name')\" prop=\"name\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('ruleFunction.display')\" prop=\"display\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('ruleFunction.implenmation')\" prop=\"implenmation\" align=\"center\"></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleFunction.commandType')\"\r\n        :formatter=\"commandTypeFormat\"\r\n        prop=\"commandType\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column :label=\"$t('ruleFunction.paraNum')\" prop=\"paraNum\" align=\"center\"></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleFunction.paraType')\"\r\n        :formatter=\"paraTypeFormat\"\r\n        prop=\"paraType\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleFunction.resultType')\"\r\n        :formatter=\"resultTypeFormat\"\r\n        prop=\"resultType\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleFunction.functionType')\"\r\n        :formatter=\"functionTypeFormat\"\r\n        prop=\"functionType\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleFunction.state')\"\r\n        :formatter=\"stateFormat\"\r\n        prop=\"state\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleFunction.functionDetail')\"\r\n        prop=\"functionDetail\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('table.actions')\" fixed=\"right\" width=\"600\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"handleUpdate(scope.row)\"\r\n            v-auth=\"'rule-function:update'\"\r\n            class=\"filter-button\"\r\n            icon=\"el-icon-edit\"\r\n          ></el-button>\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"handleTest(scope.row)\"\r\n            class=\"filter-button\"\r\n          >\r\n            <svg-icon icon-class=\"autotest\" />\r\n            {{ $t('route.test') }}\r\n          </el-button>\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"releaseForm(scope.row)\"\r\n            class=\"filter-button\"\r\n          >\r\n            <svg-icon icon-class=\"release\" />\r\n            {{ $t('ruleFunction.functionRelease.release') }}\r\n          </el-button>\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"cancelRelease(scope.row)\"\r\n            class=\"filter-button\"\r\n          >\r\n            <svg-icon icon-class=\"revision\" />\r\n            {{ $t('ruleFunction.functionRelease.revision') }}\r\n          </el-button>\r\n          <cus-del-btn\r\n            v-auth=\"'rule-function:delete'\"\r\n            @ok=\"handleDelete(scope.row)\"\r\n            style=\"margin-left: 5px;\"\r\n          />\r\n          <el-button\r\n            type=\"primary\"\r\n            size=\"mini\"\r\n            class=\"filter-button\"\r\n            @click=\"ruleMessage(scope.row)\"\r\n            style=\"margin-left: 5px;\"\r\n          >\r\n            <svg-icon icon-class=\"multilingual\" />\r\n            {{$t('ruleFunction.ruleMessage')}}\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <cus-pagination\r\n      ref=\"page\"\r\n      url=\"/rule-function/list\"\r\n      :pageSize=\"10\"\r\n      :params=\"params\"\r\n      v-model=\"configs\"\r\n    />\r\n\r\n    <!---新增修改dialog---->\r\n\r\n    <el-dialog :visible.sync=\"dialogVisible\" @close=\"dialogVisible=false\" v-dialog-drag width=\"60%\">\r\n      <cus-page-title-back @backTo=\"goBack\" :isDialog=\"true\" :label=\"titleMap[dialogStatus]\"/>\r\n          <el-form\r\n            ref=\"dataForm\"\r\n            :model=\"form\"\r\n            :rules=\"rules\"\r\n            label-width=\"150px\"\r\n            label-position=\"right\"\r\n            style=\"width: 100%;margin-left:2%\"\r\n          >\r\n            <el-row :gutter=\"10\">\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleFunction.name')+':'\" prop=\"name\">\r\n                  <el-input\r\n                    v-model=\"form.name\"\r\n                    :placeholder=\"$t('ruleFunction.name')\"\r\n                    maxlength=\"20\"\r\n                    show-word-limit\r\n                  ></el-input>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleFunction.display')+':'\" prop=\"display\">\r\n                  <el-input\r\n                    v-model=\"form.display\"\r\n                    :placeholder=\"$t('ruleFunction.display')\"\r\n                    maxlength=\"100\"\r\n                    show-word-limit\r\n                  ></el-input>\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n            <el-row :gutter=\"10\">\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleFunction.implenmation')+':'\" prop=\"implenmation\">\r\n                  <el-input\r\n                    v-model=\"form.implenmation\"\r\n                    :placeholder=\"$t('ruleFunction.implenmation')\"\r\n                    maxlength=\"100\"\r\n                    show-word-limit\r\n                  ></el-input>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleFunction.commandType')+':'\" prop=\"commandType\">\r\n                  <cus-select :codetype=\"'ibrmscommandtype'\" placeholder v-model=\"form.commandType\"></cus-select>\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n            <el-row>\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleFunction.paraNum')+':'\" prop=\"paraNum\">\r\n                  <el-input\r\n                    type=\"number\"\r\n                    v-model=\"form.paraNum\"\r\n                    :placeholder=\"$t('ruleFunction.paraNum')\"\r\n                  ></el-input>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleFunction.paraType')+':'\" prop=\"paraType\">\r\n                  <cus-select :codetype=\"'ibrmsparatype'\" placeholder v-model=\"form.paraType\"></cus-select>\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n            <el-row :gutter=\"10\">\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleFunction.resultType')+':'\" prop=\"resultType\">\r\n                  <cus-select :codetype=\"'ibrmsresulttype'\" placeholder v-model=\"form.resultType\"></cus-select>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleFunction.functionType')+':'\" prop=\"functionType\">\r\n                  <cus-select\r\n                    :codetype=\"'ibrmscommtype'\"\r\n                    placeholder\r\n                    :disabled=\"'disabled'\"\r\n                    v-model=\"form.functionType\"\r\n                  ></cus-select>\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n            <el-row>\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleFunction.functionDetail')+':'\" prop=\"functionDetail\">\r\n                  <el-input\r\n                    v-model=\"form.functionDetail\"\r\n                    :placeholder=\"$t('ruleFunction.functionDetail')\"\r\n                    maxlength=\"300\"\r\n                    show-word-limit\r\n                  ></el-input>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col :span=\"10\"></cus-col>\r\n            </el-row>\r\n            <el-row :gutter=\"10\">\r\n              <cus-col :span=\"20\">\r\n                <el-form-item :label=\"$t('ruleFunction.description')+':'\" prop=\"description\">\r\n                  <el-input\r\n                    :placeholder=\"$t('ruleFunction.description') \"\r\n                    :rows=\"5\"\r\n                    type=\"textarea\"\r\n                    maxlength=\"200\"\r\n                    show-word-limit\r\n                    v-model=\"form.discription\"\r\n                  />\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n          </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button  size=\"mini\" type=\"primary\" v-auth=\"'rule-function:add'\" @click=\"submitForm\">\r\n          <i class=\"el-icon-success el-icon--left\"></i>\r\n          {{ $t('table.save') }}\r\n        </el-button>\r\n        <el-button  size=\"mini\" @click=\"resetForm('dataForm')\" type=\"warning\">\r\n          <i class=\"el-icon-refresh el-icon--left\"></i>\r\n          {{$t('table.refresh')}}\r\n        </el-button>\r\n      </span>\r\n    </el-dialog>\r\n    <el-dialog :visible.sync=\"dialogVisible_msg\" @close=\"dialogVisible_msg=false\" v-dialog-drag width=\"80%\">\r\n      <cus-page-title-back @backTo=\"goBackInfo_msg\" :label=\"$t('ruleFunction.ruleMessage')\" :isDialog=\"true\"></cus-page-title-back>\r\n      <ruleMessageInfoBom :keyId.sync=\"keyId\" :msgType.sync=\"msgType\"></ruleMessageInfoBom>\r\n    </el-dialog>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\nimport {\r\n  addRuleFunction,\r\n  deleteRuleFunction,\r\n  updateRuleFunction,\r\n  releaseFunction,\r\n  testCompleteFunction,\r\n  findRuleFunctionByName\r\n} from \"@/api/ibrms/ruleFunction\";\r\nimport { mapGetters } from \"vuex\";\r\nimport ruleMessageInfoBom from \"@/views/ibrms/objectManagement/ruleMessageInfoBom\";\r\n\r\nexport default {\r\n  name: \"ruleFunction\",\r\n  data() {\r\n    const isNum = (rule, value, callback) => {\r\n      const bomLevel = /^[0-9]*$/;\r\n      if (!bomLevel.test(value)) {\r\n        callback(new Error(\"请输入数字\"));\r\n      } else {\r\n        callback();\r\n      }\r\n    };\r\n    const checkINChinese = (rule, value, callback) => {\r\n      var patrn = /^[\\u4e00-\\u9fa5]{0,30}$/;\r\n      if (!patrn.test(value)) {\r\n          callback();\r\n      } else {\r\n          callback(new Error(\"不能输入中文\"));\r\n\r\n      }\r\n    };\r\n    return {\r\n      //表格数据\r\n      configs: [],\r\n      //查询参数\r\n      params: {\r\n        like_name: \"\",\r\n        eq_commandType: \"\",\r\n        eq_CommType: \"\",\r\n        eq_valid: \"\",\r\n        eq_paraType: \"\"\r\n      },\r\n      form: {\r\n        //表单数据与后端实体类对应\r\n        id: \"\", //主键ID\r\n        functionType: \"1\",\r\n        commandType:\"\",\r\n        paraType:\"\",\r\n        resultType:\"\"\r\n\r\n      },\r\n      radio: false,\r\n      functionId: \"\",\r\n      functionName: \"\",\r\n      keyId: \"\",\r\n      msgType: \"\",\r\n      dialogVisible_msg: false,\r\n      //检验规则\r\n      rules: {\r\n        paraNum: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleFunction.errorMessage.paraNumMessage\"),\r\n            trigger: \"blur\"\r\n          },\r\n          {\r\n            validator: isNum,\r\n            message: this.$t(\"ruleFunction.errorMessage.paraNumMessage1\"),\r\n            trigger: \"blur\"\r\n          }\r\n        ],\r\n        name: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleFunction.errorMessage.nameMessage\"),\r\n            trigger: \"blur\"\r\n          },\r\n          {\r\n            validator: checkINChinese,\r\n            message: this.$t(\"ruleFunction.errorMessage.nameMessage1\"),\r\n            trigger: \"blur\"\r\n          }\r\n        ],\r\n        implenmation: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleFunction.errorMessage.implenmationMessage\"),\r\n            trigger: \"blur\"\r\n          }\r\n        ],\r\n        display: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleFunction.errorMessage.displayMessage\"),\r\n            trigger: \"blur\"\r\n          }\r\n        ],\r\n        commandType: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleFunction.errorMessage.commandTypeMessage\"),\r\n            trigger: \"change\"\r\n          }\r\n        ],\r\n        paraType: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleFunction.paraTypeMessage\"),\r\n            trigger: \"change\"\r\n          }\r\n        ],\r\n        resultType: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleFunction.resultTypeMessage\"),\r\n            trigger: \"change\"\r\n          }\r\n        ]\r\n      },\r\n      dialogVisible: false, //dialog显示还是隐藏\r\n      dialogStatus: \"\", //表单状态是新增还是修改\r\n      titleMap: {\r\n        update: this.$t(\"ruleFunction.update\"),\r\n        create: this.$t(\"ruleFunction.add\")\r\n      },\r\n      commandTypeArray: [],\r\n      paraTypeArray: [],\r\n      resultTypeArray: [],\r\n      functionTypeArray: [],\r\n      validTypeArray: []\r\n    };\r\n  },\r\n  computed: {},\r\n  components: {\r\n    ruleMessageInfoBom\r\n  },\r\n  created() {\r\n    let ldcodes = this.$store.state.ldcode.ldcodes;\r\n    this.commandTypeArray = ldcodes[\"ibrmscommandtype\"];\r\n    this.paraTypeArray = ldcodes[\"ibrmsparatype\"];\r\n    this.resultTypeArray = ldcodes[\"ibrmsresulttype\"];\r\n    this.functionTypeArray = ldcodes[\"ibrmscommtype\"];\r\n    this.stateArray = ldcodes[\"functionvalid\"];\r\n  },\r\n  methods: {\r\n    handleCurrentChange(val) {\r\n      this.functionId = val.id;\r\n      this.functionName = val.name;\r\n      this.radio = val.id;\r\n    },\r\n    handleSearch() {\r\n      this.$refs.page.reload();\r\n    },\r\n    handleCreate() {\r\n      this.resetForm();\r\n      this.dialogStatus = \"create\";\r\n      this.dialogVisible = true;\r\n    },\r\n    handleUpdate(row) {\r\n      if (row.state === \"2\") {\r\n        this.$message.warning(this.$t(\"ruleFunction.checkRelease\"));\r\n      } else {\r\n        this.form = Object.assign({}, row);\r\n        this.dialogStatus = \"update\";\r\n        this.dialogVisible = true;\r\n      }\r\n    },\r\n    handleTest(row) {\r\n      let baseInfo = Object.assign({}, row);\r\n      this.$router.push({\r\n        name: \"函数测试\",\r\n        params: {\r\n          baseInfo: baseInfo\r\n        }\r\n      });\r\n    },\r\n    handleDelete(row) {\r\n      deleteRuleFunction(row.id,row.name).then(response => {\r\n          var result = response.data;\r\n          if(result.type=\"success\"){\r\n              var keyId = 'FUNCTION_'+row.name;\r\n              var param = {     obj:row,\r\n                                type:'FUNCTION',\r\n                                item:'delete'\r\n                                };\r\n              var rule_param = {\r\n                                key:keyId,\r\n                                type:\"FUNCTION\"\r\n                                };\r\n              this.$store.dispatch(\"ruleCache/updateRule