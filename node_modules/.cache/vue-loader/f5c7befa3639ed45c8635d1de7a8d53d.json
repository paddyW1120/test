tem>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('invFormula.invItem')\">\r\n            <el-select v-model=\"productBnftForm.pdInv\" clearable filterable>\r\n              <el-option v-for=\"item in invOptions\" :key=\"item.key\" :label=\"item.label\" :value=\"item.value\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('rule.pdBussDetailType')\" prop=\"ruleType\">\r\n            <cus-select v-model=\"productBnftForm.ruleType\" codetype=\"ruleBusinessDetailClass\" disabled\r\n                        filter=\"1$32\"></cus-select>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n    </el-form>\r\n\r\n    <div v-if=\"productCalcRevisionShowINfo\">\r\n      <cus-table-title :label=\"$t('premFormula.pdInvItem')\"/>\r\n      <el-table :data=\"tableDataInv\" border element-loading-text=\"Loading\" fit highlight-current-row\r\n                max-height=\"300\" size=\"mini\" @row-click=\"rowClickInv\">\r\n        <el-table-column :label=\"$t('table.select')\" width=\"50\">\r\n          <template slot-scope=\"scope\">\r\n            <el-radio-group v-model=\"selectedInvInRev\" @change=\"selectedInvInRevChanged\">\r\n              <el-radio :label=\"scope.row\">{{ '' }}</el-radio>\r\n            </el-radio-group>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.id')\" align=\"center\" width=\"50\">\r\n          <template v-slot=\"scope\">\r\n            {{ initTableIndex('page_inv', scope.$index) }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('invFormula.pdInvCode')\" align=\"center\" min-width=\"200\" prop=\"pdInvCode\"\r\n                         sortable></el-table-column>\r\n        <el-table-column :label=\"$t('invFormula.pdInvName')\" align=\"center\" min-width=\"200\"\r\n                         prop=\"pdInvName\"></el-table-column>\r\n        <el-table-column :label=\"$t('invFormula.pdInvType')\" align=\"center\" min-width=\"200\"\r\n                         prop=\"pdInvType\">\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdInvType | filter_pdInvType }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('invFormula.pdInvStartPeriod')\" align=\"center\" min-width=\"200\"\r\n                         prop=\"pdInvStartPeriod\">\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdInvStartPeriod }}\r\n            <span v-if=\"scope.row.pdInvStartPeriod\">{{\r\n                scope.row.pdInvStartPeriod | filter_pdInvStartPeriodUnit\r\n              }}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('invFormula.pdInvEndPeriod')\" align=\"center\" min-width=\"200\"\r\n                         prop=\"pdInvStartPeriod\">\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdInvEndPeriod }}\r\n            <span v-if=\"scope.row.pdInvEndPeriod\">{{ scope.row.pdInvEndPeriod | filter_pdInvStartPeriodUnit }}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('invFormula.pdInvGuarPeriod')\" align=\"center\" min-width=\"200\"\r\n                         prop=\"pdInvGuarPeriod\">\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdInvGuarPeriod }}\r\n            <span v-if=\"scope.row.pdInvGuarPeriod\">{{\r\n                scope.row.pdInvGuarPeriodUnit | filter_pdInvStartPeriodUnit\r\n              }}</span>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <cus-pagination ref=\"page_inv\" v-model=\"tableDataInv\" :autoBuild=\"false\" :pageSize=\"5\" :params=\"params_inv\"\r\n                      url=\"/invAmountFormul/base/getAllInvByPdInfoId\"/>\r\n    </div>\r\n\r\n    <div v-if=\"!productCalcRevisionShowINfo || productCalcRevisionShowINfo && Object.keys(selectedInvInRev).length > 0\">\r\n      <!-- 已选择公式 -->\r\n      <cus-form-title :first=\"true\" :label=\"$t('invFormula.selectedInvFormula')\"></cus-form-title>\r\n      <cus-table-title :label=\"$t('invFormula.pdInvFormula')\"/>\r\n      <el-table :data=\"tableData_current\" border element-loading-text=\"Loading\" fit highlight-current-row\r\n                max-height=\"300\"\r\n                size=\"mini\" @row-click=\"rowClick\">\r\n        <el-table-column :label=\"$t('pdtempPdBnft.choose')\" width=\"50\">\r\n          <template slot-scope=\"scope\">\r\n            <el-radio-group v-model=\"selectedFormula\" @change=\"selectedFormulaChanged\">\r\n              <el-radio :label=\"scope.row\">{{ '' }}</el-radio>\r\n            </el-radio-group>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.id')\" align=\"center\" width=\"50\">\r\n          <template v-slot=\"scope\">\r\n            {{ initTableIndex('page_current', scope.$index) }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.detail')\" align=\"center\" width=\"80\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button size=\"small\" type=\"text\" @click=\"showFormula(scope.row)\">>></el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('invFormula.pdInvType')\" align=\"center\" min-width=\"200\">\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdInvType | filter_pdInvType }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('invFormula.invItem')\" align=\"center\" min-width=\"200\" prop=\"pdInvCode\" sortable>\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdInvCode ? scope.row.pdInvCode + ':' + scope.row.pdInvName : '' }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('rule.pdBussDetailType')\" align=\"center\" min-width=\"200\" prop=\"pdBussDetailType\">\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdBussDetailType | filter_pdBussDetailType }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('rule.pdCalcCode')\" align=\"center\" min-width=\"200\" prop=\"pdCalcCode\"\r\n                         sortable></el-table-column>\r\n        <el-table-column :label=\"$t('rule.pdCalcDesc')\" align=\"center\" min-width=\"200\"\r\n                         prop=\"pdCalcDesc\"></el-table-column>\r\n        <el-table-column :label=\"$t('rule.ruleAttr')\" align=\"center\" min-width=\"200\">\r\n          <template v-slot=\"scope\">\r\n            {{\r\n              scope.row.ruleAttr === '1' ? $t('rule.general') : (scope.row.ruleAttr === '2' ? $t('rule.individual') : '')\r\n            }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.actions')\" align=\"center\" fixed=\"right\" width=\"120\">\r\n          <template slot-scope=\"scope\">\r\n            <cus-del-btn v-auth=\"'pdtemp-pd-underwriting-amount-formula:delete'\"\r\n                         @ok=\"deleteFormula(scope.row)\"></cus-del-btn>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <cus-pagination ref=\"page_current\" v-model=\"tableData_current\" :autoBuild=\"false\" :pageSize=\"5\"\r\n                      :params=\"params_current\" url=\"/invAmountFormul/base/allFormulaByPdInfoId\"/>\r\n\r\n      <!-- 可选择公式 -->\r\n      <cus-form-title :first=\"true\" :label=\"$t('invFormula.selectableInvFormula')\"></cus-form-title>\r\n\r\n      <el-row style=\"text-align: right\">\r\n        <el-button icon=\"el-icon-connection\" size=\"small\" type=\"primary\" @click=\"toFormulaSelect\">\r\n          {{ $t('rule.moreFormula') }}\r\n        </el-button>\r\n        <el-button icon=\"el-icon-circle-plus el-icon--left\" size=\"small\" type=\"primary\" @click=\"toAddFormula\">\r\n          {{ $t('rule.toAddFormula') }}\r\n        </el-button>\r\n      </el-row>\r\n\r\n      <cus-table-title :label=\"$t('invFormula.invFormula')\"/>\r\n      <el-table :data=\"tableData_brief\" border element-loading-text=\"Loading\" fit highlight-current-row max-height=\"300\"\r\n                size=\"mini\" @row-click=\"rowClick\">\r\n        <el-table-column :label=\"$t('table.id')\" align=\"center\" width=\"50\">\r\n          <template v-slot=\"scope\">\r\n            {{ initTableIndex('page_brief', scope.$index) }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.detail')\" align=\"center\" width=\"80\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button size=\"small\" type=\"text\" @click=\"showFormula(scope.row)\">>></el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :filter-method=\"filters_method\" :filters=\"filters_pdCode\" :label=\"$t('rule.product')\"\r\n                         align=\"center\" min-width=\"200\" prop=\"pdCode\" sortable>\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdCode ? scope.row.pdCode + ':' + scope.row.pdName : '' }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('invFormula.pdInvType')\" align=\"center\" min-width=\"200\">\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdInvType | filter_pdInvType }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('invFormula.invItem')\" align=\"center\" min-width=\"200\" prop=\"pdInvCode\" sortable>\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdInvCode ? scope.row.pdInvCode + ':' + scope.row.pdInvName : '' }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('rule.pdBussDetailType')\" align=\"center\" min-width=\"200\" prop=\"pdBussDetailType\">\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdBussDetailType | filter_pdBussDetailType }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('rule.pdCalcCode')\" align=\"center\" min-width=\"200\" prop=\"pdCalcCode\"\r\n                         sortable></el-table-column>\r\n        <el-table-column :label=\"$t('rule.pdCalcDesc')\" align=\"center\" min-width=\"200\"\r\n                         prop=\"pdCalcDesc\"></el-table-column>\r\n        <el-table-column :label=\"$t('rule.ruleAttr')\" align=\"center\" min-width=\"200\">\r\n          <template v-slot=\"scope\">\r\n            {{\r\n              scope.row.ruleAttr === '1' ? $t('rule.general') : (scope.row.ruleAttr === '2' ? $t('rule.individual') : '')\r\n            }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.actions')\" align=\"center\" fixed=\"right\" width=\"120\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button v-auth=\"'invAmountFormul:associate'\" icon=\"el-icon-connection\"\r\n                       size=\"mini\" type=\"primary\" @click=\"handleSelectFormula(scope.row)\">\r\n              {{ $t('form.select') }}\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <cus-pagination ref=\"page_brief\" v-model=\"tableData_brief\" :autoBuild=\"false\" :pageSize=\"5\" :params=\"params_brief\"\r\n                      url=\"/invAmountFormul/base/briefFormula\"/>\r\n    </div>\r\n\r\n    <!-- 规则引擎公式定义弹窗 -->\r\n    <cus-rule-definition :attributeObj=\"attributeObj\" :ruleDefinitionClosed=\"ruleModuleClosed\"\r\n                         :visible=\"formulaVisible\"></cus-rule-definition>\r\n\r\n    <!-- 公式查看和决策树编辑弹窗 -->\r\n    <cus-rule-modify :flag=\"formulaView.flag\" :performType=\"formulaView.performType\" :ruleCode=\"formulaView.ruleCode\"\r\n                     :ruleModifyClosed=\"formulaViewClosed\" :ruleType=\"'2'\"\r\n                     :visible=\"formulaView.visible\"></cus-rule-modify>\r\n\r\n  </cus-wraper>\r\n</template>\r\n\r\n<script>\r\n\r\nimport {associate, deleteFormula, getCalcCode, getInvByCode} from \"@/api/basicinfo/invAmountFormula\"\r\nimport {findInvBypdInvType} from '@/api/basicinfo/pdtemppdinv'\r\n\r\nlet _this\r\n\r\nexport default {\r\n  name: 'invFormula',\r\n  filters: {\r\n    filter_pdBussDetailType(code) {\r\n      return _this.getCodeName('ruleBusinessDetailClass', code)\r\n    },\r\n    filter_pdInvType(code) {\r\n      return _this.getCodeName('pdinvtype', code)\r\n    },\r\n    filter_pdInvStartPeriodUnit(code) {\r\n      return _this.getCodeName('pdsbnftdy', code, true)\r\n    }\r\n  },\r\n  props: {\r\n    // 公式大类\r\n    guaranteeDetails: {\r\n      type: String,\r\n      required: true\r\n    },\r\n    // 公式细类\r\n    calculationFormulaType: {\r\n      type: String,\r\n      required: true\r\n    },\r\n    // 收益编码\r\n    pdUwSiTypeOptions: {\r\n      type: String,\r\n      default: undefined\r\n    },\r\n    // 收益类型\r\n    pdUwSiTypeDtOptions: {\r\n      type: String,\r\n      default: undefined\r\n    },\r\n    // 订正标识\r\n    productCalcRevisionShowINfo: {\r\n      type: Boolean,\r\n      required: false,\r\n      default: false\r\n    }\r\n  },\r\n  data() {\r\n    _this = this\r\n    return {\r\n      selectedInvInRev: {},\r\n      params_inv: {\r\n        eq_pdInfoId: undefined\r\n      },\r\n      params_current: {\r\n        eq_pdInfoId: undefined,\r\n        eq_pdBussType: '',\r\n        eq_pdBussDetailType: '',\r\n        eq_pdBussCode: ''\r\n      },\r\n      params_brief: {\r\n        product: undefined,\r\n        pdBussId: undefined,\r\n        productDetailType: '',\r\n        pdBussDetailType: this.calculationFormulaType\r\n      },\r\n      formulaView: {\r\n        flag: '',\r\n        ruleCode: '',\r\n        performType: '',\r\n        visi