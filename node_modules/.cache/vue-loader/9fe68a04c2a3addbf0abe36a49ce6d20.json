t\t\t\t\t\t\t\t\treturn\r\n\t\t\t\t\t\t\t\t}\r\n\t\t\t\t\t\t\t} else {\r\n\t\t\t\t\t\t\t\tthis.messageError(this.$t('productRevision.pleaseSelectReviseFlag'))\r\n\t\t\t\t\t\t\t\treturn\r\n\t\t\t\t\t\t\t}\r\n\r\n\r\n\t\t\t\t\t\t\trevisePdInfoData(this.buildRequestData(map)).then(res => {\r\n\t\t\t\t\t\t\t\tif (res.code == 200) {\r\n\t\t\t\t\t\t\t\t\tthis.notifySuccess(res.message);\r\n\t\t\t\t\t\t\t\t\tthis.$refs.page.reload();\r\n\t\t\t\t\t\t\t\t\tthis.chooseRowId = '';\r\n\t\t\t\t\t\t\t\t\tcount = 0;\r\n\t\t\t\t\t\t\t\t}\r\n\r\n\t\t\t\t\t\t\t});\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t}\r\n\t\t\t\t)\r\n\t\t\t\tthis.dialogTableVisible = false;\r\n\t\t\t},\r\n\t\t\tprepareData() {\r\n\t\t\t\tif (this.refForm.reviseFlag) {\r\n\t\t\t\t\tthis.pdpdReviseInfo.pdReturnFuncCode = this.refForm.changeCheckList;\r\n\t\t\t\t\tthis.pdpdReviseInfo.pdReviseType = this.refForm.reviseFlag;\r\n\t\t\t\t}\r\n\t\t\t\tthis.pdSelForm.pdName = this.pdpdInfo.pdName;\r\n\t\t\t\tthis.pdSelForm.pdCode = this.pdpdInfo.pdCode;\r\n\t\t\t\tthis.pdSelForm.pdEdition = this.pdpdInfo.pdEdition;\r\n\t\t\t\tthis.pdSelForm.pdApplDate = this.pdpdInfo.pdApplDate;\r\n\t\t\t}\r\n\r\n\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n\r\n  .expandWidth {\r\n    width: 100% !important;\r\n  }\r\n\r\n\r\n  .notTextarea .el-textarea__inner {\r\n    border: 0;\r\n    color: #606266;\r\n    resize: none;\r\n    background-color: white;\r\n    padding: 0;\r\n    overflow: hidden;\r\n    margin-top: 3px;\r\n    width: 65%;\r\n  }\r\n\r\n  .pre-icon .el-form-item__label:before {\r\n    content: \"\\007C\"\r\n  }\r\n</style>\r\n<style lang=\"scss\" scoped>\r\n  .min-table {\r\n    width: 50%\r\n  }\r\n\r\n  .listRight {\r\n    padding-right: 30px;\r\n  }\r\n  .el-link{\r\n    font-size: 12px;\r\n  }\r\n\r\n\r\n</style>\r\n"]}]}