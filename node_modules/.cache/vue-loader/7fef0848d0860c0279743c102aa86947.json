rDataFlag=false;\r\n            this.chooseRow = row;\r\n            this.numberParams.pdInfoId = row.id;\r\n            this.findAuditNumberData()\r\n          }\r\n        }\r\n      },\r\n      //选择审核次数表格\r\n      async chooseNumberAuditRow(row, column, event) {\r\n        if (event.target.nodeName === \"INPUT\") {\r\n          if (row.id === this.chooseNumberRow.id) {\r\n            this.chooseNumberRow = {};\r\n            this.showAuditNumberDataFlag = false;\r\n            this.pdAuditConc = {}\r\n          } else {\r\n            this.showAuditNumberDataFlag = true;\r\n            this.chooseNumberRow = row;\r\n            this.pdAuditConc = row.pdAuditConc;\r\n            await this.auditDetailsData(row);\r\n            await this.auditDetailsNoteData(this.pdAuditDetailInfo)\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n</style>\r\n"]}]}