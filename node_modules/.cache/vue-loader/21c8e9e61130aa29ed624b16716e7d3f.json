"activeName\" @click=\"onClick\">\r\n      <van-tab :title=\"tabTitle1\" name=\"product\" style=\"height: 1000px\">\r\n        <van-divider\r\n          :style=\"{ color: '#2C40A6', borderColor: '#2C40A6', padding: '0 16px' }\"\r\n        >\r\n          {{$t('i18nView.pdPostAmount')}}:{{ releasePdCount }}\r\n        </van-divider>\r\n        <bar-chart :title=\"title1\" v-if=\"chart1Show\" :callBack=\"refreshChBar\" :id=\"chart1\"\r\n                   :style=\"{height:'450px',width:'100%'}\" :xData=\"chart1xData\" :yData=\"chart1yData\"/>\r\n\r\n        <br/>\r\n        <br/>\r\n        <br/>\r\n\r\n        <bar-chart :title=\"title2\" v-if=\"chart2Show\" :id=\"chart2\" :style=\"{height:'450px',width:'100%'}\"\r\n                   :xData=\"chart2xData\" :yData=\"chart2yData\"/>\r\n      </van-tab>\r\n\r\n\r\n      <van-tab title=\"产品套餐 (0)\" name=\"pdPackage\">\r\n\r\n\r\n\r\n      </van-tab>\r\n\r\n\r\n    </van-tabs>\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\n\r\nimport Vue from 'vue'\r\nimport { NavBar, CellGroup, Cell, Row, Col, Tab, Tabs,Divider } from 'vant'\r\n\r\nimport {\r\n  indexData\r\n} from '@/api/system/index'\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\nVue.use(Divider)\r\nVue.use(Col)\r\nVue.use(Row)\r\nVue.use(CellGroup)\r\nVue.use(Cell)\r\nVue.use(NavBar)\r\nVue.use(Tab)\r\nVue.use(Tabs)\r\n\r\nimport BarChart from '@/views/dashboard/admin/components/BarChart'\r\n\r\n\r\nexport default {\r\n  name: 'productOverview',\r\n  props: {},\r\n  components: {\r\n    BarChart\r\n  },\r\n  data() {\r\n    return {\r\n      pdId: '',\r\n      chart1Show: false,\r\n      chartShow: false,\r\n      chart1: null,\r\n      chart1xData: [],\r\n      chart1yData: [],\r\n      chart2Show: false,\r\n      chart2: null,\r\n      chart2xData: [],\r\n      chart2yData: [],\r\n      barCount: '',\r\n      releasePdCount: '',\r\n      activeName: 'product',\r\n      title1: this.$t('monitorUser.productDefStatistics'),\r\n      tabTitle1: '',\r\n      title2: this.$t('monitorUser.productOperationStatistics'),\r\n      processShow: 'pdInfo',\r\n      statusData: {},\r\n      form: {\r\n        referenceProducts: []\r\n      },\r\n      pdform: {\r\n        pdCode: '',\r\n        pdEdition: '',\r\n        pdName: ''\r\n      }\r\n    }\r\n  },\r\n\r\n  created() {\r\n    let pdtempPdInfo = this.getJsonData('phone-pdInfo')\r\n    if (pdtempPdInfo) {\r\n      this.pdId = pdtempPdInfo.id\r\n      this.pdform.pdCode = pdtempPdInfo.pdCode\r\n      this.pdform.pdEdition = pdtempPdInfo.pdEdition\r\n      this.pdform.pdName = pdtempPdInfo.pdName\r\n    }\r\n    this.getIndexData()\r\n  },\r\n  mounted() {\r\n\r\n  },\r\n  methods: {\r\n    onClick(name) {\r\n\r\n\r\n    },\r\n    onClickLeft() {\r\n      this.$router.go(-1)\r\n    },\r\n    getIndexData() {\r\n      indexData(this.buildRequestData({})).then(res => {\r\n        let data = res.data\r\n        this.releasePdCount = data.releasePdCount\r\n        this.barData = data.barData\r\n        this.chart1xData = data.barData.xData\r\n        this.chart1yData = data.barData.yData\r\n        this.chart2xData = data.barData.productOperating.xData\r\n        this.chart2yData = data.barData.productOperating.yData\r\n\r\n        this.barCount = this.chart1yData.reduce(function(prev, curr, idx, arr) {\r\n          return prev + curr\r\n        })\r\n\r\n        this.tabTitle1 = \"产品(\"+this.barCount+\")\"\r\n\r\n        this.cardData = { 'taskCount': data.taskCount,monitorShow: this.monitorShow}\r\n        this.cardShow = true\r\n        this.initChart()\r\n      })\r\n    },\r\n    initChart() {\r\n      this.chart1 = 'chart1'\r\n      this.chart1Show = true\r\n\r\n      this.chart2 = 'chart2'\r\n      this.title2 = this.$t('monitorUser.productOperationStatistics')\r\n      this.chart2Show = true\r\n\r\n    },\r\n    refreshChBar(name) {\r\n      if (name === '产品营运定义') {\r\n        this.title2 = this.$t('monitorUser.productOperationStatistics')\r\n        this.chart2xData = this.barData.productOperating.xData\r\n        this.chart2yData = this.barData.productOperating.yData\r\n\r\n      } else if (name === '产品测试') {\r\n        this.title2 = '险种测试统计'\r\n        this.chart2xData = this.barData.productTest.xData\r\n        this.chart2yData = this.barData.productTest.yData\r\n      }\r\n    },\r\n  },\r\n  destroyed() {\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n\r\n</style>\r\n"]}]}