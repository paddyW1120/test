;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/views/product-definition/pd-operation-def/pd-underwriting-def/underwritingAmountFormula","sourcesContent":["<template>\r\n  <cus-wraper>\r\n\r\n    <cus-page-title-back v-if=\"!this.mark\" :isDialog=\"true\" :label=\"$t('pdtempPdUnderwritingAmountFormula.pdUwFormulaDef')\" @backTo=\"back\"/>\r\n\r\n    <div v-show=\"this.mark\">\r\n      <el-row>\r\n        <cus-page-title-back :isDialog=\"true\" :label=\"$t('pdtempPdUnderwritingAmountFormula.pdFormulaRevDetail')\" @backTo=\"back\"/>\r\n      </el-row>\r\n      <cus-product-info></cus-product-info>\r\n    </div>\r\n\r\n    <!-- 核保保额类型 -->\r\n    <el-form label-width=\"auto\">\r\n      <el-row :gutter=\"10\" type=\"flex\">\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('pdtempPdUnderwritingAmountFormula.pdUwSiType')\" prop=\"pdUwSiType\">\r\n            <el-select v-model=\"pdUwSiType\" disabled>\r\n              <el-option v-for=\"item in pdUwSiTypeOptions\" :key=\"item.value\" :label=\"item.label\"\r\n                         :value=\"item.value\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n    </el-form>\r\n\r\n    <!-- 已选择公式 -->\r\n    <cus-form-title :first=\"true\" :label=\"$t('rule.selectedFormula')\"></cus-form-title>\r\n    <cus-table-title :label=\"$t('pdtempPdUnderwritingAmountFormula.underwritingProductAmountFormula')\"/>\r\n    <el-table :data=\"tableData_current\" border element-loading-text=\"Loading\" fit highlight-current-row\r\n              max-height=\"300\" size=\"mini\" style=\"margin: 20px 0 0 0\" @row-click=\"rowClick\">\r\n      <el-table-column :label=\"$t('table.select')\" align=\"center\" width=\"50\">\r\n        <template slot-scope=\"scope\">\r\n          <el-radio-group v-model=\"selectedFormula\" @change=\"selectedFormulaChanged\">\r\n            <el-radio :label=\"scope.row\">{{ '' }}</el-radio>\r\n          </el-radio-group>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" width=\"50\">\r\n        <template v-slot=\"scope\">\r\n          {{ initTableIndex('page_current', scope.$index) }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.detail')\" align=\"center\" width=\"80\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button dialog-disable=\"disable\" size=\"small\" type=\"text\" @click=\"showFormula(scope.row)\">>></el-button>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('pdtempPdUnderwritingAmountFormula.pdUwSiType')\" align=\"center\" min-width=\"200\" prop=\"pdUwSiType\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.row.pdUwSiType | filter_pdUwSiType }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('pdtempPdUnderwritingAmountFormula.pdCalcCode')\" align=\"center\"\r\n                       min-width=\"200\" prop=\"pdCalcCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('pdtempPdUnderwritingAmountFormula.pdCalcDesc')\" align=\"center\"\r\n                       min-width=\"200\" prop=\"pdCalcDesc\"></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('rule.ruleAttr')\" min-width=\"200\">\r\n        <template v-slot=\"scope\">\r\n          {{\r\n            scope.row.ruleAttr === '1' ? $t('rule.general') : (scope.row.ruleAttr === '2' ? $t('rule.individual') : '')\r\n          }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.actions')\" align=\"center\" fixed=\"right\" width=\"120\">\r\n        <template slot-scope=\"scope\">\r\n          <cus-popover v-auth=\"'pdtemp-pd-underwriting-amount-formula:delete'\" :content=\"$t('confirmMessage.delete')\"\r\n                       icon=\"el-icon-delete\" size=\"mini\" type=\"danger\"\r\n                       @confirm=\"deleteFormula(scope.row)\"></cus-popover>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <cus-pagination ref=\"page_current\" v-model=\"tableData_current\" :autoBuild=\"false\" :pageSize=\"5\"\r\n                    :params=\"params_current\" url=\"/pdtemp-pd-underwriting-amount-formula/base/currentFormula\"/>\r\n\r\n    <!-- 可选择公式 -->\r\n    <cus-form-title :first=\"true\" :label=\"$t('rule.selectableFormula')\"></cus-form-title>\r\n\r\n    <el-row style=\"text-align: right\">\r\n      <el-button dialog-disable=\"disable\" icon=\"el-icon-connection\" size=\"small\" type=\"primary\" @click=\"toFormulaSelect\">\r\n        {{ $t('rule.moreFormula') }}\r\n      </el-button>\r\n      <el-button dialog-disable=\"disable\" icon=\"el-icon-circle-plus el-icon--left\" size=\"small\" type=\"primary\" @click=\"toAddFormula\">\r\n        {{ $t('rule.toAddFormula') }}\r\n      </el-button>\r\n    </el-row>\r\n\r\n    <cus-table-title :label=\"$t('pdtempPdUnderwritingAmountFormula.underwritingAmountFormula')\"/>\r\n    <el-table :data=\"tableData_brief\" border element-loading-text=\"Loading\" fit highlight-current-row\r\n              max-height=\"300\" size=\"mini\" style=\"margin: 20px 0 0 0\" @row-click=\"rowClick\">\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" width=\"50\">\r\n        <template v-slot=\"scope\">\r\n          {{ initTableIndex('page_brief', scope.$index) }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column  align=\"center\" :label=\"$t('i18nView.pdCode')\" min-width=\"200\" prop=\"pdCode\" >\r\n      </el-table-column>\r\n      <el-table-column  align=\"center\" :label=\"$t('i18nView.pdName')\" min-width=\"200\" prop=\"pdName\" >\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('pdtempPdUnderwritingAmountFormula.pdUwSiType')\" align=\"center\" min-width=\"200\" prop=\"pdUwSiType\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.row.pdUwSiType | filter_pdUwSiType }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('pdtempPdUnderwritingAmountFormula.pdCalcCode')\" align=\"center\"\r\n                       min-width=\"200\" prop=\"pdCalcCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('pdtempPdUnderwritingAmountFormula.pdCalcDesc')\" align=\"center\"\r\n                       min-width=\"200\" prop=\"pdCalcDesc\"></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('rule.ruleAttr')\" min-width=\"200\">\r\n        <template v-slot=\"scope\">\r\n          {{\r\n            scope.row.ruleAttr === '1' ? $t('rule.general') : (scope.row.ruleAttr === '2' ? $t('rule.individual') : '')\r\n          }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.actions')\" align=\"center\" fixed=\"right\" width=\"120\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button dialog-disable=\"disable\" icon=\"el-icon-paperclip\" size=\"mini\" type=\"primary\" @click=\"associate(scope.row)\">\r\n            {{ $t('form.select') }}\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <cus-pagination ref=\"page_brief\" v-model=\"tableData_brief\" :autoBuild=\"false\" :pageSize=\"5\"\r\n                    :params=\"params_brief\" url=\"/pdtemp-pd-underwriting-amount-formula/base/briefFormula\"/>\r\n\r\n    <!-- 规则引擎公式定义弹窗 -->\r\n    <cus-rule-definition :attributeObj=\"attributeObj\" :ruleDefinitionClosed=\"ruleModuleClosed\"\r\n                         :visible=\"formulaVisible\"></cus-rule-definition>\r\n\r\n    <!-- 公式查看和决策树编辑弹窗 -->\r\n    <cus-rule-modify :flag=\"ruleView.flag\" :performType=\"ruleView.performType\" :ruleCode=\"ruleView.ruleCode\"\r\n                     :ruleModifyClosed=\"formulaViewClosed\" :ruleType=\"'2'\"\r\n                     :visible=\"ruleView.visible\"></cus-rule-modify>\r\n\r\n  </cus-wraper>\r\n</template>\r\n\r\n<script>\r\n\r\nimport {deleteAmountFormula} from \"@/api/basicinfo/underwritingAmountFormula\"\r\nimport {associate, getCalcCode} from \"@/api/basicinfo/underwritingAmountFormulaSelect\"\r\n\r\nlet _this\r\n\r\nexport default {\r\n  name: 'underwritingAmountFormula',\r\n  props: {\r\n    pdUwSiTypeForFormula: {\r\n      type: String,\r\n      required: false,\r\n      default: ''\r\n    },\r\n    mark: {\r\n      type: Boolean,\r\n      default: undefined\r\n    },\r\n    pdUwSiTypeOptions: {\r\n      type: Array,\r\n      required: false,\r\n      default: () => {\r\n        return []\r\n      }\r\n    },\r\n    definingAmount: {\r\n      type: Object,\r\n      required: false,\r\n      default: () => {\r\n        return {}\r\n      }\r\n    }\r\n  },\r\n  filters: {\r\n    filter_pdUwSiType(codeArray) {\r\n      if (codeArray) {\r\n        return _this.getCascaderCodeName('underwritingAmountType', codeArray)\r\n      } else {\r\n        return ''\r\n      }\r\n    }\r\n  },\r\n  data() {\r\n    _this = this\r\n    return {\r\n      ruleView: {\r\n        visible: false,\r\n        performType: '',\r\n        flag: '',\r\n        ruleCode: '',\r\n      },\r\n      pdUwSiType: '',\r\n      selectedFormula: {},\r\n      params_current: {\r\n        pdInfoId: undefined,\r\n        currentBussId: this.definingAmount.id\r\n      },\r\n      params_brief: {\r\n        product: undefined,\r\n        pdBussId: this.definingAmount.id,\r\n        productDetailType: '',\r\n        pdBussDetailType: this.pdUwSiTypeForFormula.startsWith('FX') ? '41' : '42'\r\n      },\r\n      tableData_current: [],\r\n      tableData_brief: [],\r\n      attributeObj: {},\r\n      formulaVisible: false,\r\n      product: {\r\n        id: undefined,\r\n        bType: '',\r\n        cType: '',\r\n        pdCode: '',\r\n        pdEdition: '',\r\n        pdVerId: undefined,\r\n        pdSalechnl: '',\r\n        pdName: '',\r\n        pdShortname: '',\r\n        pdEnName: '',\r\n        pdShortEnName: '',\r\n        operator: '',\r\n        pdApplDate: undefined,\r\n        pdProcessId: undefined,\r\n        createTime: undefined,\r\n        updateTime: undefined\r\n      },\r\n      filters_pdCode: []\r\n    }\r\n  },\r\n  watch: {\r\n    tableData_brief() {\r\n      this.filters_pdCode = []\r\n      this.tableData_brief.forEach(val => {\r\n        if (!this._.includes(this._.map(this.filters_pdCode, 'value'), val.pdCode)) {\r\n          this.filters_pdCode.push({\r\n            text: val.pdCode + ':' + val.pdName,\r\n            value: val.pdCode\r\n          })\r\n        }\r\n      })\r\n    }\r\n  },\r\n  mounted() {\r\n  },\r\n  created() {\r\n    this.initProductInfo()\r\n    // 初始化核保保额类型\r\n    this.pdUwSiType = this.pdUwSiTypeForFormula\r\n    this.$nextTick(() => {\r\n      this.$refs.page_current.reload()\r\n      this.$refs.page_brief.reload()\r\n    })\r\n  },\r\n  methods: {\r\n    initProductInfo() {\r\n      let pdtempPdInfo = this.getPdData();\r\n      if (Object.keys(pdtempPdInfo).length > 0) {\r\n        Object.assign(this.product, pdtempPdInfo)\r\n        this.params_current.pdInfoId = this.product.id\r\n        this.params_brief.product = this.product\r\n        this.params_brief.productDetailType = this.product.cType\r\n      } else {\r\n        this.notifyError(this.$t('message.invalidProductInfo'))\r\n      }\r\n    },\r\n    deleteFormula(row) {\r\n      deleteAmountFormula(row).then(() => {\r\n        this.notifySuccess(this.$t('successMessage.delete'))\r\n        this.$refs.page_current.reload()\r\n        this.$refs.page_brief.reload()\r\n      })\r\n    },\r\n    back() {\r\n      this.$emit('changeDialogVisible_formula', false)\r\n    },\r\n    toFormulaSelect() {\r\n      if (!this.pdUwSiType) {\r\n        this.notifyError(this.$t('pdtempPdUnderwritingAmountFormula.pleaseSelectPdUwSiType'))\r\n        return\r\n      }\r\n      this.$emit('changeDialogVisible_formulaSelect', true, this.pdUwSiType)\r\n    },\r\n    rowClick(row, column, event) {\r\n      if (event.target.nodeName === \"INPUT\") {\r\n        if (row.id === this.selectedFormula.id) {\r\n          this.selectedFormula = {}\r\n        }\r\n      }\r\n    },\r\n    ruleModuleClosed() {\r\n      this.formulaVisible = false\r\n      this.$refs.page_brief.reload()\r\n    },\r\n    formulaViewClosed() {\r\n      this.ruleView.visible = false\r\n      this.selectedFormula = {}\r\n    },\r\n    selectedFormulaChanged() {\r\n      this.ruleView.flag = this.selectedFormula.ruleAttr === '1' ? '1' : '2'\r\n      this.ruleView.performType = this.selectedFormula.performType\r\n      this.ruleView.ruleCode = this.selectedFormula.pdCalcCode\r\n      this.ruleView.visible = true\r\n    },\r\n    toAddFormula() {\r\n      const param = {\r\n        pdBussType: '8',\r\n        pdBussDetailType: this.pdUwSiTypeForFormula.startsWith('FX') ? '41' : '42'\r\n      }\r\n      getCalcCode(this.buildRequestData(param)).then(response => {\r\n        this.attributeObj = {\r\n          code: response.data,\r\n          ruleBussModu: '8',\r\n          ruleBussModuDeta: param.pdBussDetailType,\r\n          ruleType: '2',\r\n          ruleAttr: '2',\r\n          pdType: '1',\r\n          riskType: this.product.bType,\r\n          riskDetail: this.product.cType,\r\n          pdCode: this.product.pdCode,\r\n          pdEdition: this.product.pdEdition\r\n        }\r\n        this.formulaVisible = true\r\n      })\r\n    },\r\n    showFormula(row) {\r\n      this.ruleView.flag = '1'\r\n      this.ruleView.performType = row.performType\r\n      this.ruleView.ruleCode = row.pdCalcCode\r\n      this.ruleView.visible = true\r\n    },\r\n    associate(row) {\r\n      const param = {\r\n        product: this.product,\r\n        formula: row,\r\n        amount: this.definingAmount,\r\n        needChecking: true\r\n      }\r\n      associate(this.buildRequestData(param)).then(response => {\r\n        let flag = response.data.flag\r\n        if (flag === true) {\r\n          this.notifySuccess(this.$t('rule.associationSuccess'))\r\n          this.$refs.page_current.reload()\r\n          this.$refs.page_brief.reload()\r\n        } else {\r\n          this.$confirm(String(this.$t('underwritingAmountFormulaSelect.confirm_overwrite_formula')), String(this.$t('confirmMessage.tip')), {\r\n            confirmButtonText: String(this.$t('confirmMessage.confirm')),\r\n            cancelButtonText: String(this.$t('confirmMessage.cancel')),\r\n            type: 'warning'\r\n          }).then(() => {\r\n            param.needChecking = false\r\n            associate(this.buildRequestData(param)).then(() => {\r\n              this.notifySuccess(this.$t('rule.associationSuccess'))\r\n              this.$refs.page_current.reload()\r\n              this.$refs.page_brief.reload()\r\n            })\r\n          }).catch(() => {\r\n            this.messageInfo(this.$t('underwritingAmountFormulaSelect.canceled'))\r\n          })\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n</style>\r\n"]}]}