true\"\r\n        :label=\"$t('pdReleaseInfoAll.pdaudith')\"\r\n        @backTo=\"backAuditPage\"\r\n      ></cus-page-title-back>\r\n      <cus-product-info></cus-product-info>\r\n      <cus-table-title :label=\"$t('pdReleaseInfoAll.audith')\"></cus-table-title>\r\n      <el-table\r\n        :data=\"tableData\"\r\n        border\r\n        element-loading-text=\"Loading\"\r\n        fit\r\n        highlight-current-row\r\n        size=\"mini\"\r\n      >\r\n        <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\" width=\"55px\"></el-table-column>\r\n        <el-table-column\r\n          :label=\"$t('i18nView.auditRes')\"\r\n          align=\"center\"\r\n          prop=\"pdAuditConc\"\r\n        >\r\n          <template v-slot=\"scope\">{{ scope.row.pdAuditConc | getPdAuditConc }}</template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          :label=\"$t('i18nView.auditContent')\"\r\n          align=\"center\"\r\n          prop=\"pdAuditIdea\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          :label=\"$t('i18nView.auditOper')\"\r\n          align=\"center\"\r\n          prop=\"pdAuditStaff\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n          :label=\"$t('i18nView.auditDate')\"\r\n          align=\"center\"\r\n          prop=\"pdAuditDate\"\r\n        ></el-table-column>\r\n      </el-table>\r\n    </el-dialog>\r\n    <el-dialog\r\n      v-if=\"dialogReleaseNoteVisible\"\r\n      v-dialog-drag\r\n      :close-on-click-modal=\"false\"\r\n      :visible.sync=\"dialogReleaseNoteVisible\"\r\n      append-to-body\r\n      width=\"75%\"\r\n    >\r\n      <cus-page-title-back :isDialog=\"true\" :label=\"$t('Releasedstatistics.pdreleaserecordstatistics')\"\r\n                           @backTo=\"backReleaseRecord\"></cus-page-title-back>\r\n      <cus-product-info></cus-product-info>\r\n      <cus-table-title :label=\"$t('i18nView.releRecord')\"></cus-table-title>\r\n      <el-table\r\n        :data=\"releInfoData\"\r\n        border\r\n        element-loading-text=\"Loading\"\r\n        fit\r\n        highlight-current-row\r\n        size=\"mini\"\r\n        tooltip-effect=\"dark\"\r\n      >\r\n        <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\"></el-table-column>\r\n        <el-table-column\r\n          :label=\"$t('Releasedstatistics.pdVerId')\"\r\n          align=\"center\"\r\n          prop=\"pdVerId\"\r\n        ></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.pdReleType')\" align=\"center\" prop=\"pdReleType\">\r\n          <template slot-scope=\"scope\">{{ scope.row.pdReleType | filterReleasetypeAll }}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.postContent')\" align=\"center\" prop=\"pdReleContent\">\r\n          <template slot-scope=\"scope\">{{ scope.row.pdReleContent | filterReleaseContent }}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.releaseMethod')\" align=\"center\" prop=\"pdReleWay\">\r\n          <template slot-scope=\"scope\">{{ scope.row.pdReleWay | filterReleaseWay }}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.pdAuditConc')\" align=\"center\" prop=\"pdReleEnv\">\r\n          <template\r\n            slot-scope=\"scope\"\r\n          >{{ _.map(scope.row.pdoperPdSysRlsTrcList, 'pdReleSyspathName').join(', ') }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.releaseDate')\" align=\"center\" prop=\"pdReleDate\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.operator')\" align=\"center\" prop=\"operator\"></el-table-column>\r\n        <el-table-column :label=\"$t('table.download')\" align=\"center\" width=\"80px\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              icon=\"el-icon-download\"\r\n              size=\"small\"\r\n              type=\"text\"\r\n              @click=\"downloadReleFile(scope.row)\"\r\n            ></el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-dialog>\r\n\r\n    <cus-page-dialog :component=\"dialogComponent\" :dialogShow=\"dialogShow\" :dialogTitle=\"dialogTitle\"\r\n                     @close=\"closePageDialog\"></cus-page-dialog>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\nimport {\r\n  getAllPath,\r\n  getPdFiles,\r\n  pdReviewHistory,\r\n  productRelease,\r\n  queryPdSelectList,\r\n  searchPDInRelease\r\n} from \"@/api/basicinfo/pdtemppdrelease\";\r\nimport {downloadReleFile, getPdtempPdVerRls, pdReleOver} from \"@/api/basicinfo/pdtemppdverrls\";\r\n\r\nlet count = 0;\r\nlet _this;\r\n\r\nexport default {\r\n  name: \"releaseAll\",\r\n  filters: {\r\n    /*审核结论*/\r\n    getPdAuditConc(state) {\r\n      return state === \"N\" ? \"不通过\" : \"通过\";\r\n    },\r\n    /*退回功能点*/\r\n    getPdReturnFuncNode(node) {\r\n      let mnode = JSON.parse(node);\r\n      return mnode.join(\",\");\r\n    },\r\n    filter_getFileType(code) {\r\n      if (code) {\r\n        return _this.getCascaderCodeName(\"pdfiletype\", code);\r\n      } else {\r\n        return \"\";\r\n      }\r\n    },\r\n    filterReleaseWay(val) {\r\n      return _this.getCodeName(\"releaseway\", val);\r\n    },\r\n    filterReleasetypeAll(val) {\r\n      return _this.getCodeName(\"releasetypeall\", val);\r\n    },\r\n    filter_getFileName(code) {\r\n      if (code) {\r\n        let filename = JSON.parse(code);\r\n        let rename = \"\";\r\n        for (let i = 0; i < filename.length; i++) {\r\n          if (i === filename.length - 1) {\r\n            rename += filename[i];\r\n          } else {\r\n            rename += filename[i] + \"; \";\r\n          }\r\n        }\r\n        return rename;\r\n      } else {\r\n        return \"\";\r\n      }\r\n    },\r\n    filter_getUpdDepmt(code) {\r\n      if (code) {\r\n        return _this.getCodeName(\"pddepmt\", code);\r\n      } else {\r\n        return \"\";\r\n      }\r\n    },\r\n    filter_getPdFilePurp(code) {\r\n      if (code) {\r\n        return _this.getCodeName(\"pdpurp\", code);\r\n      } else {\r\n        return \"\";\r\n      }\r\n    },\r\n    filterReleaseContent(val) {\r\n      return _this.getCodeName(\"pdReleContent\", val);\r\n    },\r\n    filterFlag(val) {\r\n      if (!val) {\r\n        return \"1:未发布\";\r\n      } else {\r\n        return _this.getCodeName(\"pdReleStu\", val);\r\n      }\r\n    },\r\n    filter_FuncNodeName(val) {\r\n      let funcNode = JSON.parse(val);\r\n      let returnPoints = [];\r\n      for (let i = 0; i < funcNode.length; i++) {\r\n        for (let j = 0; j < _this.nodeOptions.length; j++) {\r\n          if (funcNode[i] === _this.nodeOptions[j].key) {\r\n            returnPoints.push(_this.nodeOptions[j].label);\r\n          }\r\n        }\r\n      }\r\n      if (returnPoints.length > 0) {\r\n        return returnPoints;\r\n      } else {\r\n        return val;\r\n      }\r\n    },\r\n    filter_pdBussType(code) {\r\n      return _this.getCodeName(\"ruleBusinessModule\", code);\r\n    },\r\n    filter_pdBussDetailType(code) {\r\n      return _this.getCodeName(\"ruleBusinessDetailClass\", code);\r\n    }\r\n  },\r\n  watch: {\r\n    \"PdtempPdVerRls.pdAuditConc\"(val) {\r\n      this.envList = val;\r\n    },\r\n    \"params.pdAuditDate\"(val) {\r\n      if (val) {\r\n        this.params.startDate = val[0];\r\n        this.params.endDate = val[1];\r\n      } else {\r\n        this.params.startDate = \"\";\r\n        this.params.endDate = \"\";\r\n      }\r\n    }\r\n  },\r\n  data() {\r\n    _this = this;\r\n    return {\r\n      nodeOptions: [\r\n        {key: \"productInfoFlag\", label: \"产品框架定义\"},\r\n        {key: \"productDistributionFlag\", label: \"产品销售信息\"},\r\n        {key: \"productCoverFlag\", label: \"产品承保信息\"},\r\n        {key: \"productUnderwritingFlag\", label: \"产品核保信息\"},\r\n        {key: \"productRenewalFlag\", label: \"产品续期续保信息\"},\r\n        {key: \"productPreservationFlag\", label: \"产品保全信息\"},\r\n        {key: \"productHePeiFlag\", label: \"产品核赔信息\"},\r\n        {key: \"productRuleFlag\", label: \"产品规则订正\"},\r\n        {key: \"productFormulaFlag\", label: \"产品公式订正\"}\r\n      ],\r\n      dialogShow: false,\r\n      dialogTitle: '',\r\n      dialogComponent: '',\r\n      // 产品资料\r\n      filesData: [],\r\n      rulesData: [], // 规则数据\r\n      formulaData: [], // 公式数据\r\n      pdParams: [],\r\n      auditDateValue: \"\",\r\n      // 表格多选\r\n      multipleSelection: [],\r\n      checkId: [],\r\n      distinguishFieldPresentFlag: \"Y\",\r\n      PdtempPdVerRls: {\r\n        pdReleWay: \"\", //发布方式\r\n        pdEdition: \"\", //  产品版本\r\n        pdType: \"\", //  产品类型\r\n        pdInfoId: \"\", //  产品id\r\n        pdVerId: \"\", //  版别id\r\n        pdCode: \"\", //  产品编码\r\n        pdReleOrder: \"\", //  发布序号\r\n        pdReleType: \"\", //  发布类型 : 全量发布 增量发布\r\n        pdReleSys: \"\", //  发至系统\r\n        pdReleEnv: \"\", //  发至环境\r\n        pdReleFilePath: \"\", //  发布文件地址\r\n        pdReleStu: \"\", //  发布状态 : 发布成功 发布失败\r\n        pdReleContent: \"\", //  发布内容\r\n        pdReleDetails: \"\", //  发布说明\r\n        pdReleFile: \"\", //  生成文件名\r\n        pdAuditConc: [] //  发布环境\r\n      },\r\n      releButton: true,\r\n      showRele: false,\r\n      tableflag: false,\r\n      showRadio: false,\r\n      showCheck: false,\r\n      showRules: false,\r\n      showCalc: false,\r\n      pathtableDataBc: [],\r\n      pathtableData: [],\r\n      envList: [],\r\n      checkPath: [],\r\n\r\n      tableData: [],\r\n\r\n      readyReleaseInfo: false,\r\n      dialogTableVisible: false,\r\n      dialogReleaseNoteVisible: false,\r\n      pdOptions: [],\r\n      selectEdition: [],\r\n      pdInfo: {},\r\n      pdAuditDetailInfo: {\r\n        pdType: \"1\",\r\n        pdInfoId: \"\",\r\n        pdCode: \"\",\r\n        pdAuditOrderNum: \"1\",\r\n        pdAuditInfoId: \"\",\r\n        pdAuditConc: \"\",\r\n        pdAuditStaff: \"\",\r\n        pdAuditDate: \"\",\r\n        pdAuditIdea: \"\",\r\n        pdReturnFuncNode: []\r\n      },\r\n      paramsRule: {\r\n        pdInfoId: undefined\r\n      },\r\n      paramsCalc: {\r\n        pdInfoId: undefined\r\n      },\r\n      //表格数据\r\n      configs: [],\r\n      releInfoData: [],\r\n      //查询参数\r\n      params: {\r\n        pdReleType: \"1\",\r\n        startDate: \"\",\r\n        endDate: \"\",\r\n        pdCode: \"\",\r\n        pdEdition: \"\",\r\n        pdReleStu: \"\"\r\n      },\r\n      pickerOptions: {\r\n        shortcuts: [\r\n          {\r\n            text: \"最近一周\",\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 7);\r\n              picker.$emit(\"pick\", [start, end]);\r\n            }\r\n          },\r\n          {\r\n            text: \"最近一个月\",\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 30);\r\n              picker.$emit(\"pick\", [start, end]);\r\n            }\r\n          },\r\n          {\r\n            text: \"最近三个月\",\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 90);\r\n              picker.$emit(\"pick\", [start, end]);\r\n            }\r\n          }\r\n        ]\r\n      },\r\n      //检验规则\r\n      rules: {\r\n        pdReleWay: [\r\n          {required: true, message: \"请选择发布方式\", trigger: \"change\"}\r\n        ],\r\n        pdReleDetails: [\r\n          {required: true, message: \"请输入发布说明\", trigger: \"change\"}\r\n        ],\r\n        pdAuditConc: [\r\n          {required: true, message: \"请选择发布环境\", trigger: \"change\"}\r\n        ]\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.queryPdSelectList();\r\n    // this.refreshTableData(this.params);\r\n  },\r\n  methods: {\r\n    refreshTableData(param) {\r\n      searchPDInRelease(this.buildRequestData(param)).then(response => {\r\n        this.configs = response.data;\r\n      });\r\n    },\r\n    /* 父组件刷新子组件表格 */\r\n    refreshChildTable() {\r\n      searchPDInRelease(this.buildRequestData(this.params)).then(response => {\r\n        this.configs = response.data;\r\n      });\r\n      this.multipleSelection = [];\r\n      this.checkId = [];\r\n    },\r\n    /*  多选待发布数据，获取并准备 发布数据  */\r\n    handleSelectionChange(val) {\r\n      this.multipleSelection = [];\r\n      this.checkId = [];\r\n      if (val) {\r\n        this.showRele = val.length > 0;\r\n        if (this.showRele) {\r\n          for (const valElement of val) {\r\n            this.checkId.push(valElement.id);\r\n          }\r\n          this.multipleSelection = val;\r\n        } else {\r\n          this.refreshPage();\r\n        }\r\n      }\r\n    },\r\n    backAuditPage() {\r\n      this.dialogTableVisible = false;\r\n    },\r\n    backReleaseRecodePage() {\r\n      this.readyReleaseInfo = false;\r\n    },\r\n    /* 刷新 */\r\n    refreshPage() {\r\n      this.$refs.multipleTable.clearSelection();\r\n      this.$refs.PdtempPdVerRlsForm.resetFields();\r\n      this.pathtableData = [];\r\n      this.tableflag = false;\r\n    },\r\n    /* 发布确认 */\r\n    submitReleInfo() {\r\n      this.$refs.PdtempPdVerRlsForm.validate(pass => {\r\n        if (pass) {\r\n          let params = {\r\n            productReleaseInfoVoList: this.multipleSelection,\r\n            pdtempPdReleSyspathList: Array.isArray(this.envList)\r\n              ? this.envList\r\n              : Array(this.envList),\r\n            pdReleWay: this.PdtempPdVerRls.pdReleWay,\r\n            pdReleDetails: this.PdtempPdVerRls.pdReleDetails,\r\n            releType: \"1\",\r\n            releTypeDet: \"0\",\r\n            distinguishFieldPresentFlag: this.distinguishFieldPresentFlag\r\n          };\r\n          productRelease(this.buildRequestData(params)).then(res => {\r\n            if (res.data.message) {\r\n              this.notifyError(res.data.message);\r\n            } else {\r\n              this.PdtempPdVerRls.pdReleFile = \"\";\r\n              for (const pdoperPdVerRls of res.data.pdoperPdVerRlsList) {\r\n                this.PdtempPdVerRls.pdReleFile +=\r\n                  pdoperPdVerRls.pdReleFileName + \"; \";\r\n                let filePath =\r\n                  pdoperPdVerRls.pdReleFilePath +\r\n                  \"//\" +\r\n                  pdoperPdVerRls.pdReleFileName;\r\n                let fileName = pdoperPdVerRls.pdReleFileName;\r\n                let data = {filePath: filePath};\r\n                downloadReleFile(this.buildRequestData(data)).then(res => {\r\n                  this.downloadResponseFile(res, fileName);\r\n                });\r\n              }\r\n              this.PdtempPdVerRls.pdReleFile = this.PdtempPdVerRls.pdReleFile.substring(\r\n                0,\r\n                this.PdtempPdVerRls.pdReleFile.length - 2\r\n              );\r\n              this.notifySuccess(\"发布成功\");\r\n              if (this.PdtempPdVerRls.pdReleWay === \"2\") {\r\n                this.$emit(\r\n                  \"parentByClick\",\r\n                  \"已发布\" + this.PdtempPdVerRls.pdReleEnv\r\n                );\r\n              }\r\n              // this.$emit(\"initTableData\");\r\n              this.$refs.releaseAllPage.reload()\r\n            }\r\n          });\r\n        } else {\r\n          this.focusItem();\r\n        }\r\n      });\r\n    },\r\n    /* 待发布--审核结论 */\r\n    reviewHistory(row) {\r\n      let keyName = this.$route.path;\r\n      this.cacheJsonData(keyName, row);\r\n      let data = {\r\n        pdInfoId: row.id,\r\n        pdVerId: row.pdVerId\r\n      };\r\n      pdReviewHistory(this.buildRequestData(data)).then(res => {\r\n        this.tableData = res.data;\r\n        this.pdInfo = this.getPdData();\r\n        this.dialogTableVisible = true;\r\n      });\r\n    },\r\n    /* 发布多选-显示环境 */\r\n    showPath(val) {\r\n      this.PdtempPdVerRls.pdReleEnv = \"\";\r\n      for (const argument of val) {\r\n        this.PdtempPdVerRls.pdReleEnv += argument.pdReleSysname + \";\";\r\n      }\r\n      this.pathtableData = [];\r\n      this.pathtableData = val;\r\n      this.tableflag = this.pathtableData.length !== 0;\r\n    },\r\n    /* 发布单选-显示环境 */\r\n    showPathOne(val) {\r\n      this.pathtableData = [];\r\n      this.pathtableData.push(val);\r\n      this.tableflag = this.pathtableData.length !== 0;\r\n    },\r\n    /* 根据发布选项获取环境 */\r\n    getSysPath(val) {\r\n      this.PdtempPdVerRls.pdAuditConc = [];\r\n      this.PdtempPdVerRls.pdReleFile = \"\";\r\n      if (!val) {\r\n        this.releButton = true;\r\n        this.$emit(\"parentByClick\", \"0\", this.checkId);\r\n        this.showCheck = false;\r\n        this.showRadio = false;\r\n      } else {\r\n        if (val !== \"2\") {\r\n          this.PdtempPdVerRls.pdReleFile = \"\";\r\n          this.showCheck = true;\r\n          this.showRadio = false;\r\n          this.envList = [];\r\n          this.tableflag = false;\r\n          this.PdtempPdVerRls.pdReleEnv = \"\";\r\n          this.pathtableData = [];\r\n          this.$emit(\"parentByClick\", \"1\", this.checkId);\r\n        } else {\r\n          this.envList = [];\r\n          this.showRadio = true;\r\n          this.showCheck = false;\r\n          this.PdtempPdVerRls.pdReleEnv = \"\";\r\n          this.tableflag = false;\r\n          this.pathtableData = [];\r\n        }\r\n        this.releButton = false;\r\n        this.checkPath = [];\r\n        getAllPath().then(res => {\r\n          this.pathtableDataBc = res.data;\r\n          for (const argument of res.data) {\r\n            this.checkPath.push(argument.pdReleSysname);\r\n          }\r\n        });\r\n      }\r\n      this.$nextTick(() => {\r\n        this.$refs.PdtempPdVerRlsForm.clearValidate();\r\n      });\r\n    },\r\n    /*待发布内容：按钮组、产品资料*/\r\n    readyReleaseInfoClick(productReleaseInfoVo) {\r\n      this.showRules = false;\r\n      this.showCalc = false;\r\n      let keyName = this.$route.path;\r\n      this.cacheJsonData(keyName, productReleaseInfoVo);\r\n      this.pdInfo = productReleaseInfoVo;\r\n      this.readyReleaseInfo = !this.readyReleaseInfo;\r\n      this.paramsRule.pdInfoId = productReleaseInfoVo.id;\r\n      this.paramsCalc.pdInfoId = productReleaseInfoVo.id;\r\n      // 查询待发布的产品资料\r\n      this.filesData = [];\r\n      let params = {\r\n        productReleaseInfoVo: productReleaseInfoVo\r\n      };\r\n      getPdFiles(this.buildRequestData(params)).then(response => {\r\n        this.filesData = response.data;\r\n      });\r\n      this.$nextTick(() => {\r\n        if (\r\n          productReleaseInfoVo.defType != \"3\" &&\r\n          productReleaseInfoVo.pdReturnFuncCode.indexOf(\"productRuleFlag\") != -1\r\n        ) {\r\n          this.showRules = true;\r\n          this.$refs.rulePage.buildData();\r\n        }\r\n        if (\r\n          productReleaseInfoVo.defType != \"3\" &&\r\n          productReleaseInfoVo.pdReturnFuncCode.indexOf(\"productFormulaFlag\") !=\r\n          -1\r\n        ) {\r\n          this.showCalc = true;\r\n          this.$refs.calcPage.buildData();\r\n        }\r\n      });\r\n    },\r\n    /*页面跳转按钮组*/\r\n    getPdDesignPage() {\r\n      if (this.pdInfo) {\r\n        this.cacheCurrPagePdData(this.pdInfo)\r\n        this.dialogShow = true\r\n        this.dialogTitle = '产品框架信息查看'\r\n        this.dialogComponent = 'product-definition/pd-framework-def/main'\r\n        // this.$router.push({name: '产品框架定义主页', query: {showPageFlag: '1'},params:this.pdInfo})\r\n      }\r\n    },\r\n    getPdSalePage() {\r\n      if (this.pdInfo) {\r\n        this.cacheCurrPagePdData(this.pdInfo)\r\n        this.dialogShow = true\r\n        this.dialogTitle = '产品销售信息查看'\r\n        this.dialogComponent = 'product-definition/pd-operation-def/pd-sales-def/main'\r\n      }\r\n    },\r\n    getPdUWPage() {\r\n      if (this.pdInfo) {\r\n        this.cacheCurrPagePdData(this.pdInfo)\r\n        this.dialogShow = true\r\n        this.dialogTitle = '产品承保信息查看'\r\n        this.dialogComponent = 'product-definition/pd-operation-def/pd-acceptance-def/main'\r\n      }\r\n    },\r\n    getPdAUPage() {\r\n      if (this.pdInfo) {\r\n        this.cacheCurrPagePdData(this.pdInfo)\r\n        this.dialogShow = true\r\n        this.dialogTitle = '产品核保信息查看'\r\n        this.dialogComponent = 'product-definition/pd-operation-def/pd-underwriting-def/main'\r\n      }\r\n    },\r\n    getPdRWPage() {\r\n      if (this.pdInfo) {\r\n        this.cacheCurrPagePdData(this.pdInfo)\r\n        this.dialogShow = true\r\n        this.dialogTitle = '产品续期续保信息查看'\r\n        this.dialogComponent = 'product-definition/pd-operation-def/pd-renewal-def/main'\r\n      }\r\n    },\r\n    getPdPVPage() {\r\n      if (this.pdInfo) {\r\n        this.cacheCurrPagePdData(this.pdInfo)\r\n        this.dialogShow = true\r\n        this.dialogTitle = '产品保全信息查看'\r\n        this.dialogComponent = 'product-definition/pd-operation-def/pd-preservation-def/main'\r\n      }\r\n    },\r\n    getPdCLPage() {\r\n      if (this.pdInfo) {\r\n        this.cacheCurrPagePdData(this.pdInfo)\r\n        this.dialogShow = true\r\n        this.dialogTitle = '产品核赔信息查看'\r\n        this.dialogComponent = 'product-definition/pd-operation-def/pd-compensation-def/main'\r\n      }\r\n    },\r\n    closePageDialog() {\r\n      this.dialogShow = false\r\n      this.dialogComponent = ''\r\n    },\r\n    /*查询产品下拉*/\r\n    queryPdSelectList() {\r\n      queryPdSelectList(this.buildRequestData({})).then(res => {\r\n        this.getSelectOptions(res.data);\r\n      });\r\n    },\r\n    /*产品下拉选*/\r\n    getSelectOptions(configs) {\r\n      if (configs) {\r\n        this.pdOptions = [];\r\n        for (let data of configs) {\r\n          let option = {pdCode: \"\", pdName: \"\", pdEdition: \"\"};\r\n          option.pdCode = data.pdCode;\r\n          option.pdName = data.pdName;\r\n          option.pdEdition = data.pdEdition;\r\n          this.pdOptions.push(option);\r\n        }\r\n      }\r\n    },\r\n    //获取版本\r\n    getEdition(val) {\r\n      if (val.length > 0) {\r\n        this.pdSelForm.pdEdition = val[0].pdEdition;\r\n      }\r\n    },\r\n    /*待发布搜索*/\r\n    queryTableData() {\r\n      if (this.params.startDate > this.params.endDate) {\r\n        this.notifyError(\"审核起始日期不得晚于结束日期！\");\r\n        return;\r\n      }\r\n      if (this.pdParams.length > 0) {\r\n        this.params.pdCode = this.pdParams[0].pdCode;\r\n      } else {\r\n        this.params.pdCode = \"\";\r\n        this.params.pdEdition = \"\";\r\n      }\r\n      this.$refs.releaseAllPage.reload();\r\n    },\r\n    showReleRecord(row) {\r\n      this.dialogReleaseNoteVisible = true;\r\n      let keyName = this.$route.path;\r\n      this.cacheJsonData(keyName, row);\r\n      let data = {\r\n        pdInfoId: row.id,\r\n        pdVerId: row.pdVerId\r\n      };\r\n      getPdtempPdVerRls(this.buildRequestData(data)).then(res => {\r\n        this.releInfoData = res.data;\r\n        this.pdInfo = this.getPdData();\r\n      });\r\n    },\r\n    /*下载发布文件*/\r\n    downloadReleFile(row) {\r\n      let filePath = row.pdReleFilePath + \"//\" + row.pdReleFileName;\r\n      let data = {filePath: filePath};\r\n      let fileName = row.pdReleFileName;\r\n      downloadReleFile(this.buildRequestData(data)).then(res => {\r\n        this.downloadResponseFile(res, fileName);\r\n        this.notifySuccess(\"下载成功\");\r\n      });\r\n    },\r\n    backReleaseRecord() {\r\n      this.dialogReleaseNoteVisible = false;\r\n    },\r\n    /*发布完毕*/\r\n    releOver() {\r\n      // 校验选中的是否有未发布的产品\r\n      for (const productReleaseInfoVo of this.multipleSelection) {\r\n        if (!productReleaseInfoVo.pdReleStu || productReleaseInfoVo.pdReleStu === \"1\") {\r\n          this.notifyError(\"该产品未发布：\" + productReleaseInfoVo.pdCode + ':' + productReleaseInfoVo.pdName)\r\n          return\r\n        }\r\n      }\r\n      let data = {\r\n        productReleaseInfoVoList: Array.from(this.multipleSelection)\r\n      }\r\n      for (const row of this.multipleSelection) {\r\n        if (row.pdReleType === \"1\") {\r\n          row.releTypeDet = \"0\"\r\n        } else {\r\n          if (\r\n            row.defType !== \"3\" &&\r\n            (row.pdReturnFuncCode.indexOf(\"productInfoFlag\") !== -1 ||\r\n              row.pdReturnFuncCode.indexOf(\"productDistributionFlag\") !== -1 ||\r\n              row.pdReturnFuncCode.indexOf(\"productCoverFlag\") !== -1 ||\r\n              row.pdReturnFuncCode.indexOf(\"productUnderwritingFlag\") !== -1 ||\r\n              row.pdReturnFuncCode.indexOf(\"productHePeiFlag\") !== -1 ||\r\n              row.pdReturnFuncCode.indexOf(\"productPreservationFlag\") !== -1 ||\r\n              row.pdReturnFuncCode.indexOf(\"productRenewalFlag\") !== -1)\r\n          ) {\r\n            // 产品发布\r\n            row.releTypeDet = \"1\"\r\n          } else if (\r\n            row.defType !== \"3\" &&\r\n            row.pdReturnFuncCode.indexOf(\"productRuleFlag\") !== -1\r\n          ) {\r\n            // 规则发布\r\n            row.releTypeDet = \"2\"\r\n          } else if (\r\n            row.defType !== \"3\" &&\r\n            row.pdReturnFuncCode.indexOf(\"productFormulaFlag\") !== -1\r\n          ) {\r\n            // 公式发布\r\n            row.releTypeDet = \"3\"\r\n          } else if (row.defType === \"3\") {\r\n            // 停售发布\r\n            row.releTypeDet = \"4\"\r\n          }\r\n        }\r\n      }\r\n      pdReleOver(this.buildRequestData(data)).then(() => {\r\n        this.$refs.releaseAllPage.reload()\r\n        this.notifySuccess(\"发布完毕\")\r\n        this.$emit(\"initTableData\")\r\n        this.$store.dispatch('product/storageProducts')\r\n      })\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.el-button {\r\n  text-align: center;\r\n  width: 100px;\r\n}\r\n\r\n.swbu {\r\n  width: 160px;\r\n}\r\n</style>\r\n"]}]}