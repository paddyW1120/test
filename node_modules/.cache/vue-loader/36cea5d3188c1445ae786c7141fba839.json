AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/views/product-package-definition/pp-operation-def/pp-acceptance-def/accpt-page-def","sourcesContent":["<template>\r\n  <cus-wraper>\r\n    <div style=\"margin-left: 10px\">\r\n      <el-button  size=\"small\" @click=\"isShowQuick\"\r\n                 type=\"text\">{{$t('pageDefinition.quickDefi')}}<i :class=\"quickicon\"></i>\r\n      </el-button>\r\n    </div>\r\n    <div style=\"margin-top: 20px;\">\r\n      <el-collapse-transition>\r\n        <div v-if=\"quickshow\">\r\n          <el-form label-width=\"auto\" ref=\"form_quick\" style=\"padding-left: 50px\">\r\n            <el-row :gutter=\"20\">\r\n              <el-col :span=\"7\">\r\n                <el-form-item :label=\"$t('pageDefinition.referPage')\">\r\n                  <el-select  clearable filterable v-model=\"referPage\" @change=\"changeParams\">\r\n                    <el-option :key=\"item.value\" :label=\"item.label\" :value=\"item.value\" v-for=\"item in packageOptions\">\r\n                    </el-option>\r\n                  </el-select>\r\n                </el-form-item>\r\n              </el-col>\r\n              <el-col :span=\"1\">\r\n                <el-button dialog-disable=\"disable\"  size=\"small\" icon=\"el-icon-view\" type=\"primary\" @click=\"showElPage\">{{$t('pageDefinition.show')}}</el-button>\r\n              </el-col>\r\n              <el-col :span=\"15\">\r\n              </el-col>\r\n            </el-row>\r\n          </el-form>\r\n        </div>\r\n      </el-collapse-transition>\r\n    </div>\r\n\r\n    <el-button dialog-disable=\"disable\"  size=\"small\" icon=\"el-icon-view\" type=\"primary\" @click=\"showElPage\">{{$t('pageDefinition.show')}}</el-button>\r\n    <cus-table-title :label=\"$t('pageDefinition.pageElIn')\"/>\r\n    <el-table :data=\"configs\" size=\"mini\" fit element-loading-text=\"Loading\" border highlight-current-row align=\"center\"\r\n              @row-click=\"\" class=\"wraper-table\">\r\n      <el-table-column width=\"50\" :label=\"$t('pageDefinition.choose')\">\r\n        <template slot-scope=\"scope\">\r\n          <el-radio v-model=\"chooseRowId\" :label=\"scope.row.id\">\r\n            <span class=\"el-radio__label\"></span>\r\n          </el-radio>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.id')\" type=\"index\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('pageDefinition.pageEl')\" prop=\"pdPgElChName\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('pageDefinition.elType')\" prop=\"pdPgElDefiType\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('pageDefinition.elOrder')\" prop=\"pdPgElOrder\" align=\"center\"></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('table.actions')\" fixed=\"right\" width=\"100\">\r\n        <template slot-scope=\"scope\">\r\n          <el-popconfirm :title=\"$t('pageDefinition.sure')\" @onConfirm=\"deleteInfo(scope.row.id)\">\r\n            <el-button dialog-disable=\"disable\" slot=\"reference\" size=\"mini\" type=\"danger\" v-auth=\"'pageDefinition:delete'\"\r\n                       class=\"filter-button\" icon=\"el-icon-delete\">\r\n            </el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <cus-pagination ref=\"page\" :pageSize=\"10\" :params=\"params\" v-model=\"configs\" url=\"/pageDefinition/base\"/>\r\n\r\n    <cus-form-title :first=\"true\" :label=\"$t('pageDefinition.pageElDeploy')\" />\r\n    <el-row style=\"margin-bottom: 10px\">\r\n      <el-col :span=\"3\" :offset=\"17\">\r\n        <el-button dialog-disable=\"disable\"  size=\"small\" type=\"primary\" icon=\"el-icon-success\" @click=\"savePdPgEl\" v-auth=\"'pageDefinition:save'\">\r\n          {{$t('pageDefinition.save')}}\r\n        </el-button>\r\n      </el-col>\r\n      <el-col :span=\"3\">\r\n        <el-button dialog-disable=\"disable\" size=\"small\" type=\"primary\" icon=\"el-icon-edit\" @click=\"openRelaPage\">{{$t('pageDefinition.pageElLinkDefi')}}\r\n        </el-button>\r\n      </el-col>\r\n    </el-row>\r\n    <el-dialog :visible.sync=\"showRelaPage\" v-dialog-drag center :close-on-click-modal=\"false\" :show-close=\"false\"\r\n               @open=\"initDialog\">\r\n      <cus-page-title-back :isDialog=\"true\" :label=\"$t('pageDefinition.pageElLinkDefi')\"  @backTo=\"showRelaPage = false\"/>\r\n          <cus-table-title :label=\"$t('pageDefinition.pageElLinkIn')\" />\r\n          <el-table :data=\"relaConfigs\" size=\"mini\" fit element-loading-text=\"Loading\" border highlight-current-row\r\n                    align=\"center\"\r\n                    @row-click=\"chooseRelaRow\"\r\n                    class=\"wraper-table\">\r\n            <el-table-column width=\"50\" :label=\"$t('table.select')\">\r\n              <template slot-scope=\"scope\">\r\n                <el-radio v-model=\"chooseRelaRowId\" :label=\"scope.row.id\">\r\n                  <span class=\"el-radio__label\"></span>\r\n                </el-radio>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('table.id')\" type=\"index\" align=\"center\"></el-table-column>\r\n            <el-table-column :label=\"$t('pageDefinition.linkPageEl')\" prop=\"linkEl\" align=\"center\"></el-table-column>\r\n            <el-table-column :label=\"$t('pageDefinition.linkLevel')\" prop=\"linkLevel\" align=\"center\"></el-table-column>\r\n            <el-table-column align=\"center\" :label=\"$t('table.actions')\" fixed=\"right\" width=\"100\">\r\n              <template slot-scope=\"scope\">\r\n                <el-popconfirm :title=\"$t('pdgenlPgElInfo.sure')\" @onConfirm=\"handleDelete(scope.row.id)\">\r\n                  <el-button dialog-disable=\"disable\" slot=\"reference\" size=\"mini\" type=\"danger\" v-auth=\"'pageDefinition:delete'\"\r\n                             class=\"filter-button\" icon=\"el-icon-delete\">\r\n                  </el-button>\r\n                </el-popconfirm>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n      <cus-form-title :first=\"true\" :label=\"$t('pageDefinition.elLinkDefi')\" />\r\n        <el-form label-width=\"auto\">\r\n          <el-row :gutter=\"20\">\r\n            <el-col :span=\"7\" :offset=\"2\">\r\n              <el-form-item :label=\"$t('pageDefinition.linkEl')\">\r\n                <el-select v-model=\"ccParams.linkEl\" :placeholder=\"$t('pageDefinition.pleaseSelect')\" clearable>\r\n                  <el-option\r\n                    v-for=\"(item,index) in linkOptions\"\r\n                    :key=\"index\"\r\n                    :label=\"item.pdPgElCode+':'+item.pdPgElChName\"\r\n                    :value=\"item.pdPgElCode +':'+item.pdPgElChName + ':' + item.id\">\r\n                  </el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col :span=\"7\">\r\n              <el-form-item :label=\"$t('pageDefinition.linkLevel')\">\r\n                <el-input v-model=\"ccParams.linkLevel\" :disabled=true></el-input>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col :span=\"2\">\r\n              <el-button dialog-disable=\"disable\" size=\"small\" type=\"primary\" icon=\"el-icon-plus\" @click=\"addCols\">{{$t('pageDefinition.add')}}</el-button>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row :gutter=\"20\">\r\n            <el-col :span=\"17\" :offset=\"2\">\r\n              <cus-table-title :label=\"$t('pageDefinition.linkElDefi')\" />\r\n              <el-table :data=\"relaDefiConfigs\" size=\"mini\" fit element-loading-text=\"Loading\" border\r\n                        highlight-current-row\r\n                        align=\"center\"\r\n                        class=\"wraper-table\">\r\n                <el-table-column :label=\"$t('table.id')\" type=\"index\" align=\"center\"></el-table-column>\r\n                <el-table-column v-for=\"(col,i) in linkCols\" :prop=\"col.prop\" :key=\"i\" :label=\"col.label\" width=\"260px\">\r\n                  <template slot-scope=\"scope\">\r\n                    <el-select :placeholder=\"$t('pageDefinition.pleaseSelect')\" v-model=\"scope.row.linkData[i]\">\r\n                      <el-option v-for=\"(item,index) in tableLinkOptions[i]\"\r\n                                 :key=\"index\"\r\n                                 :label=\"item.pdElDpDnCode+':'+item.pdElDpDnDesc\"\r\n                                 :value=\"item.pdElDpDnCode\">\r\n                      </el-option>\r\n                    </el-select>\r\n                  </template>\r\n                </el-table-column>\r\n                <el-table-column align=\"center\" :label=\"$t('table.actions')\" fixed=\"right\" width=\"100px\">\r\n                  <template slot-scope=\"scope\">\r\n                    <el-button  dialog-disable=\"disable\" slot=\"reference\" size=\"mini\" type=\"danger\" @click=\"delDblRow(scope.$index)\"\r\n                               class=\"filter-button\">-\r\n                    </el-button>\r\n                  </template>\r\n                </el-table-column>\r\n              </el-table>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row :gutter=\"20\">\r\n            <el-col :span=\"16\" :offset=\"2\">\r\n              <el-button  dialog-disable=\"disable\"  @click=\"addDblRow\" size=\"mini\" type=\"primary\">+</el-button>\r\n            </el-col>\r\n          </el-row>\r\n          <div style=\"margin-top:20px\">\r\n            <el-row align=\"right\">\r\n              <el-col :span=\"3\" :offset=\"16\">\r\n                <el-button  dialog-disable=\"disable\" size=\"small\" type=\"primary\" icon=\"el-icon-success\" @click=\"saveRelaDefi\">{{$t('pageDefinition.save')}}\r\n                </el-button>\r\n              </el-col>\r\n              <el-col :span=\"3\">\r\n                <el-button  dialog-disable=\"disable\" size=\"small\" type=\"primary\" icon=\"el-icon-refresh\" @click=\"refreshDialog\">\r\n                  {{$t('pageDefinition.refresh')}}\r\n                </el-button>\r\n              </el-col>\r\n            </el-row>\r\n          </div>\r\n        </el-form>\r\n    </el-dialog>\r\n    <el-dialog :visible.sync=\"showRelaElPage\" v-dialog-drag center :close-on-click-modal=\"false\" :show-close=\"false\"\r\n               @open=\"initElPage\" width=\"80%\">\r\n      <cus-page-title-back :isDialog=\"true\" :label=\"$t('pageDefinition.show')\"  @backTo=\"showRelaElPage = false\"/>\r\n      <div>\r\n        <el-row class=\"center-board-row\" :gutter=15>\r\n          <el-form size=\"medium\" label-position=\"right\" :disabled=false label-width=\"auto\" :rules=\"rules\" ref=\"relaElForm\">\r\n            <el-col :span=8 v-for=\"(item,index) in ElDatas\">\r\n              <el-form-item :label=\"item.label\" :prop=\"item.prop\">\r\n                <el-date-picker v-if=\"item.type === 'date'\" :disabled=\"item.disabled\" :readonly=\"item.readonly\"\r\n                                :placeholder=\"item.placeholder\" v-model=\"item.defaultValue\"></el-date-picker>\r\n                <el-select v-else-if=\"item.type === 'select'\" :disabled=\"item.disabled\" :readonly=\"item.readonly\"\r\n                           @change=\"changeChildOptions(item)\" clearable\r\n                           :placeholder=\"item.placeholder\" :multiple=\"item.multiple\" v-model=\"item.config.defaultValue\">\r\n                  <el-option v-for=\"(sItem,sIndex) in item.slot.options\" :key=\"sIndex\"\r\n                             :label=\"sItem.label\"\r\n                             :value=\"sItem.value\">\r\n                  </el-option>\r\n                </el-select>\r\n                <el-input v-else-if=\"item.type === 'textarea'\" type=\"textarea\" :disabled=\"item.disabled\"\r\n                          :readonly=\"item.readonly\"\r\n                          v-model=\"item.config.defaultValue\" :placeholder=\"item.placeholder\"></el-input>\r\n                <el-input v-else-if=\"item.type === 'text'\" type=\"text\" :disabled=\"item.disabled\"\r\n                          :readonly=\"item.readonly\"\r\n                          v-model=\"item.config.defaultValue\" :placeholder=\"item.placeholder\"></el-input>\r\n                <el-checkbox-group v-else-if=\"item.type === 'checkbox'\" :disabled=\"item.disabled\"\r\n                                   :readonly=\"item.readonly\" v-model=\"item.config.defaultValue\"\r\n                                   :placeholder=\"item.placeholder\">\r\n                  <el-checkbox v-for=\"(cItem,cIndex) in item.slot.options\"\r\n                               :label=\"cItem.label\"></el-checkbox>\r\n                </el-checkbox-group>\r\n                <el-radio-group v-else-if=\"item.type === 'radio'\" :disabled=\"item.disabled\"\r\n                                :readonly=\"item.readonly\" v-model=\"item.config.defaultValue\"\r\n                                :placeholder=\"item.placeholder\">\r\n                  <el-radio v-for=\"(rItem,rIndex) in item.slot.options\"\r\n                            :key=\"rIndex\"\r\n                            :label=\"rItem.label\" :value=\"rItem.value\"></el-radio>\r\n                </el-radio-group>\r\n              </el-form-item>\r\n            </el-col>\r\n          </el-form>\r\n        </el-row>\r\n      </div>\r\n    </el-dialog>\r\n    <div>\r\n      <Home ref=\"defineHome\"></Home>\r\n    </div>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\n  import Home from './draggable/Home'\r\n  import {\r\n    save,\r\n    initDragForm,\r\n    deletePgElInfo,\r\n    getAllSelects,\r\n    findSelectOptions,\r\n    findLinkGrop,\r\n    initLinkTable,\r\n    saveLinkData,\r\n    deleteGroup,\r\n    initElPage\r\n  } from '@/api/basicinfo/pageDefinition';\r\n  import {packageList} from \"@/api/basicinfo/insureRuleDefinition\";\r\n  export default {\r\n    name: 'pageDefinition',\r\n    components: {\r\n      Home,\r\n    },\r\n    data() {\r\n      return {\r\n        product: {},\r\n        quickicon: 'el-icon-arrow-down',\r\n        quickshow: false,\r\n        showRelaPage: false,\r\n        referPage: '',\r\n        configs: [],\r\n        selectConfigs: [],\r\n        packageOptions:[],\r\n        relaDefiConfigs: [],\r\n        relaConfigs: [],\r\n        chooseRowId: '',\r\n        chooseRelaRowId: '',\r\n        showRelaElPage: false,\r\n        pdtempPdPgElInfos: [],\r\n        linkOptions: [],\r\n        linkCols: [],\r\n        tableLinkOptions: [],\r\n        ElDatas: [],\r\n        params: {\r\n          eq_pdInfoId: '',\r\n          eq_pdCode: ''\r\n        }\r\n        ,\r\n        ccParams: {\r\n          linkEl: '',\r\n          linkLevel: 1\r\n        },\r\n        rules: {}\r\n      }\r\n    },\r\n    computed: {},\r\n    created() {\r\n      this.product = this.getPdData()\r\n      this.params.eq_pdInfoId = this.product.id;\r\n      this.params.eq_pdCode = this.product.ppCode;\r\n      this.initDragForm();\r\n      this.initPackageList();\r\n    },\r\n    methods: {\r\n      //动态修改级联下拉选\r\n      changeChildOptions(item) {\r\n        if (item.child) {\r\n          for (var val of this.ElDatas) {\r\n            for (var key in item.child) {\r\n              if (val.vModel == key) {\r\n                let target = item.child[key];\r\n                for (var targetOption of target) {\r\n                  if (targetOption.key == item.config.defaultValue) {\r\n                    val.slot.options = targetOption.value;\r\n                  }\r\n                }\r\n              }\r\n            }\r\n          }\r\n        }\r\n      },\r\n      //预览页面初始化\r\n      initElPage() {\r\n        let map = {\r\n          \"pdCode\": this.params.eq_pdCode,\r\n          \"pdInfoId\": this.params.eq_pdInfoId\r\n        };\r\n        initElPage(this.buildRequestData(map)).then(response => {\r\n          this.ElDatas = response.data;\r\n        })\r\n      },\r\n      showElPage() {\r\n        this.showRelaElPage = true;\r\n      },\r\n      initPackageList(){\r\n        packageList().then(res => {\r\n          let data = res.data;\r\n          this.packageOptions = [];\r\n          if (data.length) {\r\n            data.forEach(val => {\r\n              this.packageOptions.push({\r\n                value: val.id + \":\" + val.ppCode,\r\n                label: val.ppCode + \":\" + val.ppName\r\n              });\r\n            });\r\n          }\r\n        });\r\n      },\r\n      changeParams(val){\r\n        this.params.eq_pdInfoId=this.getCaption(val, 0);\r\n        this.params.eq_pdCode=this.getCaption(val, 1)\r\n      },\r\n      getCaption(obj, state) {\r\n        const index = obj.lastIndexOf(\":\")\r\n        if (state === 0) {\r\n          return obj.substring(0, index)\r\n        } else {\r\n          return obj.substring(index + 1, obj.length)\r\n        }\r\n      },\r\n      handleDelete(id) {\r\n        deleteGroup(id).then(response => {\r\n          this.notifySuccess(response.message)\r\n          this.initDialog();\r\n        })\r\n      },\r\n      saveRelaDefi() {\r\n        let map = {\r\n          \"pdInfoId\": this.params.eq_pdInfoId,\r\n          \"pdCode\": this.params.eq_pdCode,\r\n          \"rowId\": this.chooseRelaRowId,\r\n          \"nodes\": this.linkCols,\r\n          \"dpdnLink\": this.relaDefiConfigs,\r\n        };\r\n        saveLinkData(this.buildRequestData(map)).then(res => {\r\n          if (res.code == 200) {\r\n            this.notifySuccess(res.message);\r\n            this.initDialog();\r\n          }\r\n        })\r\n      },\r\n      refreshDialog() {\r\n        if (this.chooseRelaRowId) {\r\n          this.initLinkTable(this.chooseRelaRowId)\r\n        } else {\r\n          this.linkCols = [];\r\n          this.relaDefiConfigs = [];\r\n          this.tableLinkOptions = [];\r\n          this.ccParams.linkLevel = 1;\r\n        }\r\n      },\r\n      //初始化级联表格及值\r\n      initLinkTable(id) {\r\n        let map = {\r\n          \"pdCode\": this.params.eq_pdCode,\r\n          \"pdInfoId\": this.params.eq_pdInfoId,\r\n          \"groupId\": id\r\n        };\r\n        initLinkTable(this.buildRequestData(map)).then(res => {\r\n          if (res.code == 200) {\r\n            //塞值\r\n            this.relaDefiConfigs = [];\r\n            this.linkCols = res.data.linkCols;\r\n            this.tableLinkOptions = res.data.tableLinkOptions;\r\n            res.data.linkData.forEach(val => {\r\n              this.relaDefiConfigs.push({\r\n                linkData: val\r\n              })\r\n            });\r\n            this.ccParams.linkLevel = this.linkCols.l