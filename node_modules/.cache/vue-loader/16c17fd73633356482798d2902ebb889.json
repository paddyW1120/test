   <el-form-item align=\"right\">\r\n        <el-button  size=\"small\" @click=\"saveBnftForm('D')\" icon=\"el-icon-success\" type=\"primary\" v-show=\"!radio2\" dialog-disable=\"disable\">\r\n          {{$t('table.save')}}\r\n        </el-button>\r\n        <el-button  size=\"small\" @click=\"updateBnftForm('D')\" icon=\"el-icon-success\" type=\"primary\" v-show=\"radio2\" dialog-disable=\"disable\">\r\n          {{$t('table.update')}}\r\n        </el-button>\r\n        <el-button  size=\"small\" @click=\"cleanBnftForm\" icon=\"el-icon-refresh\" type=\"primary\" dialog-disable=\"disable\">\r\n          {{$t('table.refresh')}}\r\n        </el-button>\r\n      </el-form-item>\r\n      <div v-show=\"bnftForm.pdBnftFactorType=='3'\">\r\n        <h4>\r\n          <el-divider class=\"divider\" direction=\"vertical\"></el-divider>\r\n          {{$t('i18nView.deductionCustom')}}\r\n        </h4>\r\n        <el-row :gutter=\"10\" class=\"row-bg\" justify=\"center\" type=\"flex\">\r\n          <el-col :span=\"8\">\r\n            <el-form-item :label=\"$t('i18nView.deductionCalc')\">\r\n              <el-input readonly v-model=\"bnftForm.pdBnftFactorValueCalCode\">\r\n                <svg-icon @click=\"toFormula\" icon-class=\"calc\" slot=\"suffix\"\r\n                          style=\"color: #606266;font-size: 18px;cursor: pointer\"/>\r\n              </el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"8\">\r\n          </el-col>\r\n          <el-col :span=\"8\">\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n    </el-form>\r\n\r\n    <!-- 弹窗（已关联公式列表） -->\r\n    <el-dialog :close-on-click-modal=\"false\" :visible.sync=\"formulaVisible\" append-to-body center\r\n               v-dialog-drag v-if=\"formulaVisible\" width=\"80%\">\r\n      <clmCtrlFormula :pdBussDetailType=\"this.pdBussDetailType\"\r\n                      :pdtempClmCtrlMain=\"this.selectedClmCtrl\" :pdtempPdClmBnft=\"configsForm\"\r\n                      @bringInCalcCode=\"bringInCalcCode\"\r\n                      @changeDialogVisible_ClmCtrlFormula=\"changeDialogVisible_ClmCtrlFormula\"\r\n                      @changeDialogVisible_ClmCtrlFormulaSelect=\"changeDialogVisible_ClmCtrlFormulaSelect\"></clmCtrlFormula>\r\n    </el-dialog>\r\n\r\n    <!-- 弹窗（公式关联） -->\r\n    <el-dialog :close-on-click-modal=\"false\" :visible.sync=\"formulaSelectVisible\" append-to-body\r\n               center v-dialog-drag v-if=\"formulaSelectVisible\" width=\"70%\">\r\n      <clmCtrlFormulaSelect :pdBussDetailType=\"this.pdBussDetailType\" :pdtempClmCtrlMain=\"this.selectedClmCtrl\"\r\n                            :pdtempPdClmBnft=\"configsForm\" @bringInCalcCode=\"bringInCalcCode\"\r\n                            @changeDialogVisible_ClmCtrlFormulaSelect=\"changeDialogVisible_ClmCtrlFormulaSelect\"></clmCtrlFormulaSelect>\r\n    </el-dialog>\r\n\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\nlet _this;\r\nimport {\r\n  deleteObBnftForm,\r\n  findperiodConfigs,\r\n  linkMedConnTable,\r\n  saveMedObBnftForm,\r\n  searchperiodConfigs,\r\n  selectPdMedFeeBill,\r\n  selectPdMedFeeBillOptions,\r\n  selectpdtempPdMedFeeBills,\r\n  updateObBnftForm,\r\n  savePdtempClmCtrlLim,\r\n  findBusinessItemByCtrlCode1\r\n} from \"@/api/basicinfo/pdtemppdclmbnft\";\r\nimport {pdtempPdClsLabList} from \"@/api/basicinfo/ProductAttributeDefinition\";\r\nimport {findReferenceProducts} from \"@/api/basicinfo/pdtemppdbnft\";\r\nimport clmCtrlFormula from \"@/views/basicinfo/clm-formula/clmCtrlFormula/index\";\r\nimport clmCtrlFormulaSelect from \"@/views/basicinfo/clm-formula/clmCtrlFormulaSelect/index\";\r\nimport {findProductByDetailType} from '@/api/basicinfo/productCompensation';\r\nimport {\r\n  selectPdBnftCode,\r\n  selectPdClmBnftCode,\r\n  selectPdMedFeeBillCode,\r\n  selectPdProgCode,\r\n} from \"@/api/basicinfo/pdClaimVerificationRules\"\r\n\r\nexport default {\r\n  name: \"pdtemppdmedobperiod\",\r\n  components: {clmCtrlFormula, clmCtrlFormulaSelect},\r\n  filters: {\r\n    filter_pdBnftCtrlLimLevel(code) {\r\n      if (code) {\r\n        return _this.getCodeName('pdBnftRuleLimitLevel', code);\r\n      } else {\r\n        return ''\r\n      }\r\n    },\r\n    filter_pdBnftRuleObservationUnit(code) {\r\n      return _this.getCodeName(\"pdsbnftdy\", code, true);\r\n    },\r\n    filter_pdBnftCumType(code) {\r\n      return _this.getCodeName(\"pdBnftCumType\", code);\r\n    },\r\n    filter_pdBnftCumFrequency(code) {\r\n      return _this.getCodeName(\"pdBnftCumFrequency\", code);\r\n    },\r\n    filter_pdBnftRuleLevel(code) {\r\n      return _this.getCodeName(\"pdBnftRuleLevel\", code);\r\n    },\r\n    filter_pdBnftFactorType(code) {\r\n      return _this.getCodeName(\"pdBnftFactorType\", code);\r\n    },\r\n    filter_dataFlag(code) {\r\n      if (code == \"Y\") {\r\n        return code;\r\n      } else {\r\n        return \"N\";\r\n      }\r\n    },\r\n  },\r\n  props: {\r\n    bnftForm1: {\r\n      type: Object,\r\n      default: {},\r\n    },\r\n    configsForm: {\r\n      type: Object,\r\n      default: {},\r\n    },\r\n    pdBnftArr: {\r\n      type: Array,\r\n      default: () => {\r\n        return []\r\n      }\r\n    },\r\n    pdClmBnftArr: {\r\n      type: Array,\r\n      default: [],\r\n    },\r\n    feeBillData: {\r\n      type: Object,\r\n      default: {},\r\n    },\r\n    showMedDeductible: {\r\n      type: Boolean,\r\n      default: false,\r\n    },\r\n  },\r\n  data() {\r\n    _this = this;\r\n    return {\r\n      params: {\r\n        pdBnftCode: this.bnftForm1.pdBnftCode,\r\n        pdClmBnftCode: this.configsForm.pdClmBnftCode,\r\n        pdBnftCtrlType: 'D',\r\n        pdBnftCtrlLimLevelList:'7'\r\n      },\r\n      PdMedFeeBillOptions:[],\r\n      pdClmBnftOptions:[],\r\n      pdtempPdInfo: {},\r\n      chooseFee: [],\r\n      pdType: null,\r\n      radio1: null,\r\n      radio2: null,\r\n      quickicon: \"el-icon-arrow-right\",\r\n      quickshow: false,\r\n      searchForm: {\r\n        pdBnftCtrlLimLevel:'',\r\n        pdBnftFactorType:'',\r\n        productTypeDetail:'',\r\n        product:'',\r\n        medBillType:''\r\n      },\r\n      bnftForm: {\r\n        pdObPeriodStartRule: null,\r\n        pdBnftRuleObservation: null,\r\n        pdBnftFactorValue2:null,\r\n        pdBnftFactorValue:null,\r\n        pdBnftRuleObservationUnit: \"Y\",\r\n        pdBnftCumType: null,\r\n        pdBnftCumFrequency: '2',\r\n        pdBnftRuleConCode: null,\r\n        pdBnftCtrlConCode: [],\r\n        pdBnftCtrlConCodeFee:[]\r\n      },\r\n      pdBnftCtrlLevel: \"1\",\r\n      pdBnftRuleObservationUnit: \"1$Y$M$D\",\r\n      feeConfigs: [],\r\n      pdMedFeeBillTypes: [],\r\n      periodConfigs: [],\r\n      searchConfigs: [],\r\n      Options: [],\r\n      productOptions: [],\r\n      medOptions: [],\r\n      medOptions1: [],\r\n      medFeeOptions: [],\r\n      ClmBnftOptions: [],\r\n      cusObperiod: false,\r\n      showConnTable: false,\r\n      connTableConfigs: [],\r\n      selectedClmCtrl: {},\r\n      pdBussDetailType: '',\r\n      formulaVisible: '',\r\n      formulaSelectVisible: ''\r\n    };\r\n  },\r\n  watch: {\r\n    showMedDeductible: function () {\r\n      if (this.showMedDeductible == true) {\r\n        this.findMedPeriodConfigs();\r\n        if (this.configsForm.pdClmBnftCode != null) {\r\n          this.selectpdtempPdMedFeeBills(this.configsForm.pdClmBnftCode);\r\n        }\r\n      }\r\n    },\r\n  },\r\n  async created() {\r\n    let pdtempPdInfo = this.getPdData();\r\n    this.pdtempPdInfo = pdtempPdInfo;\r\n    this.bnftForm.pdBnftCtrlLimLevel = '9'\r\n    await this.findTypeRegion();\r\n    this.findMedPeriodConfigs();\r\n    this.findOptions();\r\n    if (this.configsForm.pdClmBnftCode != null) {\r\n      this.selectpdtempPdMedFeeBills(this.configsForm.pdClmBnftCode);\r\n    }\r\n    // this.selectPdBnftCodeForBill();\r\n    this.selectPdMedFeeBillCode();\r\n    this.$refs.page_D.reload();\r\n  },\r\n  methods: {\r\n    clearAll(val){\r\n      // console.log(val)\r\n      this.medOptions = []\r\n      this.bnftForm.pdtempPdMedFeeBillList = undefined\r\n      this.medOptions1 = []\r\n      this.bnftForm.pdtempPdMedBillVoList = undefined\r\n      this.PdMedFeeBillOptions = []\r\n      this.bnftForm.pdtempPdMedFeeBill = this.feeBillData\r\n      this.pdClmBnftOptions = []\r\n      this.bnftForm.pdtempPdMedBillVo = this.feeBillData\r\n      // this.selectPdBnftCodeForBill();\r\n    },\r\n    bringSelectedPdClmBnftMoreBill(selectedItemList){\r\n      this.bnftForm.pdtempPdMedBillVoList = []\r\n      this.medOptions1 = []\r\n      for (const selectedItem of selectedItemList) {\r\n        this.bnftForm.pdtempPdMedBillVoList.push(selectedItem)\r\n        this.medOptions1.push(selectedItem)\r\n      }\r\n    },\r\n    bringSelectedFeeBill(selectedItemList){\r\n      this.bnftForm.pdtempPdMedFeeBillList = []\r\n      this.medOptions = []\r\n      for (const selectedItem of selectedItemList) {\r\n        this.bnftForm.pdtempPdMedFeeBillList.push(selectedItem)\r\n        this.medOptions.push(selectedItem)\r\n      }\r\n    },\r\n    //产品账单费用下拉\r\n    selectPdMedFeeBillCode() {\r\n      const params = {product: this.pdtempPdInfo}\r\n      selectPdMedFeeBillCode(this.buildRequestData(params)).then(response => {\r\n        this.PdMedFeeBillOptions = response.data\r\n      })\r\n    },\r\n    //产品账单下拉选\r\n    selectPdBnftCodeForBill() {\r\n      const params = {pdInfoId: this.pdtempPdInfo.id}\r\n      console.log(111111)\r\n      selectPdClmBnftCode(this.buildRequestData(params)).then(response => {\r\n        for(var i=0;i<response.data.length;i++){\r\n          if(_this.getCodeName(\"medBillType\", this.feeBillData.pdMedBillType, true) === response.data[i].pdMedBillTypeName){\r\n            this.pdClmBnftOptions.push(response.data[i])\r\n            this.bnftForm.pdtempPdMedBillVo = this.pdClmBnftOptions[0]\r\n            break\r\n          }\r\n        }\r\n      })\r\n    },\r\n    onInput(){\r\n      this.$forceUpdate();\r\n    },\r\n    clearInput(){\r\n      this.bnftForm.pdBnftFactorValue=\"\",\r\n        this.bnftForm.pdBnftFactorValue2=\"\"\r\n    },\r\n    bringInCalcCode(pdBussId, calcCode) {\r\n      for (const ob of this.periodConfigs) {\r\n        if (ob.id === Number(pdBussId)) {\r\n          this.$set(ob, 'pdBnftFactorValueCalCode', calcCode)\r\n          break\r\n        }\r\n      }\r\n      if (this.bnftForm.id === Number(pdBussId)) {\r\n        this.bnftForm.pdBnftFactorValueCalCode = calcCode\r\n      }\r\n    },\r\n    changeDialogVisible_ClmCtrlFormula(visible) {\r\n      this.formulaVisible = visible\r\n    },\r\n    changeDialogVisible_ClmCtrlFormulaSelect(visible) {\r\n      this.formulaSelectVisible = visible\r\n      if (visible === false) {\r\n        this.formulaVisible = false\r\n        this.$nextTick(() => {\r\n          this.formulaVisible = true\r\n        })\r\n      }\r\n    },\r\n    toFormula() {\r\n      if (Object.keys(this.selectedClmCtrl).length === 0) {\r\n        this.notifyError('请选择一条医疗账单免赔额')\r\n        return\r\n      }\r\n      this.pdBussDetailType = '52'\r\n      this.formulaVisible = true\r\n    },\r\n    async findTypeRegion() {\r\n      await pdtempPdClsLabList(this.pdtempPdInfo.id).then((response) => {\r\n        for (let i = 0; i < response.data.length; i++) {\r\n          if (response.data[i].pdClsType === '1') {\r\n            this.pdType = response.data[i].pdCls2;\r\n          }\r\n        }\r\n      });\r\n    },\r\n    findReferenceProducts(val) {\r\n      let data = {\r\n        page: null,\r\n        params: {\r\n          pdClsType: \"1\",\r\n          pdCls2: val,\r\n        },\r\n        limit: null,\r\n      };\r\n      findReferenceProducts(data).then((res) => {\r\n        this.productOptions = [];\r\n        res.data.forEach((product) => {\r\n          if (product.id != this.pdtempPdInfo.id) {\r\n            this.productOptions.push({\r\n              value: product.id,\r\n              label: product.pdCode + \":\" + product.pdName,\r\n            });\r\n          }\r\n        });\r\n      });\r\n    },\r\n    productDetailTypeChanged(productDetailType) {\r\n      if (productDetailType) {\r\n        findProductByDetailType(productDetailType).then(res => {\r\n          this.searchForm.pdInfoId = undefined\r\n          this.productOptions = [];\r\n          res.data.forEach((product) => {\r\n          if (product.id != this.pdtempPdInfo.id) {\r\n            this.productOptions.push({\r\n              value: product.id,\r\n              label: product.pdCode + \":\" + product.pdName,\r\n            });\r\n          }\r\n        });\r\n        })\r\n      } else {\r\n        this.searchForm.pdInfoId = undefined\r\n        this.productOptions = []\r\n      }\r\n    },\r\n    isShowQuick() {\r\n      if (!this.quickshow) {\r\n        this.quickicon = \"el-icon-arrow-down\";\r\n        this.searchForm.pdBnftCtrlLimLevel = this.bnftForm.pdBnftCtrlLimLevel\r\n        this.searchForm.productTypeDetail = this.pdtempPdInfo.cType\r\n        this.productDetailTypeChanged(this.searchForm.productTypeDetail)\r\n      } else {\r\n        this.quickicon = \"el-icon-arrow-right\";\r\n      }\r\n      return (this.quickshow = !this.quickshow);\r\n    },\r\n    selectpdtempPdMedFeeBills(code) {\r\n      selectpdtempPdMedFeeBills(code).then((res) => {\r\n        this.feeConfigs = res.data;\r\n        this.medOptions = [];\r\n        this.medFeeOptions = [];\r\n        this.feeConfigs.forEach((pdtempPdMedFeeBill) => {\r\n          let sameFlag = false;\r\n          if (this.medOptions.length > 0) {\r\n            this.medOptions.forEach((val) => {\r\n              if (val.value == pdtempPdMedFeeBill.pdMedBillType) {\r\n                sameFlag = true;\r\n              }\r\n            });\r\n          }\r\n          if (sameFlag == false) {\r\n            this.medOptions.push({\r\n              value: pdtempPdMedFeeBill.pdMedBillType,\r\n              label:\r\n                pdtempPdMedFeeBill.pdMedBillType +\r\n                \":\" +\r\n                this.getCodeName(\r\n                  \"medBillType\",\r\n                  pdtempPdMedFeeBill.pdMedBillType\r\n                ),\r\n            });\r\n          }\r\n          this.medFeeOptions.push({\r\n            value: pdtempPdMedFeeBill.pdMedFeeBillType,\r\n            label:\r\n              pdtempPdMedFeeBill.pdMedFeeBillType +\r\n              \":\" +\r\n              pdtempPdMedFeeBill.pdMedFeeName,\r\n          });\r\n        });\r\n      });\r\n    },\r\n    findMedPeriodConfigs() {\r\n      selectPdMedFeeBill(this.buildRequestData({pdClmBnftCode:this.configsForm.pdClmBnftCode,\r\n      pdMedBillType:this.feeBillData.pdMedBillType})).then((res) => {\r\n        let configs = res.data;\r\n        this.pdMedFeeBillTypes = configs;\r\n        let data = {\r\n          pdBnftCtrlType: \"D\",\r\n          pdBnftCtrlLevel: \"2\",\r\n          pdBnftCode:this.configsForm.pdClmBnftCode,\r\n          medFeeBillCodes: [],\r\n        };\r\n        configs.forEach((val) => {\r\n          data.medFeeBillCodes.push(val.pdMedFeeBillType);\r\n        });\r\n      });\r\n    },\r\n    findOptions() {\r\n      this.ClmBnftOptions = [];\r\n      this.pdClmBnftArr.forEach((data) => {\r\n        this.ClmBnftOptions.push({\r\n          value: data.pdClmBnftCode,\r\n          label: data.pdClmBnftCode + \":\" + data.pdClmBnftName,\r\n        });\r\n      });\r\n      this.bnftOptions = [];\r\n      this.pdBnftArr.forEach((data) => {\r\n        this.bnftOptions.push({\r\n          value: data.pdBnftCode,\r\n          label: data.pdBnftCode + \":\" + data.pdBnftName,\r\n        });\r\n      });\r\n    },\r\n    handleSelectionChange(row) {\r\n      this.chooseFee = row;\r\n    },\r\n    cleanBnftForm() {\r\n      this.$refs[\"bnftForm\"].resetFields();\r\n      this.radio2 = null;\r\n      this.$refs.periodTable.setCurrentRow();\r\n      this.cusObperiod = false;\r\n    },\r\n    chooseone1(row