ceRoot":"src/views/product-definition/pd-operation-def/pd-compensation-def/main/pdtemppdbnftmedinfo","sourcesContent":["<template>\r\n  <cus-wraper>\r\n    <!-- <el-row>\r\n      <el-col :span=\"24\">\r\n        <el-button dialog-disable=\"disable\" size=\"small\" type=\"text\" @click=\"backToMenu\"><i class=\"el-icon-back\"></i>{{ \"返回\" }}</el-button>\r\n        <label style=\"font-size:14px\"> 关联赔付费用选择</label>\r\n      </el-col>\r\n    </el-row>\r\n    <hr/> -->\r\n<cus-page-title-back :isDialog=\"true\" :label=\"$t('pdClaimVerificationRules1.conMedFeeChoose')\" @backTo=\"backToMenu\"></cus-page-title-back>\r\n    <br/>\r\n    <el-form ref=\"selectForm1\" :model=\"paymentForm\" :rules=\"rules\" label-width=\"120px\" style=\"width:98%;\">\r\n      <el-row :gutter=\"10\" class=\"row-bg\" justify=\"center\" type=\"flex\">\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('pdtempPdBnft.finelinesRegion')\" >\r\n            <cus-select v-model=\"paymentForm.productTypeDetail\" :codetype=\"'ProductCategory'\" @change=\"findProducts\">\r\n            </cus-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('pdtempPdBnft.product')\" prop=\"product\">\r\n            <el-select v-model=\"paymentForm.product\" :placeholder=\"$t('pdtempPdBnft.pleaseChoose')\" clearable\r\n                       filterable>\r\n              <el-option v-for=\"item in productOptions\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('i18nView.medBillType')\">\r\n            <cus-select v-model=\"paymentForm.pdBnftDetailType\" :codetype=\"'medBillType'\"></cus-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <cus-btn-wrapper>\r\n            <el-button dialog-disable=\"disable\"  size=\"small\" @click=\"selectPdBnft\" icon=\"el-icon-search\" type=\"primary\">\r\n              {{ $t('table.search') }}\r\n            </el-button>\r\n        </cus-btn-wrapper>\r\n      </el-row>\r\n    </el-form>\r\n    <cus-table-title :label=\"$t('i18nView.chooseAssociatedBillMoney')\"/>\r\n    <el-table ref=\"bnftTable\" :data=\"bnftConfigs\" align=\"center\" border class=\"wraper-table\" element-loading-text=\"Loading\"\r\n              fit highlight-current-row size=\"mini\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\" width=\"50\"></el-table-column>\r\n      <el-table-column :label=\"$t('table.detail')\" align=\"center\" style=\"width: 50%\" type=\"expand\">\r\n        <template slot-scope=\"props\">\r\n          <el-form class=\"demo-table-expand\" inline label-position=\"left\">\r\n            <el-form-item :label=\"$t('i18nView.pdCode')\" style=\"width: 50%\">\r\n              {{ props.row.pdCode }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('customComponents.pdEdition')\" style=\"width: 50%\">\r\n              {{ props.row.pdEdition }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdName')\" style=\"width: 50%\">\r\n              {{ props.row.pdName }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.medFeeBillCusFlag')\" style=\"width: 50%\">\r\n              {{ props.row.pdMedFeeCalMode | filter_pdMedFeeCalMode }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdBeduFlag')\" style=\"width: 50%\">\r\n              {{ props.row.pdBnftCtrlType=='D'?'Y':'N' }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.limitFlag')\" style=\"width: 50%\">\r\n              {{ props.row.pdBnftCtrlType=='L'?'Y':'N' }}\r\n            </el-form-item>\r\n          </el-form>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('i18nView.feeBillBigClass')\" prop=\"pdMedBillType\"></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('i18nView.feeType')\" prop=\"pdMedFeeBillType\"></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('i18nView.feeName')\" prop=\"pdMedFeeName\"></el-table-column>\r\n    </el-table>\r\n    <cus-pagination ref=\"page\" v-model=\"bnftConfigs\" :pageSize=\"5\"\r\n                    :params=\"paymentForm\" url=\"/pdtemp-pd-claim-verification-rule/base/selectPdMedFeeBill\"/>\r\n    <br/>\r\n    <el-form>\r\n      <el-form-item align=\"right\">\r\n        <el-button dialog-disable=\"disable\"  size=\"small\" v-auth=\"'select:AllPdAccount'\" icon=\"el-icon-circle-plus-outline\" type=\"primary\" @click=\"addPdProg\">\r\n          {{$t('i18nView.add')}}\r\n        </el-button>\r\n        <el-button dialog-disable=\"disable\" icon=\"el-icon-refresh\" size=\"small\" type=\"primary\" @click=\"researchData\">\r\n          {{$t('table.refresh')}}\r\n        </el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n    <cus-table-title :label=\"$t('i18nView.conMedFeeBill')\"/>\r\n    <el-table ref=\"paymentTable\" :data=\"paymentConfigs\" align=\"center\" border class=\"wraper-table\"\r\n              element-loading-text=\"Loading\" fit highlight-current-row max-height=\"300px\" size=\"mini\"\r\n              @row-click=\"chooseone2\">\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\" width=\"50\"></el-table-column>\r\n      <el-table-column :label=\"$t('table.detail')\" align=\"center\" style=\"width: 50%\" type=\"expand\">\r\n        <template slot-scope=\"props\">\r\n          <el-form class=\"demo-table-expand\" inline label-position=\"left\">\r\n            <el-form-item :label=\"$t('i18nView.pdCode')\" style=\"width: 50%\">\r\n              {{ props.row.pdCode }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('customComponents.pdEdition')\" style=\"width: 50%\">\r\n              {{ props.row.pdEdition }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdName')\" style=\"width: 50%\">\r\n              {{ props.row.pdName }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.medFeeBillCusFlag')\" style=\"width: 50%\">\r\n              {{ props.row.pdMedFeeCalMode | filter_pdMedFeeCalMode }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdBeduFlag')\" style=\"width: 50%\">\r\n              {{ props.row.pdBnftCtrlType=='D'?'Y':'N' }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.limitFlag')\" style=\"width: 50%\">\r\n              {{ props.row.pdBnftCtrlType=='L'?'Y':'N' }}\r\n            </el-form-item>\r\n          </el-form>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('i18nView.medBillType')\" prop=\"pdMedBillType\"></el-table-column>\r\n      <el-table-column :label=\"$t('pdtempclmbnft.pdMedFeeCode')\" align=\"center\"\r\n                       prop=\"pdMedFeeCode\"></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('i18nView.feeType')\" prop=\"pdMedFeeBillType\"></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('i18nView.feeName')\" prop=\"pdMedFeeName\"></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('table.actions')\" width=\"60px\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button dialog-disable=\"disable\" icon=\"el-icon-delete\" size=\"mini\" type=\"danger\" @click=\"disConnData(scope.row)\"></el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\nimport {\r\n  bnftInfoConnData,\r\n  bnftMoreSearch,\r\n  deleteClmCtrlRela,\r\n  findProductsByProductType\r\n} from \"@/api/basicinfo/pdtemppdclmbnft\";\r\nimport {findBusinessItemByCtrlCode, findPdMedFeeBillTypeByCode} from \"@/api/basicinfo/productCompensation\"\r\n\r\n\r\nexport default {\r\n  name: \"pdtemppdbnftmedinfo\",\r\n  filters:{\r\n    filter_pdMedFeeCalMode(code){\r\n      if(code){\r\n        return \"Y\"\r\n      }else{\r\n        return \"N\"\r\n      }\r\n    }\r\n  },\r\n  props: {\r\n    moreType: {\r\n      type: String,\r\n      default: ''\r\n    },\r\n    pdBnftCtrlCode: {\r\n      type: String,\r\n      default: ''\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      radio2: null,\r\n      pdtempPdInfo: {},\r\n      paymentForm: {},\r\n      paymentConfigs: [],\r\n      bnftConfigs: [],\r\n      productOptions: [],\r\n      chooseBnft: [],\r\n      paymentForm: {\r\n        productTypeDetail: '',\r\n        product: '',\r\n        pdBnftDetailType: '',\r\n      },\r\n      rules:{\r\n          product: [\r\n            {required: true, message: \"请输入产品\", trigger: \"change\"}\r\n          ],\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    let pdtempPdInfo = this.getPdData();\r\n    this.pdtempPdInfo = pdtempPdInfo;\r\n    this.paymentForm.productTypeDetail = this.pdtempPdInfo.cType;\r\n    this.findProducts(this.paymentForm.productTypeDetail)\r\n    this.findConnBnftInfo();\r\n  },\r\n  methods: {\r\n    addPdProg() {\r\n      for (const item of this.chooseBnft) {\r\n        let duplicated = false\r\n        for (const selectedItem of this.paymentConfigs) {\r\n          if (selectedItem.id === item.id) {\r\n            setTimeout(() => {\r\n              this.notifyError(item.pdProgCode + ':' + item.pdProgName + '已添加，请勿重复添加')\r\n            }, 1)\r\n            duplicated = true\r\n            break\r\n          }\r\n        }\r\n        if (!duplicated) {\r\n          this.paymentConfigs.push(item)\r\n        }\r\n      }\r\n    },\r\n    selectPdBnft() {\r\n        this.$refs[\"selectForm1\"].validate(valid => {\r\n          if(valid){\r\n            this.$refs.page.reload();\r\n          }\r\n        })\r\n    },\r\n    findConnBnftInfo() {\r\n      if (this.pdBnftCtrlCode) {\r\n        findBusinessItemByCtrlCode(this.pdBnftCtrlCode).then(response => {\r\n          this.paymentConfigs = response.data\r\n        })\r\n      }\r\n    },\r\n    findProducts(val) {\r\n      let data = {\r\n        pdCls2: val\r\n      }\r\n      findProductsByProductType(this.buildRequestData(data)).then(res => {\r\n        this.productOptions = [];\r\n        res.data.forEach(product => {\r\n          if (product.id != this.pdtempPdInfo.id) {\r\n            this.productOptions.push({\r\n              value: product.pdCode,\r\n              label: product.pdCode + \":\" + product.pdName\r\n            });\r\n          }\r\n        });\r\n      })\r\n    },\r\n    researchData() {\r\n      this.paymentForm.productTypeDetail = ''\r\n      this.paymentForm.product = ''\r\n      this.bnftConfigs = []\r\n      this.$refs.page.reload()\r\n    },\r\n    chooseone2(row, column, event) {\r\n      if (event.target.nodeName == \"INPUT\") {\r\n        if (row.id == this.radio2) {\r\n          this.radio2 = null;\r\n          this.$refs.paymentTable.setCurrentRow();\r\n        } else {\r\n          this.radio2 = row.id;\r\n        }\r\n      }\r\n    },\r\n    disConnData(row) {\r\n        this.paymentConfigs.splice(this.paymentConfigs.indexOf(row), 1)\r\n    },\r\n    handleSelectionChange(row) {\r\n      this.chooseBnft = row;\r\n    },\r\n    backToMenu() {\r\n      //this.moreType=meddemore\r\n      this.$emit('backToFeeBill', 'medBnftMore', this.paymentConfigs)\r\n    },\r\n    paymentFormSearch() {\r\n      let data = {\r\n        productTypeDetail: this.paymentForm.productTypeDetail,\r\n        productCode: this.paymentForm.product,\r\n        medBillType: this.paymentForm.medBillType\r\n      }\r\n      bnftMoreSearch(this.buildRequestData(data)).then(res => {\r\n        let data = res.data;\r\n        this.bnftConfigs = data;\r\n      })\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.divider {\r\n  color: black;\r\n  background-color: black;\r\n  width: 1px;\r\n}\r\n</style>\r\n"]}]}