e-column :label=\"$t('pdgenlPaInfo.calNumber')\" align=\"center\"\r\n                       prop=\"calNumber\" sortable></el-table-column>\r\n      <!-- <el-table-column :label=\"$t('i18nView.pdDesignType')\" align=\"center\"\r\n                        prop=\"pdPaType\" sortable>\r\n         <template v-slot=\"scope\">\r\n             <span v-for=\"list in scope.row.pdgenlPdPaDesTypeList\" v-if=\"scope.row.pdgenlPdPaDesTypeList.length > 0\">\r\n               {{ list.pdDesType | filter_pdPaType }}\r\n               <span v-if=\"scope.row.pdgenlPdPaDesTypeList.indexOf(list) !== scope.row.pdgenlPdPaDesTypeList.length - 1\">,</span>\r\n             </span>\r\n         </template>\r\n       </el-table-column> -->\r\n    </el-table>\r\n    <cus-pagination ref=\"page1\" v-model=\"configs\" :pageSize=\"10\" :params=\"params\" url=\"/common-pd-pa/base/list\"/>\r\n\r\n    <el-dialog v-dialog-drag :close-on-click-modal=\"false\" :close-on-press-escape=\"false\" :show-close=\"false\"\r\n               :visible.sync=\"dialogTableVisible1\" center modal-append-to-body width=\"75%\">\r\n      <cus-page-title-back :isDialog=\"true\" :label=\"$t('pdgenlPaInfo.paGeneralDef')\"\r\n                           @backTo=\"back\"></cus-page-title-back>\r\n      <el-form inline label-position=\"left\">\r\n        <el-form-item :label=\"$t('i18nView.preservationItem')\" style=\"width: 100%\">\r\n          <el-tag type=\"info\">{{ pdPaInfo.pdPaCode + \":\" + pdPaInfo.pdPaName }}</el-tag>\r\n        </el-form-item>\r\n      </el-form>\r\n\r\n      <!-- 信息项表格 -->\r\n      <!--      <cus-table-title  v-show=\"false\" :label=\"$t('pdgenlPaInfo.ppInformation')\"></cus-table-title>\r\n            <el-table :data=\"configs1\" border element-loading-text=\"Loading\" fit highlight-current-row size=\"mini\" v-show=\"false\">\r\n              <el-table-column :label=\"$t('i18nView.pdPaMsName')\" align=\"center\"\r\n                               prop=\"pdPaMessName\" sortable></el-table-column>\r\n     