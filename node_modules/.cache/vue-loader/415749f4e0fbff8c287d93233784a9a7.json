uctDesignType', code)\r\n    },\r\n    filter_ruleBussModu(code) {\r\n      return _this.getCodeName('ruleBusinessModule', code)\r\n    },\r\n    filter_ruleBussModuDeta(code) {\r\n      return _this.getCodeName('ruleBusinessDetailClass', code)\r\n    },\r\n    filter_pdPaMsInputType(code) {\r\n      return _this.getCodeName('pdPaMsInputType', code)\r\n    }\r\n  },\r\n  components: {pdgenlPaRule, pdgenlPaFormula},\r\n  data() {\r\n    _this = this\r\n    return {\r\n      attributeObj: {},\r\n      ruleVisible: false,\r\n      ruleView: {\r\n        ruleType: '',\r\n        visible: false,\r\n        flag: '',\r\n        ruleCode: '',\r\n        performType: ''\r\n      },\r\n      params: {\r\n        pdPaCode: '',\r\n        pdPaName: '',\r\n        pdAttr: ''\r\n      },\r\n      pdPaMsValidCodes: [],\r\n      dialogTableVisible1: false,\r\n      dialogTableVisible2: false,\r\n      dialogTableVisible3: false,\r\n      selectedDef: {},\r\n      selectedDef1: {},\r\n      selectedDef2: {},\r\n      configs: [],\r\n      configs1: [],\r\n      configs2: [],\r\n      configs3: [],\r\n      params_currentRule: {\r\n        current_pdPaInfo: this.pdPaInfo\r\n      },\r\n      params_currentFormula: {\r\n        current_pdPaInfo: this.pdPaInfo\r\n      },\r\n      selectedRules: [],\r\n      selectedFormula: [],\r\n      pdPaCode: '',\r\n      pdPaInfo: {\r\n        id: '',\r\n        pdPaCode: '',\r\n        pdPaName: '',\r\n        pdAttr: '',\r\n        pdPaType: [],\r\n        pdgenlPdPaDesTypeList: []\r\n      },\r\n      formAddRule: {\r\n        pdRuleDtCls: ''\r\n      },\r\n      formAddFormula: {\r\n        pdBussDetailType: ''\r\n      },\r\n      rulesForAddRule: {\r\n        pdRuleDtCls: [{required: true, message: '请选择规则细类', trigger: 'change'}]\r\n      },\r\n      rulesForAddFormula: {\r\n        pdBussDetailType: [{required: true, message: '请选择公式细类', trigger: 'change'}]\r\n      }\r\n    }\r\n  },\r\n  computed: {},\r\n  created() {\r\n    this.open()\r\n  },\r\n  methods: {\r\n    toRules() {\r\n      this.$refs.formAddRule.validate(valid => {\r\n        if (valid) {\r\n          this.dialogTableVisible2 = true\r\n          this.dialogTableVisible1 = false\r\n        }\r\n      })\r\n    },\r\n    toCalc() {\r\n      this.$refs.formAddFormula.validate(valid => {\r\n        if (valid) {\r\n          this.dialogTableVisible3 = true\r\n          this.dialogTableVisible1 = false\r\n        }\r\n      })\r\n    },\r\n    closeDialogRule() {\r\n      this.dialogTableVisible2 = false\r\n      this.dialogTableVisible1 = true\r\n      this.params_currentRule.current_pdPaInfo = this.pdPaInfo\r\n      this.$refs.page_currentRule.reload()\r\n    },\r\n    closeDialogFormula() {\r\n      this.dialogTableVisible3 = false\r\n      this.dialogTableVisible1 = true\r\n      this.params_currentFormula.current_pdPaInfo = this.pdPaInfo\r\n      this.$refs.page_currentFormula.reload()\r\n    },\r\n    /*选择当前行*/\r\n    choose(row) {\r\n      this.dialogTableVisible1 = true\r\n      this.pdPaInfo = row\r\n      queryMessage(this.buildRequestData({\"id\": row.id})).then(res => {\r\n        this.configs1 = res.data\r\n        this.pdPaCode = row.pdPaCode\r\n        this.params_currentRule.current_pdPaInfo = row\r\n        this.$refs.page_currentRule.reload()\r\n        this.params_currentFormula.current_pdPaInfo = row\r\n        this.$refs.page_currentFormula.reload()\r\n        this.$nextTick(() => {\r\n          this.$refs.formAddRule.clearValidate()\r\n          this.$refs.formAddFormula.clearValidate()\r\n        })\r\n      })\r\n    },\r\n    /*取消选中*/\r\n    rowClick(row, column, event) {\r\n      if (event.target.nodeName === \"INPUT\") {\r\n        if (row.id === this.selectedDef.id) {\r\n          this.selectedDef = {}\r\n        }\r\n      }\r\n    },\r\n    back() {\r\n      this.dialogTableVisible1 = false\r\n      this.selectedDef = {}\r\n    },\r\n    removeRule(ruleOrCalcId, ruleType) {\r\n      const params = {\r\n        