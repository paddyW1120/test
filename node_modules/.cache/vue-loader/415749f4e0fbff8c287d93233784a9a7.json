\"el-icon-circle-plus el-icon--left\" size=\"small\" type=\"primary\" @click=\"toAddRule\">\r\n                {{ $t('rule.toAddRule') }}\r\n              </el-button>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n\r\n      <cus-table-title :label=\"$t('i18nView.defDoneRule')\"></cus-table-title>\r\n      <el-table :data=\"configs2\" border element-loading-text=\"Loading\" fit highlight-current-row max-height=\"300\"\r\n                size=\"mini\">\r\n        <el-table-column :label=\"$t('table.select')\" width=\"55\"  align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <el-radio-group v-model=\"selectedDef1\">\r\n              <span><el-radio :label=\"scope.row\" @change=\"toEditDecisionTree_rule(scope.row)\">{{ '' }}</el-radio></span>\r\n            </el-radio-group>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.id')\" align=\"center\" width=\"50\">\r\n          <template v-slot=\"scope\">\r\n            {{ initTableIndex('page_currentRule', scope.$index) }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.detail')\" align=\"center\" width=\"80\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button size=\"small\" type=\"text\" @click=\"showRule(scope.row)\">>></el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.ruleCode')\" align=\"center\"\r\n                         min-width=\"200\" prop=\"pdRuleCode\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.ruleType')\" align=\"center\"\r\n                         min-width=\"200\" prop=\"pdRuleCls\">\r\n          <template v-slot=\"scope\">{{ scope.row.pdRuleCls | filter_ruleBussModu }}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('rule.pdRuleDtCls')\" align=\"center\"\r\n                         min-width=\"200\" prop=\"pdRuleDtCls\">\r\n          <template v-slot=\"scope\">{{ scope.row.pdRuleDtCls | filter_ruleBussModuDeta }}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.ruleDec')\" align=\"center\" min-width=\"200\"\r\n                         prop=\"pdRuleDesc\"></el-table-column>\r\n        <el-table-column :label=\"$t('rule.ruleAttr')\" align=\"center\" min-width=\"200\">\r\n          <template v-slot=\"scope\">\r\n            {{\r\n              scope.row.ruleAttr === '1' ? $t('rule.general') : (scope.row.ruleAttr === '2' ? $t('rule.individual') : '')\r\n            }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.actions')\" align=\"center\" fixed=\"right\" width=\"120\">\r\n          <template v-slot=\"scope\">\r\n            <cus-del-btn @ok=\"removeRule(scope.row.id, '1')\"></cus-del-btn>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <cus-pagination ref=\"page_currentRule\" v-model=\"configs2\" :autoBuild=\"false\" :pageSize=\"5\"\r\n                      :params=\"params_currentRule\" url=\"/pd-pa-info-general/queryRulesThis\"/>\r\n\r\n      <!-- 公式 -->\r\n      <cus-form-title :label=\"$t('pdgenlPaInfo.ppFormula')\"></cus-form-title>\r\n\r\n      <el-form ref=\"formAddFormula\" :model=\"formAddFormula\" :rules=\"rulesForAddFormula\" label-width=\"auto\">\r\n        <el-row :gutter=\"10\" type=\"flex\">\r\n          <el-col :span=\"8\">\r\n            <el-form-item :label=\"$t('i18nView.formulaDetial')\" prop=\"pdBussDetailType\">\r\n              <cus-select v-model=\"formAddFormula.pdBussDetailType\" codetype=\"ruleBusinessDetailClass\"\r\n                          filter=\"1$43$44$45$46$47$48$49\"></cus-select>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"8\"></el-col>\r\n          <el-col :span=\"16\" style=\"text-align: right\">\r\n            <el-form-item>\r\n  