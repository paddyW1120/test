BussType\">\r\n          <template v-slot=\"scope\">{{ scope.row.pdBussType | filter_ruleBussModu }}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.formulaDetial')\" align=\"center\"\r\n                         min-width=\"200\" prop=\"pdBussDetailType\">\r\n          <template v-slot=\"scope\">{{ scope.row.pdBussDetailType | filter_ruleBussModuDeta }}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.formulaDec')\" align=\"center\"\r\n                         min-width=\"200\" prop=\"pdCalcDesc\"></el-table-column>\r\n        <el-table-column :label=\"$t('rule.ruleAttr')\" align=\"center\" min-width=\"200\">\r\n          <template v-slot=\"scope\">\r\n            {{\r\n              scope.row.ruleAttr === '1' ? $t('rule.general') : (scope.row.ruleAttr === '2' ? $t('rule.individual') : '')\r\n            }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.actions')\" align=\"center\" fixed=\"right\" width=\"120\">\r\n          <template v-slot=\"scope\">\r\n            <cus-popover icon=\"el-icon-delete\" size=\"mini\" type=\"danger\"\r\n                         @confirm=\"removeRule(scope.row.id, '2')\"></cus-popover>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <cus-pagination ref=\"page_currentFormula\" v-model=\"configs3\" :autoBuild=\"false\" :pageSize=\"5\"\r\n                      :params=\"params_currentFormula\" url=\"/pd-pa-info-general/queryFormulaThis\"/>\r\n\r\n      <el-row style=\"margin-top: 35px\">\r\n        <el-col :span=\"24\" style=\"text-align: right\">\r\n          <el-button icon=\"el-icon-success\" size=\"small\" type=\"primary\" @click=\"savepdgenlPa\">\r\n            {{ $t('pdgenlPaInfo.save') }}\r\n          </el-button>\r\n        </el-col>\r\n      </el-row>\r\n    </el-dialog>\r\n\r\n    <!--关联规则-->\r\n    <el-dialog v-if=\"dialogTableVisible2\" v-dialog-drag :