leView.ruleType = '1'\r\n      this.ruleView.flag = '2'\r\n      this.ruleView.ruleCode = row.pdRuleCode\r\n      this.ruleView.performType = row.performType\r\n      this.ruleView.visible = true\r\n    },\r\n    open() {\r\n      let data = this.$route.params.ruleAttribute\r\n      if (data) {\r\n        queryAllPa(this.buildRequestData({\"pdPaCode\": data.rulePosItem})).then(res => {\r\n          this.pdPaInfo = res.data.records[0]\r\n          if (data.ruleType === '2') {\r\n            this.dialogTableVisible3 = true\r\n          } else {\r\n            this.dialogTableVisible2 = true\r\n          }\r\n          this.$route.params.ruleAttribute = null\r\n        })\r\n      }\r\n    },\r\n    toAddRule() {\r\n      this.$refs.formAddRule.validate(valid => {\r\n        if (valid) {\r\n          const param = {\r\n            pdBussType: '9',\r\n            pdBussDetailType: this.formAddRule.pdRuleDtCls\r\n          }\r\n          getRuleCode(this.buildRequestData(param)).then(res => {\r\n            this.attributeObj = {\r\n              code: res.data,\r\n              ruleBussModu: '9',\r\n              ruleBussModuDeta: this.formAddRule.pdRuleDtCls,\r\n              rulePosItem: this.pdPaInfo.pdPaCode,\r\n              pdAttr: this.pdPaInfo.pdAttr,\r\n              ruleType: '1',\r\n              pdType: '1'\r\n            }\r\n            this.ruleVisible = true\r\n          })\r\n        }\r\n      })\r\n    },\r\n    toAddFormula() {\r\n      this.$refs.formAddFormula.validate(pass => {\r\n        if (pass) {\r\n          const param = {\r\n            pdBussType: '9',\r\n            pdBussDetailType: this.formAddFormula.pdBussDetailType\r\n          }\r\n          getCalcCode(this.buildRequestData(param)).then(response => {\r\n            this.attributeObj = {\r\n              code: response.data,\r\n              ruleBussModu: '9',\r\n              ruleBussModuDeta: this.formAddFormula.pdBussDetailType,\r\n              rulePosItem: this.pdPaInfo.pdPaCode