CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/views/commodity-audit/package-audit/comboAuditRecords","sourcesContent":["<template>\r\n  <cus-wraper>\r\n    <el-form ref=\"params\" :model=\"params\"  label-width=\"auto\">\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('i18nView.combo')\" prop=\"eq_pdInfoId\">\r\n            <el-select clearable filterable v-model=\"params.ppCode\">\r\n              <el-option v-for=\"combo in comboOptionList\" :label=\"combo.ppCode + ':' + combo.ppName\"\r\n                         :value=\"combo.ppCode\" :key=\"combo.id\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('i18nView.auditDate')\">\r\n            <el-date-picker\r\n              v-model=\"auditDateValue\" type=\"daterange\" align=\"right\" unlink-panels\r\n              value-format=\"yyyy-MM-dd\"\r\n              :range-separator=\"$t('comboAuditRecords.to')\"\r\n              :start-placeholder=\"$t('comboAuditRecords.comboAuditStartDate')\"\r\n              :end-placeholder=\"$t('comboAuditRecords.comboAuditEndDate')\"\r\n              :picker-options=\"pickerOptions\">\r\n            </el-date-picker>\r\n          </el-form-item>\r\n        </el-col>\r\n        <cus-btn-wrapper>\r\n          <el-button  size=\"small\" type=\"primary\" icon=\"el-icon-search\" @click=\"queryTableData\">\r\n            {{$t('comboAuditRecords.search')}}\r\n          </el-button>\r\n        </cus-btn-wrapper>\r\n      </el-row>\r\n    </el-form>\r\n    <cus-table-title :label=\"$t('i18nView.comboAuditRecordStatistics')\"></cus-table-title>\r\n    <el-table :data=\"comboAuditRecordStatistical\" size=\"mini\" @row-click=\"chooseOneRow\"\r\n              fit element-loading-text=\"Loading\" border align=\"center\" class=\"wraper-table\">\r\n      <el-table-column width=\"50\" align=\"center\" :label=\"$t('comboAuditRecords.choose')\">\r\n        <template slot-scope=\"scope\">\r\n          <el-radio-group v-model=\"chooseRow\">\r\n            <el-radio :label=\"scope.row\">{{''}}</el-radio>\r\n          </el-radio-group>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" width=\"50px\">\r\n        <template v-slot=\"scope\">\r\n          {{ initTableIndex('page', scope.$index) }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.ppCode')\" prop=\"ppCode\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.ppName')\" prop=\"ppName\" align=\"center\" width=\"220px\"></el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.ppEdition')\" prop=\"pdVerId\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.applyDoneDate')\" prop=\"pdAuditDate\" align=\"center\"></el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.pdAuditTimes')\" prop=\"pdCount\" align=\"center\"></el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <cus-pagination ref=\"page\" url=\"/combo-audit-info/records\" :pageSize=\"10\" :params=\"params\"\r\n                    v-model=\"comboAuditRecordStatistical\"/>\r\n\r\n    <div v-if=\"showComboAuditNumberFlag\">\r\n      <cus-table-title :label=\"$t('i18nView.pdAuditTimes')\"></cus-table-title>\r\n      <el-table :data=\"auditNumberTable\" size=\"mini\" @row-click=\"chooseNumberAuditRow\"\r\n                fit element-loading-text=\"Loading\" border align=\"center\" class=\"wraper-table\">\r\n        <el-table-column width=\"50\" align=\"center\" :label=\"$t('comboAuditRecords.choose')\">\r\n          <template slot-scope=\"scope\">\r\n            <el-radio-group v-model=\"chooseNumberRow\">\r\n              <el-radio :label=\"scope.row\">{{''}}</el-radio>\r\n            </el-radio-group>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column type=\"index\" width=\"50\" :label=\"$t('table.id')\" align=\"center\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.ppCode')\" align=\"center\" prop=\"pdCode\">\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.ppEdition')\" align=\"center\" prop=\"pdVerId\">\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.auditRes')\" align=\"center\" prop=\"pdAuditConc\">\r\n          <template v-slot=\"scope\">{{scope.row.pdAuditConc | filter_pdAuditConcthis}}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.pdReleType')\" align=\"center\" prop=\"pdReleType\">\r\n          <template v-slot=\"scope\">{{scope.row.pdReleType | filter_pdReleType}}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.postContent')\" align=\"center\" prop=\"pdReleContent\">\r\n          <template v-slot=\"scope\">{{scope.row.pdReleContent | filter_pdReleContent}}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.auditOper')\" align=\"center\" prop=\"pdAuditStaff\">\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.auditDate')\" align=\"center\" prop=\"pdAuditDate\">\r\n        </el-table-column>\r\n      </el-table>\r\n      <div v-if=\"showComboAuditNumberDataFlag\">\r\n        <cus-form-title :label=\"$t('i18nView.auditDetail')\"/>\r\n        <div v-if=\"comboDefineData.length>0\">\r\n          <cus-table-title :label=\"$t('applyAudit.pdPostAudit')\"></cus-table-title>\r\n          <el-table :data=\"comboDefineData\" size=\"mini\" element-loading-text=\"Loading\" border align=\"center\">\r\n            <el-table-column :label=\"$t('table.id')\" type=\"index\"\r\n                             align=\"center\"></el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.defineContent')\" prop=\"pdDefineContent\"\r\n                             align=\"center\">\r\n              <template v-slot=\"scope\">{{scope.row.pdDefineContent | filter_pdNodeCode}}</template>\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.auditRes')\" prop=\"pdAuditConcthis\"\r\n                             align=\"center\">\r\n              <template v-slot=\"scope\">{{scope.row.pdAuditConcthis | filter_pdAuditConcthis}}</template>\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.isCancelDefinition')\"\r\n                             prop=\"pdIsCancelDefinition\" align=\"center\">\r\n              <template v-slot=\"scope\">{{scope.row.pdIsCancelDefinition | filter_pdIsCancelDefinition}}</template>\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.notPassOpinion')\" prop=\"pdNotPassOpinion\"\r\n                             align=\"center\"></el-table-column>\r\n          </el-table>\r\n        </div>\r\n        <div v-if=\"ruleDataTable.length>0\">\r\n          <cus-table-title :label=\"$t('applyAudit.rulesPostAudit')\"></cus-table-title>\r\n          <el-table :data=\"ruleDataTable\" size=\"mini\" element-loading-text=\"Loading\" border align=\"center\">\r\n            <el-table-column :label=\"$t('table.id')\" type=\"index\"\r\n                             align=\"center\"></el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.ruleCode')\" prop=\"pdFormulaRuleCode\"\r\n                             align=\"center\" width=\"180px\"></el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.ruleBussType')\" prop=\"pdFormulaRuleType\"\r\n                             align=\"center\">\r\n              <template v-slot=\"scope\">{{scope.row.pdFormulaRuleType | filter_pdRuleType}}</template>\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.ruleBussDetail')\" prop=\"pdFormulaRuleDetails\"\r\n                             align=\"center\">\r\n              <template v-slot=\"scope\">{{scope.row.pdFormulaRuleDetails | filter_pdRuleDetails}}</template>\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.ruleDec')\"\r\n                             prop=\"pdFormulaRuleDescribe\" align=\"center\"></el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.auditRes')\" prop=\"pdAuditConcthis\" align=\"center\">\r\n              <template v-slot=\"scope\">{{scope.row.pdAuditConcthis | filter_pdAuditConcthis}}</template>\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.isCancelDefinition')\"\r\n                             prop=\"pdIsCancelDefinition\" align=\"center\">\r\n              <template v-slot=\"scope\">{{scope.row.pdIsCancelDefinition | filter_pdIsCancelDefinition}}</template>\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.notPassOpinion')\" prop=\"pdNotPassOpinion\"\r\n                             align=\"center\"></el-table-column>\r\n          </el-table>\r\n        </div>\r\n        <div v-if=\"formulaDataTable.length>0\">\r\n          <cus-table-title :label=\"$t('applyAudit.formulaPostAudit')\"></cus-table-title>\r\n          <el-table :data=\"formulaDataTable\" size=\"mini\" element-loading-text=\"Loading\" border align=\"center\">\r\n            <el-table-column :label=\"$t('table.id')\" type=\"index\"\r\n                             align=\"center\"></el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.formulaCode')\" prop=\"pdFormulaRuleCode\"\r\n                             align=\"center\" width=\"180px\"></el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.formulaBussType')\" prop=\"pdFormulaRuleType\"\r\n                             align=\"center\">\r\n              <template v-slot=\"scope\">{{scope.row.pdFormulaRuleType | filter_pdFormulaType}}</template>\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.formulaBussDetail')\" prop=\"pdFormulaRuleDetails\"\r\n                             align=\"center\">\r\n              <template v-slot=\"scope\">{{scope.row.pdFormulaRuleDetails | filter_pdFormulaDetails}}</template>\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.formulaDec')\"\r\n                             prop=\"pdFormulaRuleDescribe\"\r\n                             align=\"center\"></el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.auditRes')\" prop=\"pdAuditConcthis\"\r\n                             align=\"center\">\r\n              <template v-slot=\"scope\">{{scope.row.pdAuditConcthis | filter_pdAuditConcthis}}</template>\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.isCancelDefinition')\"\r\n                             prop=\"pdIsCancelDefinition\"\r\n                             align=\"center\">\r\n              <template v-slot=\"scope\">{{scope.row.pdIsCancelDefinition | filter_pdIsCancelDefinition}}</template>\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('i18nView.notPassOpinion')\" prop=\"pdNotPassOpinion\"\r\n                             align=\"center\"></el-table-column>\r\n          </el-table>\r\n        </div>\r\n        <el-form :model=\"pdAuditDetailInfo\"  label-width=\"auto\" label-position=\"right\"\r\n                 ref=\"pdAuditDetailInfo\" style=\"margin-top: 10px\">\r\n          <el-row :gutter=\"20\">\r\n            <el-col :span=\"8\">\r\n              <el-form-item style=\"width: 100%\" :label=\"$t('i18nView.auditContent')\">\r\n                <span>{{ pdAuditDetailInfo.pdAuditIdea }}</span>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col :span=\"8\">\r\n              <el-form-item v-show=\"showAuditType || showAuditType1\" :label=\"$t('i18nView.pdReleType')\">\r\n                <span>{{ pdAuditDetailInfo.pdReleType | filter_pdReleType }}</span>\r\n              </el-form-item>\r\n            </el-col>\r\n            <el-col :span=\"8\">\r\n              <el-form-item v-show=\"showAuditContent\" style=\"width: 100%\" :label=\"$t('i18nView.pdReleType')\">\r\n                <span>{{ pdAuditDetailInfo.pdReleType | filter_pdReleType }}</span>\r\n              </el-form-item>\r\n            </el-col>\r\n          </el-row>\r\n        </el-form>\r\n      </div>\r\n    </div>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\n  import {auditNumberList, auditRecordsList, queryAuditNotesList, queryComboAuditInfo} from \"@/api/basicinfo/comboAuditInfo\";\r\n\r\n  let _this;\r\n  export default {\r\n    name: 'comboAuditRecords',\r\n    filters: {\r\n      filter_pdNodeCode(val) {\r\n        let returnText = '';\r\n        switch (val) {\r\n          case 'productRuleFlag':\r\n            returnText = '产品规则订正';\r\n            break;\r\n          case 'productFormulaFlag':\r\n            returnText = '产品公式订正';\r\n            break;\r\n          case 'productInfoFlag':\r\n            returnText = '产品框架定义';\r\n            break;\r\n          case 'productDistributionFlag':\r\n            returnText = '产品销售信息';\r\n            break;\r\n          case 'productCoverFlag':\r\n            returnText = '产品承保信息';\r\n            break;\r\n          case 'productUnderwritingFlag':\r\n            returnText = '产品核保信息';\r\n            break;\r\n          case 'productPreservationFlag':\r\n            returnText = '产品保全信息';\r\n            break;\r\n          case 'productHePeiFlag':\r\n            returnText = '产品核赔信息';\r\n            break;\r\n          case 'productRenewalFlag':\r\n            returnText = '产品续期续保信息';\r\n            break;\r\n        }\r\n        return returnText;\r\n      },\r\n      filter_pdReleType(code) {\r\n        return _this.getCodeName('releasetype', code)\r\n      },\r\n      filter_pdReleContent(code) {\r\n        return _this.getCodeName('pdReleContent', code)\r\n      },\r\n      filter_pdIsReviewReturn(code) {\r\n        return _this.getCodeName('isReviewReturn', code)\r\n      },\r\n      filter_pdIsPostReturn(code) {\r\n        return _this.getCodeName('isPostReturn', code)\r\n      },\r\n      filter_pdAuditType(code) {\r\n        return _this.getCodeName('pdAuditType', code)\r\n      },\r\n      filter_pdAuditConcthis(code) {\r\n        return _this.getCodeName('pdAuditConc', code)\r\n      },\r\n      filter_pdIsCancelDefinition(code) {\r\n        return _this.getCodeName('cancelDefinition', code)\r\n      },\r\n      filter_pdRuleType(code) {\r\n        return _this.getCodeName('ruleBusinessModule', code)\r\n      },\r\n      filter_pdFormulaType(code) {\r\n        return _this.getCodeName('ruleBusinessModule', code)\r\n      },\r\n      filter_pdRuleDetails(code) {\r\n        return _this.getCodeName('ruleBusinessDetailClass', code)\r\n      },\r\n      filter_pdFormulaDetails(code) {\r\n        return _this.getCodeName('ruleBusinessDetailClass', code)\r\n      }\r\n    },\r\n    data() {\r\n      _this = this;\r\n      return {\r\n        //表格数据\r\n        comboAuditRecordStatistical: [],\r\n        auditRecordStatisticalAllData: [],\r\n        auditNumberTable: [],\r\n        auditProductTable: [],\r\n        ruleDataTable: [],\r\n        formulaDataTable: [],\r\n        comboDefineData: [],\r\n        chooseRow: {},\r\n        chooseNumberRow: {},\r\n        pdAuditConc: '',\r\n        pdAuditDetailInfo: {\r\n          id: '',\r\n          pdAuditConc: '',\r\n          pdReturnFuncNode: [],\r\n          pdReleContent: '',\r\n          pdAuditIdea: ''\r\n        },\r\n        showComboAuditNumberFlag: false,\r\n        showComboAuditNumberDataFlag: false,\r\n        showAuditType:false,\r\n        showAuditType1:false,\r\n        showAuditContent:false,\r\n        comboOptionList: [],\r\n        params: {\r\n          ppCode: '',\r\n          comboAuditStartDate: undefined,\r\n          comboAuditEndDate: undefined\r\n        },\r\n        numberParams: {\r\n          pdInfoId: ''\r\n        },\r\n        //审核日期\r\n        auditDateValue: [],\r\n        pickerOptions: {\r\n          shortcuts: [{\r\n            text: '最近一周',\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 7);\r\n              picker.$emit('pick', [start, end]);\r\n            }\r\n          }, {\r\n            text: '最近一个月',\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 30);\r\n              picker.$emit('pick', [start, end]);\r\n            }\r\n          }, {\r\n            text: '最近三个月',\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 90);\r\n              picker.$emit('pick', [start, end]);\r\n            }\r\n          }]\r\n        },\r\n        showpdReleType: false,\r\n        showpostContent: false\r\n      }\r\n    },\r\n    computed: {},\r\n    created() {\r\n      this.comboAuditRecordsList();\r\n    },\r\n    mounted() {\r\n    },\r\n    watch: {\r\n      auditDateValue(val) {\r\n        if (val) {\r\n          this.params.comboAuditStartDate = val[0];\r\n          this.params.comboAuditEndDate = val[1];\r\n        } else {\r\n          this.params.comboAuditStartDate = '';\r\n          this.params.comboAuditEndDate = ''\r\n        }\r\n      },\r\n    },\r\n    methods: {\r\n      //表格搜索\r\n      queryTableData() {\r\n        if (this.$refs.page) {\r\n          this.$refs.page.reload();\r\n          this.showComboAuditNumberFlag = false;\r\n          this.showComboAuditNumberDataFlag = false;\r\n        }\r\n      },\r\n      comboAuditRecordsList() {\r\n        let params = {};\r\n        auditRecordsList(this.buildRequestData(params)).then(res => {\r\n          this.auditRecordStatisticalAllData = res.data;\r\n          this.comboOptionList = [];\r\n          for (const comboOption of  this.auditRecordStatisticalAllData) {\r\n            let flag = false;\r\n            for (const audit of this.comboOptionList) {\r\n              if (audit.ppCode === comboOption.ppCode) {\r\n                flag = true;\r\n                break\r\n              }\r\n            }\r\n            if (!flag) {\r\n              this.comboOptionList.push(comboOption)\r\n            }\r\n          }\r\n        })\r\n      },\r\n      findAuditNumberData() {\r\n        auditNumberList(this.buildRequestData(this.numberParams)).then(res => {\r\n          this.auditNumberTable = res.data\r\n          console.log(this.auditNumberTable);\r\n        })\r\n      },\r\n      //审核明细数据\r\n      async auditDetailsData(row) {\r\n        await queryComboAuditInfo(this.buildRequestData({\"pdAuditInfoId\": row.id})).then(res => {\r\n          this.pdAuditDetailInfo = res.data\r\n        });\r\n      },\r\n      //审核明细表格数据\r\n      async auditDetailsNoteData(row) {\r\n        await queryAuditNotesList(this.buildRequestData({\"pdAuditNotesInfoId\": row.id})).then(res => {\r\n          this.comboDefineData = res.data.comboDefineData;\r\n          this.ruleDataTable = res.data.ruleDataTable;\r\n          this.formulaDataTable = res.data.formulaDataTable\r\n        });\r\n      },\r\n      //选择表格数据\r\n      c