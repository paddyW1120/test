tatus=='1'?'el-icon-circle-check': 'el-icon-circle-close'\"\r\n                         :status=\"item2.moduleStatus=='1'? 'finish' : 'wait'\" :title=\"item2.moduleName\"></el-step>\r\n              </el-steps>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </van-tab>\r\n    </van-tabs>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\n\r\nimport { getStatus } from '@/api/basicinfo/pdtemppdmodulestatus'\r\n\r\nimport Vue from 'vue'\r\nimport { NavBar, CellGroup, Cell, Row, Col, Tab, Tabs } from 'vant'\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\nVue.use(Col)\r\nVue.use(Row)\r\nVue.use(CellGroup)\r\nVue.use(Cell)\r\nVue.use(NavBar)\r\nVue.use(Tab)\r\nVue.use(Tabs)\r\n\r\nexport default {\r\n  name: 'phoneProcess',\r\n  props: {},\r\n  data() {\r\n    return {\r\n      pdProcessId: '',\r\n      moduleNode: 'pdInfo',\r\n      processShow: 'pdInfo',\r\n      statusData: {},\r\n      form: {\r\n        referenceProducts: []\r\n      },\r\n      pdform: {\r\n        pdCode: '',\r\n        pdEdition: '',\r\n        pdName: ''\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    let pdtempPdInfo = this.getJsonData('phone-pdInfo')\r\n    if (pdtempPdInfo) {\r\n      this.pdProcessId = pdtempPdInfo.pdProcessId\r\n      this.pdform.pdCode = pdtempPdInfo.pdCode\r\n      this.pdform.pdEdition = pdtempPdInfo.pdEdition\r\n      this.pdform.pdName = pdtempPdInfo.pdName\r\n    }\r\n    this.checkProcess(this.moduleNode)\r\n  },\r\n  mounted() {\r\n\r\n  },\r\n  methods: {\r\n    onClick(name) {\r\n      this.checkProcess(name)\r\n    },\r\n    onClickLeft() {\r\n      this.$router.go(-1)\r\n    },\r\n    checkProcess(type) {\r\n      this.moduleNode = type\r\n      getStatus(this.buildRequestData({ 'pdProcessId': this.pdProcessId, 'moduleNode': this.moduleNode })).then(res => {\r\n        this.statusData = res.data[0]\r\n      })\r\n    }\r\n  },\r\n  destroyed() {\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.wraper {\r\n  position: unset;\r\n}