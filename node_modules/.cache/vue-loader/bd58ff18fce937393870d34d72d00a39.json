r\n          <cus-btn-wrapper>\r\n            <el-button  size=\"mini\"\r\n                v-waves\r\n                type=\"primary\"\r\n                @click=\"handleSearch\"\r\n                class=\"filter-button\"\r\n                icon=\"el-icon-search\"\r\n              >{{$t('ruleCommand.search') }}</el-button>\r\n              <el-button  size=\"mini\"\r\n                v-waves\r\n                type=\"primary\"\r\n                v-auth=\"'rule-command:add'\"\r\n                @click=\"handleCreate\"\r\n                icon=\"el-icon-plus\"\r\n                class=\"filter-button\"\r\n              >\r\n                {{\r\n                $t('ruleCommand.add') }}\r\n              </el-button>\r\n          </cus-btn-wrapper>\r\n        </el-row>\r\n        <el-row  :gutter=\"10\">\r\n          <cus-col :span=\"6\">\r\n            <el-form-item :label=\"$t('ruleCommand.valid')\" prop=\"valid\">\r\n              <el-switch active-value=\"1\" inactive-value=\"0\" v-model=\"params.eq_valid\"></el-switch>\r\n            </el-form-item>\r\n          </cus-col>\r\n        </el-row>\r\n      </el-form>\r\n    </cus-filter-wraper>\r\n    <cus-table-title :label=\"$t('ruleCommand.tableName')\"></cus-table-title>\r\n    <el-table\r\n      :data=\"configs\"\r\n      size=\"mini\"\r\n      fit\r\n      element-loading-text=\"Loading\"\r\n      border\r\n      highlight-current-row\r\n      align=\"center\"\r\n      class=\"wraper-table\"\r\n      ref=\"page\"\r\n    >\r\n      <el-table-column :label=\"$t('table.id')\"  align=\"center\" sortable type=\"index\"  width=\"60px\">\r\n             <template v-slot=\"scope\">\r\n                     {{ initTableIndex('page', scope.$index) }}\r\n               </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleCommand.operatorName')\"\r\n        width=\"150\"\r\n        prop=\"name\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column :label=\"$t('ruleCommand.display')\" width=\"150\" prop=\"display\" align=\"center\"></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleCommand.implenmation')\"\r\n        width=\"150\"\r\n        prop=\"implenmation\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleCommand.commandType')\"\r\n        :formatter=\"commandTypeFormat\"\r\n        prop=\"commandType\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column :label=\"$t('ruleCommand.paraNum')\" prop=\"paraNum\" align=\"center\"></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleCommand.paraType')\"\r\n        :formatter=\"paraTypeFormat\"\r\n        prop=\"paraType\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleCommand.resultType')\"\r\n        :formatter=\"resultTypeFormat\"\r\n        prop=\"resultType\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleCommand.commType')\"\r\n        :formatter=\"commTypeFormat\"\r\n        prop=\"commType\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleCommand.commDetail')\"\r\n        width=\"200\"\r\n        prop=\"commDetail\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('ruleCommand.valid')\"\r\n        :formatter=\"validFormat\"\r\n        prop=\"valid\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('table.actions')\" fixed=\"right\" width=\"330\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"handleUpdate(scope.row)\"\r\n            v-auth=\"'rule-command:update'\"\r\n            class=\"filter-button\"\r\n            icon=\"el-icon-edit\"\r\n          ></el-button>\r\n          <cus-del-btn\r\n            v-auth=\"'rule-command:delete'\"\r\n            @ok=\"handleDelete(scope.row)\"\r\n            style=\"margin-left: 5px;\"\r\n          />\r\n\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"primary\"\r\n            @click=\"ruleMessage(scope.row)\"\r\n            style=\"margin-left: 5px;\"\r\n          >\r\n            <svg-icon icon-class=\"multilingual\" />\r\n            {{$t('ruleCommand.ruleMessage')}}\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <cus-pagination\r\n      ref=\"page\"\r\n      url=\"/rule-command/list\"\r\n      :pageSize=\"10\"\r\n      :params=\"params\"\r\n      v-model=\"configs\"\r\n    />\r\n\r\n    <!---新增修改dialog---->\r\n\r\n    <el-dialog :visible.sync=\"dialogVisible\" @close=\"dialogVisible=false\" v-dialog-drag width=\"60%\">\r\n      <cus-page-title-back @backTo=\"goBack\" :isDialog=\"true\" :label=\"titleMap[dialogStatus]\"/>\r\n          <el-form\r\n            ref=\"dataForm\"\r\n            :model=\"form\"\r\n            :rules=\"rules\"\r\n            label-position=\"right\"\r\n            label-width=\"150px\"\r\n          >\r\n            <el-row :gutter=\"10\">\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleCommand.name')+':'\" prop=\"name\">\r\n                  <el-input\r\n                    v-model=\"form.name\"\r\n                    :placeholder=\"$t('ruleCommand.name')\"\r\n                    maxlength=\"20\"\r\n                    show-word-limit\r\n                  ></el-input>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleCommand.display')+':'\" prop=\"display\">\r\n                  <el-input\r\n                    v-model=\"form.display\"\r\n                    :placeholder=\"$t('ruleCommand.display')\"\r\n                    maxlength=\"100\"\r\n                    show-word-limit\r\n                  ></el-input>\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n            <el-row :gutter=\"10\">\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleCommand.implenmation')+':'\" prop=\"implenmation\">\r\n                  <el-input\r\n                    v-model=\"form.implenmation\"\r\n                    :placeholder=\"$t('ruleCommand.implenmation')\"\r\n                    maxlength=\"100\"\r\n                    show-word-limit\r\n                  ></el-input>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleCommand.commandType')+':'\" prop=\"commandType\">\r\n                  <cus-select :codetype=\"'ibrmscommandtype'\" placeholder v-model=\"form.commandType\"></cus-select>\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n            <el-row :gutter=\"10\">\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleCommand.paraNum')+':'\" prop=\"paraNum\">\r\n                  <el-input\r\n                    type=\"number\"\r\n                    v-model=\"form.paraNum\"\r\n                    :placeholder=\"$t('ruleCommand.paraNum')\"\r\n                  ></el-input>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleCommand.paraType')+':'\" prop=\"paraType\">\r\n                  <cus-select :codetype=\"'ibrmsparatype'\" placeholder v-model=\"form.paraType\"></cus-select>\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n            <el-row :gutter=\"10\">\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleCommand.resultType')+':'\" prop=\"resultType\">\r\n                  <cus-select :codetype=\"'ibrmsresulttype'\" placeholder v-model=\"form.resultType\"></cus-select>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleCommand.commType')+':'\" prop=\"commType\">\r\n                  <cus-select\r\n                    :codetype=\"'ibrmscommtype'\"\r\n                    placeholder\r\n                    v-model=\"form.commType\"\r\n                    :disabled=\"'disabled'\"\r\n                  ></cus-select>\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n            <el-row :gutter=\"10\">\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleCommand.commDetail')+':'\" prop=\"commDetail\">\r\n                  <el-input\r\n                    v-model=\"form.commDetail\"\r\n                    :placeholder=\"$t('ruleCommand.commDetail')\"\r\n                    maxlength=\"300\"\r\n                    show-word-limit\r\n                  ></el-input>\r\n                </el-form-item>\r\n              </cus-col>\r\n              <cus-col :span=\"10\">\r\n                <el-form-item :label=\"$t('ruleCommand.valid')+':'\" prop=\"valid\">\r\n                  <el-switch active-value=\"1\" inactive-value=\"0\" v-model=\"form.valid\"></el-switch>\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n            <el-row :gutter=\"10\">\r\n              <cus-col :span=\"20\">\r\n                <el-form-item :label=\"$t('ruleCommand.description')+':'\" prop=\"description\">\r\n                  <el-input\r\n                    :placeholder=\"$t('ruleCommand.description') \"\r\n                    :rows=\"5\"\r\n                    type=\"textarea\"\r\n                    v-model=\"form.discription\"\r\n                    maxlength=\"200\"\r\n                    show-word-limit\r\n                  />\r\n                </el-form-item>\r\n              </cus-col>\r\n            </el-row>\r\n          </el-form>\r\n          <span slot=\"footer\" class=\"dialog-footer\">\r\n            <el-button  size=\"mini\" type=\"primary\" v-auth=\"'rule-command:add'\" @click=\"submitForm\">\r\n              <i class=\"el-icon-success el-icon--left\"></i>\r\n              {{ $t('table.save') }}\r\n            </el-button>\r\n            <el-button  size=\"mini\" @click=\"resetForm('dataForm')\" type=\"warning\">\r\n              <i class=\"el-icon-refresh el-icon--left\"></i>\r\n              {{$t('table.refresh')}}\r\n            </el-button>\r\n          </span>\r\n    </el-dialog>\r\n    <el-dialog :visible.sync=\"dialogVisible_msg\" @close=\"dialogVisible_msg=false\"  v-dialog-drag  width=\"80%\">\r\n      <cus-page-title-back @backTo=\"goBackInfo_msg\" :label=\"$t('ruleCommand.ruleMessage')\" :isDialog=\"true\"></cus-page-title-back>\r\n      <ruleMessageInfoBom :keyId.sync=\"keyId\" :msgType.sync=\"msgType\"></ruleMessageInfoBom>\r\n    </el-dialog>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\nimport {\r\n  addRuleCommand,\r\n  deleteRuleCommand,\r\n  updateRuleCommand,\r\n  findRuleCommandByName\r\n} from \"@/api/ibrms/ruleCommand\";\r\nimport { mapGetters } from \"vuex\";\r\nimport ruleMessageInfoBom from \"@/views/ibrms/objectManagement/ruleMessageInfoBom\";\r\nexport default {\r\n  name: \"ruleCommand\",\r\n  data() {\r\n    const isNum = (rule, value, callback) => {\r\n      const bomLevel = /^[0-9]*$/;\r\n      if (!bomLevel.test(value)) {\r\n        callback(new Error(\"请输入数字\"));\r\n      } else {\r\n        callback();\r\n      }\r\n    };\r\n    return {\r\n      //表格数据\r\n      configs: [],\r\n      computed: {\r\n        ...mapGetters([\"ldcodes\"])\r\n      },\r\n      //查询参数\r\n      params: {\r\n        like_name: \"\",\r\n        eq_commandType: \"\",\r\n        eq_commType: \"0\",\r\n        eq_valid: \"1\"\r\n      },\r\n      form: {\r\n        //表单数据与后端实体类对应\r\n        id: \"\", //主键ID\r\n        name: \"\",\r\n        commandType: \"\",\r\n        commType: \"1\",\r\n        valid: \"1\",\r\n        functionType: \"0\",\r\n        paraType:\"\",\r\n        resultType:\"\"\r\n      },\r\n      radio: false,\r\n      commandId: \"\",\r\n      commandName: \"\",\r\n      keyId: \"\",\r\n      msgType: \"\",\r\n      dialogVisible_msg: false,\r\n      //检验规则\r\n      rules: {\r\n        paraNum: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleCommand.paraNumMessage\"),\r\n            trigger: \"blur\"\r\n          },\r\n          {\r\n            validator: isNum,\r\n            message: this.$t(\"ruleCommand.paraNumMessage1\"),\r\n            trigger: \"blur\"\r\n          }\r\n        ],\r\n        name: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleCommand.nameMessage\"),\r\n            trigger: \"blur\"\r\n          }\r\n        ],\r\n        implenmation: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleCommand.implenmationMessage\"),\r\n            trigger: \"blur\"\r\n          }\r\n        ],\r\n        display: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleCommand.displayMessage\"),\r\n            trigger: \"blur\"\r\n          }\r\n        ],\r\n        commandType: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleCommand.commandTypeMessage\"),\r\n            trigger: \"change\"\r\n          }\r\n        ],\r\n        paraType: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleCommand.paraTypeMessage\"),\r\n            trigger: \"change\"\r\n          }\r\n        ],\r\n        resultType: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"ruleCommand.resultTypeMessage\"),\r\n           