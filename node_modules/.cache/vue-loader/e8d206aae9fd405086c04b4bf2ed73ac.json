AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"index.vue","sourceRoot":"src/views/commodity-statistic/package-statistic","sourcesContent":["<template>\r\n  <cus-wraper>\r\n    <cus-filter-wraper>\r\n      <cus-page-title :label=\"$t('route.comboOverview')\"></cus-page-title>\r\n      <el-form ref=\"params\" label-width=\"auto\" :model=\"params\" >\r\n        <el-row :gutter=\"10\">\r\n          <!-- 套餐类型下拉选 -->\r\n          <el-col :span=\"8\">\r\n            <el-form-item :label=\"$t('i18nView.ppType')\" prop=\"ComboType\">\r\n              <cus-select :codetype=\"'ppType'\" v-model=\"params.comboType\"></cus-select>\r\n            </el-form-item>\r\n          </el-col>\r\n          <!-- 套餐形态下拉选 -->\r\n          <el-col :span=\"8\">\r\n            <el-form-item :label=\"$t('i18nView.comboType')\" prop=\"ComboForm\">\r\n              <cus-select :codetype=\"'packageType'\" v-model=\"params.comboForm\"></cus-select>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row :gutter=\"10\" >\r\n          <!-- 套餐定义申请日期选项 -->\r\n          <el-col :span=\"8\">\r\n            <el-form-item :label=\"$t('comboOverview.comboMealApplyDate')\">\r\n              <el-date-picker\r\n                value-format=\"yyyy-MM-dd\"\r\n                v-model=\"dateValue\" type=\"daterange\" align=\"right\" unlink-panels\r\n                :range-separator=\"$t('comboOverview.to')\"\r\n                :start-placeholder=\"$t('comboOverview.comboApplyStartDate')\"\r\n                :end-placeholder=\"$t('comboOverview.comboApplyEndDate')\"\r\n                :picker-options=\"pickerOptions\">\r\n              </el-date-picker>\r\n            </el-form-item>\r\n          </el-col>\r\n          <cus-btn-wrapper>\r\n            <el-button  size=\"small\" @click=\"showChart\" icon=\"el-icon-search\" type=\"primary\">\r\n              {{ $t('pdPackageSaleDefinition.search') }}\r\n            </el-button>\r\n          </cus-btn-wrapper>\r\n        </el-row>\r\n      </el-form>\r\n    </cus-filter-wraper>\r\n    <el-row :gutter=\"32\">\r\n      <el-col :xs=\"24\" :sm=\"24\" :lg=\"12\">\r\n        <div class=\"chart-wrapper\">\r\n          <bar-chart :title=\"title1\" v-if=\"chart1Show\" :id=\"chart1\" :callBack=\"clickThisChart\"\r\n                     :style=\"{height:'450px',width:'200%'}\" :xData=\"chart1xData\" :yData=\"chart1yData\"/>\r\n        </div>\r\n      </el-col>\r\n    </el-row>\r\n    <!-- 点击页面所有图表中的x坐标可弹窗显示对应柱状图 -->\r\n    <el-dialog :close-on-click-modal=\"false\" :visible.sync=\"showThisDialog1\" center v-if=\"showThisDialog1\"\r\n               v-dialog-drag width=\"1200px\">\r\n      <el-row :gutter=\"32\">\r\n        <el-col :xs=\"24\" :sm=\"24\" :lg=\"12\">\r\n          <div class=\"chart-wrapper\">\r\n            <bar-chart :title=\"title2\" v-if=\"chart2Show\" :id=\"chart2\" :style=\"{height:'450px',width:'200%'}\"\r\n                       :callBack=\"clickThisChart2\" :xData=\"chart2xData\" :yData=\"chart2yData\"/>\r\n          </div>\r\n        </el-col>\r\n      </el-row>\r\n    </el-dialog>\r\n    <!-- 点击页面所有图表中的柱状图都可弹窗显示对应表格 -->\r\n    <el-dialog :close-on-click-modal=\"false\" :visible.sync=\"showThisDialog2\" center v-if=\"showThisDialog2\"\r\n               v-dialog-drag width=\"1200px\">\r\n      <el-table :data=\"configs\" border element-loading-text=\"Loading\" fit size=\"mini\">\r\n        <el-table-column :label=\"$t('table.id')\"  align=\"center\" width=\"50\">\r\n          <template v-slot=\"scope\">\r\n            {{ initTableIndex('dataPage', scope.$index) }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.ppCode')\" prop=\"ppCode\"\r\n                         align=\"center\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.ppName')\" prop=\"ppName\"\r\n                         align=\"center\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.comboType')\" prop=\"defType\"\r\n                         align=\"center\">\r\n          <template v-slot=\"scope\">{{ filterHandler('packageType', scope.row.defType) }}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.comboMealApplyDate')\" prop=\"applyTime\"\r\n                         align=\"center\"></el-table-column>\r\n      </el-table>\r\n      <!-- 分页 -->\r\n      <cus-pagination ref=\"dataPage\" :url=\"url\" :pageSize=\"10\" :params=\"params\" v-model=\"configs\"/>\r\n    </el-dialog>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\n  import {\r\n    comboOverviewIndexData, fwData, opData, testData, release1Data, release2Data\r\n  } from '@/api/basicinfo/comboOverview'\r\n  import BarChart from '@/views/dashboard/admin/components/BarChart'\r\n\r\n  export default {\r\n    name: 'comboOverview',\r\n    components: {\r\n      BarChart\r\n    },\r\n    data() {\r\n      return {\r\n        barData: {},\r\n        title1: '套餐概况总览',\r\n        title2: '',\r\n        configs: [],\r\n        chart1Show: false,\r\n        chart1xData: [],\r\n        chart1yData: [],\r\n        chart1: null,\r\n        showThisDialog1: false,\r\n        showThisDialog2: false,\r\n        chart2Show: false,\r\n        chart2: null,\r\n        chart2xData: [],\r\n        chart2yData: [],\r\n        dateValue: [],\r\n        pickerOptions: {\r\n          shortcuts: [{\r\n            text: '最近一周',\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 7);\r\n              picker.$emit('pick', [start, end]);\r\n            }\r\n          }, {\r\n            text: '最近一个月',\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 30);\r\n              picker.$emit('pick', [start, end]);\r\n            }\r\n          }, {\r\n            text: '最近三个月',\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 90);\r\n              picker.$emit('pick', [start, end]);\r\n            }\r\n          }]\r\n        },\r\n        params: {\r\n          comboType: '',\r\n          comboForm: '',\r\n          comboApplyStartDate: '',\r\n          comboApplyEndDate: '',\r\n          pageName:''\r\n        },\r\n        //检验规则\r\n        rules: {}\r\n      }\r\n    },\r\n    watch: {\r\n      dateValue(val) {\r\n        if (val) {\r\n          this.params.comboApplyStartDate = val[0];\r\n          this.params.comboApplyEndDate = val[1]\r\n        } else {\r\n          this.params.comboApplyStartDate = '';\r\n          this.params.comboApplyEndDate = ''\r\n        }\r\n      }\r\n    },\r\n    computed: {},\r\n    created() {\r\n      this.showChart()\r\n    },\r\n    methods: {\r\n      filterHandler(codetype, value) {\r\n        return this.getCodeName(codetype, value);\r\n      },\r\n      dataReload() {\r\n        if (this.$refs.dataPage) {\r\n          this.$refs.dataPage.reload();\r\n        }\r\n      },\r\n      showChart() {\r\n        comboOverviewIndexData(this.buildRequestData(this.params)).then(res => {\r\n          let data = res.data;\r\n          this.barData = data.barData;\r\n          this.chart1xData = data.barData.xData;\r\n          this.chart1yData = data.barData.yData;\r\n          this.taskCount = this.chart1yData.reduce(function (prev, curr, idx, arr) {\r\n            return prev + curr;\r\n          });\r\n          this.initChart()\r\n        })\r\n      },\r\n      initChart() {\r\n        this.chart1 = 'chart1';\r\n        this.chart1Show = true;\r\n        this.chart2 = 'chart2';\r\n        this.chart2Show = true\r\n      },\r\n      //点击表格显示相对应的数据\r\n      clickThisChart(params) {\r\n        this.title2 = params.value + '概况';\r\n        if (params.componentType == \"xAxis\") {\r\n          if (params.value == \"套餐框架定义\") {\r\n            fwData(this.buildRequestData(this.params)).then(res => {\r\n              let data = res.data;\r\n              this.barData = data.barData;\r\n              this.chart2xData = data.barData.xData;\r\n              this.chart2yData = data.barData.yData\r\n            });\r\n            this.showThisDialog1 = true;\r\n          }\r\n          if (params.value == \"套餐营运定义\") {\r\n            opData(this.buildRequestData(this.params)).then(res => {\r\n              let data = res.data;\r\n              this.barData = data.barData;\r\n              this.chart2xData = data.barData.xData;\r\n              this.chart2yData = data.barData.yData\r\n            })\r\n            this.showThisDialog1 = true;\r\n          }\r\n          if (params.value == \"套餐测试\") {\r\n            testData(this.buildRequestData(this.params)).then(res => {\r\n              let data = res.data;\r\n              this.barData = data.barData;\r\n              this.chart2xData = data.barData.xData;\r\n              this.chart2yData = data.barData.yData\r\n            });\r\n            this.showThisDialog1 = true;\r\n          }\r\n          if (params.value == \"套餐审核\") {\r\n            this.messageError(\"无分支信息提供！\")\r\n          }\r\n          if (params.value == \"待发布\") {\r\n            release1Data(this.buildRequestData(this.params)).then(res => {\r\n              let data = res.data;\r\n              this.barData = data.barData;\r\n              this.chart2xData = data.barData.xData;\r\n              this.chart2yData = data.barData.yData\r\n            });\r\n            this.showThisDialog1 = true;\r\n          }\r\n          if (params.value == \"已发布\") {\r\n            release2Data(this.buildRequestData(this.params)).then(res => {\r\n              let data = res.data;\r\n              this.barData = data.barData;\r\n              this.chart2xData = data.barData.xData;\r\n              this.chart2yData = data.barData.yData\r\n            });\r\n            this.showThisDialog1 = true;\r\n          }\r\n          if (params.value == \"已停售\") {\r\n            this.messageError(\"无分支信息提供！\")\r\n          }\r\n        } else {\r\n          if (params == \"套餐框架定义\") {\r\n            this.url = '/comboOverview/base/comboFrameworkList';\r\n            this.dataReload();\r\n          }\r\n          if (params == \"套餐营运定义\") {\r\n            this.url = '/comboOverview/base/comboOperationalList';\r\n            this.dataReload();\r\n          }\r\n          if (params == \"套餐测试\") {\r\n            this.url = '/comboOverview/base/comboTestList';\r\n            this.dataReload();\r\n          }\r\n          if (params == \"套餐审核\") {\r\n            this.url = '/comboOverview/base/comboAuditList';\r\n            this.dataReload();\r\n          }\r\n          if (params == \"待发布\") {\r\n            this.url = '/comboOverview/base/comboReleaseList1';\r\n            this.dataReload();\r\n          }\r\n          if (params == \"已发布\") {\r\n            this.url = '/comboOverview/base/comboReleaseList2';\r\n            this.dataReload();\r\n          }\r\n          if (params == \"套餐停售\") {\r\n            this.url = '/comboOverview/base/comboDiscontinuationList';\r\n            this.dataReload();\r\n          }\r\n          this.showThisDialog2 = true;\r\n        }\r\n      },\r\n      clickThisChart2(val){\r\n        if (val.componentType == \"xAxis\") {\r\n            return\r\n        }\r\n        else{\r\n        this.params.pageName=val;\r\n        this.url = '/comboOverview/base/secondPagePackageDataList';\r\n        this.showThisDialog2 = true\r\n        }\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n</style>\r\n\r\n"]}]}