" align=\"center\" type=\"index\" width=\"50\"></el-table-column>\r\n      <el-table-column :label=\"$t('table.detail')\" align=\"center\" style=\"width: 50%\" type=\"expand\">\r\n        <template slot-scope=\"props\">\r\n          <el-form class=\"demo-table-expand\" inline label-position=\"left\">\r\n            <el-form-item :label=\"$t('pdtempProgInfo.medBillType')\" style=\"width: 50%\">\r\n              {{ props.row.pdMedBillType | filter_medBillType }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('pdtempProgInfo.medFeeBillCode')\" style=\"width: 50%\">\r\n              {{ props.row.pdMedFeeCode }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('pdtempProgInfo.medFeeBillType')\" style=\"width: 50%\">\r\n              {{ props.row.medFeeBillName }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('pdtempProgInfo.medFeeBillName')\" style=\"width: 50%\">\r\n              {{ props.row.pdMedFeeName }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.hospitalType')\" style=\"width: 50%\">\r\n              {{ props.row.pdMedHospType | filter_pdMedHospType }}\r\n            </el-form-item>\r\n          </el-form>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.medBillType')\" align=\"center\"\r\n                       prop=\"pdMedBillType\">\r\n        <template v-slot=\"scope\">{{ scope.row.pdMedBillType | filter_medBillType }}</template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.pdMedBillCode')\" align=\"center\"\r\n                       prop=\"pdMedFeeCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.feeType')\" align=\"center\"\r\n                       prop=\"medFeeBillName\"></el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.feeName')\" align=\"center\"\r\n                       prop=\"pdMedFeeName\"></el-table-column>\r\n    </el-table>\r\n    <cus-pagination ref=\"page\" v-model=\"bnftConfigs\" :pageSize=\"5\"\r\n                    :params=\"paymentForm\" url=\"/pdtemp-pd-claim-verification-rule/base/selectPdMedFeeBill\"/>\r\n    <br/>\r\n    <el-form>\r\n      <el-form-item style=\"text-align: right\">\r\n        <el-button dialog-disable=\"disable\" v-auth=\"'select:AllPdAccount'\" icon=\"el-icon-circle-plus-outline\" size=\"small\" type=\"primary\"\r\n                   @click=\"addPdProg\">\r\n          {{$t('i18nView.add')}}\r\n        </el-button>\r\n        <el-button dialog-disable=\"disable\" icon=\"el-icon-refresh\" size=\"small\" type=\"primary\" @click=\"researchData\">\r\n          {{$t('table.refresh')}}\r\n        </el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n    <cus-table-title :label=\"$t('i18nView.AssociatedPlanFee')\"/>\r\n    <el-table ref=\"paymentTable\" :data=\"paymentConfigs\" border element-loading-text=\"Loading\" fit highlight-current-row\r\n              max-height=\"300px\" size=\"mini\">\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\" width=\"50\"></el-table-column>\r\n      <el-table-column :label=\"$t('table.detail')\" align=\"center\" style=\"width: 50%\" type=\"expand\">\r\n        <template slot-scope=\"props\">\r\n          <el-form class=\"demo-table-expand\" inline label-position=\"left\">\r\n            <el-form-item :label=\"$t('pdtempProgInfo.medBillType')\" style=\"width: 50%\">\r\n              {{ props.row.pdMedBillType | filter_medBillType }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('pdtempProgInfo.medFeeBillCode')\" style=\"width: 50%\">\r\n              {{ props.row.pdMedFeeCode }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('pdtempProgInfo.medFeeBillType')\" style=\"width: 50%\">\r\n              {{ props.row.medFeeBillName }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('pdtempProgInfo.medFeeBillName')\" style=\"width: 50%\">\r\n              {{ props.row.pdMedFeeName }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.hospitalType')\" style=\"width: 50%\">\r\n              {{ props.row.pdMedHospType | filter_pdMedHospType }}\r\n            </el-form-item>\r\n          </el-form>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.medBillType')\" align=\"center\"\r\n                       prop=\"pdMedBillType\">\r\n        <template v-slot=\"scope\">{{ scope.row.pdMedBillType | filter_medBillType }}</template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.pdMedBillCode')\" align=\"center\"\r\n                       prop=\"pdMedFeeCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.feeType')\" align=\"center\"\r\n                       prop=\"medFeeBillName\">\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.feeName')\" align=\"center\"\r\n                       prop=\"pdMedFeeName\"></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('table.actions')\" width=\"100\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button dialog-disable=\"disable\" size=\"mini\" type=\"danger\" @click=\"deleteRela(scope.row)\">-</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\nimport {findProductsByProductType} from \"@/api/basicinfo/pdtemppdclmbnft\";\r\nimport {findBusinessItemByCtrlCode} from \"@/api/basicinfo/productCompensation\"\r\n\r\nlet _this\r\n\r\nexport default {\r\n  filters: {\r\n    filter_medBillType(code) {\r\n      return _this.getCodeName('medBillType', code)\r\n    },\r\n    filter_pdMedHospType(code) {\r\n      return _this.getCodeName('pdMedHospType', code)\r\n    }\r\n  },\r\n  name: \"pdtemppdbnftinfo\",\r\n  props: {\r\n    pdBnftCtrlCode: {\r\n      type: String,\r\n      default: ''\r\n    }\r\n  },\r\n  data() {\r\n    _this = this\r\n    return {\r\n      pdtempPdInfo: {},\r\n      paymentForm: {\r\n        productTypeDetail: '',\r\n        product: '',\r\n        pdBnftDetailType: '',\r\n      },\r\n      paymentConfigs: [],\r\n      bnftConfigs: [],\r\n      productOptions: [],\r\n      chooseBnft: [],\r\n      chooseProg: [],\r\n       rules:{\r\n        product: [\r\n          {required: true, message: \"请输入产品\", trigger: \"change\"}\r\n        ],\r\n       }\r\n    }\r\n  },\r\n  created() {\r\n    this.pdtempPdInfo = this.getPdData();\r\n    this.initPdMedBill();\r\n  },\r\n  methods: {\r\n    //初始化关联账单信息\r\n    initPdMedBill() {\r\n      if (this.pdBnftCtrlCode) {\r\n        findBusinessItemByCtrlCode(this.pdBnftCtrlCode).then(response => {\r\n          this.paymentConfigs = response.data\r\n        })\r\n      }\r\n    },\r\n    //保存产品方案\r\n    addPdProg() {\r\n      for (const item of this.chooseProg) {\r\n        let duplicated = false\r\n        for (const selectedItem of this.paymentConfigs) {\r\n          if (selectedItem.id === item.id) {\r\n            setTimeout(() => {\r\n              this.notifyError(item.pdProgCode + ':' + item.pdProgName + '已添加，请勿重复添加')\r\n            }, 1)\r\n            duplicated = true\r\n            break\r\n          }\r\n        }\r\n        if (!duplicated) {\r\n          this.paymentConfigs.push(item)\r\n        }\r\n      }\r\n    },\r\n    //产品搜索\r\n    selectPdBnft() {\r\n        this.$refs[\"selectForm1\"].validate(valid => {\r\n            if(valid){\r\n              this.$refs.page.reload()\r\n            }\r\n        })\r\n    },\r\n    findProducts(val) {\r\n      let data = {\r\n        pdCls2: val\r\n      }\r\n      findProductsByProductType(this.buildRequestData(data)).then(res => {\r\n        this.productOptions = [];\r\n        res.data.forEach(product => {\r\n          this.productOptions.push({\r\n            value: product.pdCode,\r\n            label: product.pdCode + \":\" + product.pdName,\r\n            key: product.id\r\n          });\r\n        });\r\n      })\r\n    },\r\n    researchData() {\r\n      this.paymentForm.productTypeDetail = ''\r\n      this.paymentForm.product = ''\r\n      this.bnftConfigs = []\r\n      this.$refs.page.reload()\r\n    },\r\n    deleteRela(row) {\r\n      this.paymentConfigs.splice(this.paymentConfigs.indexOf(row), 1)\r\n    },\r\n    handleSelectionChange(row) {\r\n      this.chooseProg = row\r\n    },\r\n    backToMenu() {\r\n      this.$emit('backToFeeBill', 'pdmedfeebill', this.paymentConfigs)\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.divider {\r\n  color: black;\r\n  background-color: black;\r\n  width: 1px;\r\n}\r\n</style>\r\n"]}]}