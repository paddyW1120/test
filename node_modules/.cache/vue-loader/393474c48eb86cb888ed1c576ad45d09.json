bel\" :value=\"item.value\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('rule.pdRuleDtCls')\" prop=\"ruleType\">\r\n            <cus-select v-model=\"productBnftForm.ruleType\" codetype=\"ruleBusinessDetailClass\" filter=\"1$7\"\r\n                        @change=\"handleSearch\"></cus-select>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n    </el-form>\r\n\r\n    <div v-if=\"ruleRevisionShowINfo\">\r\n      <cus-table-title :label=\"$t('premFormula.pdInvItem')\"/>\r\n      <el-table :data=\"tableDataInv\" border element-loading-text=\"Loading\" fit highlight-current-row\r\n                max-height=\"300\" size=\"mini\" @row-click=\"rowClickInv\">\r\n        <el-table-column :label=\"$t('table.select')\" width=\"50\">\r\n          <template slot-scope=\"scope\">\r\n            <el-radio-group v-model=\"selectedInvInRev\" @change=\"selectedInvInRevChanged\">\r\n              <el-radio :label=\"scope.row\">{{ '' }}</el-radio>\r\n            </el-radio-group>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.id')\" align=\"center\" width=\"50\">\r\n          <template v-slot=\"scope\">\r\n            {{ initTableIndex('page_inv', scope.$index) }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('invFormula.pdInvCode')\" align=\"center\" min-width=\"200\" prop=\"pdInvCode\"\r\n                         sortable></el-table-column>\r\n        <el-table-column :label=\"$t('invFormula.pdInvName')\" align=\"center\" min-width=\"200\"\r\n                         prop=\"pdInvName\"></el-table-column>\r\n        <el-table-column :label=\"$t('invFormula.pdInvType')\" align=\"center\" min-width=\"200\"\r\n                         prop=\"pdInvType\">\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdInvType | filter_pdInvType }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('invFormula.pdInvStartPeriod')\" align=\"center\" min-width=\"200\"\r\n                         prop=\"pdInvStartPeriod\">\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdInvStartPeriod }}\r\n            <span v-if=\"scope.row.pdInvStartPeriod\">{{\r\n                scope.row.pdInvStartPeriod | filter_pdInvStartPeriodUnit\r\n              }}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('invFormula.pdInvEndPeriod')\" align=\"center\" min-width=\"200\"\r\n                         prop=\"pdInvStartPeriod\">\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdInvEndPeriod }}\r\n            <span v-if=\"scope.row.pdInvEndPeriod\">{{ scope.row.pdInvEndPeriod | filter_pdInvStartPeriodUnit }}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('invFormula.pdInvGuarPeriod')\" align=\"center\" min-width=\"200\"\r\n                         prop=\"pdInvGuarPeriod\">\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdInvGuarPeriod }}\r\n            <span v-if=\"scope.row.pdInvGuarPeriod\">{{\r\n                scope.row.pdInvGuarPeriodUnit | filter_pdInvStartPeriodUnit\r\n              }}</span>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <cus-pagination ref=\"page_inv\" v-model=\"tableDataInv\" :autoBuild=\"false\" :pageSize=\"5\" :params=\"params_inv\"\r\n                      url=\"/invAmountFormul/base/getAllInvByPdInfoId\"/>\r\n    </div>\r\n\r\n    <div v-if=\"!ruleRevisionShowINfo || ruleRevisionShowINfo && Object.keys(selectedInvInRev).length > 0\">\r\n      <!-- 已选择规则 -->\r\n      <cus-form-title :first=\"true\" :label=\"$t('invRule.selectedInvRules')\"></cus-form-title>\r\n      <cus-table-title :label=\"$t('invRule.pdInvRule')\"/>\r\n      <el-table :data=\"tableData_current\" border element-loading-text=\"Loading\" fit highlight-current-row size=\"mini\">\r\n        <el-table-column :label=\"$t('table.select')\" align=\"center\" width=\"80\">\r\n          <template slot-scope=\"scope\">\r\n            <el-radio-group v-model=\"selectedDef\" @change=\"selectedChanged\">\r\n              <el-radio :label=\"scope.row\">{{ '' }}</el-radio>\r\n            </el-radio-group>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.id')\" align=\"center\" width=\"50\">\r\n          <template v-slot=\"scope\">\r\n            {{ initTableIndex('page_current', scope.$index) }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.detail')\" align=\"center\" width=\"100\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button size=\"small\" type=\"text\" @click=\"showRule(scope.row)\">>></el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('invRule.invItem')\" align=\"center\" min-width=\"200\">\r\n          <template slot-scope=\"scope\">\r\n            <span>{{ scope.row.pdInvCode }}<span v-show=\"scope.row.pdInvCode\">:</span>{{ scope.row.invName }}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('invRule.invType')\" align=\"center\" min-width=\"200\" prop=\"invType\">\r\n          <template v-slot=\"scope\">{{ scope.row.invType | filter_getType }}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('rule.pdRuleCode')\" align=\"center\" min-width=\"200\"\r\n                         prop=\"pdRuleCode\"></el-table-column>\r\n        <el-table-column :label=\"$t('rule.pdRuleDtCls')\" align=\"center\" min-width=\"200\">\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdRuleDtCls | filter_pdBussDetailType }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('rule.pdRuleDesc')\" align=\"center\"\r\n                         min-width=\"200\" prop=\"pdRuleDesc\"></el-table-column>\r\n        <el-table-column :label=\"$t('rule.ruleAttr')\" align=\"center\" min-width=\"200\">\r\n          <template v-slot=\"scope\">\r\n            {{\r\n              scope.row.ruleAttr === '1' ? $t('rule.general') : (scope.row.ruleAttr === '2' ? $t('rule.individual') : '')\r\n            }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.actions')\" align=\"center\" fixed=\"right\" width=\"120\">\r\n          <template slot-scope=\"scope\">\r\n            <cus-del-btn v-auth=\"'invRules:delete'\" icon=\"el-icon-delete-solid\" size=\"mini\" type=\"danger\"\r\n                         @ok=\"delRuleRow(scope.row)\"></cus-del-btn>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <cus-pagination ref=\"page_current\" v-model=\"tableData_current\" :autoBuild=\"false\" :pageSize=\"5\"\r\n                      :params=\"params_current\" url=\"/invRules/base/currentRule\"/>\r\n\r\n      <!-- 可选择规则 -->\r\n      <cus-form-title :first=\"true\" :label=\"$t('invRule.selectableInvRules')\"></cus-form-title>\r\n\r\n      <el-row style=\"text-align: right\">\r\n        <el-button icon=\"el-icon-connection\" size=\"small\" type=\"primary\" @click=\"toRuleSelect\">\r\n          {{ $t('rule.moreRule') }}\r\n        </el-button>\r\n        <el-button icon=\"el-icon-circle-plus el-icon--left\" size=\"small\" type=\"primary\" @click=\"toAddRule\">\r\n          {{ $t('rule.toAddRule') }}\r\n        </el-button>\r\n      </el-row>\r\n\r\n      <cus-table-title :label=\"$t('invRule.invRule')\"/>\r\n      <el-table :data=\"tableData_brief\" border element-loading-text=\"Loading\" fit highlight-current-row size=\"mini\">\r\n        <el-table-column :label=\"$t('table.id')\" align=\"center\" width=\"50\">\r\n          <template v-slot=\"scope\">\r\n            {{ initTableIndex('page_brief', scope.$index) }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.detail')\" align=\"center\" width=\"100\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button size=\"small\" type=\"text\" @click=\"showRule(scope.row)\">>></el-button>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :filter-method=\"filters_method\" :filters=\"filters_pdCode\" :label=\"$t('rule.product')\"\r\n                         align=\"center\"\r\n                         min-width=\"200\" prop=\"pdCode\" sortable>\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdCode ? scope.row.pdCode + ':' + scope.row.pdName : '' }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('invRule.invItem')\" align=\"center\" min-width=\"200\">\r\n          <template slot-scope=\"scope\">\r\n            <span>{{ scope.row.pdInvCode }}<span v-show=\"scope.row.pdInvCode\">:</span>{{ scope.row.invName }}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('invRule.invType')\" align=\"center\" min-width=\"200\" prop=\"invType\">\r\n          <template v-slot=\"scope\">{{ scope.row.invType | filter_getType }}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('rule.pdRuleCode')\" align=\"center\"\r\n                         min-width=\"200\" prop=\"pdRuleCode\"></el-table-column>\r\n        <el-table-column :label=\"$t('rule.pdRuleDtCls')\" align=\"center\" min-width=\"200\">\r\n          <template v-slot=\"scope\">\r\n            {{ scope.row.pdRuleDtCls | filter_pdBussDetailType }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('rule.pdRuleDesc')\" align=\"center\" min-width=\"200\"\r\n                         prop=\"pdRuleDesc\"></el-table-column>\r\n        <el-table-column :label=\"$t('rule.ruleAttr')\" align=\"center\" min-width=\"200\">\r\n          <template v-slot=\"scope\">\r\n            {{\r\n              scope.row.ruleAttr === '1' ? $t('rule.general') : (scope.row.ruleAttr === '2' ? $t('rule.individual') : '')\r\n            }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.actions')\" align=\"center\" fixed=\"right\" width=\"120\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button v-auth=\"'invAmountFormul:associate'\" icon=\"el-icon-connection\" size=\"mini\" type=\"primary\"\r\n                       @click=\"selectAssociation(scope.row)\">\r\n              {{ $t('form.select') }}\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <cus-pagination ref=\"page_brief\" v-model=\"tableData_brief\" :autoBuild=\"false\" :pageSize=\"5\"\r\n                      :params=\"params_brief\" url=\"/invRules/base/briefRule\"/>\r\n    </div>\r\n\r\n    <!-- 规则引擎规则定义弹窗 -->\r\n    <cus-rule-definition :attributeObj=\"attributeObj\" :ruleDefinitionClosed=\"ruleModuleClosed\"\r\n                         :visible=\"formulaVisible\"></cus-rule-definition>\r\n\r\n    <!-- 规则查看和决策树编辑弹窗 -->\r\n    <cus-rule-modify :flag=\"formulaView.flag\" :performType=\"formulaView.performType\" :ruleCode=\"formulaView.ruleCode\"\r\n                     :ruleModifyClosed=\"formulaViewClosed\" :ruleType=\"'1'\" :visible=\"formulaView.visible\"\r\n                     width=\"70%\"></cus-rule-modify>\r\n\r\n  </cus-wraper>\r\n</template>\r\n\r\n<script>\r\n\r\nimport {associate, deleteInvRule, getRuleCode} from \"@/api/basicinfo/invRules\"\r\nimport {findInvBypdInvType} from '@/api/basicinfo/pdtemppdinv'\r\n\r\nlet _this\r\n\r\nexport default {\r\n  name: 'invRule',\r\n  watch: {\r\n    'tableData_current'() {\r\n      if (this.ruleRevisionShowINfo) {\r\n        // const params = {\r\n        //     eq_pdInfoId: this.product.id,\r\n        //     eq_pdRuleCls: '3',\r\n        // }\r\n        // allFormulaByPdInfoId(this.buildRequestData(params)).then(res => {\r\n        //     this.underwritingRulesVo = res.data.records\r\n        //     this.tableData_current = res.data.records\r\n        // })\r\n      }\r\n    },\r\n    tableData_brief() {\r\n      this.filters_pdCode = []\r\n      this.tableData_brief.forEach(val => {\r\n        if (!this._.includes(this._.map(this.filters_pdCode, 'value'), val.pdCode)) {\r\n          this.filters_pdCode.push({\r\n            text: val.pdCode + ':' + val.pdName,\r\n            value: val.pdCode\r\n          })\r\n        }\r\n      })\r\n    }\r\n  },\r\n  filters: {\r\n    filter_pdBussDetailType(code) {\r\n      return _this.getCodeName('ruleBusinessDetailClass', code)\r\n    },\r\n    filter_getType(code) {\r\n      return _this.getCodeName('pdinvtype', code)\r\n    },\r\n    filter_pdInvType(code) {\r\n      return _this.getCodeName('pdinvtype', code)\r\n    },\r\n    filter_pdInvStartPeriodUnit(code) {\r\n      return _this.getCodeName('pdsbnftdy', code, true)\r\n    }\r\n  },\r\n  props: {\r\n    // 规则大类\r\n    guaranteeDetails: {\r\n      type: String,\r\n      default: undefined\r\n    },\r\n    // 规则细类\r\n    calculationFormulaType: {\r\n      type: String,\r\n      default: undefined\r\n    },\r\n    // 收益编码\r\n    pdUwSiTypeOptions: {\r\n      type: String,\r\n      default: undefined\r\n    },\r\n    // 收益类型\r\n    pdUwSiTypeDtOptions: {\r\n      type: String,\r\n      default: undefined\r\n    },\r\n    global: {\r\n      type: Boolean,\r\n      required: false,\r\n      default: false\r\n    },\r\n    ruleRevisionShowINfo: {\r\n      type: Boolean,\r\n      required: false,\r\n      default: false\r\n    }\r\n  },\r\n  data() {\r\n    _this = this\r\n    return {\r\n      params_inv: {\r\n        eq_pdInfoId: undefined\r\n      },\r\n      tableDataInv: [],\r\n      selectedInvInRev: {},\r\n      filters_pdCode: [],\r\n      formulaVisible: false,\r\n      attributeObj: {},\r\n      formulaView: {\r\n        flag: '',\r\n        performType: '',\r\n        ruleCode: '',\r\n        visible: false\r\n      },\r\n      invOptions: [],\r\n      productBnftForm: {\r\n        type: '',\r\n        pdInvType: '',\r\n        ruleType: '',\r\n        pdInv: ''\r\n      },\r\n      selectedDef: '',//产品承保核保规则表格单选按钮\r\n      //产品承保核保规则表格数据\r\n      tableData_current: [],\r\n      tableData_brief: [],\r\n      params_current: {\r\n        eq_pdInfoId: undefined,\r\n        eq_pdType: '1',\r\n        eq_pdRuleCls: '3',\r\n        eq_pdRuleDtCls: this.ruleRevisionShowINfo ? '' : this.calculationFormulaType,\r\n        eq_pdInvCode: this.ruleRevisionShowINfo ? '' : this.pdUwSiTypeOptions\r\n      },\r\n      params_brief: {\r\n        product: undefined,\r\n        productDetailType: '',\r\n        pdRuleDtCls: this.ruleRevisionShowINfo ? '' : this.calculationFormulaType,\r\n        pdInvCode: this.ruleRevisionShowINfo ? '' : this.pdUwSiTypeOptions\r\n      },\r\n      product: {\r\n        id: undefined,\r\n        bType: '',\r\n        cType: '',\r\n        pdCode: '',\r\n        pdEdition: '',\r\n        pdVerId: undefined,\r\n        pdSalechnl: '',\r\n        pdName: '',\r\n        pdShortname: '',\r\n        pdEnName: '',\r\n        pdShortEnName: '',\r\n        operator: '',\r\n        pdApplDate: undefined,\r\n        pdProcessId: undefined,\r\n        createTime: undefined,\r\n        updateTime: undefined\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n  },\r\n  created() {\r\n    this.initProductInfo()\r\n    if (this.ruleRevisionShowINfo) {\r\n      this.$nextTick(() => {\r\n        this.$refs.page_inv.reload()\r\n      })\r\n    } else {\r\n      if (this.guaranteeDetails !== undefined) {\r\n        this.productBnftForm.type = this.guaranteeDetails\r\n        this.productBnftForm.pdInvType = this.pdUwSiTypeDtOptions\r\n        this.productBnftForm.ruleType = this.calculationFormulaType\r\n        this.findInv(this.productBnftForm.pdInvType).then(() => {\r\n          this.productBnftForm.pdInv = this.pdUwSiTypeOptions\r\n        })\r\n      }\r\n      this.$nextTick(() => {\r\n        this.$refs.page_current.reload()\r\n        this.$refs.page_brief.reload()\r\n      })\r\n    }\r\n  },\r\n  methods: {\r\n    rowClickInv(row, column, event) {\r\n      if (event.target.nodeName === \"INPUT\") {\r\n        if (row.id === this.selectedInvInRev.id) {\r\n          this.selectedInvInRev = {}\r\n          this.params_brief.pdBussId = undefined\r\n          this.tableData_current = []\r\n          this.$refs.page_current.total = 0\r\n          this.$refs.page_current.page = 1\r\n          this.tableData_brief = []\r\n          this.$refs.page_brief.total = 0\r\n          this.$refs.page_brief.page = 1\r\n        }\r\n      }\r\n    },\r\n    selectedInvInRevChanged() {\r\n      this.params_current.eq_pdRuleDtCls = '7'\r\n      this.params_current.eq_pdInvCode = this.selectedInvInRev.pdInvCode\r\n      this.params_brief.pdRuleDtCls = '7'\r\n      this.params_brief.pdInvCode = this.selectedInvInRev.pdInvCode\r\n      this.$nextTick(() => {\r\n        this.$refs.page_current.reload()\r\n        this.$refs.page_brief.reload()\r\n      })\r\n    },\r\n    ruleModuleClosed() {\r\n      this.formulaVisible = false\r\n      this.$refs.page_brief.reload()\r\n    },\r\n    showRule(row) {\r\n      this.formulaView.flag = '1'\r\n      this.formulaView.performType = row.performType\r\n      this.formulaView.ruleCode = row.pdRuleCode\r\n      this.formulaView.visible = true\r\n    },\r\n    formulaViewClosed() {\r\n      this.formulaView.visible = false\r\n      this.selectedDef = {}\r\n    },\r\n    //收益信息\r\n    async findInv(val) {\r\n      return new Promise(resolve => {\r\n        this.invOptions = []\r\n        this.productBnftForm.pdInv = ''\r\n        if (!val) {\r\n          resolve()\r\n          return\r\n        }\r\n        let paramsinv = {\r\n          p