rms/ruleMake\";\r\n\r\nexport default {\r\n  name: \"dynamicFormArray\",\r\n  data() {\r\n    return {\r\n      dynamicForm: {\r\n        isArray: \"\",\r\n        bom: {},\r\n        bomItem: [],\r\n        bomItemData: [],\r\n        child: []\r\n      },\r\n      isShow: true,\r\n      optionArray: [],\r\n      loadingArray: []\r\n    };\r\n  },\r\n  props: [\"dForm\", \"dItems\"],\r\n  mounted() {\r\n    this.dynamicForm = this.dForm;\r\n  },\r\n  created(){\r\n\r\n  },\r\n  methods: {\r\n    selectClick(item,value,index) {\r\n          item.optionArray = [];\r\n          this.loadingArray[index] = true;\r\n          let param = { sql: item.sqlStr };\r\n          getComboxData(this.buildRequestData(param)).then(response => {\r\n            let dataList = response.data;\r\n            for (let i = 0; i < dataList.length; i++) {\r\n                item.optionArray.push({\r\n                value: dataList[i].code,\r\n                label:dataList[i].code +\":\"+ dataList[i].codeName\r\n              });\r\n            }\r\n           this.loadingArray[index] = false;\r\n          })\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.stepTitle {\r\n  margin: 10px 0px 10px 10px;\r\n}\r\n\r\n.needItemBlack .el-form-item__label {\r\n\r\n}\r\n\r\n.el-dialog-div {\r\n  height: 50vh;\r\n  overflow: auto;\r\n}\r\n.el-input{\r\n  width: 220px !important;\r\n}\r\n\r\n.ruleItem ::v-deep.el-form-item__label {\r\n    width: 200px !important;\r\n}\r\n\r\n.el-select {\r\n    width: 220px !important;\r\n}\r\n.needItemBlue ::v-deep .el-form-item__label {\r\n      color: #1D2A6D !important;\r\n      font-weight: bold;\r\n}\r\n</style>\r\n"]}]}