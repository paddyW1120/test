urn that.getCodeName(\"releasetypeall\", val);\r\n    },\r\n    filterReleaseWay(val) {\r\n      return that.getCodeName(\"releaseway\", val);\r\n    },\r\n    filterReleaseContent(val) {\r\n      return that.getCodeName(\"pdReleContent\", val);\r\n    },\r\n    filterFlag(val) {\r\n      if (!val) {\r\n        return \"1:未发布\";\r\n      } else {\r\n        return that.getCodeName(\"pdReleStu\", val);\r\n      }\r\n    }\r\n  },\r\n  components: {\r\n    releaseAll,\r\n    releaseIncr,\r\n    reback,\r\n    releaserecord,\r\n    pdoperPdVerRls\r\n  },\r\n  data() {\r\n    that = this;\r\n    const validatePass = (rule, value, callback) => {\r\n      if (value === \"\") {\r\n        callback(new Error(\"请输入密码\"));\r\n      } else {\r\n        if (this.PdtempPdReleSyspath.repdPassword !== \"\") {\r\n          this.$refs.PdtempPdReleSyspath.validateField(\"repdPassword\");\r\n        }\r\n        callback();\r\n      }\r\n    };\r\n    const validatePasscheck = (rule, value, callback) => {\r\n      if (value === \"\") {\r\n        callback(new Error(\"请再次输入密码\"));\r\n      } else if (value !== this.PdtempPdReleSyspath.pdPassword) {\r\n        callback(new Error(\"两次输入密码不一致!\"));\r\n      } else {\r\n        callback();\r\n      }\r\n    };\r\n    return {\r\n      releProgress: \"待发布\",\r\n      activeTabName: \"releaseAll\",\r\n      activatedTabNames: [\"releaseAll\"],\r\n      chooseRowId: \"\",\r\n      repdPassword: \"\", //确认密码\r\n      sysdesDialogVisible: false, //dialog可见\r\n      dialogTableVisible: false, //  发布记录\r\n      rssysdesDialogVisible: false, //发布记录统计弹窗\r\n      releInfoData: [],\r\n      PdtempPdReleSyspathData: [], //发布环境定义查询数据\r\n      auditDateValue: \"\",\r\n      /*发布环境定义*/\r\n      PdtempPdReleSyspath: {\r\n        id: undefined,\r\n        operator: \"\",\r\n        pdPathType: \"\",\r\n        pdPathValue: \"\",\r\n        pdReleSysname: \"\",\r\n        pdReleIp: \"\",\r\n        pdRelePort: \"\",\r\n        pdUsername: \"\",\r\n        pdPassword: \"\",\r\n        repdPassword: \"\",\r\n        pdReleOrder: undefined,\r\n        pdSysCode: \"\",\r\n        pdDatasourceType: \"\",\r\n        pdDatasourceName: \"\",\r\n        pdDatasourceParams: \"\",\r\n        createTime: undefined,\r\n        updateTime: undefined\r\n      },\r\n      hisvervisible: {\r\n        formula: false\r\n      },\r\n      //查询参数\r\n      paramsysdes: {\r\n        like_xxxxx: \"\",\r\n        eq_xxxx: \"\"\r\n      },\r\n      sysdesrules: {\r\n        pdReleSysname: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"operation.tipmess\"),\r\n            trigger: \"change\"\r\n          }\r\n        ],\r\n        pdReleIp: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"operation.tipmess\"),\r\n            trigger: \"change\"\r\n          }\r\n        ],\r\n        pdRelePort: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"operation.tipmess\"),\r\n            trigger: \"change\"\r\n          }\r\n        ],\r\n        pdUsername: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"operation.tipmess\"),\r\n            trigger: \"change\"\r\n          }\r\n        ],\r\n        pdPassword: [{ validator: validatePass, trigger: \"change\" }],\r\n        repdPassword: [{ validator: validatePasscheck, trigger: \"change\" }],\r\n        pdReleOrder: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"operation.tipmess\"),\r\n            trigger: \"change\"\r\n          }\r\n        ],\r\n        pdSysCode: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"operation.tipmess\"),\r\n            trigger: \"change\"\r\n          }\r\n        ],\r\n        pdDatasourceType: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"operation.tipmess\"),\r\n            trigger: \"change\"\r\n          }\r\n        ],\r\n        pdDatasourceName: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"operation.tipmess\"),\r\n            trigger: \"change\"\r\n          }\r\n        ],\r\n        pdDatasourceParams: [\r\n          {\r\n            required: false,\r\n            message: this.$t(\"operation.tipmess\"),\r\n            trigger: \"change\"\r\n          }\r\n        ]\r\n      },\r\n      //待发布查询 表格数据\r\n      configReadyToRele: [],\r\n      pdParams: [],\r\n      pdInfo: {},\r\n      //待发布查询    查询参数\r\n      pdSelForm: {\r\n        startDate: \"\",\r\n        endDate: \"\",\r\n        pdReleType: \"\",\r\n        pdCode: \"\",\r\n        pdEdition: \"\"\r\n      },\r\n      pdOptions: [],\r\n      pickerOptions: {\r\n        shortcuts: [\r\n          {\r\n            text: \"最近一周\",\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 7);\r\n              picker.$emit(\"pick\", [start, end]);\r\n            }\r\n          },\r\n          {\r\n            text: \"最近一个月\",\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 30);\r\n              picker.$emit(\"pick\", [start, end]);\r\n            }\r\n          },\r\n          {\r\n            text: \"最近三个月\",\r\n            onClick(picker) {\r\n              const end = new Date();\r\n              const start = new Date();\r\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 90);\r\n              picker.$emit(\"pick\", [start, end]);\r\n            }\r\n          }\r\n        ]\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.queryPdSelectList();\r\n  },\r\n  watch: {\r\n    auditDateValue(val) {\r\n      if (val) {\r\n        this.pdSelForm.startDate = val[0];\r\n        this.pdSelForm.endDate = val[1];\r\n      } else {\r\n        this.pdSelForm.startDate = \"\";\r\n        this.pdSelForm.endDate = \"\";\r\n      }\r\n    },\r\n    configReadyToRele(newVal, oldVal) {\r\n      if (count === 0) {\r\n        this.getSelectOptions(newVal);\r\n        count = 1;\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    toHisver() {\r\n      this.hisvervisible.formula = true;\r\n    },\r\n    changeHisvervisible_formula(flag) {\r\n      this.hisvervisible.formula = flag;\r\n    },\r\n    changeDialogReleaseRecord(flag) {\r\n      this.rssysdesDialogVisible = flag;\r\n    },\r\n    backReleaseRecord() {\r\n      this.dialogTableVisible = false;\r\n    },\r\n    backReleSyspath() {\r\n      this.sysdesDialogVisible = false;\r\n    },\r\n    /*查询产品下拉*/\r\n    queryPdSelectList() {\r\n      queryPdSelectList(this.buildRequestData({})).then(res => {\r\n        this.getSelectOptions(res.data);\r\n      });\r\n    },\r\n    /*发布进度*/\r\n    releSchedule(val, arr) {\r\n      for (const arrElement of this.configReadyToRele) {\r\n        if (this.isInArray(arrElement.id, arr)) {\r\n          arrElement.backFlag = val;\r\n        }\r\n      }\r\n    },\r\n    /*下载发布文件*/\r\n    downloadReleFile(row) {\r\n      let filePath = row.pdReleFilePath + \"//\" + row.pdReleFileName;\r\n      let data = { filePath: filePath };\r\n      let fileName = row.pdReleFileName;\r\n      downloadReleFile(this.buildRequestData(data)).then(res => {\r\n        this.downloadResponseFile(res, fileName);\r\n        this.notifySuccess(\"下载成功\");\r\n      });\r\n    },\r\n    /*发布记录*/\r\n    showReleRecord(row) {\r\n      this.dialogTableVisible = true;\r\n      let keyName = this.$route.path;\r\n      this.cacheJsonData(keyName, row);\r\n      let data = {\r\n        pdInfoId: row.id,\r\n        pdVerId: row.pdVerId\r\n      };\r\n      getPdtempPdVerRls(this.buildRequestData(data)).then(res => {\r\n        this.releInfoData = res.data;\r\n        this.pdInfo = this.getPdData();\r\n      });\r\n    },\r\n    /*发布完毕*/\r\n    releOver(row) {\r\n      if (!row.pdReleStu || row.pdReleStu === \"1\") {\r\n        this.notifyError(\"该产品未发布\");\r\n        return;\r\n      }\r\n      let data = {\r\n        pdInfoId: row.id,\r\n        pdVerId: row.pdVerId,\r\n        pdCode: row.pdCode,\r\n        pdName: row.pdName,\r\n        pdEdition: row.pdEdition,\r\n        releTypeDet: \"\",\r\n        processInstanceId: row.pdProcessId\r\n      };\r\n      if (row.pdReleType === \"1\") {\r\n        data.releTypeDet = \"0\";\r\n      } else {\r\n        if (\r\n          row.defType !== \"3\" &&\r\n          (row.pdReturnFuncCode.indexOf(\"productInfoFlag\") !== -1 ||\r\n            row.pdReturnFuncCode.indexOf(\"productDistributionFlag\") !== -1 ||\r\n            row.pdReturnFuncCode.indexOf(\"productCoverFlag\") !== -1 ||\r\n            row.pdReturnFuncCode.indexOf(\"productUnderwritingFlag\") !== -1 ||\r\n            row.pdReturnFuncCode.indexOf(\"productHePeiFlag\") !== -1 ||\r\n            row.pdReturnFuncCode.indexOf(\"productPreservationFlag\") !== -1 ||\r\n            row.pdReturnFuncCode.indexOf(\"productRenewalFlag\") !== -1)\r\n        ) {\r\n          // 产品发布\r\n          data.releTypeDet = \"1\";\r\n        } else if (\r\n          row.defType !== \"3\" &&\r\n          row.pdReturnFuncCode.indexOf(\"productRuleFlag\") !== -1\r\n        ) {\r\n          // 规则发布\r\n          data.releTypeDet = \"2\";\r\n        } else if (\r\n          row.defType !== \"3\" &&\r\n          row.pdReturnFuncCode.indexOf(\"productFormulaFlag\") !== -1\r\n        ) {\r\n          // 公式发布\r\n          data.releTypeDet = \"3\";\r\n        } else if (row.defType === \"3\") {\r\n          // 停售发布\r\n          data.releTypeDet = \"4\";\r\n        }\r\n      }\r\n      pdReleOver(this.buildRequestData(data)).then(() => {\r\n        this.$refs.page.reload();\r\n        this.notifySuccess(\"发布完毕\");\r\n        if (this.$refs.relaaseAllPage) {\r\n          this.$refs.relaaseAllPage.refreshChildTable();\r\n        }\r\n        if (this.$refs.releaseIncrPage) {\r\n          this.$refs.releaseIncrPage.refreshChildTable();\r\n        }\r\n        if (this.$refs.rebackPage) {\r\n          this.$refs.rebackPage.refreshChildTable();\r\n        }\r\n      });\r\n    },\r\n    /*删除一条数据*/\r\n    deletePdtempPdReleSyspath(id) {\r\n      deletePdtempPdReleSyspath(id).then(res => {\r\n        this.$refs.pagesysdes.reload();\r\n        this.notifySuccess(res.message);\r\n      });\r\n    },\r\n    resetEntityPdtempPdReleSyspath() {\r\n      this.PdtempPdReleSyspath = {\r\n        id: undefined,\r\n        operator: \"\",\r\n        pdPathType: \"\",\r\n        pdPathValue: \"\",\r\n        pdReleSysname: \"\",\r\n        pdReleIp: \"\",\r\n        pdRelePort: \"\",\r\n        pdUsername: \"\",\r\n        pdPassword: \"\",\r\n        repdPassword: \"\",\r\n        pdReleOrder: undefined,\r\n        pdSysCode: \"\",\r\n        pdDatasourceType: \"\",\r\n        pdDatasourceName: \"\",\r\n        pdDatasourceParams: \"\",\r\n        createTime: undefined,\r\n        updateTime: undefined\r\n      };\r\n      this.$nextTick(() => {\r\n        this.$refs.PdtempPdReleSyspath.clearValidate();\r\n      });\r\n    },\r\n    /*选取一条数据*/\r\n    chooseRowRele(row, column, event) {\r\n      if (event.target.nodeName === \"INPUT\") {\r\n        if (this.chooseRowId === row.id) {\r\n          this.chooseRowId = \"\";\r\n          this.resetEntityPdtempPdReleSyspath();\r\n        } else {\r\n          this.chooseRowId = row.id;\r\n          Object.assign(this.PdtempPdReleSyspath, row);\r\n          this.PdtempPdReleSyspath.pdReleOrder = String(\r\n            this.PdtempPdReleSyspath.pdReleOrder\r\n          );\r\n          this.$nextTick(() => {\r\n            this.$refs.PdtempPdReleSyspath.clearValidate();\r\n          });\r\n        }\r\n      }\r\n    },\r\n    queryPdtempPdReleSyspath() {\r\n      // 配置环境显示\r\n      this.sysdesDialogVisible = true;\r\n      this.$nextTick(() => {\r\n        this.$refs.PdtempPdReleSyspath.clearValidate();\r\n      });\r\n    },\r\n\r\n    displayReleasedstatistics() {\r\n      // 发布记录统计\r\n      this.rssysdesDialogVisible = true;\r\n    },\r\n    savePdtempPdReleSyspath(formName) {\r\n      //添加环境\r\n      this.$refs[formName].validate(valid => {\r\n        if (valid) {\r\n          // this.$delete(this.PdtempPdReleSyspath, 'repdPassword');//vue方法\r\n          savePdtempPdReleSyspath(this.PdtempPdReleSyspath).then(res => {\r\n            this.$refs.pagesysdes.reload();\r\n            this.notifySuccess(res.message);\r\n            this.$refs.PdtempPdReleSyspath.clearValidate();\r\n          });\r\n        }\r\n      });\r\n    },\r\n    resetPdtempPdReleSyspath(formName) {\r\n      // 重置表单\r\n      this.$refs[formName].resetFields();\r\n    },\r\n    //切换标签页\r\n    tabClick(tab) {\r\n      let flag = false;\r\n      this.activatedTabNames.forEach(value => {\r\n        if (value === tab.name) {\r\n          flag = true;\r\n        }\r\n      });\r\n      if (flag === false) {\r\n        this.activatedTabNames.push(tab.name);\r\n      }\r\n    },\r\n    isAlreadyActivated(val) {\r\n      let flag = false;\r\n      this.activatedTabNames.forEach(value => {\r\n        if (val === value) {\r\n          flag = true;\r\n        }\r\n      });\r\n      return flag;\r\n    },\r\n    //获取版本\r\n    getEdition(val) {\r\n      if (val.length > 0) {\r\n        this.pdSelForm.pdEdition = val[0].pdEdition;\r\n      }\r\n    },\r\n    /*待发布搜索*/\r\n    queryTableData() {\r\n      // if(!this.pdSelForm.startDate1){\r\n      //   this.pdSelForm.startDate = '1970-01-01'\r\n      // }\r\n      // if (!this.pdSelForm.endDate){\r\n      //   this.pdSelForm.endDate = this.getCurrentDateStr()\r\n      // }\r\n      if (this.pdSelForm.startDate > this.pdSelForm.endDate) {\r\n        this.notifyError(\"审核起始日期不得晚于结束日期！\");\r\n        return;\r\n      }\r\n      if (this.pdParams.length > 0) {\r\n        this.pdSelForm.pdCode = this.pdParams[0].pdCode;\r\n      } else {\r\n        this.pdSelForm.pdCode = \"\";\r\n        this.pdSelForm.pdEdition = \"\";\r\n      }\r\n      this.$refs.page.reload();\r\n    },\r\n    /*产品下拉选*/\r\n    getSelectOptions(configs) {\r\n      if (configs) {\r\n        this.pdOptions = [];\r\n        for (let data of configs) {\r\n          let option = { pdCode: \"\", pdName: \"\", pdEdition: \"\" };\r\n          option.pdCode = data.pdCode;\r\n          option.pdName = data.pdName;\r\n          option.pdEdition = data.pdEdition;\r\n          this.pdOptions.push(option);\r\n        }\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.sbut {\r\n  width: 100px;\r\n}\r\n\r\n.getname {\r\n  width: 500px;\r\n}\r\n</style>\r\n"]}]}