elect>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"6\">\r\n            <el-button dialog-disable=\"disable\" icon=\"el-icon-search\" size=\"small\" type=\"primary\"\r\n                       @click=\"paymentFormSearch\">{{ $t('pdtempPdBnft.search') }}\r\n            </el-button>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n      <cus-table-title :label=\"$t('pdtempPdBnft.pdBnft')\"/>\r\n      <el-table ref=\"unTable\" :data=\"unpaymentConfigs\" align=\"center\" border class=\"wraper-table\"\r\n                element-loading-text=\"Loading\"\r\n                fit highlight-current-row size=\"mini\" @row-click=\"chooseone1\">\r\n        <el-table-column :label=\"$t('pdtempPdBnft.choose')\" width=\"55\">\r\n          <template slot-scope=\"scope\">\r\n            <el-radio v-model=\"radio1\" :label=\"scope.row.id\">\r\n              <span class=\"el-radio__label\"></span>\r\n            </el-radio>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('table.id')\" align=\"center\" width=\"50\">\r\n          <template v-slot=\"scope\">{{ initTableIndex('unpaymentPage', scope.$index) }}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('pdtempPdBnft.desc')\" align=\"center\" prop type=\"expand\" width=\"60\">\r\n          <template slot-scope=\"props\">\r\n            <el-form class=\"demo-table-expand\" inline label-position=\"left\">\r\n              <el-form-item :label=\"$t('i18nView.pdCode')\">\r\n                <span>{{ props.row.pdCode }}</span>\r\n              </el-form-item>\r\n              <el-form-item :label=\"$t('i18nView.pdName')\">\r\n                <span>{{ props.row.pdName }}</span>\r\n              </el-form-item>\r\n              <el-form-item :label=\"$t('pdtempPdBnft.pdBnftDe')\">\r\n                <span>{{ props.row.pdBnftType | filter_pdBnftType }}</span>\r\n              </el-form-item>\r\n              <el-form-item :label=\"$t('pdtempPdBnft.pdBnftEndPeriod')\">\r\n                <span>{{\r\n                    props.row.pdBnftEndPeriod\r\n                  }}{{ props.row.pdBnftEndPeriodUnit | filter_pdBnftStartPeriodUnit }}</span>\r\n              </el-form-item>\r\n              <el-form-item :label=\"$t('pdtempPdBnft.pdAmntCalcMode')\">\r\n                <span>{{ props.row.pdAmntCalcMode }}</span>\r\n              </el-form-item>\r\n              <el-form-item :label=\"$t('pdtempPdBnft.pdAmntInc')\">\r\n                <span>{{ props.row.pdAmntIncFlag }}</span>\r\n              </el-form-item>\r\n              <el-form-item :label=\"$t('pdtempPdBnft.pdCalcId')\">\r\n                <span>{{ props.row.pdCalcId }}</span>\r\n              </el-form-item>\r\n              <el-form-item :label=\"$t('pdtempPdBnft.pdsbnftgettype')\">\r\n                <span>{{ props.row.pdsbnftgettype | filter_pdsbnftgettype }}</span>\r\n              </el-form-item>\r\n              <el-form-item :label=\"$t('pdtempPdBnft.pdsbnftgettimes')\">\r\n                <span>{{ props.row.pdsbnftgettimes | filter_pdsbnftgettimes }}</span>\r\n              </el-form-item>\r\n              <el-form-item :label=\"$t('pdtempPdBnft.desc')\" prop=\"desc\">\r\n                <el-button dialog-disable=\"disable\" size=\"small\" type=\"text\" @click=\"showBnftDesc(props.row)\">\r\n                  >>\r\n                </el-button>\r\n              </el-form-item>\r\n            </el-form>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column :filter-method=\"filters_pdBnftType1_mehtod\" :filters=\"filters_pdBnftType1\"\r\n                         :label=\"$t('pdtempPdBnft.pdBnftType')\" align=\"center\"\r\n                         prop=\"pdBnftType\"\r\n                         sortable>\r\n          <template v-slot=\"scope\">{{ scope.row.pdBnftType | filter_pdBnftType }}</template>\r\n        </el-table-column>\r\n        <el-table-column :filter-method=\"filters_pdBnftDetailType1_mehtod\" :filters=\"filters_pdBnftDetailType1\"\r\n                         :label=\"$t('pdtempPdBnft.pdBnftDetailType')\" align=\"center\"\r\n                         prop=\"pdBnftDetailType\"\r\n                         sortable>\r\n          <template v-slot=\"scope\">{{ scope.row | filter_pdBnftDetailType }}</template>\r\n        </el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.guaranteeCode')\" align=\"center\" prop=\"pdBnftCode\"></el-table-column>\r\n        <el-table-column :label=\"$t('i18nView.guaranteeCode')\" align=\"center\" prop=\"pdBnftName\"></el-table-column>\r\n        <el-table-column :label=\"$t('pdtempPdInsuAgeInfo.operation')\" align=\"center\" width=\"140px\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button dialog-disable=\"disable\" size=\"small\" type=\"primary\" @click=\"copyForBnft(scope.row)\"><i\r\n              class=\"el-icon-document-copy\"></i>{{ $t('pdtempPdBnft.bnftCopy') }}\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <cus-pagination ref=\"unpaymentPage\" v-model=\"unpaymentConfigs\" :pageSize=\"10\" :params=\"paymentForm\"\r\n                      url=\"/pdtemp-pd-bnft/base/unrelation\"/>\r\n      <br/>\r\n    </div>\r\n    <br/>\r\n    <cus-table-title :label=\"$t('pdtempPdBnft.benefit')\"></cus-table-title>\r\n    <el-table ref=\"singleTable\" :data=\"paymentConfigs\" border element-loading-text=\"Loading\"\r\n              fit highlight-current-row size=\"mini\" @row-click=\"chooseone2\">\r\n      <el-table-column :label=\"$t('pdtempPdBnft.choose')\" width=\"55\">\r\n        <template slot-scope=\"scope\">\r\n          <el-radio-group v-model=\"radio2\">\r\n            <el-radio :label=\"scope.row.id\">{{ '' }}</el-radio>\r\n          </el-radio-group>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" width=\"50\">\r\n        <template v-slot=\"scope\">{{ initTableIndex('paymentPage', scope.$index) }}</template>\r\n      </el-table-column>\r\n      <el-table-column :filter-method=\"filters_pdBnftType_mehtod\" :filters=\"filters_pdBnftType\"\r\n                       :label=\"$t('pdtempPdBnft.pdBnftType')\" align=\"center\"\r\n                       prop=\"pdBnftType\"\r\n                       sortable>\r\n        <template v-slot=\"scope\">{{ scope.row.pdBnftType | filter_pdBnftType }}</template>\r\n      </el-table-column>\r\n      <el-table-column :filter-method=\"filters_pdBnftDetailType_mehtod\" :filters=\"filters_pdBnftDetailType\"\r\n                       :label=\"$t('pdtempPdBnft.pdBnftDetailType')\" align=\"center\"\r\n                       prop=\"pdBnftDetailType\"\r\n                       sortable>\r\n        <template v-slot=\"scope\">{{ scope.row | filter_pdBnftDetailType }}</template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('pdtempPdBnft.pdBnftOrder')\" align=\"center\" prop=\"pdBnftOrder\"></el-table-column>\r\n      <el-table-column :label=\"$t('pdtempPdSrvlBnft.pdBnftCode')\" align=\"center\" prop=\"pdBnftCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('pdtempPdBnft.pdBnftName')\" align=\"center\" prop=\"pdBnftName\"></el-table-column>\r\n      <el-table-column :filter-method=\"filters_pdAmntCalcMode_mehtod\" :filters=\"filters_pdAmntCalcMode\"\r\n                       :label=\"$t('pdtempPdBnft.pdAmntCalcMode')\"\r\n                       align=\"center\"\r\n                       prop=\"pdAmntCalcMode\">\r\n        <template v-slot=\"scope\">{{ scope.row.pdAmntCalcMode | filter_pdAmntCalcMode }}</template>\r\n      </el-table-column>\r\n      <el-table-column :filter-method=\"filters_pdAmntIncFlag_mehtod\" :filters=\"filters_pdAmntIncFlag\"\r\n                       :label=\"$t('pdtempPdBnft.pdAmntIncFlag')\"\r\n                       align=\"center\"\r\n                       prop=\"pdAmntIncFlag\">\r\n        <template v-slot=\"scope\">{{ scope.row.pdAmntIncFlag | filter_pdAmntIncFlag }}</template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('pdtempPdInsuAgeInfo.operation')\" align=\"center\" width=\"100px\">\r\n        <template slot-scope=\"scope\">\r\n          <cus-del-btn @ok=\"deletebnftForm(scope.row)\"></cus-del-btn>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <cus-pagination ref=\"paymentPage\" v-model=\"paymentConfigs\" :pageSize=\"10\" :params=\"paymentParams\"\r\n                    url=\"/pdtemp-pd-bnft/base/relation\"/>\r\n    <br/>\r\n    <br/>\r\n    <el-tabs v-model=\"activeName\" type=\"border-card\" @tab-click=\"clickTab\">\r\n      <el-tab-pane :disabled=\"tabflag\" :label=\"$t('pdtempPdBnft.generalMedicalBnft')\" name=\"pdtemppdclmbnft\">\r\n        <pdtemppdclmbnft ref=\"pdtemppdclmbnft\" :activeName=\"activeName\" :configLength=\"configLength\"\r\n                         :pdBnft=\"bnftForm\"\r\n                         :pdBnftArr=\"paymentConfigs\"\r\n                         @changeDialogVisible_radioSelect=\"changeDialogVisible_radioSelect\"\r\n                         @childFn=\"parentFn\"></pdtemppdclmbnft>\r\n      </el-tab-pane>\r\n      <el-tab-pane :label=\"$t('pdtempPdBnft.noGeneralMedicalBnft')\" name=\"pdtemppdnomedicalclmbnft\">\r\n        <pdtemppdnomedicalclmbnft v-if=\"activeName==='pdtemppdnomedicalclmbnft'\" ref=\"pdtemppdnomedicalclmbnft\"\r\n                                  :activeName=\"activeName\" :configLength=\"configLength\"\r\n                                  :pdBnft=\"bnftForm\"\r\n                                  :pdBnftArr=\"paymentConfigs\"\r\n                                  @bringInCalcCode=\"bringInCalcCode\"\r\n                                  @changeDialogVisible_radioSelect=\"changeDialogVisible_radioSelect\"\r\n                                  @childFn=\"parentFn\"></pdtemppdnomedicalclmbnft>\r\n      </el-tab-pane>\r\n      <el-tab-pane :label=\"$t('route.pdtemppdsrvlbnft')\" name=\"pdtemppdsrvlbnft\">\r\n        <pdtemppdsrvlbnft v-if=\"activeName==='pdtemppdsrvlbnft'\" ref=\"pdtemppdsrvlbnft\" :activeName=\"activeName\"\r\n                          :existLength=\"existLength\"\r\n                          :pdBnft=\"bnftForm\"\r\n                          @bringInCalcCode=\"bringInCalcCode\"\r\n                          @changeDialogVisible_radioSelect=\"changeDialogVisible_radioSelect\"\r\n                          @childFn=\"parentFn\"></pdtemppdsrvlbnft>\r\n      </el-tab-pane>\r\n    </el-tabs>\r\n    <!-- 一般赔付保障页面 -->\r\n    <el-dialog v-if=\"dialogVisible.clmBnft\" v-dialog-drag :visible.sync=\"dialogVisible.clmBnft\" center title=\"赔付保障详情\"\r\n               width=\"70%\">\r\n      <pdtemppdclmbnft :pdBnft=\"selectedClmBnft\"></pdtemppdclmbnft>\r\n    </el-dialog>\r\n    <!-- 非一般赔付保障页面 -->\r\n    <el-dialog v-if=\"dialogVisible.noClmBnft\" v-dialog-drag :visible.sync=\"dialogVisible.noClmBnft\" center\r\n               title=\"赔付保障详情\"\r\n               width=\"70%\">\r\n      <pdtemppdnomedicalclmbnft :pdBnft=\"selectedClmBnft\"></pdtemppdnomedicalclmbnft>\r\n    </el-dialog>\r\n    <!-- 生存保障页面 -->\r\n    <el-dialog v-if=\"dialogVisible.srvBnft\" v-dialog-drag :visible.sync=\"dialogVisible.srvBnft\" center title=\"生存保障详情\"\r\n               width=\"70%\">\r\n      <pdtemppdsrvlbnft :pdBnftReadOnly=\"selectedSrvBnft\"></pdtemppdsrvlbnft>\r\n    </el-dialog>\r\n  </cus-wraper>\r\n</template>\r\n\r\n<script>\r\nimport {addPdtempPdBnft, deletePdtempPdBnft, findReferenceProducts} from \"@/api/basicinfo/pdtemppdbnft\"\r\nimport {pdtempPdClsLabList} from \"@/api/basicinfo/ProductAttributeDefinition\"\r\nimport {getSrvlMaxOrder} from '@/api/basicinfo/pdtemppdsrvlbnft'\r\nimport {getBnftMaxOrder} from '@/api/basicinfo/pdtemppdnomedicalclmbnft'\r\nimport pdtemppdclmbnft\r\n  from \"@/views/product-definition/pd-framework-def/pd-structure-def/guarantee-def/general-medical/index\"\r\nimport pdtemppdnomedicalclmbnft\r\n  from \"@/views/product-definition/pd-framework-def/pd-structure-def/guarantee-def/no-general-medical/index\"\r\nimport pdtemppdsrvlbnft from \"@/views/product-definition/pd-framework-def/pd-structure-def/guarantee-def/survival/index\"\r\n\r\nlet _this\r\nexport default {\r\n  name: \"paymentMain\",\r\n  filters: {\r\n    filter_pdAmntCalcMode(code) {\r\n      return _this.getCodeName(\"pdAmntCalMode\", code)\r\n    },\r\n    filter_pdBnftType(code) {\r\n      return _this.getCodeName(\"pdBnftType\", code)\r\n    },\r\n    filter_pdBnftDetailType1(code) {\r\n      return _this.getCodeName(\"pdclmbnfttype2\", code)\r\n    },\r\n    filter_pdBnftDetailType2(code) {\r\n      return _this.getCodeName(\"pdsbnftnjxl\", code)\r\n    },\r\n    filter_pdDutyType(code) {\r\n      return _this.getCodeName(\"pdDutyType\", code)\r\n    },\r\n    filter_pdBnftStartDateRule(code) {\r\n      return _this.getCodeName(\"pdclmstartcalref\", code)\r\n    },\r\n    filter_pdBnftStartDateMode(code) {\r\n      return _this.getCodeName(\"pdBnftStartDateMode\", code)\r\n    },\r\n    filter_pdBnftStartPeriodUnit(code) {\r\n      return _this.getCodeName(\"pdsbnftdy\", code, true)\r\n    },\r\n    filter_pdBnftEndDateRule(code) {\r\n      return _this.getCodeName(\"pdsbnftendrule\", code)\r\n    },\r\n    filter_pdsbnftgettype(code) {\r\n      return _this.getCodeName(\"pdsbnftgettype\", code)\r\n    },\r\n    filter_pdsbnftgettimes(code) {\r\n      return _this.getCodeName(\"pdsbnftgettimes\", code)\r\n    },\r\n    filter_pdBnftDetailType(row) {\r\n      if (row.pdBnftType == 'C') {\r\n        return _this.getCodeName(\"pdclmbnfttype2\", row.pdBnftDetailType)\r\n      } else {\r\n        return _this.getCodeName(\"pdsbnftnjxl\", row.pdBnftDetailType)\r\n      }\r\n    },\r\n    filter_pdAmntIncFlag(code) {\r\n      if (code == null || code == '') {\r\n        return 'N'\r\n      } else {\r\n        return code\r\n      }\r\n    }\r\n  },\r\n  props: {\r\n    pdBnft: {\r\n      type: Object,\r\n      default: () => {\r\n        return {}\r\n      }\r\n    }\r\n  },\r\n  data() {\r\n    _this = this\r\n    return {\r\n      filters_pdBnftType: [],\r\n      filters_pdBnftDetailType: [],\r\n      filters_pdAmntCalcMode: [],\r\n      filters_pdAmntIncFlag: [],\r\n      filters_pdBnftType1: [],\r\n      filters_pdBnftDetailType1: [],\r\n      selectedClmBnft: {},\r\n      selectedSrvBnft: {},\r\n      tabflag: false,\r\n      showPageFlag: 0,\r\n      quickshow: false,\r\n      paymentForm: {\r\n        pdInfoId: \"\",\r\n        product: \"\",\r\n        pdBnftType: \"\",\r\n        pdBnft: \"\",\r\n        firstOpenPageFlag: \"Y\",\r\n      },\r\n      bnftForm: {},\r\n      configLength: null,\r\n      existLength: null,\r\n      copyBnft: {},\r\n      productOptions: [],\r\n      BnftTypeOptions: [\r\n        {\r\n          value: \"A\",\r\n          label: \"A:年金\"\r\n        },\r\n        {\r\n          value: \"M\",\r\n          label: \"M:满期金\"\r\n        },\r\n        {\r\n          value: \"C\",\r\n          label: \"C:赔付金\"\r\n        }\r\n      ],\r\n      bnftOptions: [],\r\n      bnftCodeOptions: [],\r\n      finelinesRegionFilter: \"\",\r\n      dialogVisible: {\r\n        clmBnft: false,\r\n        noClmBnft: false,\r\n        srvBnft: false,\r\n      },\r\n      typeRegion: null,\r\n      productTypeDetail: null,\r\n      radio1: null,\r\n      radio2: null,\r\n      //查询参数\r\n      paymentParams: {\r\n        pdInfoId: \"\",\r\n        eq_pdBnftType: \"\",\r\n        eq_pdBnftCode: \"\"\r\n      },\r\n      pdtempPdInfo: {},\r\n      paymentConfigs: [],\r\n      unpaymentConfigs: [],\r\n      dialogConfigs: [],\r\n      currentSelectItem: {}, //当前选中的值\r\n      activeName: \"pdtemppdclmbnft\",\r\n      activedNames: [\"pdtemppdclmbnft\"],\r\n      paymentFormRules: {}\r\n    }\r\n  },\r\n  components: {\r\n    pdtemppdclmbnft,\r\n    pdtemppdnomedicalclmbnft,\r\n    pdtemppdsrvlbnft\r\n  },\r\n  watch: {\r\n    paymentConfigs: function () {\r\n      //获取赔付保障最大赔付顺序\r\n      getBnftMaxOrder(this.buildRequestData({\"pdInfoId\": this.pdtempPdInfo.id})).then(res => {\r\n        this.configLength = res.data\r\n      })\r\n      //获取生存保障最大赔付顺序\r\n      getSrvlMaxOrder(this.buildRequestData({\"pdInfoId\": this.pdtempPdInfo.id})).then(res => {\r\n        this.existLength = res.data\r\n      })\r\n      //this.bnftForm = this.paymentConfigs[0]\r\n      //pdBnftType筛选\r\n      this.filters_pdBnftType = []\r\n      for (const row of this._.map(this.paymentConfigs, 'pdBnftType')) {\r\n        let filDepmt = {text: '', value: ''}\r\n        filDepmt.text = this.getCodeName('pdBnftType', row, true)\r\n        filDepmt.value = row\r\n        let flag = false\r\n        for (const filters_pdBnftTypeItem of this.filters_pdBnftType) {\r\n          if (filters_pdBnftTypeItem.value === row) {\r\n            flag = true\r\n          }\r\n        }\r\n        if (!flag) {\r\n          this.filters_pdBnftType.push(filDepmt)\r\n        }\r\n      }\r\n      //pdBnftDetailType筛选\r\n      this.filters_pdBnftDetailType = []\r\n      for (const row of this._.map(this.paymentConfigs, 'pdBnftDetailType')) {\r\n        let filDepmt = {text: '', value: ''}\r\n        filDepmt.text = this.getCodeName('pdclmbnfttype2', row, true)\r\n        filDepmt.value = row\r\n        let flag = false\r\n        for (const filters_pdBnftDetailTypeItem of this.filters_pdBnftDetailType) {\r\n          if (filters_pdBnftDetailTypeItem.value === row) {\r\n            flag = true\r\n          }\r\n        }\r\n        if (!flag) {\r\n          this.filters_pdBnftDetailType.push(filDepmt)\r\n        }\r\n      }\r\n      //pdAmntCalcMode筛选\r\n      this.filters_pdAmntCalcMode = []\r\n      for (const row of this._.map(this.paymentConfigs, 'pdAmntCalcMode')) {\r\n        let filDepmt = {text: '', value: ''}\r\n        filDepmt.text = this.getCodeName('pdAmntCalMode', row, true)\r\n        filDepmt.value = row\r\n        let flag = false\r\n        for (const filters_pdAmn