le-back :isDialog=\"true\" :label=\"$t('accountFeeFormulaSelect.pdAccFormulaSel')\" @backTo=\"back\"/>\r\n\r\n    <!-- 搜索 -->\r\n    <el-form ref=\"params\" :model=\"params\" label-position=\"right\" label-width=\"auto\" style=\"margin-top: 20px\">\r\n      <el-row :gutter=\"10\">\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('pdDiscountFormulaSelect.productDetailsType')\">\r\n            <cus-select v-model=\"params.eq_productDetailType\" :codetype=\"'ProductCategory'\"\r\n                        @change=\"productDetailTypeChanged\"></cus-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('accountFeeFormulaSelect.specifiedProduct')\">\r\n            <el-select v-model=\"params.eq_pdInfoId\" clearable filterable @change=\"selectedProductChanged\">\r\n              <el-option v-for=\"product in productList\" :key=\"product.id\" :label=\"product.pdCode+':'+product.pdName\"\r\n                         :value=\"product.id\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col v-if=\"formulaType === '33'\" :span=\"8\">\r\n          <el-form-item :label=\"$t('accountFeeFormulaSelect.designAccountFeesOpt')\">\r\n            <el-select v-model=\"params.eq_pdAccountFeeId\" clearable filterable>\r\n              <el-option v-for=\"pdAccountFee in pdAccountFeeList\" :key=\"pdAccountFee.id\"\r\n                         :label=\"pdAccountFee.pdAccFeeCode + ':' + pdAccountFee.pdAccFeeName\"\r\n                         :value=\"pdAccountFee.id\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col v-if=\"formulaType === '34'\" :span=\"8\">\r\n          <el-form-item :label=\"$t('accountFeeFormulaSelect.designInOpt')\">\r\n            <el-select v-model=\"params.eq_inFlowId\" clearable filterable>\r\n              <el-option v-for=\"inFlow in inFlowList\" :key=\"inFlow.id\"\r\n                         :label=\"inFlow.pdAccCapItemCode+':'+inFlow.pdAccCapItemName\"\r\n                         :value=\"inFlow.id\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col v-if=\"formulaType === '35'\" :span=\"8\">\r\n          <el-form-item :label=\"$t('accountFeeFormulaSelect.designOutOpt')\">\r\n            <el-select v-model=\"params.eq_outFlowId\" clearable filterable>\r\n              <el-option v-for=\"outFlow in outFlowList\" :key=\"outFlow.id\"\r\n                         :label=\"outFlow.pdAccCapItemCode + ':' + outFlow.pdAccCapItemName\"\r\n                         :value=\"outFlow.id\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col v-if=\"formulaType === '66'\" :span=\"8\">\r\n          <el-form-item :label=\"$t('accountFeeFormulaSelect.designInFeesOpt')\">\r\n            <el-select v-model=\"params.eq_pdAccCapFeeInId\" clearable filterable>\r\n              <el-option v-for=\"item in pdAccCapFeeList\" :key=\"item.id\"\r\n                         :label=\"item.pdAccCapItemCode + ':' + item.pdAccCapItemName + ':' + item.pdAccFeeCode + ':' + item.pdAccFeeName\"\r\n                         :value=\"item.id\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col v-if=\"formulaType === '67'\" :span=\"8\">\r\n          <el-form-item :label=\"$t('accountFeeFormulaSelect.designOutFeesOpt')\">\r\n            <el-select v-model=\"params.eq_pdAccCapFeeOutId\" clearable filterable>\r\n              <el-option v-for=\"item in pdAccCapFeeList\" :key=\"item.id\"\r\n                         :label=\"item.pdAccCapItemCode + ':' + item.pdAccCapItemName + ':' + item.pdAccFeeCode + ':' + item.pdAccFeeName\"\r\n                         :value=\"item.id\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row :gutter=\"10\">\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('i18nView.formulaDetial')\">\r\n            <cus-select v-model=\"params.pdBussDetailType\" codetype=\"ruleBusinessDetailClass\" disabled\r\n                        filter=\"1$33$34$35$66$67\"></cus-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('accountFeeFormulaSelect.specifiedCalcCode')\">\r\n            <el-input v-model=\"params.like_pdCalcCode\" clearable></el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\" style=\"text-align: right\">\r\n          <el-button icon=\"el-icon-search\" size=\"small\" type=\"primary\" @click=\"search\">\r\n            {{ $t('table.search') }}\r\n          </el-button>\r\n        </el-col>\r\n      </el-row>\r\n    </el-form>\r\n\r\n    <cus-table-title :label=\"$t('accountFeeFormulaSelect.AccountFormula')\"/>\r\n    <el-table :data=\"tableData_formula\" border element-loading-text=\"Loading\" fit highlight-current-row max-height=\"300\"\r\n              size=\"mini\">\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" width=\"50\">\r\n        <template v-slot=\"scope\">\r\n          {{ initTableIndex('page', scope.$index) }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.detail')\" align=\"center\" width=\"100\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button size=\"small\" type=\"text\" @click=\"showFormula(scope.row)\">>></el-button>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :filter-method=\"filters_method\" :filters=\"filters_pdCode\"\r\n                       :label=\"$t('accountFeeFormulaSelect.product')\"\r\n                       align=\"center\"\r\n                       min-width=\"200\" prop=\"pdCode\" sortable>\r\n        <template v-slot=\"scope\">\r\n          {{ scope.row.pdCode ? scope.row.pdCode + ':' + scope.row.pdName : '' }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :filter-method=\"filters_method\" :filters=\"filters_accountFee\"\r\n                       :label=\"$t('accountFeeFormulaSelect.feesOption')\"\r\n                       align=\"center\"\r\n                       min-width=\"200\" prop=\"accountFeeCode\" sortable>\r\n        <template v-slot=\"scope\">\r\n          <span v-if=\"scope.row.accountFeeCode\">{{ scope.row.accountFeeCode + ':' + scope.row.accountFeeName }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :filter-method=\"filters_method\" :filters=\"filters_inFlow\"\r\n                       :label=\"$t('accountFeeFormulaSelect.pdAccCapInf')\"\r\n                       align=\"center\"\r\n                       min-width=\"200\" prop=\"inFlowCode\" sortable>\r\n        <template v-slot=\"scope\">\r\n          <span v-if=\"scope.row.inFlowCode\">{{ scope.row.inFlowCode + ':' + scope.row.inFlowName }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :filter-method=\"filters_method\" :filters=\"filters_outFlow\"\r\n                       :label=\"$t('accountFeeFormulaSelect.pdAccCapOutf')\"\r\n                       align=\"center\"\r\n                       min-width=\"200\" prop=\"outFlowCode\" sortable>\r\n        <template v-slot=\"scope\">\r\n          <span v-if=\"scope.row.outFlowCode\">{{ scope.row.outFlowCode + ':' + scope.row.outFlowName }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('rule.pdBussDetailType')\" align=\"center\" min-width=\"200\" prop=\"pdBussDetailType\">\r\n        <template v-slot=\"scope\">\r\n          {{ scope.row.pdBussDetailType | filter_pdBussDetailType }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('accountFeeFormulaSelect.pdCalcCode')\" align=\"center\" min-width=\"200\"\r\n                       prop=\"pdCalcCode\" sortable></el-table-column>\r\n      <el-table-column :label=\"$t('accountFeeFormulaSelect.pdCalcDesc')\" align=\"center\" min-width=\"200\"\r\n                       prop=\"pdCalcDesc\"></el-table-column>\r\n      <el-table-column :label=\"$t('accountFeeFormulaSelect.rulesAttr')\" align=\"center\" min-width=\"200\">\r\n        <template v-slot=\"scope\">\r\n          {{\r\n            scope.row.ruleAttr === '1' ? $t('rule.general') : (scope.row.ruleAttr === '2' ? $t('rule.individual') : '')\r\n          }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('table.actions')\" align=\"center\" fixed=\"right\" width=\"120\">\r\n        <template v-slot=\"scope\">\r\n          <el-button v-auth=\"'pdtemp-pd-acc-formula-select:associate'\" icon=\"el-icon-paperclip\" size=\"mini\"\r\n                     type=\"primary\"\r\n                     @click=\"handleSelectFormula(scope.row)\">\r\n            {{ $t('form.select') }}\r\n          </el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- 分页 -->\r\n    <cus-pagination ref=\"page\" v-model=\"tableData_formula\" :autoBuild=\"false\" :pageSize=\"5\"\r\n                    :params=\"params\" url=\"/pdtemp-pd-acc-formula-select/base/page\"/>\r\n\r\n    <!-- 规则引擎公式定义弹窗 -->\r\n    <cus-rule-definition :attributeObj=\"attributeObj\" :ruleDefinitionClosed=\"ruleModuleClosed\"\r\n                         :visible=\"formulaVisible\"></cus-rule-definition>\r\n\r\n    <!-- 公式查看弹窗 -->\r\n    <cus-rule-modify :flag=\"formulaView.flag\" :performType=\"formulaView.performType\" :ruleCode=\"formulaView.ruleCode\"\r\n                     :ruleModifyClosed=\"ruleMakeClosed\" :ruleType=\"'2'\"\r\n                     :visible=\"formulaView.visible\"></cus-rule-modify>\r\n\r\n  </cus-wraper>\r\n</template>\r\n\r\n<script>\r\n\r\nimport {associatePdAccCalc, findAllProductByDetailType, getPdAccDetails} from \"@/api/basicinfo/accountFeeFormulaSelect\"\r\nimport ruleModule from '@/views/ibrms/ruleManagement/ruleModule/index'\r\nimport ruleMake from '@/views/ibrms/ruleManagement/ruleMake/index'\r\n\r\nlet _this\r\n\r\nexport default {\r\n  name: 'accountFormulaSelect',\r\n  components: {ruleModule, ruleMake},\r\n  filters: {\r\n    filter_pdBussType(code) {\r\n      return _this.getCodeName('pdCalcBussType', code)\r\n    },\r\n    filter_pdBussDetailType(code) {\r\n      return _this.getCodeName('ruleBusinessDetailClass', code)\r\n    }\r\n  },\r\n  props: {\r\n    bussObject: {\r\n      type: Object,\r\n      required: false,\r\n      default: () => {\r\n        return {}\r\n      }\r\n    },\r\n    formulaType: {\r\n      type: String,\r\n      required: false,\r\n      default: ''\r\n    },\r\n    revisionMark: {\r\n      type: Boolean,\r\n      required: false,\r\n      default: false\r\n    }\r\n  },\r\n  data() {\r\n    _this = this\r\n    return {\r\n      filters_pdCode: [],\r\n      filters_account: [],\r\n      filters_accountFee: [],\r\n      filters_inFlow: [],\r\n      filters_outFlow: [],\r\n      attributeObj: {},\r\n      formulaVisible: false,\r\n      formulaView: {\r\n        flag: '',\r\n        ruleCode: '',\r\n        performType: '',\r\n        visible: false\r\n      },\r\n      // 产品信息\r\n      product: {\r\n        id: '',\r\n        bType: '',\r\n        cType: '',\r\n        pdCode: '',\r\n        pdEdition: '',\r\n        pdVerId: '',\r\n        pdSalechnl: '',\r\n        pdName: '',\r\n        pdShortname: '',\r\n        pdEnName: '',\r\n        pdShortEnName: '',\r\n        operator: '',\r\n        pdApplDate: '',\r\n        pdProcessId: '',\r\n        createTime: '',\r\n        updateTime: ''\r\n      },\r\n      params: {\r\n        currentProductId: undefined,\r\n        currentBussId: undefined,\r\n        pdBussDetailType: '',\r\n        eq_productDetailType: '',\r\n        eq_pdInfoId: undefined,\r\n        eq_pdAccountFeeId: undefined,\r\n        eq_pdAccCapFeeInId: undefined,\r\n        eq_pdAccCapFeeOutId: undefined,\r\n        eq_inFlowId: undefined,\r\n        eq_outFlowId: undefined,\r\n        like_pdCalcCode: ''\r\n      },\r\n      tableData_formula: [],\r\n      productList: [],\r\n      pdAccList: [],\r\n      pdAccountFeeList: [],\r\n      pdAccCapFeeList: [],\r\n      inFlowList: [],\r\n      outFlowList: []\r\n    }\r\n  },\r\n  created() {\r\n    this.init()\r\n  },\r\n  watch: {\r\n    tableData_formula() {\r\n      // 初始化产品列的筛选器\r\n      for (const formulaVo of this.tableData_formula) {\r\n        if (formulaVo.pdCode && !this._.includes(this._.map(this.filters_pdCode, 'value'), formulaVo.pdCode)) {\r\n          this.filters_pdCode.push({\r\n            text: formulaVo.pdCode + ':' + formulaVo.pdName,\r\n            value: formulaVo.pdCode\r\n          })\r\n        }\r\n      }\r\n      // 初始化账户列的筛选器\r\n      for (const formulaVo of this.tableData_formula) {\r\n        if (formulaVo.accountCode && !this._.includes(this._.map(this.filters_account, 'value'), formulaVo.accountCode)) {\r\n          this.filters_account.push({\r\n            text: formulaVo.accountCode + ':' + formulaVo.accountName,\r\n            value: formulaVo.accountCode\r\n          })\r\n        }\r\n      }\r\n      // 初始化费用列的筛选器\r\n      for (const formulaVo of this.tableData_formula) {\r\n        if (formulaVo.accountFeeCode && !this._.includes(this._.map(this.filters_accountFee, 'value'), formulaVo.accountFeeCode)) {\r\n          this.filters_accountFee.push({\r\n            text: formulaVo.accountFeeCode + ':' + formulaVo.accountFeeName,\r\n            value: formulaVo.accountFeeCode\r\n          })\r\n        }\r\n      }\r\n      // 初始化资金流入列的筛选器\r\n      for (const formulaVo of this.tableData_formula) {\r\n        if (formulaVo.inFlowCode && !this._.includes(this._.map(this.filters_inFlow, 'value'), formulaVo.inFlowCode)) {\r\n          this.filters_inFlow.push({\r\n            text: formulaVo.inFlowCode + ':' + formulaVo.inFlowName,\r\n            value: formulaVo.inFlowCode\r\n          })\r\n        }\r\n      }\r\n      // 初始化资金流出列的筛选器\r\n      for (const formulaVo of this.tableData_formula) {\r\n        if (formulaVo.outFlowCode && !this._.includes(this._.map(this.filters_outFlow, 'value'), formulaVo.outFlowCode)) {\r\n          this.filters_outFlow.push({\r\n            text: formulaVo.outFlowCode + ':' + formulaVo.outFlowName,\r\n            value: formulaVo.outFlowCode\r\n          })\r\n        }\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    ruleModuleClosed() {\r\n      this.formulaVisible = false\r\n      this.search()\r\n    },\r\n    ruleMakeClosed() {\r\n      this.formulaView.visible = false\r\n    },\r\n    showFormula(row) {\r\n      this.formulaView.flag = '1'\r\n      this.formulaView.performType = row.performType\r\n      this.formulaView.ruleCode = row.pdCalcCode\r\n      this.formulaView.visible = true\r\n    },\r\n    async init() {\r\n      // 获取产品信息\r\n      this.product = this.getPdData()\r\n      this.params.currentProductId = this.product.id\r\n      this.params.currentBussId = this.bussObject.id\r\n      this.params.pdBussDetailType = this.formulaType\r\n      this.params.eq_productDetailType = this.product.cType\r\n      this.productDetailTypeChanged(this.product.cType)\r\n      if (Object.keys(this.bussObject).length === 0) {\r\n        this.notifyError(this.$t('accountFeeFormulaSelect.invalidBussItemInfo'))\r\n        return\r\n      }\r\n      this.$nextTick(() => {\r\n        this.selectedProductChanged()\r\n      })\r\n    },\r\n    back() {\r\n      this.$emit('changeDialogVisible_formulaSelect', false)\r\n    },\r\n    search() {\r\n      this.$refs.page.reload()\r\n    },\r\n    productDetailTypeChanged(val) {\r\n      this.params.eq_pdInfoId = undefined\r\n      this.productList = []\r\n      findAllProductByDetailType(this.buildRequestData({productDetailType: val})).then(response => {\r\n        this.productList = response.data\r\n      })\r\n    },\r\n    selectedProductChanged() {\r\n      this.pdAccountFeeList = []\r\n      this.pdAccCapFeeList = []\r\n      this.inFlowList = []\r\n      this.outFlowList = []\r\n      if (!this.params.eq_pdInfoId) {\r\n        return\r\n      }\r\n      const param = {\r\n        productId: this.params.eq_pdInfoId\r\n      }\r\n      getPdAccDetails(this.buildRequestData(param)).then(response => {\r\n        let map = response.data\r\n        this.pdAccountFeeList = map.pdAccountFeeList\r\n        this.pdAccCapFeeList = map.pdAccCapFeeList\r\n        this.inFlowList = map.inFlowList\r\n        this.outFlowList = map.outFlowList\r\n      })\r\n    },\r\n    resetParams() {\r\n      this.params = {\r\n        currentProductId: undefined,\r\n        currentBussId: undefined,\r\n        pdBussDetailType: '',\r\n        eq_productDetailType: '',\r\n        eq_pdInfoId: undefined,\r\n        eq_pdAccountFeeId: undefined,\r\n        eq_pdAccCapFeeInId: undefined,\r\n        eq_pdAccCapFeeOutId: undefined,\r\n        eq_inFlowId: undefined,\r\n        eq_outFlowId: undefined,\r\n        like_pdCalcCode: ''\r\n      }\r\n    },\r\n    handleSelectFormula(row) {\r\n      const param = {\r\n        product: this.product,\r\n        formula: row,\r\n        pdBussDetailType: this.formulaType,\r\n        pdBussId: this.bussObject.id,\r\n        needChecking: true\r\n      }\r\n      associatePdAccCalc(this.buildRequestData(param)).then(response => {\r\n        let flag = response.data.flag\r\n        let pdtempPdCalc = response.data.pdtempPdCalc\r\n        if (flag === true) {\r\n          this.notifySuccess(this.$t('accountFeeFormulaSelect.associateFormulaSuccess'))\r\n          this.$emit('bringInCalcCode', pdtempPdCalc.pdBussDetailType, pdtempPdCalc.pdBussId, pdtempPdCalc.pdCalcCode)\r\n          this.search()\r\n        } else {\r\n          this.$confirm(String(this.$t('accountFeeFormulaSelect.confirm_overwrite_formula')), String(this.$t('confirmMessage.tip')), {\r\n            confirmButtonText: String(this.$t('confirmMessage.confirm')),\r\n            cancelButtonText: String(this.$t('confirmMessage.cancel')),\r\n            type: 'warning'\r\n          }).then(() => {\r\n            param.needChecking = false\r\n            associatePdAccCalc(this.buildRequestData(param)).then(response => {\r\n              let pdtempPdCalc = response.data.pdtempPdCalc\r\n              this.notifySuccess(this.$t('accountFeeFormulaSelect.associateFormulaSuccess'))\r\n              this.$emit('bringInCalcCode', pdtempPdCalc.pdBussDetailType, pdtempPdCalc.pdBussId, pdtempPdCalc.pdCalcCode)\r\n              this.search()\r\n            })\r\n          }).catch(() => {\r\n            this.messageInfo(this.$t('accountFeeFormulaSelect.canceled'))\r\n          })\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n.el-select {\r\n  width: 220px\r\n}\r\n\r\n.el-input {\r\n  width: 220px\r\n}\r\n\r\n</style>\r\n"]}]}