> 0) {\r\n        this.monitorShow = true\r\n    }\r\n\r\n    let active = this.getStrData('active')\r\n    if (active) {\r\n      this.active = active\r\n    }\r\n    this.switchTab(this.active)\r\n  },\r\n  methods: {\r\n    switchTab(type) {\r\n      this.processShow = false\r\n      switch (type) {\r\n        case 'ToDoTasks':\r\n          this.flag1 = true\r\n          this.flag2 = false\r\n          this.flag3 = false\r\n          this.flag4 = false\r\n          break\r\n        case 'TaskAssignment':\r\n          this.flag1 = false\r\n          this.flag2 = true\r\n          this.flag3 = false\r\n          this.flag4 = false\r\n          break\r\n        case 'ProductOverview':\r\n          this.flag1 = false\r\n          this.flag2 = false\r\n          this.flag3 = true\r\n          this.flag4 = false\r\n          break\r\n        case 'SinglePdProcess':\r\n          this.flag1 = false\r\n          this.flag2 = false\r\n          this.flag3 = false\r\n          this.flag4 = true\r\n          break\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]}]}