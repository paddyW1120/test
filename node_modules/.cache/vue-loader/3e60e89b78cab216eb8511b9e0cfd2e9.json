roduct-definition/pd-framework-def/pd-structure-def/guarantee-def/general-medical/pdtemppdbnftinfo","sourcesContent":["<template>\r\n  <cus-wraper>\r\n    <cus-page-title-back :isDialog=\"true\" :label=\"$t('pdtempPdMedFeeBill.conClmBnftChoose')\" @backTo=\"backToMenu\"></cus-page-title-back>\r\n    <!-- <el-row>\r\n      <el-col :span=\"24\">\r\n        <el-button size=\"small\" type=\"text\" @click=\"backToMenu\"><i class=\"el-icon-back\"></i>{{ \"返回\" }}</el-button>\r\n        <label style=\"font-size:14px\">保障明细</label>\r\n      </el-col>\r\n    </el-row>\r\n    <hr/> -->\r\n    <br/>\r\n    <el-form ref=\"selectForm1\" :model=\"paymentForm\" :rules=\"rules\" label-width=\"120px\" style=\"width:98%;\">\r\n      <el-row :gutter=\"10\" class=\"row-bg\" justify=\"left\" type=\"flex\">\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('pdtempProgInfo.finelinesRegion')\">\r\n            <cus-select v-model=\"paymentForm.productTypeDetail\" :codetype=\"'ProductCategory'\" @change=\"findProducts\">\r\n            </cus-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('pdtempProgInfo.product')\" prop=\"product\">\r\n            <el-select v-model=\"paymentForm.product\" :placeholder=\"$t('pdtempProgInfo.pleaseChoose')\" clearable\r\n                       filterable>\r\n              <el-option v-for=\"item in productOptions\" :key=\"item.key\" :label=\"item.label\" :value=\"item.value\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-form-item :label=\"$t('i18nView.pdBnftDetailType')\">\r\n            <cus-select v-model=\"paymentForm.pdBnftDetailType\"\r\n                        :codetype=\"'pdclmbnfttype2'\"></cus-select>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n      <el-row>\r\n        <el-form-item style=\"text-align: right\">\r\n          <!-- 搜索 -->\r\n          <el-button icon=\"el-icon-search\" size=\"small\" type=\"primary\" @click=\"selectPdBnft\" dialog-disable=\"disable\">\r\n            {{ $t('pdtempProgInfo.search') }}\r\n          </el-button>\r\n        </el-form-item>\r\n      </el-row>\r\n    </el-form>\r\n    <cus-table-title :label=\"$t('pdtempProgInfo.chooseBnftinfo')\"/>\r\n    <el-table ref=\"bnftTable\" :data=\"bnftConfigs\" border element-loading-text=\"Loading\" fit highlight-current-row\r\n              size=\"mini\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\" width=\"50\"></el-table-column>\r\n      <el-table-column :label=\"$t('table.detail')\" align=\"center\" style=\"width: 50%\" type=\"expand\">\r\n        <template slot-scope=\"props\">\r\n          <el-form class=\"demo-table-expand\" inline label-position=\"left\">\r\n            <el-form-item :label=\"$t('i18nView.pdCode')\" style=\"width: 50%\">\r\n              {{ props.row.pdCode }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('customComponents.pdEdition')\" style=\"width: 50%\">\r\n              {{ props.row.pdEdition }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdName')\" style=\"width: 50%\">\r\n              {{ props.row.pdName }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdClmBnftOrder')\" style=\"width: 50%\">\r\n              {{ props.row.pdClmBnftOrder }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdClmBnftType')\" style=\"width: 50%\">\r\n              {{ props.row.pdClmBnftType | filter_pdClmBnftType}}\r\n            </el-form-item>\r\n          </el-form>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('pdtempProgInfo.pdBnftCode')\" align=\"center\"\r\n                       prop=\"pdBnftCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('pdtempProgInfo.pdBnftName')\" align=\"center\"\r\n                       prop=\"pdBnftName\"></el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.pdBnftType')\" align=\"center\"\r\n                       prop=\"pdBnftType\">\r\n        <template v-slot=\"scope\">{{ scope.row.pdBnftType | filter_pdBnftType }}</template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('pdtempProgInfo.pdBnftCode')\" align=\"center\"\r\n                       prop=\"pdClmBnftCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.bnftDtName')\" align=\"center\"\r\n                       prop=\"pdClmBnftName\"></el-table-column>\r\n    </el-table>\r\n    <cus-pagination ref=\"page\" v-model=\"bnftConfigs\" :pageSize=\"5\" :params=\"paymentForm\"\r\n                    url=\"/pdtemp-pd-claim-verification-rule/base/selectPdtempPdClmBnft\"/>\r\n    <br/>\r\n    <el-form>\r\n      <el-form-item style=\"text-align: right\">\r\n        <el-button v-auth=\"'select:AllPdAccount'\" icon=\"el-icon-circle-plus-outline\" size=\"small\" type=\"primary\" dialog-disable=\"disable\"\r\n                   @click=\"addPdProg\">\r\n          {{ $t('pdtempProgInfo.add') }}\r\n        </el-button>\r\n        <el-button icon=\"el-icon-refresh\" size=\"small\" type=\"primary\" @click=\"researchData\" dialog-disable=\"disable\">\r\n          {{ $t('pdtempProgInfo.refresh') }}\r\n        </el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n    <cus-table-title :label=\"$t('pdtempProgInfo.AssBnftinfo')\"/>\r\n    <el-table ref=\"paymentTable\" :data=\"paymentConfigs\" border element-loading-text=\"Loading\" fit highlight-current-row\r\n              max-height=\"300px\" size=\"mini\">\r\n      <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\" width=\"50\"></el-table-column>\r\n      <el-table-column :label=\"$t('table.detail')\" align=\"center\" style=\"width: 50%\" type=\"expand\">\r\n        <template slot-scope=\"props\">\r\n          <el-form class=\"demo-table-expand\" inline label-position=\"left\">\r\n            <el-form-item :label=\"$t('i18nView.pdCode')\" style=\"width: 50%\">\r\n              {{ props.row.pdCode }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('customComponents.pdEdition')\" style=\"width: 50%\">\r\n              {{ props.row.pdEdition }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdName')\" style=\"width: 50%\">\r\n              {{ props.row.pdName }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdClmBnftOrder')\" style=\"width: 50%\">\r\n              {{ props.row.pdClmBnftOrder }}\r\n            </el-form-item>\r\n            <el-form-item :label=\"$t('i18nView.pdClmBnftType')\" style=\"width: 50%\">\r\n              {{ props.row.pdClmBnftType | filter_pdClmBnftType}}\r\n            </el-form-item>\r\n          </el-form>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.pdCode')\" align=\"center\" prop=\"pdCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.pdName')\" align=\"center\"  prop=\"pdName\"></el-table-column>\r\n      <!-- <el-table-column :label=\"$t('pdtempProgInfo.pdBnftType')\" align=\"center\" label=\"保障类型\"\r\n                       prop=\"pdBnftType\">\r\n        <template v-slot=\"scope\">{{ scope.row.pdBnftType | filter_pdBnftType }}</template>\r\n      </el-table-column> -->\r\n      <el-table-column :label=\"$t('pdtempProgInfo.pdBnftCode')\" align=\"center\"\r\n                       prop=\"pdBnftCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('pdtempProgInfo.pdBnftName')\" align=\"center\"\r\n                       prop=\"pdBnftName\"></el-table-column>\r\n      <el-table-column :label=\"$t('pdtempProgInfo.pdBnftDetailType')\" align=\"center\"\r\n                       prop=\"pdBnftDetailType\">\r\n        <template v-slot=\"scope\">{{ scope.row.pdBnftDetailType | filter_pdBnftDetailType }}</template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('pdtempProgInfo.pdBnftCode')\" align=\"center\"\r\n                       prop=\"pdClmBnftCode\"></el-table-column>\r\n      <el-table-column :label=\"$t('i18nView.bnftDtName')\" align=\"center\"\r\n                       prop=\"pdClmBnftName\"></el-table-column>\r\n      <el-table-column align=\"center\" :label=\"$t('pdtempProgInfo.operating')\" width=\"100\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button size=\"mini\" type=\"danger\" @click=\"deleteRela(scope.row)\" dialog-disable=\"disable\">-</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  </cus-wraper>\r\n</template>\r\n<script>\r\n  import {findProductsByProductType} from \"@/api/basicinfo/pdtemppdclmbnft\";\r\n  import {findBusinessItemByCtrlCode} from \"@/api/basicinfo/productCompensation\"\r\n\r\n  let _this\r\n\r\n  export default {\r\n    filters: {\r\n      filter_pdBnftType(code) {\r\n        return _this.getCodeName('pdBnftType', code);\r\n      },\r\n      filter_pdBnftDetailType(code) {\r\n        return _this.getCodeName('pdclmbnfttype2', code);\r\n      },\r\n      filter_pdClmBnftType(code) {\r\n      return _this.getCodeName(\"pdclmbnfttype1\", code)\r\n    },\r\n    },\r\n    name: \"pdtemppdbnftinfo\",\r\n    props: {\r\n      bnftForm1:{\r\n        type:Object,\r\n        default:()=>{\r\n          return {}\r\n        }\r\n      },\r\n      moreType: {\r\n        type: String,\r\n        default: ''\r\n      },\r\n      pdBnftCtrlCode: {\r\n        type: String,\r\n        default: ''\r\n      }\r\n    },\r\n    data() {\r\n      _this = this\r\n      return {\r\n        pdtempPdInfo: {},\r\n        paymentForm: {\r\n          productTypeDetail: '',\r\n          product: '',\r\n          pdBnftDetailType: '',\r\n        },\r\n        paymentConfigs: [],\r\n        bnftConfigs: [],\r\n        productOptions: [],\r\n        chooseBnft: [],\r\n        chooseProg: [],\r\n        rules:{\r\n        product: [\r\n          {required: true, message: \"字段不能为空\", trigger: \"blur\"}\r\n        ],\r\n      },\r\n      };\r\n    },\r\n    created() {\r\n      this.pdtempPdInfo = this.getPdData();\r\n      this.paymentForm.productTypeDetail = this.pdtempPdInfo.cType;\r\n      this.findProducts(this.paymentForm.productTypeDetail);\r\n      this.paymentForm.pdBnftDetailType = this.bnftForm1.pdBnftDetailType;\r\n      this.initPdBnft();\r\n    },\r\n    methods: {\r\n      //初始化关联方案信息\r\n      initPdBnft() {\r\n        if (this.pdBnftCtrlCode) {\r\n          findBusinessItemByCtrlCode(this.pdBnftCtrlCode).then(response => {\r\n            this.paymentConfigs = response.data\r\n          })\r\n        }\r\n      },\r\n      //保存产品方案\r\n      addPdProg() {\r\n        for (const item of this.chooseProg) {\r\n          let duplicated = false\r\n          for (const selectedItem of this.paymentConfigs) {\r\n            if (selectedItem.id === item.id) {\r\n              setTimeout(() => {\r\n                this.notifyError(item.pdBnftCode + ':' + item.pdBnftName + '已添加，请勿重复添加')\r\n              }, 1)\r\n              duplicated = true\r\n              break\r\n            }\r\n          }\r\n          if (!duplicated) {\r\n            this.paymentConfigs.push(item)\r\n          }\r\n        }\r\n      },\r\n      //产品搜索\r\n      selectPdBnft() {\r\n        this.$refs[\"selectForm1\"].validate(valid => {\r\n        if(valid){\r\n          this.$refs.page.reload()\r\n        }\r\n      });\r\n      },\r\n      findProducts(val) {\r\n        let data = {\r\n          pdCls2: val\r\n        }\r\n        findProductsByProductType(this.buildRequestData(data)).then(res => {\r\n          this.productOptions = [];\r\n          res.data.forEach(product => {\r\n            this.productOptions.push({\r\n              value: product.pdCode,\r\n              label: product.pdCode + \":\" + product.pdName,\r\n              key: product.id\r\n            });\r\n          });\r\n        })\r\n      },\r\n      researchData() {\r\n        this.paymentForm.productTypeDetail = ''\r\n        this.paymentForm.product = ''\r\n        this.bnftConfigs = []\r\n        this.$refs.page.reload()\r\n      },\r\n      deleteRela(row) {\r\n        this.paymentConfigs.splice(this.paymentConfigs.indexOf(row), 1)\r\n      },\r\n      handleSelectionChange(row) {\r\n        this.chooseProg = row;\r\n      },\r\n      backToMenu() {\r\n        this.$emit('backToFeeBill', this.moreType, this.paymentConfigs)\r\n      }\r\n    }\r\n  };\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n  .divider {\r\n    color: black;\r\n    background-color: black;\r\n    width: 1px;\r\n  }\r\n</style>\r\n"]}]}