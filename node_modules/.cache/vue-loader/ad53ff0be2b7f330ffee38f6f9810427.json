" label-width=\"120px\" ref=\"form_dbl\" :model=\"form_dbl\">\r\n        <div>\r\n          <!-- <h5>\r\n            <el-divider class=\"divider\" direction=\"vertical\"></el-divider>\r\n            {{$t('pdtempPdDbl.pdDblMenu')}}\r\n          </h5> -->\r\n          <!-- <el-row >\r\n            <el-col :span=\"24\">\r\n              <el-form-item :label=\"$t('pdtempPdDbl.pdDblType')\">\r\n                <el-select  clearable filterable v-model=\"form_dbl.pdDblType\" value-key=\"id\">\r\n                  <el-option :label=\"item.code+':'+item.codeName\"\r\n                             :value=\"item.code\"\r\n                             v-for=\"item in pdDblTypeOptions\">\r\n                  </el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n            </el-col>\r\n          </el-row> -->\r\n          <div>\r\n          <cus-table-title :label=\"$t('pdtempPdDbl.pdDblTable')\"></cus-table-title>\r\n          <el-table :data=\"dblConfigs\"  @row-click=\"rowClick_dbl\" align=\"center\" border class=\"wraper-table\" element-loading-text=\"Loading\"\r\n                    fit highlight-current-row ref=\"pdgenlTable\" size=\"mini\">\r\n            <el-table-column width=\"60\"  align=\"center\" :label=\"$t('pdtempPdDbl.selected')\">\r\n              <template slot-scope=\"scope\">\r\n                <el-radio-group @change=\"selectedDblChanged\" v-model=\"selectedDbl\">\r\n              <span>\r\n                <el-radio :label=\"scope.row\">{{''}}</el-radio>\r\n              </span>\r\n                </el-radio-group>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('table.id')\" align=\"center\" type=\"index\" width=\"50\"></el-table-column>\r\n            <el-table-column :label=\"$t('pdtempPdDbl.pdDblType')\" align=\"center\" prop=\"pdDblType\" sortable :filter-method=\"filterHandler\" :filters=\"feeTypeFilters1\">\r\n              <template v-slot=\"scope\">{{ scope.row.pdDblType }}:{{ scope.row.codeName }}</template>\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('pdtempPdDbl.pdDblGrade')\" align=\"center\" prop=\"pdDblGrade\" sortable :filter-method=\"filterHandler\" :filters=\"feeTypeFilters2\">\r\n              <template v-slot=\"scope\">{{ scope.row.pdDblGrade}}:{{ scope.row.dblGrdName}}</template>\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('pdtempPdDbl.pdDblRate')\" align=\"center\" prop=\"pdDblRate\">\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('pdtempPdDbl.pdDblDetails')\"  align=\"center\" width=\"120px\">\r\n              <template slot-scope=\"scope\">\r\n                <el-button  size=\"small\" @click=\"showItemRow(scope.row)\" type=\"text\">></el-button>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('table.actions')\" align=\"center\" width=\"100px\">\r\n              <template v-slot=\"scope\">\r\n                <cus-popover :content=\"$t('confirmMessage.delete')\" @confirm=\"delDblRow(scope.row.id)\" icon=\"el-icon-delete\" v-auth=\"'pdtemp-pd-dbl:delete'\" type=\"danger\"></cus-popover>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n            <br>\r\n            <cus-form-title :label=\"$t('pdtempPdDbl.pdDblMenu')\"></cus-form-title>\r\n            <cus-row>\r\n              <cus-col :span=\"8\">\r\n              <el-form-item :label=\"$t('pdtempPdDbl.dblEdition')\" prop=\"dblTypeEdition\">\r\n            <el-col>\r\n              <el-select clearable\r\n                         v-model=\"form_dbl.dblTypeEdition\" @change=\"findDblType\">\r\n                <el-option v-for=\"item in typeEditionOptions\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\">\r\n                </el-option>\r\n              </el-select>\r\n            </el-col>\r\n          </el-form-item>\r\n            </cus-col>\r\n                <cus-col :span=\"8\">\r\n                <el-form-item :label=\"$t('pdtempPdDbl.pdDblType')\">\r\n                <el-select  clearable filterable v-model=\"form_dbl.pdDblType\" value-key=\"id\">\r\n                  <el-option :label=\"item.label\"\r\n                             :value=\"item.value\"\r\n                             v-for=\"item in pdDblTypeOptions\">\r\n                  </el-option>\r\n                </el-select>\r\n              </el-form-item>\r\n              </cus-col>\r\n              <cus-col :span=\"8\">\r\n              <el-form-item :label=\"$t('pdtempPdDbl.pdDblGrade')\">\r\n                  <el-select  clearable filterable  v-model=\"form_dbl.pdDblGrade\">\r\n                    <el-option :key=\"item.id\"\r\n                               :label=\"item.dblGrdCode+':'+item.dblGrdName\"\r\n                               :value=\"item.id\"\r\n                               v-for=\"item in pdDblGradeOptions\">\r\n                    </el-option>\r\n                  </el-select>\r\n                  <el-button size=\"small\" type=\"primary\" v-if=\"false\" @click=\"showItemButton\"><i class=\"el-icon-view el-icon-info\"></i></el-button>\r\n                </el-form-item>\r\n            </cus-col>\r\n            </cus-row>\r\n            <cus-row>\r\n              <el-col :span=\"8\">\r\n                  <el-form-item :label=\"$t('pdtempPdDbl.pdDblRate')\">\r\n                <el-input v-model=\"form_dbl.pdDblRate\">\r\n                  <template slot=\"suffix\">%</template>\r\n                </el-input>\r\n              </el-form-item>\r\n              </el-col>\r\n              <el-col :span=\"16\">\r\n              </el-col>\r\n            </cus-row>\r\n          </div>\r\n            <el-row style=\"float:right\">\r\n              <el-button  size=\"small\" @click=\"saveDblForm\" icon=\"el-icon-success\" type=\"primary\" v-auth=\"'pdtemp-pd-dbl:add'\" v-show=\"!this.selectedDbl.id\">\r\n                {{$t('pdtempPdDbl.save')}}\r\n              </el-button>\r\n              <el-button  size=\"small\" @click=\"updateDblForm\" icon=\"el-icon-success\" type=\"primary\" v-auth=\"'pdtemp-pd-dbl:update'\" v-show=\"this.selectedDbl.id\">\r\n                {{$t('pdtempPdDbl.update')}}\r\n              </el-button>\r\n              <el-button  size=\"small\" @click=\"resetDblForm\" icon=\"el-icon-refresh\" type=\"primary\">\r\n                {{$t('pdtempPdDbl.refresh')}}\r\n              </el-button>\r\n            </el-row>\r\n        </div>\r\n        <el-dialog :visible.sync=\"showDbl\" @close=\"showDbl = false\" width=\"50%\" v-dialog-drag center :close-on-click-modal=\"false\"  append-to-body>\r\n          <el-table :data=\"dblCodeTable\" size=\"mini\" element-loading-text=\"Loading\" border align=\"center\">\r\n            <el-table-column :label=\"$t('table.id')\" type=\"index\" align=\"center\" width=\"60\"></el-table-column>\r\n            <el-table-column :label=\"$t('pdtempPdDbl.dblGrdItemCode')\" prop=\"dblGrdItemCode\" align=\"center\">\r\n            </el-table-column>\r\n            <el-table-column :label=\"$t('pdtempPdDbl.dblGrdItemName')\"  prop=\"dblGrdItemName\" align=\"center\">\r\n            </el-table-column>\r\n          </el-table>\r\n        </el-dialog>\r\n      </el-form>\r\n    </cus-wraper>\r\n</template>\r\n<script>\r\n    import {addPdtempPdDbl,deletePdtempPdDbl,updatePdtempPdDbl,selectAllDblByType,findItemCodeList,selectpdDblTypeOptions,selectPdDblRate,finddblTypeEdition,findDblType} from '@/api/basicinfo/pdtemppddbl';\r\n    import {selectDblGradeByCode} from  \"@/api/basicinfo/pdtemppdnomedicalclmbnft\";\r\n    let _this;\r\n    export default {\r\n      name: 'pdtempPdDbl',\r\n      props: {\r\n        pdMedicalClmDetail: {\r\n          type: Object,\r\n          default: () => {\r\n            return {};\r\n          }\r\n        }\r\n      },\r\n      filters:{\r\n      },\r\n        data() {\r\n          _this = this\r\n            return {\r\n                //表格数据\r\n              dblConfigs:[],\r\n              dblCodeTable:[],\r\n              pdDblGradeOptions:[],\r\n              pdDblTypeOptions:[],\r\n              typeEditionOptions:[],\r\n              pdDblTypeData:[],\r\n              feeTypeFilters1:[],\r\n              feeTypeFilters2:[],\r\n              showDbl: false,\r\n              //hideButton:true,\r\n              selectedDbl:{\r\n                pdDblRate:'',\r\n              },\r\n              params: {\r\n                pdBnftCode: '',\r\n                pdBnftName:'',\r\n                pdClmBnftCode: '',\r\n                pdClmBnftName:''\r\n              },\r\n              itemParams:{\r\n                dblTypeCode:'',\r\n                dblGrdCode:''\r\n              },\r\n              form_dbl:{\r\n                id:'',\r\n                pdVerId:'',\r\n                operator:'',\r\n                pdClmBnftId:'',\r\n                pdClmBnftCode:'',\r\n                dblGradeId:'',\r\n                pdDblType:[],\r\n                pdDblGrade:'',\r\n                pdDblRate:'',\r\n                dblGradeId:''\r\n              },\r\n                //检验规则\r\n                rules: {\r\n\r\n                }\r\n            }\r\n        },\r\n      computed: {},\r\n      watch:{\r\n        'form_dbl.pdDblType'(){\r\n          this.form_dbl.pdDblGrade = []\r\n          this.form_dbl.pdDblRate = []\r\n          const val  =this.form_dbl.pdDblType;\r\n          this.dblTypeChange(val);\r\n          this.findAllDbl()\r\n        },\r\n        'form_dbl.pdDblGrade'(){\r\n          const val  =this.form_dbl.pdDblGrade;\r\n          this.selectPdDblRate(val);\r\n        },\r\n        'dblConfigs'(){\r\n          if (this.dblConfigs.length > 0) {\r\n          this.feeTypeFilters1 = [];\r\n          this.feeTypeFilters2 = [];\r\n          this.dblConfigs.forEach(val => {\r\n            this.feeTypeFilters1.push({\r\n              text: val.pdDblType+':'+val.codeName,\r\n              value: val.pdDblType\r\n            });\r\n          });\r\n          this.dblConfigs.forEach(val => {\r\n            this.feeTypeFilters2.push({\r\n              text: val.pdDblGrade+':'+val.dblGrdName,\r\n              value: val.pdDblGrade\r\n            });\r\n          });\r\n          this.feeTypeFilters1 = this.feeTypeFilters1.reduce(function (prev, element) {\r\n            if (!prev.find(el => el.text == element.text)) {\r\n              prev.push(element);\r\n            }\r\n            return prev;\r\n          }, []);\r\n          this.feeTypeFilters2 = this.feeTypeFilters2.reduce(function (prev, element) {\r\n            if (!prev.find(el => el.text == element.text)) {\r\n              prev.push(element);\r\n            }\r\n            return prev;\r\n          }, []);\r\n        }\r\n        }\r\n\r\n      },\r\n        created() {\r\n         this.params=this.pdMedicalClmDetail;\r\n         this.findAllDbl();\r\n          this.resetDblForm();\r\n         this.selectpdDblTypeOptions();\r\n         this.finddblTypeEdition();\r\n        },\r\n        methods: {\r\n          //查找伤残版别\r\n      finddblTypeEdition() {\r\n        let params = {\r\n          code_type: 'dbltype'\r\n        }\r\n        finddblTypeEdition(params).then(res => {\r\n          this.typeEditionOptions = []\r\n          res.data.forEach(typeEdition => {\r\n            this.typeEditionOptions.push({\r\n              value: typeEdition.codeVersion,\r\n              label: typeEdition.codeVersion\r\n            })\r\n          })\r\n        })\r\n      },\r\n      //根据伤残版别查伤残类型\r\n      findDblType(val) {\r\n        let params = {\r\n          code_version: val\r\n        }\r\n        findDblType(params).then(res => {\r\n          console.log(res)\r\n          this.pdDblTypeOptions = []\r\n          res.data.forEach(dblType => {\r\n            this.pdDblTypeOptions.push({\r\n              value: dblType.code,\r\n              label: dblType.code + ':' + dblType.codeName\r\n            })\r\n          })\r\n        })\r\n      },\r\n          selectPdDblRate(val){\r\n            const params = {\r\n              id: val,\r\n            }\r\n            selectPdDblRate(this.buildRequestData(params)).then(res =>{\r\n              if(res.data.length){\r\n                console.log(res.data)\r\n                this.form_dbl.pdDblRate = res.data[0].dblGrdRatio\r\n              }\r\n\r\n            })\r\n          },\r\n          selectpdDblTypeOptions(){\r\n            selectpdDblTypeOptions(this.buildRequestData()).then(res =>{\r\n              this.pdDblTypeData = res.data\r\n            })\r\n          },\r\n          backToMenu() {\r\n            this.$emit('changeDialogProductPdDblSelect', false)\r\n          },\r\n          findAllDbl(){\r\n            selectAllDblByType(this.buildRequestData({\"pdClmBnftId\": this.pdMedicalClmDetail.id,\"pdDblType\":this.form_dbl.pdDblType})).then(res =>{\r\n              this.dblConfigs=res.data\r\n            })\r\n          },\r\n          showItemButton(){\r\n            this.itemParams.dblTypeCode=this.form_dbl.pdDblType;\r\n            this.itemParams.dblGrdCode=this.form_dbl.pdDblGrade;\r\n            this.findDblItemCode();\r\n            this.showDbl=true\r\n          },\r\n          showItemRow(row){\r\n            this.itemParams.dblTypeCode=row.pdDblType;\r\n            this.itemParams.dblGrdCode=row.pdDblGrade;\r\n            this.findDblItemCode();\r\n            this.showDbl=true\r\n          },\r\n          filterHandler(value, row, column) {\r\n          if (column) {\r\n            const property = column[\"property\"];\r\n            return row[property] === value;\r\n          }\r\n          },\r\n          findDblItemCode(){\r\n              findItemCodeList(this.buildRequestData(this.itemParams)).then(res =>{\r\n                this.dblCodeTable=res.data\r\n              })\r\n          },\r\n          dblTypeChange(val) {\r\n            if (val) {\r\n              selectDblGradeByCode(val).then(res => {\r\n                this.pdDblGradeOptions = res.data;\r\n              });\r\n            }\r\n          },\r\n          //选中一行内容\r\n          selectedDblChanged(row) {\r\n            this.radio = true;\r\n            this.form_dbl.dblTypeEdition = row.codeVersion\r\n            this.findDblType(row.codeVersion)\r\n            this.form_dbl.pdDblRate = row.pdDblRate\r\n            this.form_dbl.pdDblGrade = row.pdDblGrade\r\n            this.form_dbl.pdDblType = row.pdDblType\r\n            this.form_dbl.id = row.id\r\n          },\r\n          rowClick_dbl(row, column, event) {\r\n            if (event.target.nodeName === \"INPUT\") {\r\n              if (row.id === this.selectedDbl.id) {\r\n                this.resetDblForm()\r\n              }\r\n            }\r\n          },\r\n          saveDblForm(){\r\n            for (const dbl of this.pdDblGradeOptions) {\r\n              if (dbl.value === this.form_dbl.pdDblGrade) {\r\n                this.form_dbl.dblGradeId=dbl.id;\r\n                break\r\n              }\r\n            }\r\n            let pdtempPdDbl =JSON.parse(JSON.stringify(this.form_dbl));\r\n            pdtempPdDbl.pdVerId=this.pdMedicalClmDetail.pdVerId;\r\n            pdtempPdDbl.pdClmBnftId=this.pdMedicalClmDetail.id;\r\n            pdtempPdDbl.pdClmBnftCode=this.pdMedicalClmDetail.pdClmBnftCode;\r\n            pdtempPdDbl.dblGradeId = this.form_dbl.pdDblGrade;\r\n            let data = {\r\n              pdtempPdDbl: pdtempPdDbl\r\n            };\r\n            addPdtempPdDbl(this.buildRequestData(data)).then(res => {\r\n              if (res.code == 200) {\r\n                this.notifySuccess(res.message);\r\n                this.findAllDbl();\r\n                this.resetDblForm();\r\n              }\r\n            });\r\n          },\r\n          updateDblForm(){\r\n            for (const dbl of this.pdDblGradeOptions) {\r\n              if (dbl.value === this.form_dbl.pdDblGrade) {\r\n                this.form_dbl.dblGradeId=dbl.id;\r\n                break\r\n              }\r\n            }\r\n            let pdtempPdDbl =JSON.parse(JSON.stringify(this.form_dbl))\r\n            let data = {\r\n              pdtempPdDbl: pdtempPdDbl,\r\n              id:this.form_dbl.id\r\n            };\r\n            updatePdtempPdDbl(this.buildRequestData(data)).then(res => {\r\n              if (res.code == 200) {\r\n                this.notifySuccess(res.message);\r\n                this.findAllDbl();\r\n                this.resetDblForm();\r\n              }\r\n            });\r\n          },\r\n          delDblRow(val){\r\n            deletePdtempPdDbl(val).then(res=>{\r\n              if (res.code == 200) {\r\n                this.notifySuccess(res.message);\r\n                this.findAllDbl();\r\n                this.resetDblForm();\r\n              }\r\n            })\r\n          },\r\n          //重置form\r\n          resetDblForm() {\r\n            this.selectedDbl = '';\r\n            this.form_dbl.pdDblGrade='';\r\n            this.form_dbl.pdDblRate=''\r\n          }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n</style>\r\n"]}]}