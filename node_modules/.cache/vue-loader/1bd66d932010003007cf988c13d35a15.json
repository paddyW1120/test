r\n  overflow: auto;\r\n  background-color: #b5c7e7;\r\n  position: relative;\r\n}\r\n\r\nbody {\r\n  margin: 0px;\r\n  padding: 0;\r\n}\r\n\r\n.login-far {\r\n  position: absolute;\r\n  top: 9%; /* 针对无法使用calc的浏览器 */\r\n  top: calc((100vh - 530px) / 2);\r\n  top: -webkit-calc((100vh - 530px) / 2); /* chrome before 19.0  safari before 6.0*/\r\n  top: -moz-calc((100vh - 530px) / 2); /* chrome before 4.0 */\r\n  bottom: 9%; /* 针对无法使用calc的浏览器 */\r\n  bottom: calc((100vh - 530px) / 2);\r\n  bottom: -webkit-calc((100vh - 530px) / 2); /* chrome before 19.0  safari before 6.0*/\r\n  bottom: -moz-calc((100vh - 530px) / 2); /* chrome before 4.0 */\r\n  white-space: nowrap;\r\n  width: 100%;\r\n}\r\n\r\n\r\n@media screen and (max-width: 980px) {\r\n  body {\r\n    margin: 0px;\r\n    padding: 0;\r\n    overflow: scroll;\r\n  }\r\n\r\n  .login-pro {\r\n    background: url('~@/assets/img/login_pro.png') no-repeat center;\r\n    position: relative;\r\n    background-size: cover;\r\n    margin-left: 0px; /* 针对无法使用calc的浏览器 */\r\n    //margin-left: calc(140px + (100vw - 1397px) / 2);\r\n    //margin-left: -webkit-calc(140px + (100vw - 1397px) / 2); /* chrome before 19.0  safari before 6.0*/\r\n    //margin-left: -moz-calc(140px + (100vw - 1397px) / 2); /* chrome before 4.0 */\r\n    width: 580px;\r\n    height: 530px;\r\n    display: inline-block;\r\n  }\r\n\r\n\r\n  .login-container {\r\n    border-radius: 15px;\r\n    background-clip: padding-box;\r\n    padding: 35px 35px 15px 35px;\r\n    border: 1px solid #eaeaea;\r\n    position: relative;\r\n    background-color: #DAE3F3;\r\n    margin-left: 0px;\r\n    margin-right: 0px;\r\n    width: 400px;\r\n    height: 530px;\r\n    display: inline-block;\r\n  }\r\n}\r\n\r\n\r\n@media screen and (max-width: 1240px) {\r\n  .login-pro {\r\n    background: url('~@/assets/img/login_pro.png') no-repeat center;\r\n    position: relative;\r\n    background-size: cover;\r\n    margin-left: 20px;\r\n    margin-left: calc(140px + (100vw - 1240px) / 2); /* 针对无法使用calc的浏览器 */\r\n    //margin-left: calc(140px + (100vw - 1397px) / 2);\r\n    //margin-left: -webkit-calc(140px + (100vw - 1397px) / 2); /* chrome before 19.0  safari before 6.0*/\r\n    //margin-left: -moz-calc(140px + (100vw - 1397px) / 2); /* chrome before 4.0 */\r\n    width: 580px;\r\n    height: 530px;\r\n    display: inline-block;\r\n  }\r\n\r\n\r\n  .login-container {\r\n    border-radius: 15px;\r\n    background-clip: padding-box;\r\n    padding: 35px 35px 15px 35px;\r\n    border: 1px solid #eaeaea;\r\n    position: relative;\r\n    background-color: #DAE3F3;\r\n    margin-right: calc(100vw - 980px - (140px + (100vw - 1240px) / 2));\r\n    margin-right: 20px;\r\n    width: 400px;\r\n    height: 530px;\r\n    display: inline-block;\r\n  }\r\n}\r\n\r\n\r\n@media screen and (min-width: 1240px) and (max-width: 1397px) {\r\n  .login-pro {\r\n    background: url('~@/assets/img/login_pro.png') no-repeat center;\r\n    position: relative;\r\n    background-size: cover;\r\n    margin-left: 140px; /* 针对无法使用calc的浏览器 */\r\n    //margin-left: calc(140px + (100vw - 1397px) / 2);\r\n    //margin-left: -webkit-calc(140px + (100vw - 1397px) / 2); /* chrome before 19.0  safari before 6.0*/\r\n    //margin-left: -moz-calc(140px + (100vw - 1397px) / 2); /* chrome before 4.0 */\r\n    width: 580px;\r\n    height: 530px;\r\n    display: inline-block;\r\n  }\r\n\r\n\r\n  .login-container {\r\n    border-radius: 15px;\r\n    background-clip: padding-box;\r\n    padding: 35px 35px 15px 35px;\r\n    border: 1px solid #eaeaea;\r\n    position: relative;\r\n    background-color: #DAE3F3;\r\n    margin-left: 220px; /* 针对无法使用calc的浏览器 */\r\n    margin-left: calc(100vw - 1120px - 120px);\r\n    margin-left: -webkit-calc(100vw - 1120px - 120px); /* chrome before 19.0  safari before 6.0*/\r\n    margin-left: -moz-calc(100vw - 1120px - 120px); /* chrome before 4.0 */\r\n    margin-right: 120px;\r\n    width: 400px;\r\n    height: 530px;\r\n    display: inline-block;\r\n  }\r\n}\r\n\r\n@media screen and (min-width: 1397px) {\r\n\r\n  .login-pro {\r\n    background: url('~@/assets/img/login_pro.png') no-repeat center;\r\n    position: relative;\r\n    background-size: cover;\r\n    margin-left: 140px; /* 针对无法使用calc的浏览器 */\r\n    margin-left: calc(140px + (100vw - 1397px) / 2);\r\n    margin-left: -webkit-calc(140px + (100vw - 1397px) / 2); /* chrome before 19.0  safari before 6.0*/\r\n    margin-left: -moz-calc(140px + (100vw - 1397px) / 2); /* chrome before 4.0 */\r\n    width: 580px;\r\n    height: 530px;\r\n    display: inline-block;\r\n  }\r\n\r\n  .login-container {\r\n    border-radius: 15px;\r\n    background-clip: padding-box;\r\n    padding: 35px 35px 15px 35px;\r\n    border: 1px solid #eaeaea;\r\n    position: relative;\r\n    background-color: #DAE3F3;\r\n    margin-left: 220px; /* 针对无法使用calc的浏览器 */\r\n    margin-left: calc(100vw - (140px + (100vw - 1397px) / 2 + 980px) - 120px);\r\n    margin-left: -webkit-calc(100vw - (140px + (100vw - 1397px) / 2 + 980px) - 120px); /* chrome before 19.0  safari before 6.0*/\r\n    margin-left: -moz-calc(100vw - (140px + (100vw - 1397px) / 2 + 980px) - 120px); /* chrome before 4.0 */\r\n    margin-right: 120px;\r\n    width: 400px;\r\n    height: 530px;\r\n    display: inline-block;\r\n  }\r\n}\r\n\r\n\r\n.tips {\r\n  font-size: 14px;\r\n  color: #fff;\r\n  margin-bottom: 10px;\r\n\r\n  span {\r\n    &:first-of-type {\r\n      margin-right: 16px;\r\n    }\r\n  }\r\n}\r\n\r\n.svg-container {\r\n  padding: 6px 5px 6px 15px;\r\n  color: $dark_gray;\r\n  vertical-align: middle;\r\n  width: 30px;\r\n  display: inline-block;\r\n}\r\n\r\n.title-container {\r\n  position: relative;\r\n\r\n  .title {\r\n    font-size: 26px;\r\n    color: #273891;\r\n    margin: 0px auto 40px auto;\r\n    text-align: center;\r\n    font-weight: bold;\r\n  }\r\n\r\n  .set-language {\r\n    position: absolute;\r\n    top: 6px;\r\n    font-size: 18px;\r\n    right: 0px;\r\n    cursor: pointer;\r\n  }\r\n}\r\n\r\n.show-pwd