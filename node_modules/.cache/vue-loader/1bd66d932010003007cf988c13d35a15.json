AACA;AACA","file":"index.vue","sourceRoot":"src/views/login","sourcesContent":["<template>\r\n  <body id=\"poster\">\r\n  <div class=\"login-far\">\r\n    <div class=\"login-pro\"></div>\r\n    <div class=\"login-container\">\r\n      <el-form :model=\"loginForm\" :rules=\"loginRules\" autocomplete=\"on\" class=\"login-form\" label-position=\"left\"\r\n               ref=\"loginForm\">\r\n\r\n        <div class=\"title-container\">\r\n          <h3 class=\"title\">\r\n            {{ $t('login.title') }}\r\n          </h3>\r\n          <lang-select class=\"set-language\"/>\r\n        </div>\r\n\r\n        <el-form-item prop=\"username\">\r\n        <span class=\"svg-container\">\r\n          <svg-icon icon-class=\"user\"/>\r\n        </span>\r\n          <el-input\r\n            :placeholder=\"$t('login.username')\"\r\n            autocomplete=\"on\"\r\n            name=\"username\"\r\n            ref=\"username\"\r\n            tabindex=\"1\"\r\n            type=\"text\"\r\n            v-model=\"loginForm.username\"\r\n            @blur=\"getTenant\"\r\n            class=\"userinput\"\r\n          />\r\n        </el-form-item>\r\n\r\n        <el-tooltip content=\"Caps lock is On\" manual placement=\"right\" v-model=\"capsTooltip\">\r\n          <el-form-item prop=\"password\">\r\n          <span class=\"svg-container\">\r\n            <svg-icon icon-class=\"password\"/>\r\n          </span>\r\n            <el-input\r\n              :key=\"passwordType\"\r\n              :placeholder=\"$t('login.password')\"\r\n              :type=\"passwordType\"\r\n              @blur=\"capsTooltip = false\"\r\n              @keyup.enter.native=\"handleLogin\"\r\n              @keyup.native=\"checkCapslock\"\r\n              autocomplete=\"on\"\r\n              name=\"password\"\r\n              ref=\"password\"\r\n              tabindex=\"2\"\r\n              v-model=\"loginForm.password\"\r\n              class=\"pwdinput\"\r\n            />\r\n            <span @click=\"showPwd\" class=\"show-pwd\">\r\n            <svg-icon :icon-class=\"passwordType === 'password' ? 'eye' : 'eye-open'\"/>\r\n          </span>\r\n          </el-form-item>\r\n        </el-tooltip>\r\n\r\n        <el-form-item prop=\"tenantId\">\r\n        <span class=\"svg-container\">\r\n          <svg-icon icon-class=\"tenant\"/>\r\n        </span>\r\n          <el-select clearable filterable :placeholder=\"$t('workflow.selectTenant')\" style=\"width: 90%\" class=\"tenantinput\"  v-model=\"loginForm.tenantId\">\r\n            <el-option\r\n              v-for=\"item in tenantList\"\r\n              :key=\"item.value\"\r\n              :label=\"item.label\"\r\n              :value=\"item.value\"\r\n             >\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n\r\n        <el-button size=\"small\" :loading=\"loading\" @click.native.prevent=\"handleLogin\" class=\"login_button\"\r\n                   type=\"primary\"\r\n                   v-waves>\r\n          {{ $t('login.logIn') }}\r\n        </el-button>\r\n\r\n        <!-- <div style=\"transform: scale(0.85);margin-left: -58px;\">\r\n        <span @click=\"showDialog=true\">\r\n          <el-image style=\"width: 120px;height: 80px;cursor: pointer;\" fill :src=\"require('../../assets/img/wecom.png')\"\r\n                    fit=\"cover\"></el-image>\r\n        </span>\r\n        </div> -->\r\n      </el-form>\r\n\r\n      <el-dialog :visible.sync=\"showDialog\" center width=\"40%\">\r\n        <social-sign v-if=\"showDialog\"/>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n  </body>\r\n</template>\r\n\r\n<script>\r\nimport { validUsername } from '@/utils/validate'\r\nimport LangSelect from '@/components/LangSelect'\r\nimport SocialSign from './components/SocialSignin'\r\nimport { getTenantByUserCode } from '@/api/system/tenant'\r\nimport { setAesKey, setDebug } from '@/utils/auth'\r\nlet _this\r\nexport default {\r\n  name: 'Login',\r\n  components: { LangSelect, SocialSign },\r\n  data() {\r\n    _this = this;\r\n    const validateUsername = (rule, value, callback) => {\r\n      if (!validUsername(value)) {\r\n        callback(new Error('请输入正确的用户名'))\r\n      } else {\r\n        callback()\r\n      }\r\n    }\r\n    const validatePassword = (rule, value, callback) => {\r\n      if (value.length < 6) {\r\n        callback(new Error('密码不得少于6个字符'))\r\n      } else {\r\n        callback()\r\n      }\r\n    }\r\n    const validateTenantId = (rule, value, callback) => {\r\n      if (!value) {\r\n        callback(new Error('请选择租户'))\r\n      } else {\r\n        callback()\r\n      }\r\n    }\r\n    return {\r\n      loginForm: {\r\n        username: '',\r\n        password: '',\r\n        tenantId: ''\r\n      },\r\n      loginRules: {\r\n        username: [{ required: true, trigger: 'blur', validator: validateUsername }],\r\n        password: [{ required: true, trigger: 'blur', validator: validatePassword }],\r\n        tenantId: [{ required: true, trigger: 'blur', validator: validateTenantId }]\r\n      },\r\n      passwordType: 'password',\r\n      capsTooltip: false,\r\n      loading: false,\r\n      showDialog: false,\r\n      redirect: undefined,\r\n      otherQuery: {},\r\n      tenantList: []\r\n    }\r\n  },\r\n  watch: {\r\n    $route: {\r\n      handler: function(route) {\r\n        const query = route.query\r\n        if (query) {\r\n          this.redirect = query.redirect\r\n          this.otherQuery = this.getOtherQuery(query)\r\n        }\r\n      },\r\n      immediate: true\r\n    }\r\n  },\r\n  created() {\r\n    setDebug('true')\r\n    // window.addEventListener('storage', this.afterQRScan)\r\n  },\r\n  mounted() {\r\n    if (this.loginForm.username === '') {\r\n      this.$refs.username.focus()\r\n    } else if (this.loginForm.password === '') {\r\n      this.$refs.password.focus()\r\n    }\r\n  },\r\n  destroyed() {\r\n    // window.removeEventListener('storage', this.afterQRScan)\r\n  },\r\n  methods: {\r\n    getTenant() {\r\n      if (this.loginForm.username) {\r\n        getTenantByUserCode(this.buildRequestData({ 'userCode': this.loginForm.username })).then(response => {\r\n          let res = response.data\r\n          setAesKey(res.aesKey)\r\n          setDebug(res.debug)\r\n          let tenants = res.tenants\r\n          if (tenants && tenants.length > 0) {\r\n            this.tenantList = []\r\n            this.loginForm.tenantId = ''\r\n            for (let i = 0; i < tenants.length; i++) {\r\n              let tenant = tenants[i]\r\n              this.tenantList.push({\r\n                label: tenant.tenantName,\r\n                value: tenant.tenantId\r\n              })\r\n            }\r\n\r\n            setTimeout(function(){\r\n              _this.loginForm.tenantId = tenants[0].tenantId\r\n            },100)\r\n          }\r\n        })\r\n      }\r\n    },\r\n    checkCapslock(e) {\r\n      const { key } = e\r\n      this.capsTooltip = key && key.length === 1 && (key >= 'A' && key <= 'Z')\r\n    },\r\n    showPwd() {\r\n      if (this.passwordType === 'password') {\r\n        this.passwordType = ''\r\n      } else {\r\n        this.passwordType = 'password'\r\n      }\r\n      this.$nextTick(() => {\r\n        this.$refs.password.focus()\r\n      })\r\n    },\r\n    handleLogin() {\r\n      this.$refs.loginForm.validate(valid => {\r\n        if (valid) {\r\n          this.loading = true\r\n          this.$store.dispatch('user/login', this.loginForm)\r\n            .then(() => {\r\n              this.$store.dispatch('ldcode/storageSelect')\r\n              this.$store.dispatch('product/storageProductsAndPackages')\r\n              this.$store.dispatch('ruleMessage/storageRuleMessage')\r\n              this.$store.dispatch('ruleCache/storageRuleCache')\r\n              this.$router.push({ path: 'dashboard' }).catch(() => {\r\n              })\r\n              this.loading = false\r\n            })\r\n            .catch(error => {\r\n              console.log(error)\r\n              this.loading = false\r\n            })\r\n        } else {\r\n          console.log('error submit!!')\r\n          return false\r\n        }\r\n      })\r\n    },\r\n    getOtherQuery(query) {\r\n      return Object.keys(query).reduce((acc, cur) => {\r\n        if (cur !== 'redirect') {\r\n          acc[cur] = query[cur]\r\n        }\r\n        return acc\r\n      }, {})\r\n    }\r\n    // afterQRScan() {\r\n    //   if (e.key === 'x-admin-oauth-code') {\r\n    //     const code = getQueryObject(e.newValue)\r\n    //     const codeMap = {\r\n    //       wechat: 'code',\r\n    //       tencent: 'code'\r\n    //     }\r\n    //     const type = codeMap[this.auth_type]\r\n    //     const codeName = code[type]\r\n    //     if (codeName) {\r\n    //       this.$store.dispatch('LoginByThirdparty', codeName).then(() => {\r\n    //         this.$router.push({ path: this.redirect || '/' })\r\n    //       })\r\n    //     } else {\r\n    //       alert('第三方登录失败')\r\n    //     }\r\n    //   }\r\n    // }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n/* 修复input 背景不协调 和光标变色 */\r\n/* Detail see https://github.com/PanJiaChen/vue-element-admin/pull/927 */\r\n\r\n$bg: #273891;\r\n$light_gray: #fff;\r\n$cursor: #fff;\r\n\r\n@supports (-webkit-mask: none) and (not (cater-color: $cursor)) {\r\n  .login-container .el-input input {\r\n    color: $cursor;\r\n  }\r\n}\r\n\r\n/* reset element-ui css */\r\n.login-container {\r\n  .el-select {\r\n    .el-input {\r\n      width: 100% !important;\r\n    }\r\n  }\r\n\r\n  .userinput {\r\n    input {\r\n      &:-webkit-autofill {\r\n        box-shadow: 0 0 0px 1000px #ecf5fa inset !important;\r\n        -webkit-text-fill-color: #273891 !important;\r\n      }\r\n    }\r\n  }\r\n\r\n\r\n  .el-input {\r\n    display: inline-block;\r\n    height: 47px;\r\n    width: 85%;\r\n\r\n    input {\r\n      background: transparent;\r\n      border: 0px;\r\n      -webkit-appearance: none;\r\n      border-radius: 0px;\r\n      padd