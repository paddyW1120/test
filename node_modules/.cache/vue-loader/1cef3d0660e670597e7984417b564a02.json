ework-def/pp-main","sourcesContent":["<template>\r\n  <div class=\"box\">\r\n    <cus-wraper>\r\n      <el-collapse v-model=\"activeName\" accordion class=\"collapseCus collapseCusPp\" id=\"boxFixed\" :class=\"{'is_fixed' : isFixed}\">\r\n        <el-collapse-item title=\"产品套餐定义\" name=\"attribute\">\r\n          <div style=\"height: 160px;\" :style=\"{height: stepCount*32 +'px'}\" class=\"stepDiv\">\r\n            <el-steps process-status=\"finish \" finish-status=\"wait\" direction=\"vertical\" :active=\"attrActive\">\r\n              <el-step icon=\"el-icon-info\" title=\"套餐内容定义\"\r\n                       @click.native=\"handleProductAttr(0,'base')\"></el-step>\r\n              <el-step icon=\"el-icon-info\" title=\"套餐方案定义\"\r\n                       @click.native=\"handleProductAttr(1,'ppPlan')\"></el-step>\r\n              <el-step icon=\"el-icon-info\" title=\"基本属性定义\" v-show=\"this.ppAttributeShow\"\r\n                       @click.native=\"handleProductAttr(2,'ppAttribute')\"></el-step>\r\n              <el-step icon=\"el-icon-info\" title=\"保费优惠定义\"\r\n                       @click.native=\"handleProductAttr(3,'ppDiscount')\"></el-step>\r\n              <el-step icon=\"el-icon-info\" title=\"产品套餐保障规则定义\" v-if=\"false\"\r\n                       @click.native=\"handleProductAttr(4,'ppRules')\"></el-step>\r\n            </el-steps>\r\n          </div>\r\n        </el-collapse-item>\r\n        <br/>\r\n      </el-collapse>\r\n\r\n\r\n      <div class=\"package-container\">\r\n        <!--套餐信息定义-->\r\n        <pdppDefinition v-if=\"isActived('base')\" @childFn=\"parentFn\" @showIt=\"showItTe\"  @hook:mounted=\"finishPage\"></pdppDefinition>\r\n        <!--套餐优惠信息定义-->\r\n        <pp-premium-discount v-if=\"isActived('ppDiscount')\" @childFn=\"parentFn\"  @hook:mounted=\"finishPage\"></pp-premium-discount>\r\n        <!--套餐方案定义 -->\r\n        <pdPpScheme v-if=\"isActived('ppPlan')\" @childFn=\"parentFn\"  @hook:mounted=\"finishPage\"></pdPpScheme>\r\n        <!--套餐属性信息定义-->\r\n        <PdPpAttribute v-if=\"isActived('ppAttribute')\" @childFn=\"parentFn\"  @hook:mounted=\"finishPage\"></PdPpAttribute>\r\n      </div>\r\n\r\n      <el-dialog :visible.sync=\"showClause\" @close=\"showClause = false\">\r\n        <clause></clause>\r\n      </el-dialog>\r\n      <div class=\"footer-step\">\r\n        <el-button  size=\"small\"  icon=\"back_step\" v-show=\"!(this.attrActive==0)\" type=\"text\" @click=\"clickStep('back')\" circle>\r\n          <svg-icon icon-class=\"back_step\"/>\r\n          上一步\r\n        </el-button>\r\n        <el-button  size=\"small\" icon=\"next_step\" v-show=\"attrActive!=3\" type=\"text\" @click=\"clickStep('next')\" circle>\r\n          <svg-icon icon-class=\"next_step\"/>\r\n          下一步\r\n        </el-button>\r\n      </div>\r\n\r\n      <div class=\"collapseCus_finishDef is_fixed_finishDef\">\r\n        <el-form>\r\n          <el-form-item :label=\"$t('productMenu.node_selection')\">\r\n            <el-select :disabled=\"showPageFlag=='1'\" clearable filterable style=\"width: 150px;margin-bottom: 10px\"\r\n                       v-model=\"workflow.selectValue\">\r\n              <el-option :disabled=\"item.disabled\" :key=\"item.key\" :label=\"item.label\" :value=\"item.key\"\r\n                         v-for=\"item in workflow.options\"></el-option>\r\n            </el-select>\r\n            <el-button  size=\"small\" :disabled=\"showPageFlag=='1'\" @click=\"finishProductInfoDef\" style=\"width: 150px\" type=\"primary\" dialog-disable=\"disable\">\r\n              <svg-icon icon-class=\"end\"></svg-icon>\r\n              {{$t('productMenu.nextWorkFlow')}}\r\n            </el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n\r\n    </cus-wraper>\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import pdPpScheme from '@/views/product-package-definition/pp-framework-def/pp-program-def/index'\r\n  import PdPpAttribute from '@/views/product-package-definition/pp-framework-def/pp-attr-def/index'\r\n  import ppPremiumDiscount from '@/views/package-revision/package-formula-rev/discount-formula-rev/index'\r\n  import PdppDefinition from \"@/views/product-package-definition/pp-framework-def/pp-info-def/index\";\r\n  import {finishProductInfoDef} from \"@/api/workflow/workflow\";\r\n  import {closeVisitTag} from \"@/utils/tagsView\";\r\n  import {mapGetters} from \"vuex\";\r\n  import {querypdPpInfo} from \"@/api/basicinfo/pdppDefinition\";\r\n  export default {\r\n    name: 'profuctPpMenu',\r\n    computed: {\r\n      ...mapGetters([\"usercode\", \"tenantCode\"])\r\n    },\r\n    data() {\r\n      return {\r\n        currentPage: \"base\",\r\n        stepCount: 3,\r\n        ppAttributeShow: true,\r\n        pdPpInfo: { },\r\n        showPageFlag:'',\r\n        workflow: {\r\n          processInstanceId: \"\",\r\n          selectValue: \"productOperatingFlag\",\r\n          options: [\r\n            {key: \"productOperatingFlag\", label: \"产品套餐营运定义\"},\r\n            {key: \"productTestFlag\", label: \"产品套餐测试\"}\r\n          ]\r\n        },\r\n        isFixed: false,\r\n        offsetTop: 20,\r\n        stepsAttr: {\r\n          attribute: [{'base': 0}, {'ppPlan': 1},{'ppAttribute':2},{'ppDiscount':3},{'ppRules':4}],\r\n        },\r\n        attrActive: 0,\r\n        activeName: 'attribute',\r\n        activedNames: ['attribute'],\r\n        showClause: false,\r\n      }\r\n    },\r\n    components: {\r\n      PdppDefinition,\r\n      pdPpScheme,\r\n      PdPpAttribute,\r\n      ppPremiumDiscount\r\n    },\r\n    mounted() {\r\n      this.$emit('finish')\r\n      window.addEventListener('scroll', this.initHeight)\r\n      this.$nextTick(() => {\r\n        this.offsetTop = document.querySelector('#boxFixed').offsetTop\r\n      })\r\n    },\r\n    async created() {\r\n      this.showPageFlag = this.$route.query.showPageFlag;\r\n      this.pdPpInfo = this.getPdData();\r\n      this.currentPage = \"base\";\r\n      if (this.pdPpInfo.ppType == \"N\") {\r\n        this.ppAttributeShow=true;\r\n        this.stepCount = 4\r\n      }else {\r\n        this.ppAttributeShow=false;\r\n      }\r\n    },\r\n    methods: {\r\n      finishPage(){\r\n        this.$emit('finish')\r\n      },\r\n        showItTe(data){\r\n            if (data) {\r\n                this.ppAttributeShow=true;\r\n                this.stepCount = 4\r\n            }else {\r\n                this.ppAttributeShow=false;\r\n            }\r\n        },\r\n      clickStep(type) {\r\n        let _currentName = this.attrActive;\r\n        querypdPpInfo(this.buildRequestData({\"ppInfoId\": this.pdPpInfo.id})).then(res => {\r\n          this.pdPpInfo = res.data;\r\n            this.cacheCurrPagePdData(this.pdPpInfo)\r\n        })\r\n        if (this.pdPpInfo.ppType == \"N\") {\r\n          this.ppAttributeShow=true;\r\n        }else {\r\n          this.ppAttributeShow=false;\r\n        }\r\n        if (type === \"next\") {\r\n          if (_currentName == 0) {\r\n            this.handleProduct('attribute', 1, 'ppPlan')\r\n            return;\r\n          } else if (_currentName == 1) {\r\n            if (this.ppAttributeShow) {\r\n              this.handleProduct('attribute', 2, 'ppAttribute')\r\n            }else {\r\n              this.handleProduct('attribute', 3, 'ppDiscount')\r\n            }\r\n            return;\r\n          } else if (_currentName == 2) {\r\n            this.handleProduct('attribute',3, 'ppDiscount')\r\n            return;\r\n          } else if (_currentName == 3) {\r\n            // this.handleProduct('attribute', 4, 'ppRules')\r\n            return;\r\n          }\r\n        } else {\r\n          if (_currentName == 1) {\r\n            this.handleProduct('attribute',0, 'base')\r\n            return;\r\n          } else if (_currentName == 2) {\r\n            this.handleProduct('attribute', 1, 'ppPlan')\r\n            return;\r\n          } else if (_currentName == 3) {\r\n            if (this.ppAttributeShow) {\r\n              this.handleProduct('attribute', 2, 'ppAttribute')\r\n            }else {\r\n              this.handleProduct('attribute', 1, 'ppPlan')\r\n            }\r\n            return;\r\n          } else if (_currentName == 4) {\r\n            this.handleProduct('attribute', 3, 'ppDiscount')\r\n            return;\r\n          }\r\n        }\r\n      },\r\n      handleProduct(type, index, page) {\r\n        this.currentPage = page\r\n        this.activeName = type\r\n        if (type == 'attribute') { //产品属性定义\r\n          this.attrActive = parseInt(index)\r\n        }\r\n      },\r\n      handleProductAttr(data, page) {\r\n        this.handleProduct('attribute', data, page)\r\n      },\r\n\r\n      parentFn(val) {\r\n        this.activeName = val\r\n      },\r\n\r\n      isActived(flag) {\r\n        return this.currentPage == flag\r\n      },\r\n      initHeight() {\r\n        let scrollTop = window.pageYOffset || document.documentElement.scrollTop || document.body.scrollTop\r\n        this.isFixed = scrollTop > this.offsetTop ? true : false\r\n      },\r\n      finishProductInfoDef() {\r\n        if (this.workflow.selectValue === \"\") {\r\n          this.messageError(this.$t(\"productMenu.tipError\"));\r\n          return;\r\n        }\r\n        this.$confirm(\r\n          this.$t(\"productMenu.warnMsg\"),\r\n          this.$t(\"productMenu.messageTip\"),\r\n          {\r\n            confirmButtonText: this.$t(\"productMenu.warnMsgButton2\"),\r\n            cancelButtonText: this.$t(\"productMenu.warnMsgButton1\"),\r\n            type: \"warning\"\r\n          }\r\n        ).then(() => {\r\n          let data = {\r\n            userCode: this.usercode,\r\n            productOperatingFlag: \"1\",\r\n            productTestFlag: \"1\",\r\n            processInstanceId: this.pdPpInfo.pdProcessId\r\n          };\r\n          data[this.workflow.selectValue] = \"0\";\r\n          finishProductInfoDef(this.buildRequestData(data)).then(res => {\r\n            this.notifySuccess(\"success\");\r\n            this.$store.dispatch('product/storagePackages')\r\n            closeVisitTag();\r\n            this.$router.push({path: \"/dashboard\"});\r\n          });\r\n        });\r\n      },\r\n    },\r\n    destroyed() {\r\n      window.removeEventListener('scroll', this.handleScroll)\r\n    },\r\n\r\n  }\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n\r\n  .collapseCusPp {\r\n    z-index: 99;\r\n    top: 20px;\r\n    right: 10px;\r\n    position: absolute;\r\n    border-top: none;\r\n    border-bottom: none;\r\n    margin-left: 50px;\r\n    width: 120px;\r\n  }\r\n\r\n  .footer-step {\r\n    text-align: center;\r\n    position: absolute;\r\n    bottom: 0;\r\n    right: 45%;\r\n  }\r\n\r\n  .is_fixed {\r\n    position: fixed;\r\n    top: 20px;\r\n    z-index: 99;\r\n  }\r\n\r\n  .cusProcess {\r\n    .el-card {\r\n      margin-bottom: 5px;\r\n      width: 130px;\r\n      cursor: pointer;\r\n      border-radius: 8px;\r\n    }\r\n\r\n    .el-card__body {\r\n      padding: 12px;\r\n      text-align: center;\r\n    }\r\n  }\r\n\r\n  .opSetting {\r\n    text-align: center;\r\n    margin-top: 30px;\r\n  }\r\n\r\n  .spacing {\r\n    margin-top: 10px;\r\n  }\r\n\r\n  .el-collapse-item {\r\n    margin-bottom: 10px;\r\n  }\r\n\r\n  .collapseCus_finishDef {\r\n    z-index: 99;\r\n    bottom: 20px;\r\n    right: 10px;\r\n    position: absolute;\r\n    border-top: none;\r\n    border-bottom: none;\r\n    margin-left: 50px;\r\n    width: 140px;\r\n    transform: scale(0.85);\r\n  }\r\n\r\n\r\n  .is_fixed_finishDef {\r\n    position: fixed;\r\n    bottom: 1px;\r\n    z-index: 99;\r\n  }\r\n\r\n</style>\r\n"]}]}